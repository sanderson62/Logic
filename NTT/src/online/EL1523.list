        MOVE CL-SOC-SEC-NO      TO SS46D.                        
                                                                 
    MOVE CL-CERT-GROUPING       TO W-CERT-GROUPING               
                                   W-ACCT-GROUPING               
                                   W-PROD-GROUPING               
                                   W-PLCY-GROUPING               
                                   W-PLAN-GROUPING.              
    MOVE CL-CERT-STATE          TO W-CERT-STATE                  
                                   W-ACCT-STATE                  
                                   W-PROD-STATE                  
                                   W-PLCY-STATE                  
                                   W-PLAN-STATE.                 
    MOVE CL-CERT-ACCOUNT        TO W-CERT-ACCOUNT                
                                   W-ACCT-ACCOUNT                
                                   W-PROD-PRODUCER               
                                   W-PLCY-PRODUCER               
                                   W-PLAN-PRODUCER.              
    MOVE CL-CERT-EFF-DT         TO W-CERT-EFF-DT                 
                                   W-PLCY-EFF-DT.                
    MOVE CL-CV-REFERENCE-NO     TO W-PLCY-REFERENCE-NO.          
                                                                 
    PERFORM 7400-MOVE-NAME THRU 7400-EXIT.                       
                                                                 
