         MOVE 'ONLINE '       TO P-LET-ORIGIN.                   
    IF  OFFLINE-CREATION                                         
         MOVE 'OFFLINE'       TO P-LET-ORIGIN.                   
    IF  INSURED-ADDRESEE                                         
         MOVE 'INSURED'       TO P-LET-ADSEE.                    
    IF  BENEFICIARY-ADDRESEE                                     
         MOVE 'BENEFICIARY'   TO P-LET-ADSEE.                    
    IF  ACCOUNT-ADDRESEE                                         
         MOVE 'ACCOUNT'       TO P-LET-ADSEE.                    
    IF  PHYSICIAN-ADDRESEE                                       
         MOVE 'PHYSICIAN'     TO P-LET-ADSEE.                    
    IF  EMPLOYER-ADDRESEE                                        
         MOVE 'EMPLOYER'      TO P-LET-ADSEE.                    
    IF  OTHER-ADDRESEE-1                                         
         MOVE 'OTHER 1'       TO P-LET-ADSEE.                    
    IF  OTHER-ADDRESEE-2                                         
         MOVE 'OTHER 2'       TO P-LET-ADSEE.                    
                                                                 
    MOVE DSP           TO WS-PASSED-CNTL-CHAR.                   
    MOVE  P-LET-LINE-1 TO WS-PASSED-DATA.                        
    PERFORM ELPRTCVP  THRU ELPRTCVP-EXIT.                        
                                                                 
    MOVE SSP           TO WS-PASSED-CNTL-CHAR.                   
    MOVE  P-LET-LINE-2 TO WS-PASSED-DATA.                        
    PERFORM ELPRTCVP  THRU ELPRTCVP-EXIT.                        
                                                                 
    MOVE SSP           TO WS-PASSED-CNTL-CHAR.                   
    MOVE  P-LET-LINE-3 TO WS-PASSED-DATA.                        
    PERFORM ELPRTCVP  THRU ELPRTCVP-EXIT.                        
                                                                 
    MOVE SSP           TO WS-PASSED-CNTL-CHAR.                   
    MOVE  P-LET-LINE-4 TO WS-PASSED-DATA.                        
    PERFORM ELPRTCVP  THRU ELPRTCVP-EXIT.                        
                                                                 
