    MOVE 'DLO034' TO DFHEIV1                                     
*>    MOVE '."C                   (   #00001515' TO DFHEIV0        
    MOVE X'2E2243202020202020202020' &                           
         X'202020202020202020202820' &                           
         X'2020233030303031353135' TO DFHEIV0                    
    CALL 'kxdfhei1' USING DFHEIV0,                               
          DFHEIV1,                                               
          DLO034-COMMUNICATION-AREA,                             
          DLO034-REC-LENGTH,                                     
          DFHEIV99,                                              
          DFHEIV99,                                              
          DFHEIV99,                                              
          DFHEIV99                                               
    GO TO 9999-DFHEXIT DEPENDING ON DFHEIGDK                     
                                                                 
           IF DL34-RETURN-CODE = 'OK'                            
               GO TO ELPRTCVP-EXIT                               
           ELSE                                                  
*>               MOVE '8339'     TO EMI-ERROR ?????ERROR MESSAGE???
               GO TO ELPRTCVP-EXIT.                              
                                                                 
    IF NOT FIRST-TIME                                            
        GO TO ELPRTCVP-020.                                      
                                                                 
    IF WS-LINE-LEN NOT GREATER ZERO                              
        GO TO ELPRTCVP-EXIT.                                     
                                                                 
    MOVE '2'                    TO WS-FIRST-TIME-SW.             
    MOVE LOW-VALUES             TO WS-BUFFER-AREA.               
                                                                 
    SET BUFFER-INDEX TO +1                                       
                                                                 
    IF EIBTRMID IS EQUAL TO 'AFLP'                               
        NEXT SENTENCE                                            
    ELSE                                                         
        IF NOT TOP-PAGE                                          
            MOVE T-TP           TO WS-BUFFER-BYTE (BUFFER-INDEX) 
            SET BUFFER-INDEX UP BY +1.                           
                                                                 
ELPRTCVP-020.                                                    
    IF WS-PROG-END = SPACES                                      
        GO TO ELPRTCVP-030.                                      
                                                                 
    MOVE SPACES                 TO WS-PROG-END.                  
                                                                 
    IF BUFFER-INDEX GREATER +1                                   
        PERFORM ELPRTCVP-PRINT-BUFFER THRU ELPRTCVP-PRINT-EXIT.  
                                                                 
    MOVE '1'                    TO WS-FIRST-TIME-SW.             
                                                                 
    GO TO ELPRTCVP-EXIT.                                         
                                                                 
ELPRTCVP-030.                                                    
    IF WS-PASSED-DATA = SPACES                                   
        SET PRT-INDEX TO +1                                      
        GO TO ELPRTCVP-050.                                      
                                                                 
    SET PRT-INDEX TO WS-LINE-LEN.                                
                                                                 
ELPRTCVP-040.                                                    
    IF WS-PRINT-BYTE (PRT-INDEX) NOT = SPACES                    
        GO TO ELPRTCVP-050.                                      
                                                                 
    IF PRT-INDEX GREATER +1                                      
        SET PRT-INDEX DOWN BY +1                                 
        GO TO ELPRTCVP-040.                                      
                                                                 
ELPRTCVP-050.                                                    
    SET WS-LINE-LENGTH TO PRT-INDEX.                             
    SET BUFFER-INDEX2 TO BUFFER-INDEX.                           
    SET BUFFER-INDEX2 UP BY WS-LINE-LENGTH.                      
                                                                 
    IF BUFFER-INDEX2 NOT LESS WS-BUFFER-SIZE                     
        PERFORM ELPRTCVP-PRINT-BUFFER THRU ELPRTCVP-PRINT-EXIT.  
                                                                 
    IF TRIPLE-SPACE                                              
         ADD +2  TO  WS-LINE-CNT                                 
         MOVE T-SS           TO WS-BUFFER-BYTE (BUFFER-INDEX)    
                                WS-BUFFER-BYTE (BUFFER-INDEX + 1)
         SET BUFFER-INDEX UP BY +2.                              
                                                                 
    IF DOUBLE-SPACE                                              
         ADD +1  TO  WS-LINE-CNT                                 
         MOVE T-SS             TO WS-BUFFER-BYTE (BUFFER-INDEX)  
         SET BUFFER-INDEX UP BY +1.                              
                                                                 
    ADD +1 TO WS-LINE-CNT                                        
*>***********************************************************      
*>     BYPASS NEW LINE SYMBOL                               *      
*>        IF FIRST BUFFER SENT AND TOP-OF-FORM SET.         *      
*>     OR IF FIRST LINE OF SUBSEQUENT BUFFERS.              *      
*>***********************************************************      
                                                                 
    IF (BUFFER-INDEX GREATER +1 AND                              
        WS-BUFFER-BYTE (BUFFER-INDEX - 1) = T-TP)  OR            
        FIRST-LINE-NEXT-BUFFER                                   
        MOVE ZERO               TO WS-FIRST-TIME-SW              
    ELSE                                                         
        MOVE T-SS               TO WS-BUFFER-BYTE (BUFFER-INDEX) 
        SET BUFFER-INDEX UP BY +1.                               
                                                                 
*>*   NOTE, SINGLE SPACE IS REQUIRED BEFORE TOP PAGE CHAR          
                                                                 
    IF TOP-PAGE                                                  
        MOVE +1                TO WS-LINE-CNT                    
        MOVE T-TP              TO WS-BUFFER-BYTE (BUFFER-INDEX)  
        SET BUFFER-INDEX UP BY +1.                               
                                                                 
    SET PRT-INDEX TO +1.                                         
                                                                 
ELPRTCVP-060.                                                    
    MOVE WS-PRINT-BYTE (PRT-INDEX)                               
                                TO WS-BUFFER-BYTE (BUFFER-INDEX).
    SET BUFFER-INDEX UP BY +1.                                   
                                                                 
    IF PRT-INDEX LESS WS-LINE-LENGTH                             
        SET PRT-INDEX UP BY +1                                   
        GO TO ELPRTCVP-060.                                      
                                                                 
ELPRTCVP-EXIT.                                                   
    EXIT.                                                        
                                                                 
ELPRTCVP-PRINT-BUFFER.                                           
    IF WS-BUFFER-BYTE (BUFFER-INDEX - 1) = T-SS                  
       MOVE SPACE               TO WS-BUFFER-BYTE (BUFFER-INDEX) 
       SET BUFFER-INDEX UP BY 1.                                 
                                                                 
    MOVE  T-EM                  TO  WS-BUFFER-BYTE (BUFFER-INDEX)
    SET WS-BUFFER-LENGTH TO BUFFER-INDEX.                        
                                                                 
                                                                 
