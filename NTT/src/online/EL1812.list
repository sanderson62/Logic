*>                     M O V E   N A M E   R O U T I N E         * 
*>                                                               * 
*>                THE FOLLOWING ROUTINE MOVES THE INSURRED'S     * 
*>            NAME FROM THE CLAIM MASTER TO A WORK AREA WITH     * 
*>            NO EMBEDDED BLANKS.                                * 
*>                                                               * 
*>                  FIELD               VALUE                    * 
*>                                                               * 
*>                LAST NAME (CL15)      SMITH                    * 
*>                1ST NAME  (CL12)      JOHN                     * 
*>                MID NAME  (CL12)      ALLEN                    * 
*>                                                               * 
*>                AFTER NAME HAS BEEN MOVED WS-NAME-WORK (CL30)  * 
*>                                                               * 
*>                        SMITH, JOHN ALLEN                      * 
*>                                                               * 
*>                TO USE THIS ROUTINE YOU ALSO NEED A WORKING    * 
*>            STORAGE COPYBOOK:                                  * 
*>                                                               * 
*>                01  WS-NAME-WORK-AREA COPY ELCNWA.             * 
*>                                                               * 
*>****************************************************************.
                                                                 
    MOVE SPACES                 TO  WS-NAME-WORK-AREA.           
    MOVE ZERO                   TO  WS-NAME-SW.                  
    SET NWA-INDEX TO +1.                                         
                                                                 
    IF CL-INSURED-1ST-NAME = SPACES  AND                         
       CL-INSURED-MID-INIT = SPACES                              
        MOVE +1                 TO  WS-NAME-SW.                  
                                                                 
    MOVE CL-INSURED-LAST-NAME  TO  WS-NAME-WORK2.                
    PERFORM 5100-MOVE-NAME THRU 5190-EXIT.                       
                                                                 
    MOVE CL-INSURED-1ST-NAME   TO  WS-NAME-WORK2.                
    PERFORM 5100-MOVE-NAME THRU 5190-EXIT.                       
                                                                 
    SET NWA-INDEX UP BY +1.                                      
    MOVE CL-INSURED-MID-INIT   TO  WS-NAME-WORK2.                
    PERFORM 5100-MOVE-NAME THRU 5190-EXIT.                       
                                                                 
5000-EXIT.                                                       
    EXIT.                                                        
                                                                 
    EJECT                                                        
5100-MOVE-NAME SECTION.                                          
    IF WS-NAME-SW GREATER THAN +1                                
        GO TO 5190-EXIT.                                         
                                                                 
    IF WS-NAME-WORK2 = SPACES                                    
        GO TO 5190-EXIT.                                         
                                                                 
    SET NWA-INDEX2 TO +1.                                        
    SET NWA-INDEX3 TO +2.                                        
                                                                 
5110-MOVE-NAME.                                                  
    MOVE WS-NW2 (NWA-INDEX2)  TO  WS-NW (NWA-INDEX).             
                                                                 
    IF NWA-INDEX LESS THAN +30                                   
        SET NWA-INDEX UP BY +1                                   
      ELSE                                                       
        ADD +2  TO  WS-NAME-SW                                   
        GO TO 5190-EXIT.                                         
                                                                 
    IF NWA-INDEX2 LESS THAN +20                                  
        SET NWA-INDEX3 UP BY +1                                  
        SET NWA-INDEX2 UP BY +1.                                 
                                                                 
    IF WS-NW2 (NWA-INDEX2) = SPACES AND                          
       WS-NW2 (NWA-INDEX3) = SPACES                              
        IF WS-NAME-SW = ZERO                                     
            MOVE ','            TO  WS-NW (NWA-INDEX)            
            SET NWA-INDEX UP BY +2                               
            MOVE +1             TO  WS-NAME-SW                   
            GO TO 5190-EXIT                                      
          ELSE                                                   
            GO TO 5190-EXIT.                                     
                                                                 
    GO TO 5110-MOVE-NAME.                                        
                                                                 
5190-EXIT.                                                       
    EXIT.                                                        
                                                                 
