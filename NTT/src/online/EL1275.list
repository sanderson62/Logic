    IF CM-AH-PAID-THRU-DT IS NOT = LOW-VALUES AND SPACES         
       IF NOT PI-USES-PAID-TO                                    
          MOVE ' '                    TO  DC-OPTION-CODE         
          MOVE CM-AH-PAID-THRU-DT     TO  DC-BIN-DATE-1          
          PERFORM 8500-DATE-CONVERSION                           
          MOVE DC-GREG-DATE-1-EDIT    TO  EPDTHRUO               
       ELSE                                                      
          MOVE '6'                    TO  DC-OPTION-CODE         
          MOVE CM-AH-PAID-THRU-DT     TO  DC-BIN-DATE-1          
          MOVE +1                     TO  DC-ELAPSED-DAYS        
          MOVE +0                     TO  DC-ELAPSED-MONTHS      
          PERFORM 8500-DATE-CONVERSION                           
          MOVE DC-GREG-DATE-1-EDIT    TO  EPDTHRUO.              
                                                                 
*>*****************************************************************
*>                                                                *
*>               DISPLAY ENTRY STATUS CODES                       *
*>                                                                *
*>*****************************************************************
                                                                 
    MOVE SPACES                 TO  EENTSTO.                     
                                                                 
    IF CM-ENTRY-STATUS = '1'                                     
        MOVE 'NORM'             TO  EENTSTO.                     
    IF CM-ENTRY-STATUS = '2'                                     
        MOVE 'PEND'             TO  EENTSTO                      
        MOVE 'P'                TO  PI-PEND-SW.                  
    IF CM-ENTRY-STATUS = '4'                                     
        MOVE 'CONV'             TO  EENTSTO.                     
    IF CM-ENTRY-STATUS = '5'                                     
        MOVE 'REIS'             TO  EENTSTO.                     
    IF CM-ENTRY-STATUS = 'M'                                     
        MOVE 'MONTHLY'          TO  EENTSTO.                     
    IF CM-ENTRY-STATUS = '9'                                     
        MOVE 'REIN'             TO  EENTSTO.                     
                                                                 
    IF CM-ENTRY-STATUS = 'D'                                     
        MOVE 'DECL'             TO  EENTSTO.                     
                                                                 
    IF CM-ENTRY-STATUS = 'V'                                     
        MOVE 'VOID'             TO  EENTSTO.                     
                                                                 
    IF CM-ENTRY-STATUS = 'U'                                     
        MOVE 'UNDERWRT'         TO  EENTSTO.                     
                                                                 
*>*****************************************************************
*>                                                                *
*>               DISPLAY CURRENT STATUS CODES                     *
*>                                                                *
*>*****************************************************************
                                                                 
    MOVE SPACES                 TO  ECURSTO.                     
                                                                 
    IF CM-AH-CURRENT-STATUS = '1' OR '4'                         
        IF CP-REMAINING-TERM-3 = ZEROS                           
            MOVE 'EXPIRED'      TO  ECURSTO                      
        ELSE                                                     
            MOVE 'ACTIVE'       TO  ECURSTO.                     
                                                                 
    IF CM-AH-CURRENT-STATUS = '2'                                
        MOVE 'PEND'             TO  ECURSTO                      
        MOVE 'P'                TO  PI-PEND-SW.                  
                                                                 
    IF CM-AH-CURRENT-STATUS = '3'                                
        MOVE 'RESTORE'          TO  ECURSTO.                     
                                                                 
    IF CM-AH-CURRENT-STATUS = '5'                                
        MOVE 'REISSUE'          TO  ECURSTO.                     
    IF CM-AH-CURRENT-STATUS = 'M'                                
        MOVE 'MONTHLY'          TO  ECURSTO.                     
                                                                 
    IF CM-AH-CURRENT-STATUS = '6'                                
        MOVE 'LMP DIS'          TO  ECURSTO.                     
                                                                 
    IF CM-AH-CURRENT-STATUS = '7'                                
        MOVE 'DEATH'            TO  ECURSTO.                     
                                                                 
    IF CM-AH-CURRENT-STATUS = '8'                                
        MOVE 'CANCEL'           TO  ECURSTO.                     
                                                                 
    IF CM-AH-CURRENT-STATUS = '9'                                
        MOVE 'RE-ONLY'          TO  ECURSTO.                     
                                                                 
    IF CM-AH-CURRENT-STATUS = 'D'                                
        MOVE 'DECLINE'          TO  ECURSTO.                     
                                                                 
    IF CM-AH-CURRENT-STATUS = 'V'                                
        MOVE 'VOID'             TO  ECURSTO.                     
                                                                 
*>*****************************************************************
*>                                                                *
*>            CALCULATE REMAINING BENEFIT AMOUNT                  *
*>                                                                *
*>*****************************************************************
                                                                 
    IF CM-AH-CURRENT-STATUS = '6' OR '7' OR '8'                  
        MOVE ZEROS              TO  EREMBENO                     
    ELSE                                                         
       PERFORM 2500-GET-ERACCT  THRU 2500-EXIT                   
       MOVE ZEROS               TO CP-R-MAX-MON-BEN              
       IF (PI-COMPANY-ID = 'DCC' OR 'CAP')                       
          AND (ACCT-FOUND)                                       
          AND (AM-DCC-PRODUCT-CODE = 'DDF')                      
          MOVE ' '             TO WS-PDEF-RECORD-SW              
          PERFORM 2600-GET-MAX-BENEFIT                           
                                THRU 2600-EXIT                   
          IF PDEF-FOUND                                          
             IF CM-INSURED-JOINT-AGE LESS CM-INSURED-ISSUE-AGE   
                 MOVE CM-INSURED-JOINT-AGE TO WS-EDIT-AGE        
             ELSE                                                
                 MOVE CM-INSURED-ISSUE-AGE TO WS-EDIT-AGE        
             END-IF                                              
             COMPUTE WS-ATT-AGE = WS-EDIT-AGE                    
                + ((CM-LF-ORIG-TERM - CP-REMAINING-TERM-3) / 12) 
                                                                 
             PERFORM VARYING P1 FROM +1 BY +1 UNTIL              
                (P1 > +11)                                       
                OR (PD-PROD-CODE (P1) = 'A'                      
                  AND PD-MAX-ATT-AGE (P1) >= WS-ATT-AGE )        
             END-PERFORM                                         
             IF P1 < +12                                         
                MOVE PD-MAX-AMT (P1) TO CP-R-MAX-MON-BEN         
             END-IF                                              
          END-IF                                                 
       END-IF                                                    
       IF CP-R-MAX-MON-BEN = ZEROS                               
          MULTIPLY CM-AH-BENEFIT-AMT BY CP-REMAINING-TERM-3      
                                   GIVING EREMBENO               
       ELSE                                                      
          MULTIPLY CP-R-MAX-MON-BEN BY CP-REMAINING-TERM-3       
                                   GIVING EREMBENO               
       END-IF                                                    
    END-IF                                                       
                                                                 
*>*****************************************************************
*>                                                                *
*>            CALCULATE UNEARNED PREMIUM AMOUNT                   *
*>                      ONE MONTH EARNED PREMIUM AMOUNT           *
*>                                                                *
*>*****************************************************************
                                                                 
    MOVE '2'                    TO  CP-PROCESS-TYPE.             
    MOVE CM-STATE               TO  CP-STATE.                    
    MOVE CM-RATE-CLASS          TO  CP-CLASS-CODE.               
    MOVE CM-AH-DEV-CODE         TO  CP-DEVIATION-CODE.           
    MOVE CM-INSURED-ISSUE-AGE   TO  CP-ISSUE-AGE.                
    MOVE CM-AH-BENEFIT-AMT      TO  CP-ORIGINAL-BENEFIT          
                                    CP-RATING-BENEFIT-AMT.       
    MOVE CM-AH-PREMIUM-AMT      TO  CP-ORIGINAL-PREMIUM.         
    MOVE CM-LOAN-APR            TO  CP-LOAN-APR.                 
    MOVE CM-LOAN-TERM           TO  CP-LOAN-TERM.                
                                                                 
    MOVE PI-COMPANY-ID          TO  WS-CFK-COMPANY-ID.           
    MOVE '1'                    TO  WS-CFK-RECORD-TYPE.          
    MOVE SPACES                 TO  WS-CFK-ACCESS.               
    MOVE +0                     TO  WS-CFK-SEQUENCE-NO.          
    PERFORM 3500-READ-CNTL THRU 3500-EXIT.                       
    MOVE CF-AH-OVERRIDE-L1      TO  CP-AH-OVERRIDE-CODE.         
    MOVE CF-CR-R78-METHOD       TO  CP-R78-OPTION.               
    MOVE PI-REM-TRM-CALC-OPTION TO  CP-REM-TRM-CALC-OPTION.      
    MOVE CF-CR-REM-TERM-CALC    TO  CP-REM-TERM-METHOD.          
                                                                 
    MOVE '3'                    TO  WS-CFK-RECORD-TYPE.          
    MOVE PI-AH-OVERRIDE-L1      TO  WS-BENEFIT-TYPE.             
    MOVE CM-AH-BENEFIT-CD       TO  WS-BENEFIT-NO.               
    MOVE SPACES                 TO  WS-CFK-ACCESS.               
    MOVE CM-STATE               TO  WS-CFK-STATE.                
    MOVE +0                     TO  WS-CFK-SEQUENCE-NO.          
    PERFORM 3500-READ-CNTL THRU 3500-EXIT.                       
    MOVE CF-STATE-ABBREVIATION  TO  CP-STATE-STD-ABBRV.          
    MOVE CF-ST-FREE-LOOK-PERIOD TO  CP-FREE-LOOK.                
                                                                 
    IF CP-STATE-STD-ABBRV = 'OR'                                 
        COMPUTE CP-RATING-BENEFIT-AMT = CM-AH-BENEFIT-AMT *      
                                        CM-AH-ORIG-TERM.         
                                                                 
*>    IF CF-TRUNCATED-LIFE (WS-INDEX)                              
*>        MOVE CM-PAY-FREQUENCY   TO  CP-PAY-FREQUENCY             
*>        MOVE CF-CO-OVRD-EARNINGS-CALC (WS-INDEX) TO              
*>                                           CP-EARNING-METHOD.    
                                                                 
    MOVE CM-LOAN-1ST-PMT-DT     TO  CP-FIRST-PAY-DATE.           
                                                                 
    IF WS-VALUATION-DT IS EQUAL TO LOW-VALUES                    
        MOVE PI-CR-MONTH-END-DT TO  CP-VALUATION-DT              
                                    WS-VALUATION-DT              
    ELSE                                                         
        MOVE WS-VALUATION-DT    TO  CP-VALUATION-DT.             
                                                                 
    PERFORM 9800-LINK-REM-TERM.                                  
    MOVE CP-REMAINING-TERM-1    TO  CP-REMAINING-TERM.           
    PERFORM 9850-LINK-REFUND-RTN THRU 9850-EXIT.                 
    MOVE CP-CALC-REFUND         TO  EUEPREMO                     
                                    WS-CALC-REFUND.              
    ADD +1                      TO  CP-REMAINING-TERM.           
    IF CP-REMAINING-TERM IS EQUAL TO CM-AH-ORIG-TERM             
        COMPUTE ONE-MON-EARNED = CM-AH-PREMIUM-AMT -             
                                               CP-CALC-REFUND    
    ELSE                                                         
        PERFORM 9850-LINK-REFUND-RTN THRU 9850-EXIT              
        COMPUTE ONE-MON-EARNED = WS-CALC-REFUND -                
                                               CP-CALC-REFUND.   
                                                                 
    MOVE ONE-MON-EARNED         TO  EOMEARNO.                    
                                                                 
    IF WS-VALUATION-DT IS NOT = LOW-VALUES AND SPACES            
        MOVE SPACES                 TO  DC-OPTION-CODE           
        MOVE WS-VALUATION-DT        TO  DC-BIN-DATE-1            
        PERFORM 8500-DATE-CONVERSION                             
        MOVE AL-UANON               TO  EUEDTA                   
        MOVE DC-GREG-DATE-1-EDIT    TO  EUEDTO.                  
                                                                 
*>*****************************************************************
*>                                                                *
*>                     DISPLAY CANCEL DATE                        *
