*>                               (WS-DAILY-RATE * PI-CDAYS)        
*>            MOVE WS-DAILY-RATE TO PI-DAILY-RATE                  
*>            GO TO 1420-CHECK-AIG-BENEFIT.                        
                                                                 
    IF WS-CALC-METHOD = '1'                                      
        COMPUTE WS-DAILY-RATE ROUNDED = CM-AH-BENEFIT-AMT / 30   
        COMPUTE PI-CPYAMT ROUNDED =                              
            (CM-AH-BENEFIT-AMT * DC-ELAPSED-MONTHS) +            
            (WS-DAILY-RATE     * DC-ODD-DAYS-OVER)               
        MOVE WS-DAILY-RATE    TO PI-DAILY-RATE                   
        GO TO 1420-CHECK-AIG-BENEFIT.                            
                                                                 
    IF WS-CALC-METHOD = '2'                                      
        COMPUTE PI-DAILY-RATE ROUNDED =                          
                         (CM-AH-BENEFIT-AMT * 12) / 365          
        COMPUTE PI-CPYAMT ROUNDED =                              
            (CM-AH-BENEFIT-AMT * DC-ELAPSED-MONTHS) +            
            (PI-DAILY-RATE     * DC-ODD-DAYS-OVER)               
        GO TO 1420-CHECK-AIG-BENEFIT.                            
                                                                 
    IF WS-CALC-METHOD = '4'                                      
        MOVE CM-AH-BENEFIT-AMT   TO  WS-AH-BEN-AMT               
        COMPUTE WS-DAILY-RATE ROUNDED = (WS-AH-BEN-AMT / 30)     
        MOVE WS-DAILY-RATE TO PI-DAILY-RATE                      
        COMPUTE PI-CPYAMT ROUNDED =                              
                          WS-DAILY-RATE * DC-ELAPSED-DAYS        
        GO TO 1420-CHECK-AIG-BENEFIT.                            
                                                                 
    IF WS-CALC-METHOD = '5'                                      
        COMPUTE PI-DAILY-RATE ROUNDED =                          
                           (CM-AH-BENEFIT-AMT * 12) / 365        
        COMPUTE PI-CPYAMT ROUNDED =                              
                          PI-DAILY-RATE * DC-ELAPSED-DAYS        
        GO TO 1420-CHECK-AIG-BENEFIT.                            
                                                                 
    IF WS-CALC-METHOD NOT = '3'                                  
       GO TO 1410-CHECK-PAYMENT.                                 
                                                                 
*>*****   CALCULATION METHOD  3  ROUTINE                           
    COMPUTE PI-DAILY-RATE =                                      
                CM-AH-BENEFIT-AMT / FROM-DAYS-IN-MONTH.          
                                                                 
*>*****CHECK TO SEE IF WITHIN SAME MONTH                           
    IF (WDF-MONTH = WDT-MONTH) AND (WDF-YEAR = WDT-YEAR)         
       COMPUTE PI-CPYAMT ROUNDED = SAVE-ELAPSED-DAYS    *        
                    (CM-AH-BENEFIT-AMT / FROM-DAYS-IN-MONTH)     
        GO TO 1420-CHECK-AIG-BENEFIT.                            
                                                                 
*>*****CHECK TO SEE IF EVEN MONTHS AND NO ODD DAYS                 
    IF SAVE-ELAPSED-MONTHS NOT = ZEROS   AND                     
            SAVE-ODD-DAYS-OVER = ZEROS                           
     COMPUTE PI-CPYAMT  ROUNDED =                                
                 CM-AH-BENEFIT-AMT * SAVE-ELAPSED-MONTHS         
     GO TO 1420-CHECK-AIG-BENEFIT.                               
                                                                 
*>*****BUMP THE FROM DATE WITH THE NUMBER OF MONTHS                
*>*****AND FIND THE NUMBER OF DAYS LEFT IN THIS MONTH.             
    IF SAVE-ELAPSED-MONTHS NOT = ZERO                            
       ADD SAVE-ELAPSED-MONTHS  TO WDF-MONTH                     
       IF WDF-MONTH > 12                                         
          ADD 1                 TO WDF-YEAR                      
          SUBTRACT 12         FROM WDF-MONTH.                    
                                                                 
*>*****CONVERT THE NEW FROM MONTH TO BINARY                        
    IF SAVE-ELAPSED-MONTHS NOT = ZERO                            
       MOVE WORK-DATE-FROM   TO DC-GREG-DATE-1-MDY               
       MOVE '4'              TO DC-OPTION-CODE                   
       PERFORM 9700-LINK-DATE-CONVERT THRU 9700-EXIT             
       MOVE DC-DAYS-IN-MONTH TO FROM-DAYS-IN-MONTH.              
                                                                 
    IF WDF-MONTH = WDT-MONTH AND                                 
       WDF-YEAR  = WDT-YEAR                                      
         COMPUTE PI-CPYAMT ROUNDED = ((WDT-DAY  -  WDF-DAY)  *   
                   (CM-AH-BENEFIT-AMT / FROM-DAYS-IN-MONTH)) +   
                   (CM-AH-BENEFIT-AMT * SAVE-ELAPSED-MONTHS)     
       ELSE                                                      
         COMPUTE FROM-PMT-DAYS = FROM-DAYS-IN-MONTH - WDF-DAY    
         MOVE WDT-DAY              TO THRU-PMT-DAYS              
         COMPUTE PI-CPYAMT ROUNDED =                             
      (FROM-PMT-DAYS * (CM-AH-BENEFIT-AMT / FROM-DAYS-IN-MONTH)) 
      +                                                          
      (THRU-PMT-DAYS * (CM-AH-BENEFIT-AMT / THRU-DAYS-IN-MONTH)) 
      +                                                          
      (CM-AH-BENEFIT-AMT * SAVE-ELAPSED-MONTHS).                 
                                                                 
    GO TO 1420-CHECK-AIG-BENEFIT.                                
    EJECT                                                        
1410-CHECK-PAYMENT.                                              
*>*****PAYMENT OPTION 6                                            
*>*****CHECK TO SEE IF LESS THAN 1 MONTH AND WITHIN SAME MONTH     
    IF SAVE-ELAPSED-MONTHS > ZERO                                
       IF SAVE-ODD-DAYS-OVER = ZEROS                             
          GO TO 1412-CHECK-TO-DAY                                
       ELSE                                                      
          IF WDF-DAY > WDT-DAY                                   
             IF WDF-MONTH = 02                                   
                 PERFORM 1415-CHECK-LEAP-YEAR THRU 1415-EXIT     
                 ADD 2  TO  SAVE-ODD-DAYS-OVER                   
                 GO TO 1412-CHECK-TO-DAY                         
             ELSE                                                
                 IF FROM-DAYS-IN-MONTH = 31                      
                    SUBTRACT 1 FROM SAVE-ODD-DAYS-OVER           
                    GO TO 1412-CHECK-TO-DAY                      
                 ELSE                                            
                    GO TO 1412-CHECK-TO-DAY                      
          ELSE                                                   
              GO TO 1412-CHECK-TO-DAY.                           
                                                                 
    IF WDF-MONTH NOT = WDT-MONTH                                 
       IF WDF-MONTH = 02                                         
          PERFORM 1415-CHECK-LEAP-YEAR THRU 1415-EXIT            
          IF WDF-DAY = 29                                        
             ADD 1              TO SAVE-ODD-DAYS-OVER            
          ELSE                                                   
             ADD 2              TO SAVE-ODD-DAYS-OVER            
       ELSE                                                      
          IF FROM-DAYS-IN-MONTH = 31                             
             SUBTRACT 1         FROM SAVE-ODD-DAYS-OVER.         
                                                                 
1412-CHECK-TO-DAY.                                               
       IF WDT-MONTH = 02 AND SAVE-ODD-DAYS-OVER NOT = 0          
          IF WDT-DAY = 28                                        
             ADD 2              TO SAVE-ODD-DAYS-OVER            
          ELSE                                                   
             IF WDT-DAY = 29                                     
                ADD 1           TO SAVE-ODD-DAYS-OVER            
             ELSE                                                
                NEXT SENTENCE                                    
       ELSE                                                      
          IF WDT-DAY = 31 AND SAVE-ODD-DAYS-OVER NOT = 0         
             SUBTRACT 1         FROM SAVE-ODD-DAYS-OVER.         
                                                                 
    COMPUTE WS-DAILY-RATE ROUNDED = CM-AH-BENEFIT-AMT / 30.      
    MOVE WS-DAILY-RATE          TO PI-DAILY-RATE.                
    COMPUTE PI-CPYAMT ROUNDED =                                  
        (CM-AH-BENEFIT-AMT * SAVE-ELAPSED-MONTHS) +              
        (WS-DAILY-RATE     * SAVE-ODD-DAYS-OVER).                
                                                                 
    GO TO 1420-CHECK-AIG-BENEFIT.                                
                                                                 
1415-CHECK-LEAP-YEAR.                                            
    IF FROM-DAYS-IN-MONTH = 29                                   
       SUBTRACT 1 FROM SAVE-ODD-DAYS-OVER.                       
                                                                 
1415-EXIT.                                                       
    EXIT.                                                        
                                                                 
    EJECT                                                        
1420-CHECK-AIG-BENEFIT.                                          
                                                                 
