    IF CL-TOTAL-PAID-AMT NOT = WS-TOT-AMOUNT-PAID                
        MOVE ER-3277            TO EMI-ERROR                     
        PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT                 
    END-IF                                                       
                                                                 
    MOVE SPACE TO WS-HOLD-UNTIL-SW                               
                                                                 
    MOVE AL-UANON               TO HOLDTILA.                     
                                                                 
    IF PI-HOLDTIL = ZEROS                                        
       IF PI-OFFLINE = 'Y'                                       
          MOVE ER-0572          TO EMI-ERROR                     
          GO TO 1105-HOLDTIL-ERROR                               
       ELSE                                                      
          GO TO 1110-EDIT-PAID-FROM.                             
                                                                 
    MOVE '4'                    TO DC-OPTION-CODE.               
    MOVE PI-HOLDTIL             TO DC-GREG-DATE-1-MDY.           
    PERFORM 9700-LINK-DATE-CONVERT THRU 9700-EXIT.               
                                                                 
    IF DATE-CONVERSION-ERROR                                     
        MOVE ER-0446            TO EMI-ERROR                     
        GO TO 1105-HOLDTIL-ERROR                                 
    END-IF                                                       
                                                                 
    IF PI-OFFLINE = 'Y'                                          
       IF DC-BIN-DATE-1 < SAVE-BIN-DATE                          
          MOVE ER-0446            TO EMI-ERROR                   
          GO TO 1105-HOLDTIL-ERROR                               
       END-IF                                                    
    END-IF                                                       
                                                                 
    IF DC-BIN-DATE-1 < WS-UNPD-HOLD-UNTIL-DT                     
       MOVE ER-3278          TO EMI-ERROR                        
       GO TO 1105-HOLDTIL-ERROR                                  
    END-IF                                                       
                                                                 
    IF PI-OFFLINE = 'Y'                                          
       GO TO 1110-EDIT-PAID-FROM.                                
                                                                 
    MOVE DC-BIN-DATE-1          TO DC-BIN-DATE-2.                
    MOVE WS-TODAY-DATE          TO DC-BIN-DATE-1.                
    MOVE '1'                    TO DC-OPTION-CODE.               
    PERFORM 9700-LINK-DATE-CONVERT THRU 9700-EXIT.               
                                                                 
*>    IF DC-ELAPSED-DAYS > 30 OR DATE-CONVERSION-ERROR             
    IF DC-ELAPSED-DAYS > 60 OR DATE-CONVERSION-ERROR             
        MOVE ER-0447            TO EMI-ERROR                     
        GO TO 1105-HOLDTIL-ERROR.                                
                                                                 
    GO TO 1110-EDIT-PAID-FROM.                                   
                                                                 
1105-HOLDTIL-ERROR.                                              
    MOVE -1                     TO HOLDTILL.                     
    MOVE AL-UNBON               TO HOLDTILA.                     
    PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT.                    
                                                                 
    EJECT                                                        
1110-EDIT-PAID-FROM.                                             
    MOVE AL-UANON               TO EPYFROMA.                     
                                                                 
    IF PI-EPYFROM = ZEROS                                        
        GO TO 1120-CALCULATE-PAID-FROM.                          
                                                                 
    IF PI-PMTTYPE = '5' OR '6'                                   
        MOVE ER-0448            TO EMI-ERROR                     
        PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT                 
        MOVE AL-UNBON           TO EPYFROMA                      
        MOVE -1                 TO EPYFROML                      
        GO TO 1150-EDIT-PAID-THRU.                               
                                                                 
    MOVE '4'                    TO DC-OPTION-CODE.               
    MOVE PI-EPYFROM             TO DC-GREG-DATE-1-MDY.           
    PERFORM 9700-LINK-DATE-CONVERT THRU 9700-EXIT.               
                                                                 
    IF DATE-CONVERSION-ERROR                                     
        MOVE ER-0449            TO EMI-ERROR                     
        PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT                 
        MOVE AL-UNBON           TO EPYFROMA                      
        MOVE -1                 TO EPYFROML                      
    ELSE                                                         
        MOVE DC-BIN-DATE-1      TO WS-SV-EPYFROM.                
                                                                 
    EJECT                                                        
1120-CALCULATE-PAID-FROM.                                        
    IF PI-PMTTYPE = '5' OR '6'                                   
        GO TO 1150-EDIT-PAID-THRU.                               
                                                                 
*>    IF PI-COMPANY-ID = 'AIG' OR 'AUK'                            
*>       IF PI-PMTTYPE = '4'                                       
*>         GO TO 1150-EDIT-PAID-THRU.                              
                                                                 
    IF CL-CLAIM-TYPE = PI-LIFE-OVERRIDE-L1 OR 'O'                
        MOVE CL-INCURRED-DT     TO DC-BIN-DATE-1                 
        MOVE ' '                TO DC-OPTION-CODE                
        PERFORM 9700-LINK-DATE-CONVERT THRU 9700-EXIT            
        MOVE DC-BIN-DATE-1      TO PI-CPYFROM                    
        GO TO 1145-TEST-PAID-FROM.                               
                                                                 
    IF PI-PMTTYPE = '4'                                          
       GO TO 1150-EDIT-PAID-THRU.                                
                                                                 
    MOVE CM-AH-BENEFIT-CD       TO WS-BEN-CD.                    
    MOVE WS-ACCESS              TO CNTL-ACCESS.                  
    MOVE PI-COMPANY-ID          TO CNTL-COMP-ID.                 
    MOVE '5'                    TO CNTL-REC-TYPE.                
    MOVE ZEROS                  TO CNTL-SEQ-NO.                  
    MOVE 'BENE'                 TO FILE-SWITCH.                  
                                                                 
    IF WS-BEN-CD = ZEROS                                         
        GO TO 1950-NOT-FOUND.                                    
                                                                 
    PERFORM 7200-FIND-BENEFIT THRU 7200-EXIT.                    
                                                                 
    IF NO-BENEFIT-FOUND                                          
        GO TO 1950-NOT-FOUND.                                    
                                                                 
    MOVE CF-BENEFIT-ALPHA   (SUB-1) TO PI-BENEFIT-SAVE.          
    MOVE CF-SPECIAL-CALC-CD (SUB-1) TO WS-SPECIAL-CALC-CD.       
                                                                 
    if pdef-found                                                
       if pd-ret-elim(p1) = 'R' or 'E'                           
          move pd-ret-elim(p1)  to pi-ben-type                   
          move pd-wait-days(p1) to pi-ben-days                   
       end-if                                                    
    end-if                                                       
                                                                 
    IF PI-BEN-DAYS NOT NUMERIC                                   
       MOVE ZEROS               TO PI-BEN-DAYS.                  
                                                                 
    MOVE PI-BEN-DAYS            TO WS-BEN-DAYS.                  
                                                                 
    IF WS-HOLD-UNTIL-DT > CL-PAID-THRU-DT                        
        MOVE WS-HOLD-UNTIL-DT     TO DC-BIN-DATE-1               
        MOVE '6'                 TO DC-OPTION-CODE               
        MOVE +0                  TO DC-ELAPSED-MONTHS            
        MOVE +1                  TO DC-ELAPSED-DAYS              
        PERFORM 9700-LINK-DATE-CONVERT THRU 9700-EXIT            
        MOVE DC-BIN-DATE-2       TO PI-CPYFROM                   
        GO TO 1145-TEST-PAID-FROM.                               
                                                                 
    IF (CL-PAID-THRU-DT NOT = LOW-VALUES AND SPACES)  AND        
       (CL-TOTAL-PAID-AMT > ZEROS)                               
        MOVE CL-PAID-THRU-DT     TO DC-BIN-DATE-1                
        MOVE '6'                 TO DC-OPTION-CODE               
        MOVE +0                  TO DC-ELAPSED-MONTHS            
        MOVE +1                  TO DC-ELAPSED-DAYS              
        PERFORM 9700-LINK-DATE-CONVERT THRU 9700-EXIT            
        MOVE DC-BIN-DATE-2       TO PI-CPYFROM                   
        GO TO 1145-TEST-PAID-FROM.                               
                                                                 
*>*****************************************************************
*>***** ON FIRST PAYMENTS, ELIM/RETRO EDITS TO BE MADE AGAINST THE 
*>***** SPECIAL ENTERED DATE.                                      
*>*****************************************************************
                                                                 
    IF PI-AIGFROM = ZEROS                                        
        GO TO 1130-CONTINUE-PAID-FROM.                           
                                                                 
    MOVE AL-UANON TO AIGFROMA.                                   
                                                                 
    MOVE '4'                    TO DC-OPTION-CODE.               
    MOVE PI-AIGFROM             TO DC-GREG-DATE-1-MDY.           
    PERFORM 9700-LINK-DATE-CONVERT THRU 9700-EXIT.               
                                                                 
    IF DATE-CONVERSION-ERROR                                     
        MOVE ER-0449            TO EMI-ERROR                     
        PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT                 
        MOVE AL-UNBON           TO AIGFROMA                      
        MOVE -1                 TO AIGFROML                      
        MOVE CL-INCURRED-DT TO DC-BIN-DATE-1                     
    ELSE                                                         
        MOVE 'Y'                TO WS-AIG-PAYFROM-SW             
        MOVE DC-BIN-DATE-1      TO WS-SV-AIGFROM.                
                                                                 
    GO TO 1140-CONTINUE-PAID-FROM.                               
                                                                 
1130-CONTINUE-PAID-FROM.                                         
                                                                 
    IF (CL-PAID-THRU-DT   = LOW-VALUES OR SPACES) OR             
       (CL-TOTAL-PAID-AMT = ZEROS)                               
          MOVE CL-INCURRED-DT TO DC-BIN-DATE-1.                  
                                                                 
1140-CONTINUE-PAID-FROM.                                         
                                                                 
    IF PI-BEN-TYPE = 'R'                                         
        IF PI-BEN-DAYS = ZEROS                                   
            NEXT SENTENCE                                        
        ELSE                                                     
        IF PI-COMPANY-ID NOT = 'DMD'                             
            SUBTRACT 1 FROM PI-BEN-DAYS.                         
                                                                 
    MOVE DC-BIN-DATE-1          TO WS-HOLD-BIN-DATE-1.           
                                                                 
    IF PI-COMPANY-ID = 'DMD'                                     
        MOVE -1                 TO DC-ELAPSED-DAYS               
        MOVE +0                 TO DC-ELAPSED-MONTHS             
        MOVE '6'                TO DC-OPTION-CODE                
        PERFORM 9700-LINK-DATE-CONVERT THRU 9700-EXIT            
        MOVE DC-BIN-DATE-2      TO DC-BIN-DATE-1                 
        DIVIDE PI-BEN-DAYS BY 30 GIVING DC-ELAPSED-MONTHS        
                              REMAINDER DC-ELAPSED-DAYS          
     ELSE                                                        
        MOVE PI-BEN-DAYS        TO DC-ELAPSED-DAYS               
        MOVE +0                 TO DC-ELAPSED-MONTHS.            
                                                                 
    MOVE '6'                    TO DC-OPTION-CODE.               
    PERFORM 9700-LINK-DATE-CONVERT THRU 9700-EXIT.               
    MOVE DC-BIN-DATE-2          TO WS-RETRO-ELIM-DATE.           
                                                                 
    MOVE WS-HOLD-BIN-DATE-1     TO DC-BIN-DATE-1.                
                                                                 
    IF WS-RETRO-ELIM-DATE > WS-TODAY-DATE                        
        MOVE ER-0541            TO EMI-ERROR                     
        PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT                 
        MOVE -1                 TO EPYFROML                      
        MOVE AL-UNBON           TO EPYFROMA.                     
                                                                 
    IF (PI-COMPANY-ID = 'AIG' OR 'AUK')  AND                     
       (PI-BENEFIT-SAVE = '60E')                                 
       IF (CL-PAID-THRU-DT   = LOW-VALUES OR SPACES) OR          
          (CL-TOTAL-PAID-AMT = ZEROS)                            
            MOVE WS-HOLD-BIN-DATE-1  TO DC-BIN-DATE-1            
            MOVE 30                  TO DC-ELAPSED-DAYS          
            MOVE +0                  TO DC-ELAPSED-MONTHS        
            MOVE '6'                 TO DC-OPTION-CODE           
            PERFORM 9700-LINK-DATE-CONVERT THRU 9700-EXIT.       
                                                                 
    IF PI-BEN-TYPE = 'R'                                         
        MOVE DC-BIN-DATE-1      TO PI-CPYFROM                    
    ELSE                                                         
        MOVE DC-BIN-DATE-2      TO PI-CPYFROM                    
        IF WS-SV-EPYFROM = LOW-VALUES                            
           MOVE DC-BIN-DATE-2   TO WS-SV-EPYFROM.                
                                                                 
    IF CL-PAID-THRU-DT = LOW-VALUES OR SPACES                    
        IF WS-USE-AIG-PAYFROM   OR                               
           WS-AIG-RECALC-PAYFROM                                 
             MOVE PI-CPYFROM     TO  WS-SV-EPYFROM               
                                     DC-BIN-DATE-1               
             MOVE ' '            TO  DC-OPTION-CODE              
             MOVE +0             TO  DC-ELAPSED-MONTHS           
                                     DC-ELAPSED-DAYS             
             PERFORM 9700-LINK-DATE-CONVERT THRU 9700-EXIT       
             IF NO-CONVERSION-ERROR                              
                 MOVE DC-GREG-DATE-1-MDY TO PI-EPYFROM.          
                                                                 
    IF PI-CPYFROM < CL-INCURRED-DT                               
       MOVE -1                  TO EPYFROML                      
       MOVE ER-3542             TO EMI-ERROR                     
       PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT.                 
                                                                 
    IF CM-ENTRY-DT < CL-INCURRED-DT                              
       MOVE CM-ENTRY-DT            TO DC-BIN-DATE-1              
       MOVE CL-INCURRED-DT         TO DC-BIN-DATE-2              
       MOVE +0                     TO DC-ELAPSED-DAYS            
       MOVE +0                     TO DC-ELAPSED-MONTHS          
       MOVE '1'                    TO DC-OPTION-CODE             
       PERFORM 9700-LINK-DATE-CONVERT THRU 9700-EXIT             
       ELSE                                                      
       MOVE ZEROS TO DC-ELAPSED-MONTHS.                          
                                                                 
    IF PI-COMPANY-ID = 'AIG' OR 'AUK' OR 'DMD'                   
        NEXT SENTENCE                                            
    ELSE                                                         
        IF DC-ELAPSED-MONTHS < 2                                 
            MOVE ER-3255        TO EMI-ERROR                     
            PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT.            
                                                                 
1145-TEST-PAID-FROM.                                             
    IF LOW-VALUES = WS-SV-EPYFROM OR PI-CPYFROM                  
        GO TO 1150-EDIT-PAID-THRU.                               
                                                                 
    IF WS-SV-EPYFROM NOT = PI-CPYFROM                            
        MOVE PI-CPYFROM         TO DC-BIN-DATE-1                 
        MOVE ' '                TO DC-OPTION-CODE                
        MOVE +0                 TO DC-ELAPSED-MONTHS             
                                   DC-ELAPSED-DAYS               
        PERFORM 9700-LINK-DATE-CONVERT THRU 9700-EXIT            
        IF NO-CONVERSION-ERROR                                   
            MOVE DC-GREG-DATE-1-MDY TO EMI-DATE-FIELD            
        ELSE                                                     
            MOVE SPACES             TO EMI-DATE-FIELD.           
                                                                 
    IF PI-HOLDTIL NOT = ZEROS                                    
       CONTINUE                                                  
    ELSE                                                         
    IF WS-SV-EPYFROM NOT = PI-CPYFROM                            
        IF CL-TOTAL-PAID-AMT = ZEROS                             
            MOVE ER-3543            TO EMI-ERROR                 
            MOVE WS-SV-EPYFROM      TO PI-CPYFROM                
            PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT             
            IF EMI-MESSAGE-FORMATTED                             
               MOVE -1             TO EPYFROML                   
               MOVE AL-UNBON       TO EPYFROMA                   
            ELSE                                                 
               NEXT SENTENCE                                     
        ELSE                                                     
            MOVE ER-0452            TO EMI-ERROR                 
            MOVE WS-SV-EPYFROM      TO PI-CPYFROM                
            PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT             
            IF EMI-MESSAGE-FORMATTED                             
               MOVE -1             TO EPYFROML                   
               MOVE AL-UNBON       TO EPYFROMA.                  
                                                                 
    EJECT                                                        
1150-EDIT-PAID-THRU.                                             
                                                                 
    MOVE AL-UANON               TO EPYTHRUA.                     
                                                                 
    IF PI-EPYTHRU = ZEROS                                        
*>        IF PI-PMTTYPE = '4' OR '5' OR '6'                        
        IF PI-PMTTYPE = '5' OR '6'                               
          OR (CL-CLAIM-TYPE NOT = 'L' AND 'O'                    
            AND PI-PMTTYPE = '4')                                
            GO TO 1200-EDIT-FIND-TOLERANCES                      
        ELSE                                                     
           IF (PI-PMTTYPE = '3') OR (PI-PMTTYPE = '4') OR        
              (PI-PMTTYPE = '2' AND                              
                CL-CLAIM-TYPE = PI-LIFE-OVERRIDE-L1 OR 'O')      
              GO TO 1152-CHECK-TYPE                              
           ELSE                                                  
              MOVE ER-0473      TO EMI-ERROR                     
              GO TO 1170-PAID-THRU-FATAL-ERROR                   
    ELSE                                                         
        IF PI-PMTTYPE = '5' OR '6'                               
            MOVE ER-0474        TO EMI-ERROR                     
            GO TO 1170-PAID-THRU-FATAL-ERROR.                    
                                                                 
    MOVE '4'                    TO DC-OPTION-CODE.               
    MOVE PI-EPYTHRU             TO DC-GREG-DATE-1-MDY.           
    PERFORM 9700-LINK-DATE-CONVERT THRU 9700-EXIT.               
                                                                 
    IF DATE-CONVERSION-ERROR                                     
        MOVE ER-0475            TO EMI-ERROR                     
        GO TO 1170-PAID-THRU-FATAL-ERROR.                        
                                                                 
    MOVE DC-BIN-DATE-1       TO WS-SV-EPYTHRU.                   
                                                                 
    IF PI-COMPANY-ID = 'DMD'                                     
        IF PI-PMTTYPE NOT = '2'                                  
                AND                                              
           WS-EXP-DT > WS-SV-EPYTHRU                             
            MOVE WS-SV-EPYTHRU    TO DC-BIN-DATE-1               
            MOVE WS-EXP-DT        TO DC-BIN-DATE-2               
            MOVE '1'              TO DC-OPTION-CODE              
            PERFORM 9700-LINK-DATE-CONVERT THRU 9700-EXIT        
            IF DATE-CONVERSION-ERROR                             
                MOVE ER-0944      TO EMI-ERROR                   
                GO TO 1170-PAID-THRU-FATAL-ERROR                 
            ELSE                                                 
                IF DC-ELAPSED-MONTHS = ZEROS AND                 
                   DC-ELAPSED-DAYS   = 1                         
                    MOVE ER-0945  TO EMI-ERROR                   
                    MOVE -1       TO EPYTHRUL                    
                    MOVE AL-UABON TO EPYTHRUA                    
                    PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT.    
                                                                 
    IF PI-PMTTYPE = '4'                                          
        NEXT SENTENCE                                            
    ELSE                                                         
        IF WS-SV-EPYTHRU = WS-EXP-DT                             
            IF PI-PMTTYPE NOT = '2' AND '3'                      
                MOVE ER-3541    TO EMI-ERROR                     
                MOVE -1         TO PMTTYPEL                      
                MOVE AL-UABON   TO PMTTYPEA                      
                PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT.        
                                                                 
    IF WS-SV-EPYTHRU < WS-SV-EPYFROM                             
        MOVE ER-0501            TO EMI-ERROR                     
        GO TO 1170-PAID-THRU-FATAL-ERROR.                        
                                                                 
    IF WS-SV-EPYTHRU > WS-TODAY-DATE                             
       IF CL-CLAIM-TYPE = PI-AH-OVERRIDE-L1 OR 'I' OR 'G' or 'F' 
                                            OR 'B' or 'H'        
*>          IF PI-COMPANY-ID = 'AIG' OR 'AUK'                      
*>              MOVE ER-0540         TO EMI-ERROR                  
*>              PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT           
*>              MOVE -1              TO EPYTHRUL                   
*>              MOVE AL-UNBON        TO EPYTHRUA                   
*>          ELSE                                                   
*>          IF PI-COMPANY-ID = 'DMD'                               
*>              MOVE ER-7540         TO EMI-ERROR                  
*>              PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT           
*>              MOVE -1              TO EPYTHRUL                   
*>              MOVE AL-UNBON        TO EPYTHRUA                   
*>          ELSE                                                   
              IF PI-PMTTYPE NOT = '2' AND '3'                    
                 IF PI-COMPANY-ID = '???'                        
                     MOVE ER-7540  TO EMI-ERROR                  
                     PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT    
                     MOVE -1       TO EPYTHRUL                   
                     MOVE AL-UNBON TO EPYTHRUA                   
                 ELSE                                            
                     MOVE ER-0540  TO EMI-ERROR                  
                     PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT    
                     MOVE -1       TO EPYTHRUL                   
                     MOVE AL-UNBON TO EPYTHRUA.                  
                                                                 
    MOVE 'N' TO PI-DUPE-APPROVAL-NEEDED                          
    IF ((WS-SV-EPYTHRU < CL-PAID-THRU-DT)                        
           OR                                                    
       (WS-SV-EPYTHRU NOT = LOW-VALUES AND                       
        WS-SV-EPYTHRU < CL-PAID-THRU-DT)                         
           OR                                                    
       (PI-CPYFROM NOT = LOW-VALUES AND                          
        PI-CPYFROM < CL-PAID-THRU-DT))                           
      AND NOT PI-PMTTYPE = '4'                                   
       PERFORM 1166-CHECK-ALL-TRLRS THRU 1166-TRLR-EXIT          
    ELSE                                                         
    IF WS-SV-EPYTHRU NOT = LOW-VALUES                            
      AND WS-SV-EPYFROM NOT = LOW-VALUES                         
       SET CHECK-ALL-PMTS TO TRUE                                
       PERFORM 1166-CHECK-ALL-TRLRS THRU 1166-TRLR-EXIT.         
                                                                 
    IF PI-HOLDTIL = ZEROS                                        
      AND UNPAID-HOLD-UNTIL-EXISTS                               
      AND NOT DUPE-VOID-PMT-EXISTS                               
       MOVE ER-3276    TO EMI-ERROR                              
       MOVE -1         TO HOLDTILL                               
       MOVE AL-UABON   TO HOLDTILA                               
       GO TO 1170-PAID-THRU-FATAL-ERROR                          
    END-IF.                                                      
                                                                 
1152-CHECK-TYPE.                                                 
    IF (CL-CLAIM-TYPE = PI-AH-OVERRIDE-L1 OR 'I' OR 'G' or 'F'   
                                          OR 'B' or 'H')         
       OR (PI-PMTTYPE = '5' OR '6')                              
        GO TO 1160-EDIT-PAID-THRU-AH.                            
                                                                 
1155-EDIT-PAID-THRU-LIFE.                                        
    MOVE CM-LF-BENEFIT-CD       TO WS-BEN-CD.                    
    MOVE WS-ACCESS              TO CNTL-ACCESS.                  
    MOVE PI-COMPANY-ID          TO CNTL-COMP-ID.                 
    MOVE '4'                    TO CNTL-REC-TYPE.                
    MOVE ZEROS                  TO CNTL-SEQ-NO.                  
    MOVE 'BENE'                 TO FILE-SWITCH.                  
                                                                 
    PERFORM 7200-FIND-BENEFIT THRU 7200-EXIT.                    
                                                                 
    IF NO-BENEFIT-FOUND                                          
        GO TO 1950-NOT-FOUND.                                    
                                                                 
    MOVE CF-SPECIAL-CALC-CD (SUB-1) TO WS-SPECIAL-CALC-CD.       
                                                                 
    MOVE WS-EXP-DT              TO PI-CPYTHRU.                   
                                                                 
    IF PI-LIFE-OVERRIDE-L1 = 'P' OR                              
       PI-LF-COVERAGE-TYPE = 'P'                                 
        MOVE CL-INCURRED-DT     TO  PI-CPYTHRU                   
        GO TO 1200-EDIT-FIND-TOLERANCES.                         
                                                                 
    IF PI-EPYTHRU = ZEROS                                        
       MOVE PI-CPYTHRU          TO WS-SV-EPYTHRU.                
                                                                 
    IF WS-SV-EPYTHRU NOT = WS-EXP-DT                             
        MOVE ER-0476            TO EMI-ERROR                     
        PERFORM 1165-PAID-THRU-ERROR THRU 1165-EXIT.             
                                                                 
    MOVE CM-CERT-EFF-DT        TO DC-BIN-DATE-1.                 
*>    MOVE CM-LF-CRITICAL-PERIOD TO DC-ELAPSED-MONTHS.             
    move cl-critical-period    to dc-elapsed-months              
    MOVE +0                    TO DC-ELAPSED-DAYS.               
    MOVE '6'                   TO DC-OPTION-CODE.                
    PERFORM 9700-LINK-DATE-CONVERT THRU 9700-EXIT.               
                                                                 
    IF (NO-CONVERSION-ERROR AND                                  
        WS-SPECIAL-CALC-CD = 'C')                                
       IF CL-INCURRED-DT > DC-BIN-DATE-2                         
           MOVE ER-0657 TO EMI-ERROR                             
           PERFORM 1165-PAID-THRU-ERROR THRU 1165-EXIT.          
                                                                 
    IF PI-COMPANY-ID = 'NCL'                                     
        IF WS-SPECIAL-CALC-CD = 'C' AND                          
           cl-critical-period = zeros                            
*>           CM-LF-CRITICAL-PERIOD = +0                            
            MOVE ER-0844       TO EMI-ERROR                      
            PERFORM 1165-PAID-THRU-ERROR THRU 1165-EXIT.         
                                                                 
    GO TO 1200-EDIT-FIND-TOLERANCES.                             
                                                                 
1160-EDIT-PAID-THRU-AH.                                          
    IF PI-EPYTHRU = ZEROS                                        
       MOVE WS-EXP-DT           TO PI-CPYTHRU                    
                                   WS-SV-EPYTHRU                 
    ELSE                                                         
       MOVE WS-SV-EPYTHRU       TO PI-CPYTHRU.                   
                                                                 
    IF WS-SV-EPYTHRU < PI-CPYFROM                                
        MOVE ER-0508            TO EMI-ERROR                     
        GO TO 1170-PAID-THRU-FATAL-ERROR.                        
                                                                 
    perform 7975-read-acct      thru 7975-exit                   
                                                                 
    move zeros                  to pi-max-ext                    
    if PI-DCC-PRODUCT-CODE NOT = SPACES                          
       PERFORM 0900-GET-DDF-LIMITS                               
                                THRU 0900-EXIT                   
       IF PDEF-FOUND                                             
          PERFORM VARYING P1 FROM +1 BY +1 UNTIL                 
             (P1 > +11)                                          
             OR ((PD-PROD-CODE (P1) = CL-CLAIM-TYPE)             
             AND (PD-MAX-ATT-AGE (P1) >= WS-ATT-AGE))            
          END-PERFORM                                            
          IF P1 < +12                                            
             MOVE PD-MAX-extension (P1)                          
                                TO pi-max-ext                    
          else                                                   
             move er-1674       to emi-error                     
             PERFORM 9900-ERROR-FORMAT                           
                                THRU 9900-EXIT                   
          END-IF                                                 
       else                                                      
          move er-1672          to emi-error                     
          PERFORM 9900-ERROR-FORMAT                              
                                THRU 9900-EXIT                   
       END-IF                                                    
    end-if                                                       
                                                                 
    move spaces                 to pi-max-ext-used               
    IF PI-PMTTYPE = '3'                                          
       MOVE WS-EXP-DT           TO PI-CPYTHRU                    
       IF WS-SV-EPYTHRU NOT = WS-EXP-DT                          
          MOVE ER-0477          TO EMI-ERROR                     
          PERFORM 1165-PAID-THRU-ERROR                           
                                THRU 1165-EXIT                   
       ELSE                                                      
          continue                                               
