*>    IF PI-COMPANY-ID = 'AIG' OR 'AUK'                            
*>       IF CL-CLAIM-TYPE    = PI-AH-OVERRIDE-L1  AND              
*>          CM-AH-BENEFIT-CD = '23'               AND              
