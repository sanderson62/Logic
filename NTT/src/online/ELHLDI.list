         ,:ws-WheelBase                                          
         ,:ws-Length                                             
         ,:ws-Width                                              
         ,:ws-Height                                             
         ,:ws-RestraintCode                                      
         ,:ws-RestraintDesc                                      
         ,:ws-EngineNumber                                       
         ,:ws-EngineText                                         
         ,:ws-HorsePowerMin                                      
         ,:ws-HorsePowerMax                                      
