8050-FORMAT-LAST-NAME-1ST.                                       
*>**************************************************************** 
*>             M O V E   N A M E   R O U T I N E                 * 
*>                                                               * 
*>     THE FOLLOWING ROUTINE REARRANGES A GIVEN NAME SO          * 
*>     THAT IT READS LAST, FIRST, MIDDLE.  PLACE NAME            * 
*>     FIELDS IN THE FOLLOWING WORKING-STORAGE FIELDS.           * 
*>                                                               * 
*>                  FIELD               VALUE                    * 
*>                  -----               -----                    * 
*>           W-NAME-LAST    (CL15)      SMITH                    * 
*>           W-NAME-FIRST   (CL15)      JOHN                     * 
*>           W-NAME-MIDDLE  (CL15)      ALLEN/A                  * 
*>                                                               * 
*>     AFTER NAME HAS BEEN MOVED WS-NAME-WORK WILL CONTAIN       * 
*>                SMITH, JOHN ALLEN                              * 
*>                     OR                                        * 
*>                SMITH, JOHN A.                                 * 
*>                                                               * 
*>     TO USE THIS ROUTINE YOU NEED THE WORKING-STORAGE          * 
*>     COPYBOOK, ELCNWA.                                         * 
*>****************************************************************.
                                                                 
    MOVE SPACES                 TO WS-NAME-WORK-AREA.            
    MOVE ZERO                   TO WS-NAME-SW.                   
    SET NWA-INDEX               TO +1.                           
                                                                 
    IF W-NAME-LAST   = SPACES  AND                               
       W-NAME-MIDDLE = SPACES                                    
         MOVE +1                TO WS-NAME-SW.                   
                                                                 
    MOVE W-NAME-LAST            TO WS-NAME-WORK2.                
    PERFORM 8060-MOVE-NAME THRU 8060-EXIT.                       
                                                                 
    MOVE W-NAME-FIRST           TO WS-NAME-WORK2.                
    PERFORM 8060-MOVE-NAME THRU 8060-EXIT.                       
                                                                 
    SET NWA-INDEX UP BY +1.                                      
                                                                 
    IF W-NAME-MIDDLE NOT = SPACES                                
        IF W-NAME-MIDDLE-2 = SPACES                              
            MOVE W-NAME-MIDDLE  TO WS-NW (NWA-INDEX)             
            SET NWA-INDEX UP BY +1                               
            MOVE '.'            TO WS-NW (NWA-INDEX)             
            SET NWA-INDEX UP BY +2                               
        ELSE                                                     
            MOVE W-NAME-MIDDLE  TO WS-NAME-WORK2                 
            PERFORM 8060-MOVE-NAME THRU 8060-EXIT.               
                                                                 
8050-EXIT.                                                       
    EXIT.                                                        
                                                                 
EJECT                                                            
8060-MOVE-NAME.                                                  
    IF WS-NAME-SW GREATER THAN +1                                
        GO TO 8060-EXIT.                                         
                                                                 
    IF WS-NAME-WORK2 = SPACES                                    
        GO TO 8060-EXIT.                                         
                                                                 
    SET NWA-INDEX2            TO +1.                             
    SET NWA-INDEX3            TO +2.                             
                                                                 
8060-MOVE-NAME-CYCLE.                                            
    MOVE WS-NW2 (NWA-INDEX2)  TO WS-NW (NWA-INDEX).              
                                                                 
    IF NWA-INDEX LESS THAN +30                                   
        SET NWA-INDEX UP BY +1                                   
    ELSE                                                         
        ADD +2 TO  WS-NAME-SW                                    
        GO TO 8060-EXIT.                                         
                                                                 
    IF NWA-INDEX2 LESS THAN +20                                  
        SET NWA-INDEX3 UP BY +1                                  
        SET NWA-INDEX2 UP BY +1.                                 
                                                                 
    IF WS-NW2 (NWA-INDEX2) = SPACES  AND                         
       WS-NW2 (NWA-INDEX3) = SPACES                              
        IF WS-NAME-SW = ZERO                                     
            MOVE ','            TO WS-NW (NWA-INDEX)             
            SET NWA-INDEX UP BY +2                               
            MOVE +1             TO WS-NAME-SW                    
            GO TO 8060-EXIT                                      
        ELSE                                                     
            GO TO 8060-EXIT.                                     
                                                                 
    GO TO 8060-MOVE-NAME-CYCLE.                                  
                                                                 
8060-EXIT.                                                       
    EXIT.                                                        
                                                                 
    EJECT                                                        
                                                                 
