    MOVE -1                     TO FORML.                        
    MOVE AL-UABON               TO FORMA.                        
    PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT.                    
    GO TO 8350-SEND-DATAONLY-ERASEAUP.                           
                                EJECT                            
2100-PROCESS-FORM-CONTROL-LINE.                                  
                                                                 
    MOVE TX-TEXT-LINE           TO W-LINE-CONTROL-RECORD.        
                                                                 
    IF  W-LC-LINE-WIDTH NUMERIC                                  
            AND                                                  
        W-LC-LINE-WIDTH GREATER THAN ZEROS                       
        COMPUTE W-INITIAL-COLUMN                                 
            = (70 - W-LC-LINE-WIDTH) / 2                         
        COMPUTE W-START-COLUMN = W-INITIAL-COLUMN + 1            
        COMPUTE W-LAST-COLUMN ROUNDED                            
            = W-INITIAL-COLUMN + W-LC-LINE-WIDTH                 
        COMPUTE W-TOO-FAR = W-LAST-COLUMN + 1                    
    ELSE                                                         
        MOVE 70                 TO W-LC-LINE-WIDTH.              
                                                                 
    IF  W-LC-PARAGRAPH-INDENT NUMERIC                            
            AND                                                  
        W-LC-PARAGRAPH-INDENT GREATER THAN ZEROS                 
        MOVE W-LC-PARAGRAPH-INDENT                               
                                TO W-PARAGRAPH-INDENT.           
                                                                 
    IF  W-LC-MAX-LINES-PER-PAGE NUMERIC                          
            AND                                                  
        W-LC-MAX-LINES-PER-PAGE GREATER THAN ZEROS               
        MOVE W-LC-MAX-LINES-PER-PAGE                             
                                TO W-MAX-LINES-PER-PAGE.         
                                                                 
    IF  W-LC-LINE-ADJUST NUMERIC                                 
            AND                                                  
        W-LC-LINE-ADJUST GREATER THAN ZEROS                      
        COMPUTE W-TOTAL-LINE-LENGTH                              
            = W-LC-LINE-WIDTH + (W-LC-LINE-ADJUST * 2)           
        IF  W-TOTAL-LINE-LENGTH LESS THAN +71                    
            ADD W-LC-LINE-ADJUST                                 
                                TO W-INITIAL-COLUMN              
                                   W-LAST-COLUMN                 
                                   W-START-COLUMN                
                                   W-TOO-FAR.                    
                                                                 
    IF  W-LC-LINE-INDENT-1 NUMERIC                               
            AND                                                  
        W-LC-LINE-INDENT-1 LESS THAN W-LC-LINE-WIDTH             
        MOVE W-LC-LINE-INDENT-1 TO W-LINE-INDENT-1               
    ELSE                                                         
        MOVE +0                 TO W-LINE-INDENT-1.              
                                                                 
    IF  W-LC-LINE-INDENT-2 NUMERIC                               
            AND                                                  
        W-LC-LINE-INDENT-2 LESS THAN W-LC-LINE-WIDTH             
        MOVE W-LC-LINE-INDENT-2 TO W-LINE-INDENT-2               
    ELSE                                                         
        MOVE +0                 TO W-LINE-INDENT-2.              
                                                                 
    IF  W-LC-LINE-INDENT-3 NUMERIC                               
            AND                                                  
        W-LC-LINE-INDENT-3 LESS THAN W-LC-LINE-WIDTH             
        MOVE W-LC-LINE-INDENT-3 TO W-LINE-INDENT-3               
