    IF  PI-689-LBL-OVERRIDE GREATER THAN SPACES                  
         MOVE PI-689-LBL-OVERRIDE                                
                                TO ADDRLBLI                      
         MOVE +1                TO ADDRLBLL                      
         MOVE AL-UANON          TO ADDRLBLA                      
    ELSE                                                         
         MOVE PI-LABEL-CONTROL  TO ADDRLBLI                      
         MOVE +1                TO ADDRLBLL                      
         MOVE AL-UANON          TO ADDRLBLA.                     
                                                                 
    IF  ARCHNUML EQUAL ZEROS                                     
        MOVE -1                 TO ARCHNUML                      
        MOVE AL-UNBON           TO ARCHNUMA                      
        MOVE ER-0174            TO EMI-ERROR                     
        PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT                 
        GO TO 8200-SEND-DATAONLY.                                
                                                                 
    IF  ARCHNUMI NOT NUMERIC                                     
        MOVE -1                 TO ARCHNUML                      
        MOVE AL-UNBON           TO ARCHNUMA                      
        MOVE ER-0175            TO EMI-ERROR                     
        PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT                 
        GO TO 8200-SEND-DATAONLY.                                
                                                                 
    IF  PI-SHOW-MODE                                             
                                                                 
        IF  PI-689-ARCHIVE-NUMBER EQUAL ARCHNUMI                 
            MOVE -1             TO MAINTL                        
            GO TO 8200-SEND-DATAONLY                             
        ELSE                                                     
            PERFORM 7780-DELETE-TEMP-STOR-TEXT THRU 7780-EXIT.   
                                                                 
    MOVE +0                     TO W-ARCH-SEQ-NO                 
                                   W-ARCT-SEQ-NO.                
    MOVE PI-COMPANY-CD          TO W-ARCH-COMPANY-CD             
                                   W-ARCT-COMPANY-CD.            
    MOVE ARCHNUMI               TO W-ARCH-NUMBER                 
                                   W-ARCT-NUMBER                 
                                   PI-689-ARCHIVE-NUMBER.        
    MOVE ' '                    TO W-ARCT-REC-TYPE.              
    MOVE '1'                    TO PI-ACTION.                    
    MOVE SPACES                 TO PI-689-PRINT-SW               
                                   PI-689-FORM-NUMBER            
                                   W-DELETE-KEY.                 
    SET W-RG-NDX                TO W-ZEROS.                      
                                                                 
                                                                 
