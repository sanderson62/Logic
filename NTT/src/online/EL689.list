    CALL 'kxdfhei1' USING DFHEIV0,                               
          W-TEXT-FILE-ID,                                        
          DFHEIV11,                                              
          DFHEIV99                                               
    GO TO 9999-DFHEXIT DEPENDING ON DFHEIGDK.                    
                                                                 
                                                                 
    IF  W-TEXT-BROWSE-NOT-STARTED                                
        MOVE ER-0006            TO EMI-ERROR                     
        MOVE -1                 TO FORML                         
        MOVE AL-UABON           TO FORMA                         
        PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT                 
        GO TO 8350-SEND-DATAONLY-ERASEAUP.                       
                                                                 
    SET  W-TOTAL-TX-LINES       TO W-TG-NDX.                     
    SUBTRACT +1 FROM W-TOTAL-TX-LINES.                           
    MOVE 1                      TO PI-CURRENT-LINE.              
                                                                 
    IF  COPIESL NOT GREATER THAN ZEROS                           
        MOVE +1                 TO COPIESI                       
                                   PI-689-NUMBER-COPIES          
        MOVE +1                 TO COPIESL                       
        MOVE AL-UNNON           TO COPIESA.                      
                                                                 
*>**************************************************************   
*>    IF IT IS A FORM WITH VARIABLES, THEN RESOLVE ALL OF      *   
*>    VARIABLE SYMBOLS AND INSERT THEM INTO THE TEXT DATA.     *   
*>**************************************************************   
                                                                 
    PERFORM 6000-RESOLVE-VARIABLES THRU 6000-EXIT.               
                                                                 
*>    IF  PI-689-PRINT-RESTRICTION EQUAL 'C'                       
*>        IF  PI-689-CONTROL NOT GREATER THAN ZEROS                
*>            MOVE ER-7243        TO EMI-ERROR                     
*>            MOVE -1             TO PRTRESTL                      
*>            MOVE AL-UABON       TO PRTRESTA                      
*>            PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT.            
                                                                 
    IF  PI-CREATE-LABELS AND                                     
        NOT  PI-689-LABELS-OVERRIDEN                             
        SET W-RG-NDX            TO PI-689-NUMBER-LABEL-LINES     
        SET W-RG-NDX UP BY +1                                    
    ELSE                                                         
        SET W-RG-NDX            TO +1.                           
                                                                 
    MOVE W-TOP-FORM             TO W-RC-TEXT (W-RG-NDX).         
    SET W-RG-NDX UP BY W-TOP-MARGIN.                             
                                                                 
    PERFORM 7400-CREATE-LETTER THRU 7400-EXIT.                   
                                                                 
    SET PI-TOTAL-LINES          TO W-RG-NDX.                     
                                                                 
    IF  W-FIRST-BAD-VARIABLE-FOUND                               
        SET W-RG-NDX            TO W-FIRST-BAD-VARIABLE          
        MOVE W-FIRST-BAD-VARIABLE                                
                                TO PI-CURRENT-LINE               
    ELSE                                                         
        SET W-RG-NDX            TO 1.                            
                                                                 
    PERFORM 7000-FORMAT-SCREEN THRU 7000-EXIT                    
            VARYING                                              
        W-SC-NDX FROM 1 BY 1                                     
            UNTIL                                                
        W-SC-NDX GREATER W-NUM-LINES-PER-SCREEN.                 
    IF  PI-689-CREATE-NO-SCREENS                                 
        GO TO 2000-EXIT                                          
    ELSE                                                         
        IF  W-FORM-CHANGED                                       
            MOVE '3'            TO PI-ACTION                     
            MOVE -1             TO MAINTL                        
            GO TO 8100-SEND-INITIAL-MAP                          
        ELSE                                                     
            MOVE '3'            TO PI-ACTION                     
            MOVE -1             TO MAINTL                        
            GO TO 8350-SEND-DATAONLY-ERASEAUP.                   
                                                                 
2000-EXIT.                                                       
