*>         DATASET   (W-ARCT-FILE-ID)                              
*>         RIDFLD    (W-ARCT-KEY)                                  
*>    END-EXEC.                                                    
*>                                                                 
*>    IF  W-ARCH-PARTIAL-KEY EQUAL W-ARCT-PARTIAL-KEY              
*>        IF  LT-ADDRESS-DATA                                      
*>            IF  PI-CREATE-LABELS  AND                            
*>                NOT PI-689-LABELS-OVERRIDEN                      
*>                PERFORM 1050-MOVE-LABELS THRU 1050-EXIT          
*>                        VARYING                                  
*>                    LT-NDX FROM 1 BY 1                           
*>                        UNTIL                                    
*>                    LT-NDX GREATER THAN PI-689-NUMBER-LABEL-LINES
*>                GO TO 1010-READ-NEXT                             
*>            ELSE                                                 
*>                MOVE LT-CONTROL-PRIMARY                          
*>                                TO W-DELETE-KEY                  
*>                GO TO 1010-READ-NEXT                             
*>        ELSE                                                     
*>            PERFORM 1030-TEXT-BUILD THRU 1030-EXIT               
*>                    VARYING                                      
*>                LT-NDX FROM 1 BY 1                               
*>                    UNTIL                                        
*>                LT-NDX GREATER THAN LT-NUM-LINES-ON-RECORD       
*>                    OR                                           
*>                LT-NDX GREATER THAN +20                          
*>            GO TO 1010-READ-NEXT.                                
*>                                                                 
*>1020-ARCT-ENDBR.                                                 
*>                                                                 
*>    IF  W-ARCT-BROWSE-STARTED EQUAL 'Y'                          
*>        EXEC CICS ENDBR                                          
*>             DATASET   (W-ARCT-FILE-ID)                          
*>        END-EXEC.                                                
*>                                                                 
*>    IF  W-RG-NDX EQUAL ZEROS                                     
*>        GO TO 1080-ARCHIVE-TEXT-NOT-FOUND.                       
*>                                                                 
*>    IF  PI-689-NUMBER-LABEL-LINES GREATER THAN +0                
*>            AND                                                  
*>        PI-BYPASS-LABELS                                         
*>        MOVE W-DELETE-KEY       TO W-ARCT-KEY                    
*>        PERFORM 1060-DELETE-ADDRESS-RECORD THRU 1060-EXIT        
*>        MOVE +0                 TO PI-689-NUMBER-LABEL-LINES.    
*>                                                                 
*>    SET PI-TOTAL-LINES          TO W-RG-NDX.                     
*>    MOVE 1                      TO PI-CURRENT-LINE.              
*>    SET W-RG-NDX                TO +1.                           
*>                                                                 
*>    PERFORM 7000-FORMAT-SCREEN THRU 7000-EXIT                    
*>            VARYING                                              
*>        W-SC-NDX FROM 1 BY 1                                     
*>            UNTIL                                                
*>        W-SC-NDX GREATER THAN W-NUM-LINES-PER-SCREEN.            
                                                                 
    MOVE -1                     TO MAINTL.                       
    GO TO 8100-SEND-INITIAL-MAP.                                 
                                                                 
1020-EXIT.                                                       
    EXIT.                                                        
                                                                 
1030-TEXT-BUILD.                                                 
                                                                 
    SET W-RG-NDX UP BY 1.                                        
    MOVE LT-SKIP-CONTROL (LT-NDX)                                
                                TO W-PRINT-CONTROL.              
                                                                 
    IF  W-PRINT-CONTROL EQUAL 99                                 
        MOVE W-TOP-FORM         TO W-RC-TEXT (W-RG-NDX)          
        SET W-RG-NDX UP BY 1                                     
    ELSE                                                         
        SET W-RG-NDX UP BY W-PRINT-CONTROL.                      
                                                                 
    MOVE LT-TEXT-LINE (LT-NDX)  TO W-RC-TEXT (W-RG-NDX).         
    MOVE LT-SKIP-CONTROL (LT-NDX)                                
                                TO W-RC-PC (W-RG-NDX).           
    MOVE SPACES                 TO W-RC-SC (W-RG-NDX).           
                                                                 
1030-EXIT.                                                       
    EXIT.                                                        
                                                                 
1040-FORMAT-HEADER-DATA.                                         
                                                                 
    MOVE LA-ACCOUNT-A2          TO ACCTO.                        
    MOVE LA-ADDR-SOURCE         TO ADDRSO.                       
    MOVE LA-CARRIER-A2          TO CARRIERO.                     
    MOVE LA-CERT-NO-A2          TO CERTO.                        
    MOVE LA-CERT-SUFFIX-A2      TO SFXO.                         
    MOVE LA-DATA-SOURCE         TO DATASORO.                     
                                                                 
    IF  LA-EFFECT-DATE-A2 EQUAL LOW-VALUES                       
        MOVE ZEROES             TO DATEO                         
                                                                 
    ELSE                                                         
        MOVE LA-EFFECT-DATE-A2  TO DC-BIN-DATE-1                 
        MOVE ' '                TO DC-OPTION-CODE                
        PERFORM 9500-LINK-DATE-CONVERT THRU 9500-EXIT            
        MOVE DC-GREG-DATE-1-EDIT                                 
                                TO DATEO.                        
                                                                 
    IF  LA-FOLLOW-UP-DATE EQUAL LOW-VALUES                       
        MOVE ZEROES             TO FOLLOWO                       
    ELSE                                                         
        MOVE LA-FOLLOW-UP-DATE  TO DC-BIN-DATE-1                 
        MOVE ' '                TO DC-OPTION-CODE                
        PERFORM 9500-LINK-DATE-CONVERT THRU 9500-EXIT            
        MOVE DC-GREG-DATE-1-EDIT                                 
                                TO FOLLOWO.                      
                                                                 
    MOVE LA-FORM-A3             TO FORMO.                        
    MOVE LA-GROUPING-A2         TO GROUPO.                       
    MOVE LA-NO-OF-COPIES        TO COPIESO                       
                                   PI-689-NUMBER-COPIES.         
                                                                 
*>    IF LA-PRINT-RESTRICTION = 'C' OR 'F' OR 'P'                  
*>        MOVE LA-PRINT-RESTRICTION                                
*>                                TO PRTRESTO                      
*>    ELSE                                                         
*>        MOVE SPACES             TO PRTRESTO.                     
                                                                 
    IF  LA-RESEND-DATE = LOW-VALUES                              
        MOVE ZEROES             TO RESEND1O                      
    ELSE                                                         
        MOVE LA-RESEND-DATE     TO DC-BIN-DATE-1                 
        MOVE ' '                TO DC-OPTION-CODE                
        PERFORM 9500-LINK-DATE-CONVERT THRU 9500-EXIT            
        MOVE DC-GREG-DATE-1-EDIT                                 
                                TO RESEND1O.                     
                                                                 
    MOVE LA-STATE-A2            TO STATEO.                       
                                                                 
    MOVE LA-NUMBER-LABEL-LINES  TO PI-689-NUMBER-LABEL-LINES.    
                                                                 
1040-EXIT.                                                       
    EXIT.                                                        
                                EJECT                            
1050-MOVE-LABELS.                                                
                                                                 
    SET W-RG-NDX                TO LT-NDX.                       
    MOVE ZEROS                  TO W-RC-PC (W-RG-NDX).           
    MOVE SPACES                 TO W-RC-SC (W-RG-NDX).           
    MOVE LT-TEXT-LINE (LT-NDX)  TO W-RC-TEXT (W-RG-NDX).         
                                                                 
1050-EXIT.                                                       
    EXIT.                                                        
                                EJECT                            
1060-DELETE-ADDRESS-RECORD.                                      
                                                                 
                                                                 
