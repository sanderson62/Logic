2500-LINE-CHECK.                                                 
    IF LINE1L = ZEROS AND                                        
       LINE2L = ZEROS                                            
       MOVE ER-0069             TO EMI-ERROR                     
       PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT                  
       MOVE -1                  TO LINE1L                        
       GO TO 8200-SEND-DATAONLY                                  
    END-IF.                                                      
                                                                 
    IF LINE1L NOT = ZEROS                                        
       IF LINE1I = ZERO AND FUNCTI EQUAL 'L'                     
           MOVE 1               TO LINE1I                        
       END-IF                                                    
       IF LINE1I = ZERO AND FUNCTI EQUAL 'D'                     
          MOVE ER-0049          TO EMI-ERROR                     
          MOVE AL-UNBON         TO LINE1A                        
          MOVE -1               TO LINE1L                        
          PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT               
          GO TO 8200-SEND-DATAONLY                               
       END-IF                                                    
       IF LINE1I NOT NUMERIC OR                                  
          LINE1I > PI-TOTAL-LINES                                
          MOVE ER-0031          TO EMI-ERROR                     
          MOVE AL-UNBON         TO LINE1A                        
          MOVE -1               TO LINE1L                        
          PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT               
          GO TO 8200-SEND-DATAONLY                               
       ELSE                                                      
          IF LINE2L = ZEROS                                      
             MOVE 1             TO LINE2I                        
             IF FUNCTI = 'I'                                     
                 GO TO 2510-MAX-CHECK                            
             ELSE                                                
                 NEXT SENTENCE                                   
             END-IF                                              
          ELSE                                                   
             IF FUNCTI = 'I'                                     
                GO TO 2510-MAX-CHECK                             
             ELSE                                                
                IF LINE2I NOT NUMERIC                            
                   MOVE AL-UNBON TO LINE2A                       
                   MOVE ER-0032  TO EMI-ERROR                    
                   MOVE -1       TO LINE2L                       
                   PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT      
                   GO TO 8200-SEND-DATAONLY                      
                ELSE                                             
                   NEXT SENTENCE                                 
                END-IF                                           
             END-IF                                              
          END-IF                                                 
       END-IF                                                    
    ELSE                                                         
       IF LINE2L = ZEROS                                         
          NEXT SENTENCE                                          
       ELSE                                                      
          MOVE -1               TO LINE2L                        
          MOVE ER-0041          TO EMI-ERROR                     
          MOVE AL-UNBON         TO LINE2A                        
          PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT               
          GO TO 8200-SEND-DATAONLY                               
       END-IF                                                    
    END-IF.                                                      
    GO TO 2599-EXIT.                                             
2510-MAX-CHECK.                                                  
    IF LINE2I NOT NUMERIC                                        
       MOVE -1                  TO LINE2L                        
       MOVE ER-0032             TO EMI-ERROR                     
       MOVE AL-UNBON            TO LINE2A                        
       PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT                  
       GO TO 8200-SEND-DATAONLY                                  
    ELSE                                                         
       COMPUTE ROLL-COUNTER = LINE2I + PI-TOTAL-LINES            
       IF ROLL-COUNTER GREATER THAN MAX-LINES                    
          MOVE -1               TO LINE2L                        
          MOVE ER-0044          TO EMI-ERROR                     
          MOVE AL-UNBON         TO LINE2A                        
          PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT               
          GO TO 8200-SEND-DATAONLY                               
       END-IF                                                    
    END-IF.                                                      
2599-EXIT.                                                       
     EXIT.                                                       
    EJECT                                                        
3000-DELETE-LINES.                                               
    IF LINE2L = ZEROS AND LINE2I = 1                             
       MOVE LINE1I              TO LINE2I                        
    END-IF.                                                      
                                                                 
    IF LINE2I > PI-TOTAL-LINES OR < LINE1I                       
       MOVE ER-0049             TO EMI-ERROR                     
       MOVE AL-UNBON            TO LINE2A                        
       MOVE -1                  TO LINE2L                        
       PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT                  
       GO TO 8200-SEND-DATAONLY                                  
    END-IF.                                                      
                                                                 
    PERFORM 7450-SET-INDX THRU 7450-EXIT.                        
    PERFORM 7600-UPDATE-TABLE-FROM-SCREEN THRU 7699-EXIT         
            VARYING SC-INDX FROM 1 BY 1 UNTIL                    
            SC-INDX > NUM-LINES-PER-SCREEN                       
    SET TB-INDX TO LINE1I.                                       
                                                                 
    IF NOT EMI-NO-ERRORS                                         
       GO TO 8200-SEND-DATAONLY                                  
    END-IF.                                                      
                                                                 
    SET TB-INDX TO LINE1I                                        
    COMPUTE ROLL-COUNTER = LINE2I - LINE1I + 1.                  
                                                                 
    IF LINE2I NOT = PI-TOTAL-LINES                               
       SET TB-INDX1 TO LINE2I                                    
       SET TB-INDX1 UP BY 1                                      
       PERFORM 3100-DELETE-TABLE-ENTRIES                         
               UNTIL TB-INDX1 > PI-TOTAL-LINES                   
    END-IF.                                                      
                                                                 
    PERFORM 3150-BLANK-TABLE-ENTRIES                             
            ROLL-COUNTER TIMES.                                  
    SUBTRACT ROLL-COUNTER FROM PI-TOTAL-LINES.                   
                                                                 
    IF PI-CURRENT-LINE > PI-TOTAL-LINES                          
       MOVE PI-TOTAL-LINES      TO PI-CURRENT-LINE               
       SUBTRACT 1 FROM PI-CURRENT-LINE                           
    END-IF.                                                      
                                                                 
    SET TB-INDX  TO PI-CURRENT-LINE                              
    MOVE LOW-VALUES             TO EL1284AI                      
                                   EL1284R                       
                                                                 
    PERFORM 7800-MOVE-SAVE-TO-MAP.                               
    IF PI-CURRENT-LINE > ZERO                                    
        PERFORM 7100-FORMAT-SCREEN THRU 7100-EXIT                
            VARYING SC-INDX FROM 1 BY 1 UNTIL                    
            SC-INDX > NUM-LINES-PER-SCREEN                       
        PERFORM 7200-PUT-TEMP-STOR  THRU 7249-EXIT               
    END-IF.                                                      
                                                                 
    MOVE '1'                    TO PI-UPDATE-SW.                 
    IF PI-TOTAL-LINES = ZEROS                                    
       MOVE ZEROS               TO PI-CURRENT-LINE               
    END-IF.                                                      
                                                                 
    MOVE -1 TO FUNCTL                                            
    SET CURSOR-SET TO TRUE                                       
                                                                 
    GO TO 8100-SEND-INITIAL-MAP.                                 
    EJECT                                                        
                                                                 
3100-DELETE-TABLE-ENTRIES.                                       
    MOVE REC-ENT (TB-INDX1)     TO REC-ENT (TB-INDX)             
    SET TB-INDX TB-INDX1 UP BY 1.                                
                                                                 
3150-BLANK-TABLE-ENTRIES.                                        
    MOVE SPACES               TO REC-ENT (TB-INDX).              
    MOVE SAVE-BIN-DATE        TO REC-LAST-MAINT-DT (TB-INDX).    
    MOVE EIBTIME              TO REC-LAST-MAINT-HHMMSS (TB-INDX).
    MOVE PI-PROCESSOR-ID      TO REC-LAST-MAINT-BY (TB-INDX).    
    SET TB-INDX UP BY 1.                                         
    EJECT                                                        
3500-INSERT-LINES.                                               
                                                                 
    PERFORM 7450-SET-INDX THRU 7450-EXIT.                        
    PERFORM 7600-UPDATE-TABLE-FROM-SCREEN THRU 7699-EXIT         
            VARYING SC-INDX FROM 1 BY 1 UNTIL                    
            SC-INDX > NUM-LINES-PER-SCREEN.                      
                                                                 
    IF NOT EMI-NO-ERRORS                                         
       GO TO 8200-SEND-DATAONLY                                  
    END-IF.                                                      
                                                                 
    SET TB-INDX TO PI-TOTAL-LINES.                               
    ADD LINE2I TO PI-TOTAL-LINES.                                
    SET TB-INDX1 TO PI-TOTAL-LINES.                              
    PERFORM 3600-INSERT-TABLE-ENTRIES                            
            UNTIL TB-INDX = LINE1I.                              
    SET TB-INDX UP BY 1.                                         
                                                                 
    IF LINE1I EQUAL ZERO                                         
        SET PI-CURRENT-LINE TO 1                                 
    ELSE                                                         
        SET PI-CURRENT-LINE TO LINE1I                            
    END-IF.                                                      
                                                                 
    COMPUTE ROLL-COUNTER = PI-CURRENT-LINE +                     
                           NUM-LINES-PER-SCREEN.                 
    IF TB-INDX NOT LESS THAN ROLL-COUNTER OR                     
                   LESS THAN PI-CURRENT-LINE                     
       SET SC-INDX TO 1                                          
       SET SC-INDX DOWN BY 1                                     
    ELSE                                                         
       SET ROLL-COUNTER TO TB-INDX                               
       COMPUTE ROLL-COUNTER = ROLL-COUNTER - PI-CURRENT-LINE     
                 + 1                                             
       SET SC-INDX TO ROLL-COUNTER                               
    END-IF.                                                      
                                                                 
    PERFORM 3150-BLANK-TABLE-ENTRIES LINE2I TIMES.               
    SET TB-INDX TO PI-CURRENT-LINE.                              
    MOVE LOW-VALUES             TO EL1284AI                      
                                   EL1284R                       
                                                                 
    IF SC-INDX NOT = ZERO                                        
       MOVE -1 TO SC-TEXTL (SC-INDX)                             
       SET CURSOR-SET TO TRUE                                    
    END-IF.                                                      
                                                                 
    PERFORM 7800-MOVE-SAVE-TO-MAP.                               
                                                                 
    PERFORM 7100-FORMAT-SCREEN THRU 7100-EXIT                    
           VARYING SC-INDX FROM 1 BY 1 UNTIL                     
           SC-INDX > NUM-LINES-PER-SCREEN.                       
    PERFORM 7200-PUT-TEMP-STOR  THRU 7249-EXIT.                  
    MOVE '1'                    TO PI-UPDATE-SW.                 
                                                                 
    MOVE -1 TO FUNCTL                                            
    SET CURSOR-SET TO TRUE                                       
                                                                 
    GO TO 8100-SEND-INITIAL-MAP.                                 
                                                                 
3600-INSERT-TABLE-ENTRIES.                                       
    MOVE REC-ENT (TB-INDX)      TO REC-ENT (TB-INDX1).           
    SET TB-INDX TB-INDX1 DOWN BY 1.                              
    EJECT                                                        
                                                                 
4000-CHANGE-ROUTINE.                                             
    PERFORM 7450-SET-INDX THRU 7450-EXIT.                        
