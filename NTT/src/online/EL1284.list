4900-EXIT.                                                       
     EXIT.                                                       
                                                                 
    EJECT                                                        
5000-ADD-NEW-LINES.                                              
    PERFORM 7450-SET-INDX THRU 7450-EXIT.                        
    MOVE 'N'                    TO WS-SCREEN-LINE.               
    MOVE 1                      TO WS-SUB.                       
    MOVE ZERO                   TO WS-SUB1.                      
    PERFORM VARYING SC-INDX FROM NUM-LINES-PER-SCREEN BY -1      
            UNTIL SCREEN-LINE-FOUND OR SC-INDX < 1               
        IF SC-TEXT (SC-INDX) >  SPACES                           
            SET WS-SUB          TO SC-INDX                       
            MOVE 'Y'            TO WS-SCREEN-LINE                
        END-IF                                                   
    END-PERFORM.                                                 
    IF PI-TOTAL-LINES = 0                                        
        MOVE 1                  TO WS-SUB1                       
    ELSE                                                         
        MOVE 2                  TO WS-SUB1                       
    END-IF.                                                      
    PERFORM VARYING SC-INDX FROM WS-SUB1 BY 1                    
            UNTIL SC-INDX GREATER THAN WS-SUB                    
        IF SC-TEXTL (SC-INDX) EQUAL ZEROS                        
            MOVE 1      TO SC-TEXTL (SC-INDX)                    
            MOVE SPACES TO SC-TEXT (SC-INDX)                     
            MOVE LOW-VALUES TO SC-MTBY (SC-INDX)                 
                               SC-MTDT (SC-INDX)                 
        END-IF                                                   
    END-PERFORM.                                                 
    PERFORM 7600-UPDATE-TABLE-FROM-SCREEN THRU 7699-EXIT         
            VARYING SC-INDX FROM 1 BY 1 UNTIL                    
            SC-INDX > NUM-LINES-PER-SCREEN.                      
                                                                 
    IF NOT EMI-NO-ERRORS                                         
       GO TO 8200-SEND-DATAONLY                                  
    END-IF.                                                      
                                                                 
    MOVE PI-TOTAL-LINES         TO  PI-CURRENT-LINE.             
    PERFORM 7200-PUT-TEMP-STOR THRU 7249-EXIT.                   
    MOVE LOW-VALUES             TO  EL1284AI                     
                                    EL1284R                      
    PERFORM 7800-MOVE-SAVE-TO-MAP.                               
                                                                 
    SET TB-INDX TO PI-CURRENT-LINE.                              
    MOVE 'A'                    TO FUNCTI.                       
    MOVE -1                     TO  SC-TEXTL (2).                
    SET CURSOR-SET TO TRUE                                       
    MOVE AL-UANON               TO  FUNCTA.                      
    PERFORM 7100-FORMAT-SCREEN THRU 7100-EXIT                    
            VARYING SC-INDX FROM 1 BY 1 UNTIL                    
            SC-INDX > NUM-LINES-PER-SCREEN.                      
    MOVE '1'                    TO PI-UPDATE-SW.                 
                                                                 
    GO TO 8100-SEND-INITIAL-MAP.                                 
    EJECT                                                        
5500-LOOKUP.                                                     
    PERFORM 7500-READ-TS THRU 7599-EXIT.                         
    SET TB-INDX TO PI-CURRENT-LINE.                              
    PERFORM 7600-UPDATE-TABLE-FROM-SCREEN THRU 7699-EXIT         
            VARYING SC-INDX FROM 1 BY 1 UNTIL                    
            SC-INDX > NUM-LINES-PER-SCREEN.                      
                                                                 
    IF NOT EMI-NO-ERRORS                                         
       GO TO 8200-SEND-DATAONLY                                  
    END-IF.                                                      
                                                                 
    MOVE LINE1I                 TO  PI-CURRENT-LINE.             
    SET TB-INDX                 TO PI-CURRENT-LINE.              
    MOVE LOW-VALUES             TO  EL1284AI                     
                                    EL1284R                      
    PERFORM 7800-MOVE-SAVE-TO-MAP.                               
                                                                 
    PERFORM 7100-FORMAT-SCREEN THRU 7100-EXIT                    
            VARYING SC-INDX FROM 1 BY 1                          
            UNTIL SC-INDX > NUM-LINES-PER-SCREEN.                
    PERFORM 7200-PUT-TEMP-STOR THRU 7249-EXIT.                   
                                                                 
    MOVE -1 TO FUNCTL                                            
    SET CURSOR-SET TO TRUE                                       
                                                                 
    GO TO 8100-SEND-INITIAL-MAP.                                 
    EJECT                                                        
6000-NOT-OPEN.                                                   
    MOVE ER-2954                TO  EMI-ERROR.                   
    PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT.                    
                                                                 
    IF EIBAID = DFHCLEAR                                         
        GO TO 9410-RETURN                                        
    ELSE                                                         
        GO TO 8100-SEND-INITIAL-MAP                              
    END-IF.                                                      
    EJECT                                                        
7000-BUILD-TABLE.                                                
                                                                 
    SET TB-INDX TO 1.                                            
    MOVE ZEROS                  TO  PI-TOTAL-LINES               
                                    PI-CURRENT-LINE              
                                    PI-TEMP-STOR-ITEMS           
                                    PI-UPDATE-SW.                
    MOVE LOW-VALUES             TO  EL1284AI                     
                                    EL1284R                      
                                                                 
*>***IF TEMP STORAGE EXISTS, DELETE IT.                            
    IF PI-CHANGE-IN-NOTE-TYPE                                    
        MOVE 'N' TO PI-SET-NOTE-CHANGE                           
    ELSE                                                         
        PERFORM 7500-READ-TS THRU 7599-EXIT                      
    END-IF.                                                      
                                                                 
    IF PI-TEMP-STOR-ITEMS NOT = ZERO                             
       PERFORM 7250-DELETE-TEMP-STOR THRU 7299-EXIT              
    END-IF.                                                      
                                                                 
                                                                 
