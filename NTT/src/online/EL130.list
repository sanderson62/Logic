4000-CALCULATE-CERT-TERM.                                        
                                                                 
    PERFORM 4100-CALC-GROSS-TERM THRU 4200-EXIT.                 
                                                                 
    MOVE N                      TO WS-TERM.                      
                                                                 
    IF WS-TERM GREATER 120                                       
        MOVE 120                TO WS-TERM                       
    END-IF.                                                      
                                                                 
4000-CALC-TERM-EXIT.                                             
     EXIT.                                                       
                                                                 
4100-CALC-GROSS-TERM.                                            
    MOVE HOLD-LOAN-BAL           TO L.                           
    MOVE WS-ACVBENE              TO M.                           
    COMPUTE N = L / M.                                           
    IF N LESS 1                                                  
        MOVE 1  TO N                                             
    END-IF.                                                      
                                                                 
    IF LCVCDL GREATER ZERO                                       
        COMPUTE WS-LF-RATE = WS-LCVRATE / +1000                  
    ELSE                                                         
        MOVE ZEROS               TO WS-LF-RATE                   
    END-IF.                                                      
                                                                 
    IF ACVCDL GREATER ZERO                                       
        COMPUTE WS-AH-RATE = WS-ACVRATE / +1000                  
    ELSE                                                         
        MOVE ZEROS               TO WS-AH-RATE                   
    END-IF.                                                      
                                                                 
    COMPUTE I = WS-APR / +1200.                                  
                                                                 
4105-LOOP.                                                       
    IF N GREATER 240                                             
        GO TO 4200-EXIT                                          
    END-IF.                                                      
                                                                 
    PERFORM 4210-CALC-LEFT-RIGHTONE THRU 4220-EXIT.              
                                                                 
    IF LEFT-TOT-1 GREATER RIGHT-TOT-1                            
        ADD 1 TO N                                               
        GO TO 4105-LOOP                                          
    END-IF.                                                      
                                                                 
4200-EXIT.                                                       
     EXIT.                                                       
                                                                 
4210-CALC-LEFT-RIGHTONE.                                         
     MOVE L         TO LEFT-TOT-1.                               
     SUBTRACT 1 FROM N.                                          
     PERFORM 4300-CALC-A-N THRU 4300-EXIT.                       
     PERFORM 4350-CALC-IA-N THRU 4400-EXIT.                      
     ADD 1 TO N.                                                 
                                                                 
4211-CALC-TERM1.                                                 
     MOVE N         TO TERM1.                                    
     MULTIPLY M BY TERM1.                                        
                                                                 
4212-LOOP.                                                       
     COMPUTE TERM1 = (WS-AH-RATE + WS-LF-RATE) * TERM1.          
     ADD 1 TO A-N.                                               
     MULTIPLY A-N BY TERM1.                                      
     SUBTRACT 1 FROM A-N.                                        
     ADD TERM1 TO LEFT-TOT-1.                                    
                                                                 
4213-CALC-TERM2.                                                 
     MOVE M         TO TERM2.                                    
     COMPUTE TERM2 = (WS-AH-RATE + WS-LF-RATE) * TERM2.          
     MULTIPLY IA-N BY TERM2.                                     
     SUBTRACT TERM2 FROM LEFT-TOT-1.                             
                                                                 
4215-CALC-RIGHTONE.                                              
     MOVE M         TO RIGHT-TOT-1.                              
     PERFORM 4300-CALC-A-N THRU 4300-EXIT.                       
     MULTIPLY A-N BY RIGHT-TOT-1.                                
                                                                 
4220-EXIT.                                                       
     EXIT.                                                       
                                                                 
4300-CALC-A-N.                                                   
    IF N LESS 1                                                  
        MOVE 0     TO A-N                                        
        GO TO 4300-EXIT                                          
    END-IF.                                                      
                                                                 
    IF I = 0                                                     
        MOVE .00001 TO I                                         
    END-IF.                                                      
                                                                 
    ADD 1 TO I.                                                  
    DIVIDE I INTO 1 GIVING V.                                    
    SUBTRACT 1 FROM I.                                           
    PERFORM 4450-CALC-V-EX-N THRU 4490-EXIT.                     
    SUBTRACT V-EX-N FROM 1 GIVING TERM3.                         
    DIVIDE I INTO TERM3 GIVING A-N.                              
                                                                 
4300-EXIT.                                                       
     EXIT.                                                       
                                                                 
4350-CALC-IA-N.                                                  
    IF N LESS 1                                                  
        MOVE 0      TO IA-N                                      
        GO TO 4400-EXIT                                          
    END-IF.                                                      
                                                                 
    ADD 1 TO N.                                                  
    PERFORM 4450-CALC-V-EX-N THRU 4490-EXIT.                     
    SUBTRACT 1 FROM N.                                           
    MULTIPLY N BY V-EX-N GIVING TERM3.                           
    SUBTRACT TERM3 FROM A-N GIVING TERM3.                        
    SUBTRACT V FROM 1 GIVING TERM4.                              
    DIVIDE TERM4 INTO TERM3 GIVING IA-N.                         
                                                                 
4400-EXIT.                                                       
     EXIT.                                                       
                                                                 
4450-CALC-V-EX-N.                                                
    IF N LESS 1                                                  
        MOVE 1    TO V-EX-N                                      
        GO TO 4490-EXIT                                          
    END-IF.                                                      
                                                                 
    MOVE N        TO NV-STORE.                                   
                                                                 
    IF V-EX-ONETIME = 1  OR                                      
       V NOT = V-EXPONENT (1)                                    
         PERFORM 4470-BUILD-V-EX-TABLE THRU 4480-EXIT            
    END-IF.                                                      
                                                                 
4460-LOOP.                                                       
    IF N GREATER 248                                             
        MOVE 248 TO N                                            
    END-IF.                                                      
                                                                 
    IF N = 1                                                     
        MOVE V               TO V-EX-N                           
    ELSE                                                         
        MOVE V-EXPONENT (N)  TO V-EX-N                           
    END-IF.                                                      
                                                                 
    GO TO 4490-EXIT.                                             
                                                                 
4470-BUILD-V-EX-TABLE.                                           
    MOVE 2      TO N.                                            
    MOVE V      TO V-EXPONENT (1)                                
                   V-EX-N.                                       
                                                                 
4471-LOOP.                                                       
    MULTIPLY V BY V-EX-N.                                        
    MOVE V-EX-N   TO V-EXPONENT (N).                             
                                                                 
    ADD 1 TO N.                                                  
    IF N LESS 248                                                
        GO TO 4471-LOOP                                          
    END-IF.                                                      
                                                                 
    MOVE NV-STORE     TO N.                                      
    MOVE ZERO         TO V-EX-ONETIME.                           
                                                                 
4480-EXIT.                                                       
     EXIT.                                                       
                                                                 
4490-EXIT.                                                       
     EXIT.                                                       
                                                                 
4500-CALC-NET-TERM.                                              
    MOVE HOLD-LOAN-BAL           TO L.                           
    MOVE WS-ACVBENE              TO M.                           
    DIVIDE L BY M GIVING N REMAINDER WS-REMAIN.                  
                                                                 
    IF WS-REMAIN GREATER ZERO                                    
        ADD 1 TO N                                               
    END-IF.                                                      
                                                                 
    IF N = 0                                                     
        MOVE 1 TO N                                              
    END-IF.                                                      
                                                                 
