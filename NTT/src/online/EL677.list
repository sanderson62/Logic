       end-perform                                               
       string ws-user-reason  ' ' delimited by size              
              ws-user-cert-no (s1:11 - s1 + 1) ' '               
                 delimited by size                               
              cm-insured-last-name ' '                           
              cm-insured-first-name delimited by '  '            
          into stubo                                             
       end-string                                                
