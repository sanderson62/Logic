    if (pi-refund-on-pending-rec + pi-prev-paid) >               
       ws-tot-iss-prem                                           
       move zeros               to ws-refund-amount              
    end-if                                                       
                                                                 
    if ((dedcyni = 'Y')                                          
       or (pi-prev-ded-comm = 'Y'))                              
       and (ws-refund-amount >= ws-tot-ref-comm)                 
       compute ws-refund-amount = ws-refund-amount -             
