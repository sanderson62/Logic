    end-if                                                       
                                                                 
    display ' good get on table ' sqlcode                        
                                                                 
    if sql-py-LogicStatus = 2                                    
       move 'Remit previously processed '                        
                                to output-msg                    
       go to 0000-return                                         
    end-if                                                       
                                                                 
