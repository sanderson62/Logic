    SET  AD-INDEX  TO 1.                                         
    SET  OC-INDEX  TO 1.                                         
                                                                 
    IF OC-TABLE-LOADED                                           
       PERFORM  2710-PROCESS-OC  THRU 2710-EXIT                  
