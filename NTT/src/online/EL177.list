*>                      COPY ELC176P1.                             
*>>>((file: ELC176P1))                                             
*>**************************************************************** 
*>                                                               * 
*>                            ELC176P1.                          * 
*>           COPYBOOK REVIEWED FOR YEAR 2000 COMPLIANCE            
*>                            VMOD=2.003                         * 
*>                                                               * 
*> THIS SECTION CONVERTS A DOLLAR FIGURE INTO A SPELLED OUT AMT. * 
*>                                                               * 
*>**************************************************************** 
                                                                 
SDS-010.                                                         
    MOVE SPACES                 TO  WS-SPELLED-AMOUNT            
                                    SD-PASS-SPELLED-AMOUNT       
                                    WS-SPELLED-LINE1             
                                    WS-SPELLED-LINE2.            
                                                                 
    SET SA-INDEX TO +1.                                          
                                                                 
    MOVE SD-PASS-AMOUNT         TO  WS-AMOUNT.                   
                                                                 
    IF WS-MILLIONS GREATER ZERO                                  
        MOVE WS-MILLIONS        TO  WS-AMOUNT-WORK               
        PERFORM SPELL-AMOUNT                                     
        MOVE 'MILLION'          TO  WS-WORD                      
        PERFORM MOVE-WORD.                                       
                                                                 
    IF WS-THOUSANDS GREATER ZERO                                 
        MOVE WS-THOUSANDS       TO  WS-AMOUNT-WORK               
        PERFORM SPELL-AMOUNT                                     
        MOVE 'THOUSAND'         TO  WS-WORD                      
        PERFORM MOVE-WORD.                                       
                                                                 
    IF WS-HUNDREDS GREATER ZERO                                  
        MOVE WS-HUNDREDS        TO  WS-AMOUNT-WORK               
        PERFORM SPELL-AMOUNT.                                    
                                                                 
    IF WS-AMOUNT LESS +1.00                                      
        MOVE 'NO'               TO  WS-WORD                      
        PERFORM MOVE-WORD.                                       
                                                                 
    IF WS-CENTS NOT GREATER ZERO                                 
        MOVE 'NO'               TO  WS-CENTS-X.                  
                                                                 
    MOVE WS-CENTS-X             TO  WS-PENNEYS.                  
                                                                 
    MOVE WS-DOLLARS-AND-CENTS   TO  WS-WORD.                     
    PERFORM MOVE-WORD.                                           
                                                                 
    INSPECT WS-SPELLED-AMOUNT REPLACING ALL '-' BY SPACES.       
                                                                 
    MOVE WS-SPELLED-AMOUNT      TO  SD-PASS-SPELLED-AMOUNT.      
                                                                 
    PERFORM MOVE-SPELLED-AMOUNT.                                 
                                                                 
SDS-EXIT.                                                        
    EXIT.                                                        
                                                                 
    EJECT                                                        
SPELL-AMOUNT SECTION.                                            
                                                                 
*>SAS-NOTE.                                                        
*>                                                                 
*>    NOTE ******************************************************* 
*>         *      THIS SECTION CONVERTS A THREE DIGIT NUMBER     * 
*>         *  INTO A SPELLED AMOUNT.                             * 
*>         ******************************************************* 
                                                                 
SAS-010.                                                         
    IF WS-HUNDRED GREATER ZERO                                   
        SET SINGLE-INDEX        TO  WS-HUNDRED                   
        MOVE WS-SINGLE-DESC (SINGLE-INDEX)  TO  WS-WORD          
        PERFORM MOVE-WORD                                        
        MOVE 'HUNDRED'          TO  WS-WORD                      
        PERFORM MOVE-WORD.                                       
                                                                 
    IF WS-TEEN GREATER ZERO                                      
        IF WS-TEEN LESS +20                                      
            SET SINGLE-INDEX TO WS-TEEN                          
            MOVE WS-SINGLE-DESC (SINGLE-INDEX)  TO  WS-WORD      
            PERFORM MOVE-WORD                                    
          ELSE                                                   
            SET UPPER-INDEX TO WS-TEN                            
            MOVE WS-UPPER-DESC (UPPER-INDEX)  TO  WS-WORD        
            PERFORM MOVE-WORD                                    
            IF WS-ONE GREATER ZERO                               
                SET SINGLE-INDEX TO WS-ONE                       
                MOVE WS-SINGLE-DESC (SINGLE-INDEX)  TO  WS-WORD  
                PERFORM MOVE-WORD.                               
                                                                 
SAS-EXIT.                                                        
    EXIT.                                                        
                                                                 
    EJECT                                                        
MOVE-WORD SECTION.                                               
                                                                 
*>MWS-NOTE.                                                        
*>                                                                 
*>    NOTE ******************************************************* 
*>         *      THIS SECTION MOVES ONE WORD TO THE SPELLED     * 
*>         *  AMOUNT OUTPUT LINE.                                * 
*>         *******************************************************.
                                                                 
MWD-010.                                                         
    PERFORM MOVE-CHARACTERS                                      
        VARYING CHAR-INDEX FROM +1 BY +1                         
            UNTIL WS-CHAR2 (CHAR-INDEX) = SPACES.                
                                                                 
    SET SA-INDEX UP BY +1.                                       
                                                                 
MWD-EXIT.                                                        
    EXIT.                                                        
                                                                 
MOVE-CHARACTERS SECTION.                                         
                                                                 
*>MCS-NOTE.                                                        
*>                                                                 
*>    NOTE ******************************************************* 
*>         *      THIS SECTION MOVES ONE CHARACTER TO THE SPELLED* 
*>         *  AMOUNT OUTPUT LINE.                                * 
*>         *******************************************************.
                                                                 
MCD-010.                                                         
    MOVE WS-CHAR2 (CHAR-INDEX)  TO  WS-CHAR (SA-INDEX).          
                                                                 
    SET SA-INDEX UP BY +1.                                       
                                                                 
MCD-EXIT.                                                        
    EXIT.                                                        
                                                                 
    EJECT                                                        
MOVE-SPELLED-AMOUNT SECTION.                                     
                                                                 
*>MSA-NOTE.                                                        
*>                                                                 
*>    NOTE ******************************************************* 
*>         *      THIS SECTION MOVES THE SPELLED DOLLAR AMOUNT   * 
*>         *  TO TWO LINES IF NECESSARY.                         * 
*>         *******************************************************.
                                                                 
MSA-010.                                                         
    ADD WS-1ST-LINE-LENGTH +1 GIVING WS-1ST-LINE-LENGTH-PLUS-1.  
    ADD WS-1ST-LINE-LENGTH +2 GIVING WS-1ST-LINE-LENGTH-PLUS-2.  
    ADD WS-1ST-LINE-LENGTH -1 GIVING WS-1ST-LINE-LENGTH-MINUS-1. 
                                                                 
    MOVE WS-SPELLED-AMOUNT  TO  WS-SPELLED-LINE1.                
                                                                 
    IF SA-INDEX GREATER WS-1ST-LINE-LENGTH-PLUS-1                
        SET SL2-INDEX TO +1                                      
        IF WS-CHAR (WS-1ST-LINE-LENGTH-PLUS-1) = SPACES          
            PERFORM MOVE-LINE2 VARYING SA-INDEX2                 
              FROM WS-1ST-LINE-LENGTH-PLUS-2 BY +1               
                UNTIL SL2-INDEX GREATER WS-2ND-LINE-LENGTH       
          ELSE                                                   
            PERFORM CLEAR-LINE1 VARYING SL1-INDEX                
                FROM WS-1ST-LINE-LENGTH BY -1                    
                    UNTIL WS-SL1 (SL1-INDEX) = SPACES            
            SET SL1-INDEX UP BY +1                               
            PERFORM MOVE-LINE2                                   
              VARYING SA-INDEX2 FROM SL1-INDEX BY +1             
                UNTIL SL2-INDEX GREATER WS-2ND-LINE-LENGTH.      
                                                                 
    MOVE WS-SPELLED-LINE1       TO  SD-PSA-LINE1.                
    MOVE WS-SPELLED-LINE2       TO  SD-PSA-LINE2.                
                                                                 
MSA-EXIT.                                                        
    EXIT.                                                        
                                                                 
    EJECT                                                        
CLEAR-LINE1 SECTION.                                             
                                                                 
*>CLS-NOTE.                                                        
*>                                                                 
*>    NOTE ******************************************************* 
*>         *      THIS SECTION CLEARS THE TRAILING WORD IN THE   * 
*>         *  SPELLED LINE 1 IF THE AMOUNT IS GREATER THAN 78.   * 
*>         *******************************************************.
                                                                 
CLS-010.                                                         
    MOVE SPACES                 TO  WS-SL1 (SL1-INDEX).          
                                                                 
CLS-EXIT.                                                        
    EXIT.                                                        
                                                                 
MOVE-LINE2 SECTION.                                              
                                                                 
*>MLS-NOTE.                                                        
*>                                                                 
*>    NOTE ******************************************************* 
*>         *      THIS SECTION MOVES ONE CHARACTER TO THE SPELLED* 
*>         *  AMOUNT OUTPUT LINE.                                * 
*>         *******************************************************.
                                                                 
MLS-010.                                                         
    MOVE WS-CHAR (SA-INDEX2)    TO  WS-SL2 (SL2-INDEX).          
                                                                 
    SET SL2-INDEX UP BY +1.                                      
                                                                 
    IF WS-CHAR (SA-INDEX2)     = SPACES   AND                    
       WS-CHAR (SA-INDEX2 + 1) = SPACES                          
        SET SL2-INDEX TO +99.                                    
                                                                 
MLS-EXIT.                                                        
    EXIT.                                                        
                                                                 
