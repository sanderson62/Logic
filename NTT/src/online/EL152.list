7281-EXIT.                                                       
    EXIT.                                                        
    EJECT                                                        
                                                                 
7282-FIND-NEXT-BLANK.                                            
    IF TA21 GREATER THAN 31                                      
       GO TO 7282-EXIT.                                          
                                                                 
    IF TA21 EQUAL TO 31                                          
       IF NO-CHARACTERS-FOUND                                    
          GO TO 7282-EXIT                                        
          ELSE                                                   
          SET TA1   TO +1                                        
          MOVE SPACES             TO WS-TEMP-AREA1               
          PERFORM 7283-MOVE VARYING MOVE-INDX FROM TA2 BY 1      
                  UNTIL MOVE-INDX EQUAL TO TA21                  
          PERFORM 7285-TEST-AND-TRANSLATE THRU 7285-EXIT         
          SET TA1   TO +1                                        
          PERFORM 7284-MOVE-BACK VARYING MOVE-INDX FROM TA2 BY 1 
                  UNTIL MOVE-INDX EQUAL TO TA21                  
          GO TO 7282-EXIT.                                       
                                                                 
    IF WS-TEMP-2 (TA2)  = SPACE OR ',' OR '/'                    
        GO TO 7282-NEXT-GROUP-SEARCH.                            
                                                                 
    IF WS-TEMP-2 (TA21) = SPACES OR ',' OR '/'                   
        IF WS-TEMP-2 (TA21)  = ','                               
           AND WS-PROCESSOR-LINE = 'Y'                           
            SET THE-REST-R-CAPS TO TRUE                          
        ELSE                                                     
            CONTINUE                                             
    ELSE                                                         
        MOVE 'Y'                  TO WS-DATA-FOUND-SW            
        SET TA21 UP BY +1                                        
        GO TO 7282-FIND-NEXT-BLANK.                              
                                                                 
    SET TA1  TO +1                                               
    MOVE SPACES                   TO WS-TEMP-AREA1.              
    PERFORM 7283-MOVE VARYING MOVE-INDX FROM TA2 BY 1            
            UNTIL MOVE-INDX EQUAL TO TA21                        
    PERFORM 7285-TEST-AND-TRANSLATE THRU 7285-EXIT.              
    SET TA1  TO +1                                               
    PERFORM 7284-MOVE-BACK VARYING MOVE-INDX FROM TA2 BY 1       
            UNTIL MOVE-INDX EQUAL TO TA21                        
                                                                 
    IF THE-REST-R-CAPS                                           
        GO TO 7282-EXIT.                                         
                                                                 
    SET TA2   TO TA21                                            
    SET TA21  UP BY 1                                            
    MOVE 'N'                   TO WS-DATA-FOUND-SW.              
7282-NEXT-GROUP-SEARCH.                                          
    IF WS-TEMP-2 (TA2) EQUAL TO SPACES OR ',' OR '/'             
       SET TA2   UP BY 1                                         
       IF TA2 = 30                                               
          GO TO 7282-EXIT                                        
          ELSE                                                   
          GO TO 7282-NEXT-GROUP-SEARCH.                          
                                                                 
    SET TA2    UP BY 1                                           
    SET TA21   TO TA2                                            
    SET TA21   UP BY 1                                           
    GO TO 7282-FIND-NEXT-BLANK.                                  
                                                                 
