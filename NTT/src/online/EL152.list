    MOVE SPACES                 TO  WS-NAME-WORK-AREA.           
    MOVE ZERO                   TO  WS-NAME-SW.                  
    SET NWA-INDEX TO +1.                                         
                                                                 
    IF W-FIRST-NAME = SPACES                                     
            AND                                                  
       W-MIDDLE-NAME = SPACES                                    
        MOVE W-LAST-NAME        TO WS-NAME-WORK                  
        GO TO 5200-EXIT.                                         
                                                                 
    MOVE W-FIRST-NAME           TO  WS-NAME-WORK2.               
    PERFORM 5300-MOVE-NAME THRU 5390-EXIT.                       
                                                                 
    SET NWA-INDEX UP BY +1.                                      
                                                                 
    IF W-MIDDLE-NAME NOT = SPACES                                
       MOVE W-MIDDLE-NAME       TO  WS-NW (NWA-INDEX)            
       SET NWA-INDEX UP BY +1                                    
       MOVE '.'                 TO  WS-NW (NWA-INDEX)            
       SET NWA-INDEX UP BY +2.                                   
                                                                 
    MOVE W-LAST-NAME            TO  WS-NAME-WORK2.               
    PERFORM 5300-MOVE-NAME THRU 5390-EXIT.                       
                                                                 
5200-EXIT.                                                       
    EXIT.                                                        
                                                                 
    EJECT                                                        
5300-MOVE-NAME.                                                  
    IF WS-NAME-SW > +1                                           
        GO TO 5390-EXIT.                                         
                                                                 
    IF WS-NAME-WORK2 = SPACES                                    
        GO TO 5390-EXIT.                                         
                                                                 
    SET NWA-INDEX2 TO +1.                                        
    SET NWA-INDEX3 TO +2.                                        
                                                                 
5310-MOVE-NAME.                                                  
    MOVE WS-NW2 (NWA-INDEX2)  TO  WS-NW (NWA-INDEX).             
                                                                 
    IF NWA-INDEX < +30                                           
       SET NWA-INDEX UP BY +1                                    
    ELSE                                                         
       ADD +2  TO  WS-NAME-SW                                    
       GO TO 5390-EXIT.                                          
                                                                 
    IF NWA-INDEX2 < +20                                          
        SET NWA-INDEX2 UP BY +1                                  
        SET NWA-INDEX3 UP BY +1.                                 
                                                                 
    IF WS-NW2 (NWA-INDEX2) = SPACES AND                          
       WS-NW2 (NWA-INDEX3) = SPACES                              
       GO TO 5390-EXIT.                                          
                                                                 
    GO TO 5310-MOVE-NAME.                                        
                                                                 
5390-EXIT.                                                       
    EXIT.                                                        
                                                                 
    EJECT                                                        
