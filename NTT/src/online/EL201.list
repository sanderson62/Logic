    perform varying s1 from +1 by +1 until                       
       (s1 > +28)                                                
       or (wsw-voucher-ref (s1:6) = 'REF DI' or 'REFUND' or      
         'CANCEL')                                               
    end-perform                                                  
    if s1 < +29                                                  
       move 'Bypass- Manual Entry'    to ws-d1-message           
       move ' '                 to ws-table-sw                   
       go to 2050-continue                                       
    end-if                                                       
                                                                 
