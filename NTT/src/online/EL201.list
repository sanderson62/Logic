*>**-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_***
*>**                                                            ***
*>**    Every now and then I get bogus records with quotes      ***
*>**  so I am just going to remove them.  These are normally    ***
*>**  a result of a manual process that should not be included  ***
*>**  in this file.                                             ***
*>**                                                            ***
*>**_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-***
                                                                 
    move +1                     to o1                            
    perform varying i1 from +1 by +1 until i1 > +20              
       if raw-check-no (i1:1) <> '"'                             
          move raw-check-no (i1:1)                               
                                to wsw-check-no (o1:1)           
          add +1 to o1                                           
       end-if                                                    
    end-perform                                                  
                                                                 
    move +1                     to o1                            
    perform varying i1 from +1 by +1 until i1 > +20              
       if raw-check-dt (i1:1) <> '"'                             
          move raw-check-dt (i1:1)                               
                                to wsw-check-dt (o1:1)           
          add +1 to o1                                           
       end-if                                                    
    end-perform                                                  
                                                                 
    move +1                     to o1                            
    perform varying i1 from +1 by +1 until i1 > +15              
       if raw-check-amt (i1:1) <> '"'                            
          move raw-check-amt (i1:1)                              
                                to wsw-check-amt (o1:1)          
          add +1 to o1                                           
       end-if                                                    
    end-perform                                                  
                                                                 
    move +1                     to o1                            
    perform varying i1 from +1 by +1 until i1 > +20              
       if raw-vendor-id (i1:1) <> '"'                            
          move raw-vendor-id (i1:1)                              
                                to wsw-vendor-id (o1:1)          
          add +1 to o1                                           
       end-if                                                    
    end-perform                                                  
                                                                 
    move +1                     to o1                            
    perform varying i1 from +1 by +1 until i1 > +30              
       if raw-user-defined (i1:1) <> '"'                         
          move raw-user-defined (i1:1)                           
                                to wsw-user-defined (o1:1)       
          add +1 to o1                                           
       end-if                                                    
    end-perform                                                  
                                                                 
    move +1                     to o1                            
    perform varying i1 from +1 by +1 until i1 > +35              
       if raw-voucher-ref (i1:1) <> '"'                          
          move raw-voucher-ref (i1:1)                            
                                to wsw-voucher-ref (o1:1)        
          add +1 to o1                                           
       end-if                                                    
    end-perform                                                  
                                                                 
    if (wsw-check-no = spaces)                                   
       and (wsw-check-dt = spaces)                               
