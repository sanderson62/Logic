    perform varying s1 from +1 by +1 until                       
       (s1 > +30)                                                
       or (wsw-user-defined (s1:1) = ' ')                        
    end-perform                                                  
    if (s1 < +12)                                                
       and (s1 > +8)                                             
       continue                                                  
    else                                                         
       move 'Bypass- Manual Entry'                               
                                to ws-d1-message                 
       move ' '                 to ws-table-sw                   
       go to 2050-continue                                       
