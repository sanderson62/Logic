*>    display ' chk no    **' wsw-check-no '**'                    
*>    display ' chk dt    **' wsw-check-dt '**'                    
*>    display ' chk amt   **' wsw-check-amt '**'                   
*>    display ' vend id   **' wsw-vendor-id '**'                   
*>    display ' user def  **' wsw-user-defined '**'                
