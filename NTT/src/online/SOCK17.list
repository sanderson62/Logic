       and (cm-ah-cancel-dt <> low-values)                       
       move '00'                to cm-ah-benefit-cd              
    end-if                                                       
    if ((cm-lf-benefit-cd = '00' or spaces)                      
       and (cm-ah-benefit-cd = '00' or spaces))                  
