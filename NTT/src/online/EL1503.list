    string '  ' ws-cov-type ' ExPer ' ws-srtd-excl-per (s1)      
       ' CovEnd ' ws-srtd-cov-ends (s1)                          
       ' MaxBens ' ws-srtd-max-bens (s1)                         
       ' Recurring ' ws-srtd-rec-mos (s1)                        
       delimited by size into ws-map-output (m1)                 
    end-string                                                   
                                                                 
