        COMPUTE WS-CSO-ORIG-PORTION =                            
            EN-LF-ORIG-REF-AMT + EN-AH-ORIG-REF-AMT              
        MOVE WS-CSO-ORIG-PORTION TO OUT-CSO-ORIG-PORTION         
        MOVE ZERO              TO OUT-ACCT-ORIG-LF-PORT          
                                  OUT-ACCT-ORIG-ALT-LF-PORT      
                                  OUT-ACCT-ORIG-CMB-LF-PORT      
                                  OUT-ACCT-ORIG-AH-PORT          
                                  OUT-ACCT-ORIG-PORTION          
                                                                 
        MOVE ZERO              TO OUT-CSO-NEW-LF-PORT            
                                  OUT-CSO-NEW-ALT-LF-PORT        
        MOVE EN-LF-NEW-REF-AMT TO OUT-CSO-NEW-CMB-LF-PORT        
        MOVE EN-AH-NEW-REF-AMT TO OUT-CSO-NEW-AH-PORT            
        COMPUTE WS-CSO-NEW-PORTION =                             
            EN-LF-NEW-REF-AMT + EN-AH-NEW-REF-AMT                
        MOVE WS-CSO-NEW-PORTION TO OUT-CSO-NEW-PORTION           
        MOVE ZERO              TO OUT-ACCT-NEW-LF-PORT           
                                  OUT-ACCT-NEW-ALT-LF-PORT       
                                  OUT-ACCT-NEW-CMB-LF-PORT       
                                  OUT-ACCT-NEW-AH-PORT           
                                  OUT-ACCT-NEW-PORTION           
                                                                 
        MOVE ZERO              TO OUT-CSO-LF-PORT-CHG            
                                  OUT-CSO-ALT-LF-PORT-CHG        
        COMPUTE WS-CSO-CMB-LF-PORT-CHG =                         
          EN-LF-NEW-REF-AMT - EN-LF-ORIG-REF-AMT                 
        MOVE WS-CSO-CMB-LF-PORT-CHG TO OUT-CSO-CMB-LF-PORT-CHG   
        COMPUTE WS-CSO-AH-PORT-CHG =                             
          EN-AH-NEW-REF-AMT - EN-AH-ORIG-REF-AMT                 
        MOVE WS-CSO-AH-PORT-CHG TO OUT-CSO-AH-PORT-CHG           
        COMPUTE WS-CSO-PORTION-CHG =                             
          (EN-LF-NEW-REF-AMT + EN-AH-NEW-REF-AMT) -              
          (EN-LF-ORIG-REF-AMT + EN-AH-ORIG-REF-AMT)              
        MOVE WS-CSO-PORTION-CHG TO OUT-CSO-PORTION-CHG           
        MOVE ZERO              TO OUT-ACCT-LF-PORT-CHG           
                                  OUT-ACCT-ALT-LF-PORT-CHG       
                                  OUT-ACCT-CMB-LF-PORT-CHG       
                                  OUT-ACCT-AH-PORT-CHG           
                                  OUT-ACCT-PORTION-CHG           
    ELSE                                                         
        MOVE ZERO              TO OUT-ACCT-ORIG-LF-PORT          
                                  OUT-ACCT-ORIG-ALT-LF-PORT      
        COMPUTE WS-ACCT-ORIG-CMB-LF-PORT =                       
          (EN-LF-ORIG-REF-AMT * EN-LF-ORIG-REF-COMM-PCT)         
        MOVE WS-ACCT-ORIG-CMB-LF-PORT TO                         
                                 OUT-ACCT-ORIG-CMB-LF-PORT       
        COMPUTE WS-ACCT-ORIG-AH-PORT =                           
          (EN-AH-ORIG-REF-AMT * EN-AH-ORIG-REF-COMM-PCT)         
        MOVE WS-ACCT-ORIG-AH-PORT TO OUT-ACCT-ORIG-AH-PORT       
        COMPUTE WS-ACCT-ORIG-PORTION =                           
           WS-ACCT-ORIG-CMB-LF-PORT + WS-ACCT-ORIG-AH-PORT       
        MOVE WS-ACCT-ORIG-PORTION TO OUT-ACCT-ORIG-PORTION       
                                                                 
        MOVE ZERO              TO OUT-CSO-ORIG-LF-PORT           
                                  OUT-CSO-ORIG-ALT-LF-PORT       
        COMPUTE WS-CSO-ORIG-CMB-LF-PORT =                        
          (EN-LF-ORIG-REF-AMT - WS-ACCT-ORIG-CMB-LF-PORT)        
        MOVE WS-CSO-ORIG-CMB-LF-PORT TO OUT-CSO-ORIG-CMB-LF-PORT 
        COMPUTE WS-CSO-ORIG-AH-PORT =                            
          (EN-AH-ORIG-REF-AMT - WS-ACCT-ORIG-AH-PORT)            
        MOVE WS-CSO-AH-PORTION  TO OUT-CSO-ORIG-AH-PORT          
        COMPUTE WS-CSO-ORIG-PORTION =                            
           WS-CSO-ORIG-CMB-LF-PORT + WS-CSO-ORIG-AH-PORT         
        MOVE WS-CSO-ORIG-PORTION TO OUT-CSO-ORIG-PORTION         
                                                                 
        MOVE ZERO              TO OUT-ACCT-NEW-LF-PORT           
                                  OUT-ACCT-NEW-ALT-LF-PORT       
        COMPUTE WS-ACCT-NEW-CMB-LF-PORT =                        
          (EN-LF-NEW-REF-AMT * EN-LF-NEW-REF-COMM-PCT)           
        MOVE WS-ACCT-NEW-CMB-LF-PORT TO OUT-ACCT-NEW-CMB-LF-PORT 
        COMPUTE WS-ACCT-NEW-AH-PORT =                            
          (EN-AH-NEW-REF-AMT * EN-AH-NEW-REF-COMM-PCT)           
        MOVE WS-ACCT-NEW-AH-PORT TO OUT-ACCT-NEW-AH-PORT         
        COMPUTE WS-ACCT-NEW-PORTION =                            
           WS-ACCT-NEW-CMB-LF-PORT + WS-ACCT-NEW-AH-PORT         
        MOVE WS-ACCT-NEW-PORTION TO OUT-ACCT-NEW-PORTION         
                                                                 
        MOVE ZERO              TO OUT-CSO-NEW-LF-PORT            
                                  OUT-CSO-NEW-ALT-LF-PORT        
        COMPUTE WS-CSO-NEW-CMB-LF-PORT =                         
          (EN-LF-NEW-REF-AMT - WS-ACCT-NEW-CMB-LF-PORT)          
        MOVE WS-CSO-NEW-CMB-LF-PORT TO OUT-CSO-NEW-CMB-LF-PORT   
        COMPUTE WS-CSO-NEW-AH-PORT =                             
          (EN-AH-NEW-REF-AMT - WS-ACCT-NEW-AH-PORT)              
        MOVE WS-CSO-NEW-AH-PORT TO OUT-CSO-NEW-AH-PORT           
        COMPUTE WS-CSO-NEW-PORTION =                             
           WS-CSO-NEW-CMB-LF-PORT + WS-CSO-NEW-AH-PORT           
        MOVE WS-CSO-NEW-PORTION TO OUT-CSO-NEW-PORTION           
                                                                 
        MOVE ZERO               TO OUT-ACCT-LF-PORT-CHG          
                                   OUT-ACCT-ALT-LF-PORT-CHG      
        COMPUTE WS-ACCT-CMB-LF-PORT-CHG =                        
           WS-ACCT-NEW-CMB-LF-PORT - WS-ACCT-ORIG-CMB-LF-PORT    
        MOVE WS-ACCT-CMB-LF-PORT-CHG TO OUT-ACCT-CMB-LF-PORT-CHG 
        COMPUTE WS-ACCT-AH-PORT-CHG =                            
           WS-ACCT-NEW-AH-PORT - WS-ACCT-ORIG-AH-PORT            
        MOVE WS-ACCT-AH-PORT-CHG TO OUT-ACCT-AH-PORT-CHG         
        COMPUTE WS-ACCT-PORTION-CHG =                            
           WS-ACCT-NEW-PORTION - WS-ACCT-ORIG-PORTION            
        MOVE WS-ACCT-PORTION-CHG TO OUT-ACCT-PORTION-CHG         
                                                                 
        MOVE ZERO               TO OUT-CSO-LF-PORT-CHG           
                                   OUT-CSO-ALT-LF-PORT-CHG       
        COMPUTE WS-CSO-CMB-LF-PORT-CHG =                         
           WS-CSO-NEW-CMB-LF-PORT - WS-CSO-ORIG-CMB-LF-PORT      
        MOVE WS-CSO-CMB-LF-PORT-CHG TO OUT-CSO-CMB-LF-PORT-CHG   
        COMPUTE WS-CSO-AH-PORT-CHG =                             
           WS-CSO-NEW-AH-PORT - WS-CSO-ORIG-AH-PORT              
        MOVE WS-CSO-AH-PORT-CHG TO OUT-CSO-AH-PORT-CHG           
        COMPUTE WS-CSO-PORTION-CHG =                             
           WS-CSO-NEW-PORTION - WS-CSO-ORIG-PORTION              
        MOVE WS-CSO-PORTION-CHG TO OUT-CSO-PORTION-CHG           
                                                                 
    END-IF                                                       
                                                                 
