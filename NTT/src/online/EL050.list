    IF (PB-I-JOINT-FIRST-NAME NOT = SPACES)                      
       AND (PB-I-JOINT-FIRST-NAME (1:1) = SPACES)                
       PERFORM UNTIL PB-I-JOINT-FIRST-NAME (1:1) NOT = SPACES    
          MOVE PB-I-JOINT-FIRST-NAME (2:9)                       
                                TO PB-I-JOINT-FIRST-NAME         
       END-PERFORM                                               
    END-IF                                                       
                                                                 
    IF (PB-I-JOINT-LAST-NAME NOT = SPACES)                       
       AND (PB-I-JOINT-LAST-NAME (1:1) = SPACES)                 
       PERFORM UNTIL PB-I-JOINT-LAST-NAME (1:1) NOT = SPACES     
          MOVE PB-I-JOINT-LAST-NAME (2:14)                       
                                TO PB-I-JOINT-LAST-NAME          
       END-PERFORM                                               
    END-IF                                                       
                                                                 
    IF (PB-I-INSURED-FIRST-NAME NOT = SPACES)                    
       AND (PB-I-INSURED-FIRST-NAME (1:1) = SPACES)              
       PERFORM UNTIL PB-I-INSURED-FIRST-NAME (1:1) NOT = SPACES  
          MOVE PB-I-INSURED-FIRST-NAME (2:9)                     
                                TO PB-I-INSURED-FIRST-NAME       
       END-PERFORM                                               
    END-IF                                                       
                                                                 
    IF (PB-I-INSURED-LAST-NAME NOT = SPACES)                     
       AND (PB-I-INSURED-LAST-NAME (1:1) = SPACES)               
       PERFORM UNTIL PB-I-INSURED-LAST-NAME (1:1) NOT = SPACES   
          MOVE PB-I-INSURED-LAST-NAME (2:14)                     
                                TO PB-I-INSURED-LAST-NAME        
       END-PERFORM                                               
    END-IF                                                       
                                                                 
    if PB-I-JOINT-COVERAGE *> and                                
*>       pb-i-insured-last-name <> spaces                          
       if pb-i-insured-last-name = pb-i-joint-last-name and      
          pb-i-insured-first-name = pb-i-joint-first-name and    
          pb-i-birthday = pb-i-joint-birthday                    
          move er-3069          to ws-error                      
          perform 9900-error-format                              
                                thru 9900-exit                   
       end-if                                                    
    end-if                                                       
                                                                 
    IF PB-I-INSURED-LAST-NAME = SPACES                           
      AND WS-LF-SPECIAL-CALC-CD NOT = 'O' AND 'Z'                
      AND WS-AH-SPECIAL-CALC-CD NOT = 'O' AND 'Z'                
          MOVE ER-2660    TO WS-ERROR                            
          PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT.              
                                                                 
3136-EDIT-SEX.                                                   
                                                                 
*>*****************************************************************
*>    SEX CODE IS NOT REQUIRED FOR OUTSTANDING BUSINESS           *
*>*****************************************************************
                                                                 
    IF (WS-LF-SPECIAL-CALC-CD = 'O') OR                          
       (WS-AH-SPECIAL-CALC-CD = 'O')                             
       NEXT SENTENCE                                             
    ELSE                                                         
       IF PB-I-INSURED-SEX = 'M' OR 'F'                          
          NEXT SENTENCE                                          
       ELSE                                                      
          MOVE ER-2629            TO WS-ERROR                    
          PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT.              
                                                                 
3137-EDIT-EFF-DATE.                                              
                                                                 
    IF PB-REISSUED-CERT       OR                                 
       PB-POLICY-IS-DECLINED  OR                                 
       PB-POLICY-IS-VOIDED                                       
        GO TO 3139-EDIT-COVERAGE-CODES.                          
                                                                 
    MOVE SPACE                  TO WS-2671-SW                    
                                   WS-2672-SW.                   
                                                                 
    IF (PB-COMPANY-ID EQUAL 'HAN' OR 'JHL' OR 'BNE')             
        IF PB-CERT-EFF-DT GREATER THAN WS-CURRENT-MONTH-END      
            MOVE ER-2703        TO WS-ERROR                      
            PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT             
            GO TO 3139-EDIT-COVERAGE-CODES.                      
                                                                 
    IF PB-CERT-EFF-DT NOT LESS THAN WS-CURRENT-MONTH-END         
        GO TO 3139-EDIT-COVERAGE-CODES.                          
                                                                 
    MOVE PB-CERT-EFF-DT         TO DC-BIN-DATE-1.                
    MOVE WS-CURRENT-MONTH-END   TO DC-BIN-DATE-2.                
*>    MOVE '1'                    TO DC-OPTION-CODE.               
    SET ELAPSED-BETWEEN-BIN     TO TRUE.                         
    MOVE +0                     TO DC-ELAPSED-MONTHS             
                                   DC-ELAPSED-DAYS.              
    PERFORM 8500-DATE-CONVERT THRU 8500-EXIT.                    
                                                                 
    IF DATE-CONVERSION-ERROR                                     
        GO TO 3139-EDIT-COVERAGE-CODES.                          
                                                                 
    IF DC-ELAPSED-DAYS GREATER +180                              
        MOVE ER-2672            TO WS-ERROR                      
        MOVE 'Y'                TO WS-2672-SW                    
        PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT                 
    END-IF                                                       
                                                                 
    IF WS-ERROR = ER-2671  AND                                   
       PB-COMPANY-ID  = 'LBL'                                    
       MOVE +0     TO PB-COMMON-ERROR (PB-NO-OF-ERRORS)          
       MOVE SPACE               TO WS-2671-SW                    
       SUBTRACT +1            FROM PB-NO-OF-ERRORS.              
                                                                 
    IF PB-ISSUE-DATE-FORCED  AND                                 
     ((WS-2671-SW NOT = SPACE) OR (WS-2672-SW NOT = SPACE))      
        SUBTRACT +1           FROM PB-NO-OF-ERRORS               
        MOVE ER-2673            TO WS-ERROR                      
        PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT                 
        MOVE SPACE              TO WS-2671-SW                    
                                   WS-2672-SW.                   
                                                                 
    IF DC-ELAPSED-MONTHS IS GREATER THAN +9                      
        IF PB-ISSUE-DATE-FORCED                                  
            MOVE ER-2600          TO  WS-ERROR                   
            PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT             
        ELSE                                                     
            MOVE ER-2692          TO  WS-ERROR                   
            PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT.            
                                                                 
                                                                 
3139-EDIT-COVERAGE-CODES.                                        
                                                                 
    if pb-company-id not = 'AHL'                                 
       IF (WS-LF-SPECIAL-CALC-CD = 'O')                          
          or (ws-ah-special-calc-cd = 'O')                       
          IF (PB-I-LF-PREMIUM-AMT + PB-I-AH-PREMIUM-AMT +        
             PB-I-LF-ALT-PREMIUM-AMT) > 500000.00                
             MOVE ER-3071       TO WS-ERROR                      
             PERFORM 9900-ERROR-FORMAT                           
                                THRU 9900-EXIT                   
          end-if                                                 
       END-IF                                                    
    end-if                                                       
                                                                 
    IF (WS-LF-SPECIAL-CALC-CD NOT = 'O')                         
       and (ws-ah-special-calc-cd not = 'O')                     
       IF (PB-I-LF-BENEFIT-AMT + PB-I-LF-ALT-BENEFIT-AMT)        
                                > 250000.00                      
          MOVE ER-2719          TO WS-ERROR                      
          PERFORM 9900-ERROR-FORMAT                              
                                THRU 9900-EXIT                   
       END-IF                                                    
       IF (PB-I-LF-PREMIUM-AMT + PB-I-AH-PREMIUM-AMT +           
          PB-I-LF-ALT-PREMIUM-AMT) > 50000.00                    
          MOVE ER-2787          TO WS-ERROR                      
          PERFORM 9900-ERROR-FORMAT                              
                                THRU 9900-EXIT                   
       END-IF                                                    
    END-IF                                                       
                                                                 
    IF PB-POLICY-IS-DECLINED  OR                                 
       PB-POLICY-IS-VOIDED                                       
        GO TO 3140-EDIT-MAX-AMT-SIZES.                           
                                                                 
    IF (PB-I-LF-BENEFIT-CD = ZEROS  OR  SPACES)  AND             
       (PB-I-AH-BENEFIT-CD = ZEROS  OR  SPACES)                  
        MOVE ER-2630                TO WS-ERROR                  
        PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT.                
                                                                 
    IF PB-I-LF-BENEFIT-CD = ZERO  AND                            
       PB-I-LF-BENEFIT-AMT = ZERO  AND                           
       PB-I-LF-PREMIUM-AMT = ZERO                                
        GO TO 3140-CHECK-AH-FOR-ZERO.                            
                                                                 
    IF PB-I-LF-BENEFIT-CD NOT = ZERO  AND                        
       PB-I-LF-BENEFIT-AMT NOT = ZERO  AND                       
       PB-I-LF-PREMIUM-AMT NOT = ZERO                            
        GO TO 3140-CHECK-AH-FOR-ZERO.                            
                                                                 
    IF (PB-I-LF-BENEFIT-CD = ZERO)                               
       AND (WS-AH-BEN-CATEGORY NOT = 'G' AND 'L')                
       MOVE ER-2631             TO WS-ERROR                      
       PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT                  
    END-IF                                                       
                                                                 
                                                                 
*>*****************************************************************
*>    BENEFIT AMT. IS NOT REQUIRED FOR OUTSTANDING BUSINESS        
*>*****************************************************************
                                                                 
    IF WS-LF-SPECIAL-CALC-CD = 'O'                               
       NEXT SENTENCE                                             
    ELSE                                                         
       IF PB-I-LF-BENEFIT-AMT = ZERO                             
          MOVE ER-2632             TO WS-ERROR                   
          PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT.              
                                                                 
                                                                 
*>*****************************************************************
*>    PREMIMUM AMT. IS NOT REQUIRED FOR OUTSTANDING BUSINESS       
*>*****************************************************************
                                                                 
    IF WS-LF-SPECIAL-CALC-CD = 'O'                               
       CONTINUE                                                  
    ELSE                                                         
       IF PB-I-LF-PREMIUM-AMT = ZERO                             
          IF (NOT PB-COMP-LF-PREM)                               
             AND (WS-AH-BEN-CATEGORY NOT = 'G' AND 'L')          
             and (ws-lf-ben-category not = 'C')                  
             MOVE ER-2633       TO WS-ERROR                      
             PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT            
          END-IF                                                 
       END-IF                                                    
    END-IF                                                       
                                                                 
    .                                                            
3140-CHECK-AH-FOR-ZERO.                                          
                                                                 
    IF PB-I-AH-BENEFIT-CD = ZERO  AND                            
       PB-I-AH-BENEFIT-AMT = ZERO  AND                           
       PB-I-AH-PREMIUM-AMT = ZERO                                
        GO TO 3140-EDIT-MAX-AMT-SIZES.                           
                                                                 
    IF PB-COMPANY-ID = 'CSL'  AND                                
       PB-I-AH-BENEFIT-CD = ZERO  AND                            
       PB-I-AH-BENEFIT-AMT NOT = ZERO  AND                       
       PB-I-AH-PREMIUM-AMT = ZERO                                
        GO TO 3140-EDIT-MAX-AMT-SIZES.                           
                                                                 
    IF PB-I-AH-BENEFIT-CD NOT = ZERO  AND                        
       PB-I-AH-BENEFIT-AMT NOT = ZERO  AND                       
       PB-I-AH-PREMIUM-AMT NOT = ZERO                            
        GO TO 3140-EDIT-MAX-AMT-SIZES.                           
                                                                 
    IF PB-I-AH-BENEFIT-CD = ZERO                                 
        MOVE ER-2634            TO WS-ERROR                      
        PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT.                
                                                                 
*>*****************************************************************
*>     BENEFIT AMT. IS NOT REQUIRED FOR OUTSTANDING BUSINESS       
*>*****************************************************************
                                                                 
    IF WS-AH-SPECIAL-CALC-CD = 'O'                               
       NEXT SENTENCE                                             
    ELSE                                                         
       IF PB-I-AH-BENEFIT-AMT = ZERO                             
          MOVE ER-2635             TO WS-ERROR                   
          PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT.              
                                                                 
*>*****************************************************************
*>    PREMIMUM AMT. IS NOT REQUIRED FOR OUTSTANDING BUSINESS       
*>*****************************************************************
                                                                 
    IF WS-AH-SPECIAL-CALC-CD = 'O'                               
       NEXT SENTENCE                                             
    ELSE                                                         
       IF PB-I-AH-PREMIUM-AMT = ZERO                             
           IF NOT PB-COMP-AH-PREM                                
               MOVE ER-2636    TO WS-ERROR                       
               PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT.         
                                                                 
                                                                 
3140-EDIT-MAX-AMT-SIZES.                                         
*> CUSTOM CODE FOR CLIENT    'DMD'                                 
    MOVE ZEROS                   TO WS-RES-STATE-TAX             
                                    WS-RES-MUNI-TAX              
                                    PB-I-STATE-TAX               
                                    PB-I-MUNI-TAX.               
                                                                 
    PERFORM 6600-GET-STATE-MUNICIPAL-TAX THRU 6640-EXIT.         
                                                                 
    IF WS-AH-SPECIAL-CALC-CD = 'O' OR 'Z'                        
       GO TO 3150-RATE-OUTSTAND-BAL.                             
                                                                 
    IF WS-LF-SPECIAL-CALC-CD = 'O' OR 'Z'                        
       GO TO 3150-RATE-OUTSTAND-BAL.                             
                                                                 
    IF WS-LF-EARNINGS-CALC EQUAL '5'                             
       GO TO 3150-RATE-OUTSTAND-BAL.                             
                                                                 
    IF (PB-COMPANY-ID EQUAL 'FIA' OR 'PLI' OR 'NCB' OR 'AFC'     
                            OR 'RIC')                            
       GO TO 3150-RATE-OUTSTAND-BAL.                             
                                                                 
    IF PB-COMPANY-ID EQUAL 'TMS' OR 'ACM'                        
       IF PB-I-LOAN-APR GREATER THAN ZEROS                       
           GO TO 3150-RATE-OUTSTAND-BAL                          
       ELSE                                                      
          IF WS-AH-EARNINGS-CALC EQUAL '5' OR '7'                
              GO TO 3150-RATE-OUTSTAND-BAL.                      
                                                                 
    IF NOT PB-NO-MONTHS-SKIPPED                                  
       GO TO 3150-RATE-OUTSTAND-BAL.                             
                                                                 
    IF PB-I-LF-TERM NOT = PB-I-AH-TERM                           
       GO TO 3150-RATE-OUTSTAND-BAL.                             
                                                                 
    IF (PB-I-LF-BENEFIT-AMT NOT = ZERO)                          
       IF PB-I-AH-BENEFIT-AMT NOT = ZERO                         
          COMPUTE WS-AEC-WORK-AREA EQUAL PB-I-LF-BENEFIT-AMT -   
            (PB-I-AH-BENEFIT-AMT * PB-I-AH-TERM)                 
          MOVE WS-AEC-WORK-AREA TO WS-AEC-SIGNED-AREA            
          IF WS-AEC-SIGNED-AREA GREATER THAN PB-I-LF-TERM        
             MOVE ER-2693     TO WS-ERROR                        
             PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT            
             GO TO 3150-RATE-OUTSTAND-BAL                        
          ELSE                                                   
             GO TO 3150-RATE-OUTSTAND-BAL.                       
                                                                 
3150-RATE-OUTSTAND-BAL.                                          
                                                                 
*> DMD RATING TEST                                                 
    IF (CF-COMPANY-ID = 'DMD') AND NO-DMD-RATING                 
        MOVE PB-I-LF-PREMIUM-AMT     TO PB-I-LF-PREM-CALC        
        MOVE PB-I-LF-ALT-PREMIUM-AMT TO PB-I-LF-ALT-PREM-CALC    
        MOVE ER-2687                 TO WS-ERROR                 
        PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT                 
        MOVE 'B'                     TO PB-I-OB-FLAG             
        GO TO 3260-EDIT-CONTINUE.                                
                                                                 
    IF  PB-COMPANY-ID   EQUAL    'CRI'                           
        IF  WS-ERROR    EQUAL    ER-2693                         
            IF  WS-LF-EARNINGS-CALC   EQUAL  '5'                 
                MOVE  +0  TO PB-COMMON-ERROR (PB-NO-OF-ERRORS)   
                SUBTRACT  +1   FROM  PB-NO-OF-ERRORS             
                MOVE  +0            TO  WS-ERROR.                
                                                                 
    IF  PB-COMPANY-ID   EQUAL    'CDL'                           
        IF  WS-ERROR    EQUAL    ER-2693                         
            IF WS-STATE-ABBREVIATION EQUAL 'NC'                  
                MOVE  +0  TO PB-COMMON-ERROR (PB-NO-OF-ERRORS)   
                SUBTRACT  +1   FROM  PB-NO-OF-ERRORS             
                MOVE  +0            TO  WS-ERROR.                
                                                                 
    IF PLAN-MASTER-AH-SW = 'Y'                                   
       IF  PB-COMPANY-ID   EQUAL    'CRI'                        
           IF  WS-ERROR    EQUAL    ER-2693                      
             IF PB-I-AH-BENEFIT-AMT GREATER THAN PL-LM-MOA (SUB2)
                MOVE  PL-LM-MOA (SUB2)  TO  PB-I-AH-BENEFIT-AMT  
                MOVE  +0   TO  PB-COMMON-ERROR (PB-NO-OF-ERRORS) 
                SUBTRACT  +1   FROM  PB-NO-OF-ERRORS             
                MOVE  ER-1910           TO  WS-ERROR             
                PERFORM  9900-ERROR-FORMAT  THRU  9900-EXIT.     
                                                                 
    IF PLAN-MASTER-AH-SW = 'Y'                                   
       IF  PB-COMPANY-ID   EQUAL    'CRI'                        
           IF PB-I-AH-BENEFIT-AMT GREATER THAN PL-LM-MOA (SUB2)  
              MOVE  PL-LM-MOA (SUB2)  TO  PB-I-AH-BENEFIT-AMT    
              MOVE  ER-1910           TO  WS-ERROR               
              PERFORM  9900-ERROR-FORMAT  THRU  9900-EXIT.       
                                                                 
    IF PLAN-MASTER-AH-SW = 'Y'                                   
       IF  PB-COMPANY-ID   EQUAL    'CRI'                        
           IF  WS-ERROR    EQUAL    ER-2693                      
               IF  PB-I-AH-BENEFIT-AMT  =  PL-LM-MOA (SUB2)      
                MOVE  +0   TO  PB-COMMON-ERROR (PB-NO-OF-ERRORS) 
                SUBTRACT  +1   FROM  PB-NO-OF-ERRORS             
                MOVE  +0       TO  WS-ERROR.                     
                                                                 
    IF WS-ERROR = ER-2693                                        
       IF PB-COMPANY-ID = 'JAL' AND                              
          PB-I-AH-BENEFIT-CD = '16' OR '26' OR '34' OR '35'      
          MOVE +0        TO PB-COMMON-ERROR (PB-NO-OF-ERRORS)    
          SUBTRACT +1    FROM PB-NO-OF-ERRORS.                   
                                                                 
    IF WS-ERROR = ER-2693                                        
       IF PB-COMPANY-ID = 'NCL'  AND                             
          PB-I-AH-BENEFIT-CD = 'OA'                              
          MOVE +0        TO PB-COMMON-ERROR (PB-NO-OF-ERRORS)    
          SUBTRACT +1    FROM PB-NO-OF-ERRORS.                   
                                                                 
    IF WS-ERROR = ER-2693                                        
       IF PB-COMPANY-ID = 'CNL' AND                              
          PB-I-LF-BENEFIT-CD = '71' OR '72'                      
          MOVE +0        TO PB-COMMON-ERROR (PB-NO-OF-ERRORS)    
          SUBTRACT +1    FROM PB-NO-OF-ERRORS.                   
                                                                 
    IF WS-ERROR = ER-2693                                        
       IF PB-COMPANY-ID = 'HER'                                  
          IF WS-LEVEL                                            
             MOVE +0        TO PB-COMMON-ERROR (PB-NO-OF-ERRORS) 
             SUBTRACT +1    FROM PB-NO-OF-ERRORS.                
                                                                 
    IF ((LF-BENEFIT-FOUND) AND                                   
        (NOT NO-LIFE-COVERAGE)  AND                              
        (WS-LF-SPECIAL-CALC-CD = 'O'))                           
      OR   ((AH-BENEFIT-FOUND) AND                               
            (NOT NO-AH-COVERAGE)    AND                          
            (WS-AH-SPECIAL-CALC-CD = 'O'))                       
        NEXT SENTENCE                                            
    ELSE                                                         
        GO TO 3215-EDIT-RATES.                                   
                                                                 
    IF LF-BENEFIT-FOUND AND NOT NO-LIFE-COVERAGE                 
       AND WS-LF-SPECIAL-CALC-CD NOT = 'O'                       
          MOVE ER-2674          TO WS-ERROR                      
          PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT               
          GO TO 3215-EDIT-RATES.                                 
                                                                 
    IF AH-BENEFIT-FOUND AND NOT NO-AH-COVERAGE                   
       AND WS-AH-SPECIAL-CALC-CD NOT = 'O'                       
          MOVE ER-2674          TO WS-ERROR                      
