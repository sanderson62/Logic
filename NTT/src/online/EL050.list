       OR (CL-CLAIM-TYPE = 'F')                                  
       OR (CL-CLAIM-TYPE = 'B')                                  
       OR (CL-CLAIM-TYPE = 'H')                                  
       IF CLAIM-IS-OPEN                                          
          IF PB-C-AH-CANCEL-DT <= CL-PAID-THRU-DT                
            AND PB-C-AH-CANCEL-DT > SPACES                       
             MOVE ER-2896        TO WS-ERROR                     
             PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT            
          END-IF                                                 
          EVALUATE TRUE                                          
            WHEN WS-ST-REFUND-CLAIM-FLAG = '1'                   
                MOVE ER-2884      TO WS-ERROR                    
            WHEN WS-ST-REFUND-CLAIM-FLAG = '2'                   
                MOVE ER-2887      TO WS-ERROR                    
            WHEN WS-ST-REFUND-CLAIM-FLAG = '3'                   
                MOVE ER-2886      TO WS-ERROR                    
            WHEN WS-ST-REFUND-CLAIM-FLAG = '4'                   
                MOVE ER-2888      TO WS-ERROR                    
            WHEN WS-ST-REFUND-CLAIM-FLAG = '5'                   
                MOVE ER-2885      TO WS-ERROR                    
            WHEN OTHER                                           
                MOVE ER-2768      TO WS-ERROR                    
          END-EVALUATE                                           
          IF PB-C-AH-CANCEL-VOIDED OR                            
             ((WS-ST-REFUND-CLAIM-FLAG = '2' OR '3' OR '4') AND  
               NOT AH-COVERAGE-CANCELLED)                        
              MOVE SPACES TO WS-ST-REFUND-CLAIM-FLAG             
              MOVE ER-2768      TO WS-ERROR                      
          END-IF                                                 
          PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT               
          IF (WS-ST-REFUND-CLAIM-FLAG = '2' OR '3' OR '4') AND   
             PB-FORCE-CODE NOT EQUAL '8'                         
              MOVE SPACES TO WS-ST-REFUND-CLAIM-FLAG             
          END-IF                                                 
          PERFORM 8190-UPDATE-REFUND-CLAIM-FLAG THRU 8190-EXIT   
          GO TO 7300-NEXT-CLAIM                                  
