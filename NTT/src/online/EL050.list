       MOVE WS-DAYS-TO-1ST-PMT  TO CP-DAYS-TO-1ST-PMT            
    END-IF                                                       
                                                                 
    IF  PB-COMPANY-ID = 'TIC'                                    
        IF PB-I-REIN-TABLE = '01T' OR '02T' OR '03T'             
           MOVE ZERO TO CP-CLASS-CODE.                           
                                                                 
    IF PB-COMPANY-ID NOT = 'TIC'                                 
         MOVE ZERO          TO CP-CLASS-CODE.                    
                                                                 
    MOVE ZERO               TO CP-DEVIATION-CODE.                
                                                                 
    MOVE WS-NSP-ST-CD-AH    TO CP-STATE.                         
                                                                 
    MOVE ZERO               TO PB-I-AH-REI-RATE.                 
                                                                 
    MOVE 'R'                   TO CP-RATE-FILE.                  
    PERFORM 7000-GET-RATE THRU 7000-EXIT.                        
                                                                 
    IF CP-ERROR-RATE-NOT-FOUND                                   
        MOVE ER-2645        TO WS-ERROR                          
        PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT                 
        GO TO 3340-CALCULATE-PREMIUM-AMT.                        
                                                                 
    MOVE CP-PREMIUM-RATE    TO PB-I-AH-REI-RATE.                 
                                                                 
    MOVE CP-PREMIUM-RATE    TO EC-RT-AH-NSP-RATE.                
    MOVE WS-NSP-ST-CD-AH    TO EC-RT-AH-NSP-ST.                  
                                                                 
    IF PB-I-AH-REI-RATE = ZERO                                   
        MOVE ER-2645        TO WS-ERROR                          
        PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT.                
                                                                 
                                                                 
3340-CALCULATE-PREMIUM-AMT.                                      
                                                                 
    MOVE PB-CERT-EFF-DT         TO DC-BIN-DATE-1                 
    SET BIN-TO-GREG             TO TRUE                          
    PERFORM 8500-DATE-CONVERT   THRU 8500-EXIT                   
    move dc-greg-date-a-edit    to ws-greg-eff-date              
                                                                 
    IF PB-COMPANY-ID = 'NCL'                                     
        IF (WS-LF-SPECIAL-CALC-CD = 'O'  OR                      
            WS-AH-SPECIAL-CALC-CD = 'O')                         
            GO TO 3999-END-EDIT.                                 
                                                                 
    IF PB-COMPANY-ID = 'TIC' OR 'WDS' OR 'HAN' OR 'BNE' OR       
                       'AN1' OR 'TFS' OR 'MON' OR 'NCL' OR       
                       'TMS' OR 'GTL' OR 'CID' OR 'DCC' or 'VPP' 
                        OR 'AHL'                                 
        NEXT SENTENCE                                            
    ELSE                                                         
        IF WS-STATE-ABBREVIATION = 'CT'                          
            IF (NOT NO-LIFE-COVERAGE  AND  LF-BENEFIT-FOUND) AND 
               (NOT NO-AH-COVERAGE  AND  AH-BENEFIT-FOUND)       
                COMPUTE PB-I-LF-PREM-CALC =                      
                                    PB-I-LF-PREM-CALC * .90      
                COMPUTE PB-I-LF-ALT-PREM-CALC =                  
                                    PB-I-LF-ALT-PREM-CALC * .90  
                COMPUTE PB-I-AH-PREM-CALC =                      
                                    PB-I-AH-PREM-CALC * .90.     
                                                                 
    IF PB-COMP-LF-PREM                                           
       MOVE PB-I-LF-PREM-CALC       TO PB-I-LF-PREMIUM-AMT       
       MOVE PB-I-LF-ALT-PREM-CALC   TO PB-I-LF-ALT-PREMIUM-AMT.  
                                                                 
    IF PB-COMP-AH-PREM                                           
       MOVE PB-I-AH-PREM-CALC       TO PB-I-AH-PREMIUM-AMT.      
                                                                 
3340-CHECK-DCC-TOT-FEES.                                         
                                                                 
    IF (PB-COMPANY-ID = 'DCC' or 'VPP')                          
       AND (PB-I-LF-BENEFIT-CD (1:1) = 'N' OR 'S')               
       AND (PB-I-AH-BENEFIT-CD (1:1) = 'N' OR 'S')               
       AND (PB-I-POLICY-IS-MONTHLY)                              
       CONTINUE                                                  
    ELSE                                                         
       GO TO 3340-EDIT-LF-AH-TOL                                 
    END-IF                                                       
                                                                 
    MOVE WS-DCC-LF-RATE         TO CP-DCC-LF-RATE                
    MOVE WS-DCC-AH-RATE         TO CP-DCC-AH-RATE                
                                                                 
    IF PB-I-RATE-DEV-PCT-LF NOT = ZEROS                          
       COMPUTE CP-DCC-LF-RATE = WS-DCC-LF-RATE *                 
          PB-I-RATE-DEV-PCT-LF                                   
    END-IF                                                       
    IF PB-I-RATE-DEV-PCT-AH NOT = ZEROS                          
       COMPUTE CP-DCC-AH-RATE = WS-DCC-AH-RATE *                 
          PB-I-RATE-DEV-PCT-AH                                   
    END-IF                                                       
    MOVE 'N'                    TO CP-BEN-CATEGORY               
                                                                 
    MOVE WS-LF-SPECIAL-CALC-CD  TO CP-SPECIAL-CALC-CD.           
                                                                 
    MOVE EC-ST-FST-PMT-DAYS-CHG TO CP-EXT-DAYS-CALC.             
    MOVE AM-MAX-MON-BEN         TO CP-R-MAX-MON-BEN.             
    MOVE AM-MAX-TOT-BEN         TO CP-R-MAX-TOT-BEN.             
    MOVE PB-LIFE-OVERRIDE-L1    TO CP-LIFE-OVERRIDE-CODE.        
    MOVE PB-AH-OVERRIDE-L1      TO CP-AH-OVERRIDE-CODE.          
    MOVE WS-STATE-ABBREVIATION  TO CP-STATE-STD-ABBRV.           
    MOVE WS-LF-COVERAGE-TYPE    TO CP-BENEFIT-TYPE.              
    MOVE '3'                    TO CP-PROCESS-TYPE.              
    MOVE WS-LF-EARNINGS-CALC    TO CP-EARNING-METHOD.            
    MOVE WS-PL-SALES-TAX        TO CP-SALES-TAX.                 
                                                                 
    MOVE PB-SV-STATE            TO CP-STATE.                     
    MOVE PB-I-RATE-CLASS        TO CP-CLASS-CODE.                
    MOVE PB-I-RATE-DEVIATION-LF TO CP-DEVIATION-CODE.            
    MOVE PB-I-RATE-DEV-PCT-LF   TO CP-RATE-DEV-PCT.              
    MOVE PB-I-LF-BENEFIT-CD     TO CP-BENEFIT-CD.                
    MOVE PB-I-LF-BENEFIT-AMT    TO CP-ORIGINAL-BENEFIT           
                                   CP-RATING-BENEFIT-AMT.        
    IF CP-STATE-STD-ABBRV = 'OR'                                 
        COMPUTE CP-RATING-BENEFIT-AMT = PB-I-LF-BENEFIT-AMT +    
                                        PB-I-LF-ALT-BENEFIT-AMT. 
    MOVE PB-I-LF-PREMIUM-AMT    TO CP-ORIGINAL-PREMIUM.          
    MOVE PB-I-1ST-PMT-DT        TO CP-FIRST-PAY-DATE.            
    MOVE WS-EDIT-AGE            TO CP-ISSUE-AGE.                 
    MOVE PB-CERT-EFF-DT         TO CP-CERT-EFF-DT.               
    MOVE PB-COMPANY-ID          TO CP-COMPANY-ID.                
    MOVE PB-COMPANY-CD          TO CP-COMPANY-CD.                
    MOVE PB-I-LOAN-APR          TO CP-LOAN-APR.                  
    MOVE PB-I-LF-CRIT-PER       TO CP-CRITICAL-MONTHS            
    MOVE PB-I-LF-TERM           TO CP-ORIGINAL-TERM              
                                   CP-REMAINING-TERM             
                                   CP-LOAN-TERM.                 
                                                                 
    IF (CP-RATE-AS-STANDARD) AND (CP-EARN-AS-NET-PAY)            
       COMPUTE CP-ORIGINAL-BENEFIT = PB-I-LF-BENEFIT-AMT -       
          PB-I-LF-PREMIUM-AMT                                    
    end-if                                                       
                                                                 
    IF CP-TRUNCATED-LIFE                                         
       MOVE PB-I-LOAN-TERM      TO CP-LOAN-TERM.                 
                                                                 
    IF CP-TERM-IS-DAYS                                           
        MOVE PB-I-TERM-IN-DAYS  TO CP-TERM-OR-EXT-DAYS           
    ELSE                                                         
        MOVE PB-I-EXTENTION-DAYS  TO CP-TERM-OR-EXT-DAYS.        
                                                                 
                                                                 
    MOVE WS-DAYS-TO-1ST-PMT  TO CP-DAYS-TO-1ST-PMT               
                                                                 
    IF CP-SPECIAL-CALC-CD = 'H'                                  
        MOVE PB-I-LF-ALT-BENEFIT-AMT                             
                                   TO CP-ALTERNATE-BENEFIT       
        MOVE PB-I-AH-BENEFIT-AMT   TO CP-MONTHLY-PAYMENT.        
                                                                 
    MOVE PB-I-AH-BENEFIT-AMT    TO CP-MONTHLY-PAYMENT            
    MOVE 'R'                    TO CP-RATE-FILE                  
    PERFORM 7000-GET-RATE       THRU 7000-EXIT                   
                                                                 
    IF NOT CP-ERROR-RATE-NOT-FOUND                               
       MOVE CP-CALC-PREMIUM     TO PB-I-TOT-FEES-CALC            
       IF (CP-DCC-LF-RATE NOT = ZEROS)                           
          AND (CP-BENEFIT-CD (1:1) = 'S')                        
          MOVE CP-LF-PREM       TO PB-I-LF-PREM-CALC             
                                   PB-I-LF-PREMIUM-AMT           
       END-IF                                                    
    ELSE                                                         
       MOVE +0                  TO PB-I-TOT-FEES-CALC            
    END-IF                                                       
                                                                 
    .                                                            
3340-EDIT-LF-AH-TOL.                                             
                                                                 
*>**    UNSIGN-WRK1    =    LIFE DIFFERENCE                        
*>**    UNSIGN-WRK2    =    AH   DIFFERENCE                        
*>**    UNSIGN-WRK3    =    LIFE + AH DIFFERENCE                   
*>**                                                               
    ADD PB-I-LF-PREMIUM-AMT   PB-I-LF-ALT-PREMIUM-AMT            
        PB-I-AH-PREMIUM-AMT                                      
         GIVING WS-CALC-WORK.                                    
                                                                 
    IF WS-CALC-WORK LESS THAN WS-MIN-PREMIUM                     
       AND WS-CALC-WORK NOT = ZERO                               
        MOVE ER-2649            TO WS-ERROR                      
        PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT.                
                                                                 
    SUBTRACT PB-I-LF-PREM-CALC                                   
            FROM PB-I-LF-PREMIUM-AMT                             
                GIVING WS-UNSIGN-WRK1.                           
                                                                 
    SUBTRACT PB-I-LF-ALT-PREM-CALC                               
            FROM PB-I-LF-ALT-PREMIUM-AMT                         
                GIVING WS-UNSIGN-WRK4.                           
                                                                 
    COMPUTE WS-UNSIGN-WRK1 = WS-UNSIGN-WRK1                      
                               + WS-UNSIGN-WRK4.                 
                                                                 
    SUBTRACT PB-I-AH-PREM-CALC                                   
            FROM PB-I-AH-PREMIUM-AMT                             
                GIVING WS-UNSIGN-WRK2.                           
                                                                 
*>*****************************************************************
*>    DO NOT CHECK TOLERANCE FOR LIFE AND/OR A&H OVERRIDE.        *
*>*****************************************************************
                                                                 
    IF PB-OVERRIDE-LIFE OR PB-OVERRIDE-BOTH                      
        MOVE ZERO TO WS-UNSIGN-WRK1.                             
                                                                 
    IF PB-OVERRIDE-AH OR PB-OVERRIDE-BOTH                        
        MOVE ZERO TO WS-UNSIGN-WRK2.                             
                                                                 
    ADD WS-UNSIGN-WRK1 WS-UNSIGN-WRK2                            
            GIVING WS-UNSIGN-WRK3.                               
                                                                 
3342-CONTINUE.                                                   
                                                                 
    IF (PB-I-LF-RATE = ZERO  AND                                 
        PB-I-AH-RATE = ZERO)                                     
        IF WS-LF-SPECIAL-CALC-CD = 'O' OR                        
           WS-AH-SPECIAL-CALC-CD = 'O'                           
             NEXT SENTENCE                                       
        ELSE                                                     
            GO TO 3345-EDIT-CONTINUE.                            
                                                                 
    IF  PB-RECORD-BILL   =  'B' OR  'L'                          
        NEXT SENTENCE                                            
    ELSE                                                         
    IF PB-COMPANY-ID EQUAL 'PEK' OR 'MON'                        
       IF WS-UNSIGN-WRK3 GREATER THAN WS-TOL-LF-PREM             
          IF (PB-REISSUED-CERT         OR                        
              PB-POLICY-IS-DECLINED    OR                        
              PB-POLICY-IS-VOIDED)                               
             NEXT SENTENCE                                       
          ELSE                                                   
             IF WS-PREM-REJECT-SW = SPACE                        
                MOVE ER-2652    TO WS-ERROR                      
                PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT         
                GO TO 3344-CHECK-AH                              
             ELSE                                                
                MOVE ER-2650    TO WS-ERROR                      
                PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT         
                GO TO 3344-CHECK-AH.                             
                                                                 
*>**                                                               
*>**   COMPUTE HIGH AND LOW LIFE PERCENT TOLERANCES                
*>**                                                               
                                                                 
    IF WS-TOL-LF-PREM-PCT IS GREATER THAN ZEROS                  
        COMPUTE WS-TOL-LF-HIGH-PREM-PCT =                        
           (PB-I-LF-PREM-CALC + PB-I-LF-ALT-PREM-CALC) *         
            (1 + WS-TOL-LF-PREM-PCT)                             
        COMPUTE WS-TOL-LF-LOW-PREM-PCT  =                        
           (PB-I-LF-PREM-CALC + PB-I-LF-ALT-PREM-CALC) *         
            (1 - WS-TOL-LF-PREM-PCT)                             
    ELSE                                                         
        MOVE +9999999.99        TO  WS-TOL-LF-HIGH-PREM-PCT      
        MOVE +0                 TO  WS-TOL-LF-LOW-PREM-PCT.      
                                                                 
*>**                                                               
*>**   COMPUTE HIGH AND LOW LIFE AMOUNT TOLERANCES                 
*>**                                                               
                                                                 
    COMPUTE WS-TOL-LF-HIGH-PREM-AMT EQUAL                        
        PB-I-LF-PREM-CALC + PB-I-LF-ALT-PREM-CALC +              
         WS-TOL-LF-PREM.                                         
                                                                 
    COMPUTE WS-TOL-LF-LOW-PREM-AMT EQUAL                         
        (PB-I-LF-PREM-CALC + PB-I-LF-ALT-PREM-CALC) -            
         WS-TOL-LF-PREM.                                         
                                                                 
    IF PB-COMPANY-ID EQUAL 'FLC' OR 'CDC'                        
       MOVE +0                  TO WS-TOL-LF-LOW-PREM-AMT.       
                                                                 
    IF PB-REISSUED-CERT                                          
       or PB-POLICY-IS-DECLINED                                  
       or PB-POLICY-IS-VOIDED                                    
       continue                                                  
    ELSE                                                         
       IF (((PB-I-LF-PREMIUM-AMT + PB-I-LF-ALT-PREMIUM-AMT)      
             > WS-TOL-LF-HIGH-PREM-AMT)                          
          OR ((PB-I-LF-PREMIUM-AMT + PB-I-LF-ALT-PREMIUM-AMT)    
             < WS-TOL-LF-LOW-PREM-AMT))                          
                     or                                          
          (((PB-I-LF-PREMIUM-AMT + PB-I-LF-ALT-PREMIUM-AMT)      
             > WS-TOL-LF-HIGH-PREM-PCT)                          
          OR ((PB-I-LF-PREMIUM-AMT + PB-I-LF-ALT-PREMIUM-AMT)    
             < WS-TOL-LF-LOW-PREM-PCT))                          
          IF WS-PREM-REJECT-SW = SPACE                           
             MOVE ER-2652       TO WS-ERROR                      
             PERFORM 9900-ERROR-FORMAT                           
                                THRU 9900-EXIT                   
          ELSE                                                   
             MOVE ER-2650       TO WS-ERROR                      
             PERFORM 9900-ERROR-FORMAT                           
                                THRU 9900-EXIT                   
          end-if                                                 
       end-if                                                    
    end-if                                                       
                                                                 
*>    IF  WS-ERROR  =  ER-2650  OR  ER-2652                        
*>        IF  PB-RECORD-BILL   =  'B' OR  'L'                      
*>            MOVE  +0     TO  PB-COMMON-ERROR (PB-NO-OF-ERRORS)   
*>            SUBTRACT  +1     FROM  PB-NO-OF-ERRORS.              
                                                                 
    IF WS-ERROR  =  ER-2650  OR  ER-2652                         
       IF (PB-RECORD-BILL   =  'B' OR  'L')                      
                     OR                                          
          ((PB-COMPANY-ID = 'DCC' or 'VPP')                      
          AND (PB-I-LF-BENEFIT-CD = '00' OR '  ' or 'DD'))       
          MOVE  +0     TO  PB-COMMON-ERROR (PB-NO-OF-ERRORS)     
          SUBTRACT  +1     FROM  PB-NO-OF-ERRORS                 
       END-IF                                                    
    END-IF                                                       
                                                                 
    IF PB-COMPANY-ID NOT EQUAL 'FLC' AND 'CDC'                   
       IF WS-COMP-MAX-CAP GREATER THAN +0                        
          IF WS-UNSIGN-WRK1 GREATER THAN WS-COMP-MAX-CAP         
             IF (PB-REISSUED-CERT         OR                     
                 PB-POLICY-IS-DECLINED    OR                     
                 PB-POLICY-IS-VOIDED)                            
                NEXT SENTENCE                                    
             ELSE                                                
                 MOVE ER-2646   TO  WS-ERROR                     
                 PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT.       
                                                                 
3344-CVL-TOLERANCE-CHECK.                                        
                                                                 
     IF PB-COMPANY-ID  IS NOT EQUAL TO  'CVL'                    
         GO TO 3344-CHECK-AH.                                    
                                                                 
     IF PB-I-LF-ALT-PREMIUM-AMT  IS EQUAL TO  ZEROS              
         GO TO 3344-CHECK-AH.                                    
                                                                 
     MOVE ZERO                  TO  WS-UNSIGN-WRK5               
                                    WS-UNSIGN-WRK6.              
                                                                 
     IF PB-I-LF-PREMIUM-AMT  IS NOT EQUAL TO  ZERO               
         COMPUTE WS-UNSIGN-WRK5 = WS-UNSIGN-WRK5                 
                                + PB-I-LF-PREMIUM-AMT            
                                - PB-I-LF-PREM-CALC.             
                                                                 
     IF PB-I-LF-ALT-PREMIUM-AMT  IS NOT EQUAL TO  ZERO           
         COMPUTE WS-UNSIGN-WRK6 = WS-UNSIGN-WRK6                 
                                + PB-I-LF-ALT-PREMIUM-AMT        
                                - PB-I-LF-ALT-PREM-CALC.         
                                                                 
     IF WS-UNSIGN-WRK5  IS GREATER THAN  ZERO                    
         IF WS-UNSIGN-WRK5  IS GREATER THAN  WS-TOL-LF-PREM      
             IF WS-PREM-REJECT-SW  IS EQUAL TO  SPACE            
                MOVE ER-2652    TO  WS-ERROR                     
                PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT         
             ELSE                                                
                MOVE ER-2650    TO  WS-ERROR                     
                PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT.        
                                                                 
     IF WS-UNSIGN-WRK6  IS GREATER THAN  ZERO                    
         IF WS-UNSIGN-WRK6  IS GREATER THAN  WS-TOL-LF-PREM      
             IF WS-PREM-REJECT-SW  IS EQUAL TO  SPACE            
                MOVE ER-2652    TO  WS-ERROR                     
                PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT         
             ELSE                                                
                MOVE ER-2650    TO  WS-ERROR                     
                PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT.        
                                                                 
3344-CHECK-AH.                                                   
                                                                 
*>***************************************************************  
*>**   COMPUTE HIGH AND LOW AH PERCENT TOLERANCES                  
*>**                                                               
                                                                 
    IF WS-TOL-AH-PREM-PCT IS GREATER THAN ZEROS                  
        COMPUTE WS-TOL-AH-HIGH-PREM-PCT = PB-I-AH-PREM-CALC *    
            (1 + WS-TOL-AH-PREM-PCT)                             
        COMPUTE WS-TOL-AH-LOW-PREM-PCT  = PB-I-AH-PREM-CALC *    
            (1 - WS-TOL-AH-PREM-PCT)                             
    ELSE                                                         
        MOVE +9999999.99        TO  WS-TOL-AH-HIGH-PREM-PCT      
        MOVE +0                 TO  WS-TOL-AH-LOW-PREM-PCT.      
                                                                 
*>**                                                               
*>**   COMPUTE HIGH AND LOW AH AMOUNT TOLERANCES                   
*>**                                                               
                                                                 
    COMPUTE WS-TOL-AH-HIGH-PREM-AMT EQUAL                        
        PB-I-AH-PREM-CALC + WS-TOL-AH-PREM.                      
                                                                 
    COMPUTE WS-TOL-AH-LOW-PREM-AMT EQUAL                         
        PB-I-AH-PREM-CALC - WS-TOL-AH-PREM.                      
                                                                 
    IF PB-COMPANY-ID EQUAL 'FLC' OR 'CDC'                        
       MOVE +0                  TO WS-TOL-AH-LOW-PREM-AMT.       
                                                                 
    IF (PB-REISSUED-CERT)                                        
       OR (PB-POLICY-IS-DECLINED)                                
       OR (PB-POLICY-IS-VOIDED)                                  
       OR (AM-DCC-PRODUCT-CODE = 'DDF')                          
       CONTINUE                                                  
    ELSE                                                         
       IF ((PB-I-AH-PREMIUM-AMT > WS-TOL-AH-HIGH-PREM-AMT)       
             OR (PB-I-AH-PREMIUM-AMT < WS-TOL-AH-LOW-PREM-AMT))  
                         or                                      
          ((PB-I-AH-PREMIUM-AMT > WS-TOL-AH-HIGH-PREM-PCT)       
             OR (PB-I-AH-PREMIUM-AMT < WS-TOL-AH-LOW-PREM-PCT))  
          IF WS-PREM-REJECT-SW = SPACE                           
             MOVE ER-3052       TO WS-ERROR                      
             PERFORM 9900-ERROR-FORMAT                           
                                THRU 9900-EXIT                   
          ELSE                                                   
             MOVE ER-3050       TO WS-ERROR                      
             PERFORM 9900-ERROR-FORMAT                           
                                THRU 9900-EXIT                   
          END-IF                                                 
       END-IF                                                    
    END-IF                                                       
                                                                 
    if (pb-company-id = 'DCC' or 'VPP')                          
       and ((pb-i-ah-benefit-cd (1:1) = 'N' or 'S')              
       or (pb-i-lf-benefit-cd (1:1) = 'N' or 'S'))               
       and (pb-i-policy-is-monthly)                              
       compute ws-tol-ah-high-prem-amt =                         
          pb-i-tot-fees-calc + ws-tol-ah-prem                    
       compute ws-tol-ah-low-prem-amt =                          
          pb-i-tot-fees-calc - ws-tol-ah-prem                    
       if ws-tol-ah-prem-pct > zeros                             
          compute ws-tol-ah-high-prem-pct = pb-i-tot-fees-calc * 
             (1 + ws-tol-ah-prem-pct)                            
          compute ws-tol-ah-low-prem-pct  = pb-i-tot-fees-calc * 
             (1 - ws-tol-ah-prem-pct)                            
       else                                                      
          move +9999999.99      to ws-tol-ah-high-prem-pct       
          move +0               to ws-tol-ah-low-prem-pct        
       end-if                                                    
       if ((pb-i-tot-fees > ws-tol-ah-high-prem-amt)             
           or                                                    
          (pb-i-tot-fees < ws-tol-ah-low-prem-amt))              
                         or                                      
          ((pb-i-tot-fees > ws-tol-ah-high-prem-pct)             
          or                                                     
          (pb-i-tot-fees < ws-tol-ah-low-prem-pct))              
           move er-3049         to ws-error                      
          perform 9900-error-format                              
                                thru 9900-exit                   
                                                                 
       end-if                                                    
    end-if                                                       
                                                                 
    IF  WS-ERROR  =  ER-3050   OR   ER-3052                      
        IF  PB-RECORD-BILL   =  'B' OR 'A'                       
            MOVE  +0     TO  PB-COMMON-ERROR (PB-NO-OF-ERRORS)   
            SUBTRACT  +1     FROM  PB-NO-OF-ERRORS.              
                                                                 
    IF PB-COMPANY-ID NOT EQUAL 'FLC' AND 'CDC'                   
       IF WS-COMP-MAX-CAP GREATER THAN +0                        
          IF WS-UNSIGN-WRK2 GREATER THAN WS-COMP-MAX-CAP         
             IF (PB-REISSUED-CERT         OR                     
                 PB-POLICY-IS-DECLINED    OR                     
                 PB-POLICY-IS-VOIDED)                            
                NEXT SENTENCE                                    
             ELSE                                                
                 MOVE ER-2646   TO  WS-ERROR                     
                 PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT.       
                                                                 
    IF (PB-COMPANY-ID NOT EQUAL 'FLC' AND 'CDC') OR              
       (WS-COMP-MAX-CAP EQUAL +0)                                
          GO TO 3345-EDIT-CONTINUE.                              
                                                                 
    IF ((PB-I-AH-PREM-CALC + PB-I-LF-PREM-CALC) - WS-CALC-WORK)  
       GREATER THAN WS-COMP-MAX-CAP                              
          MOVE ER-2646          TO  WS-ERROR                     
          PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT.              
                                                                 
3345-EDIT-CONTINUE.                                              
                                                                 
*>    IF PB-COMPANY-ID    EQUAL    'NCL'                           
*>       IF PB-ENTRY-BATCH   NUMERIC                               
*>          IF PB-ISSUE                                            
*>              MOVE PB-ENTRY-BATCH  TO  PB-I-MICROFILM-NO         
*>          ELSE                                                   
*>              IF PB-CANCELLATION                                 
*>                 MOVE PB-ENTRY-BATCH TO  PB-C-MICROFILM-NO.      
                                                                 
*>    IF  PB-COMPANY-ID    EQUAL    'HER'                          
*>        IF  PB-I-MICROFILM-NO     NUMERIC    AND                 
*>            PB-I-MICROFILM-NO  NOT EQUAL  ZEROS                  
*>            MOVE  PB-I-MICROFILM-NO  TO  CM-ISS-MICROFILM-NO     
*>        ELSE                                                     
*>            MOVE  ER-1909            TO  WS-ERROR                
*>            PERFORM  9900-ERROR-FORMAT  THRU  9900-EXIT.         
                                                                 
    IF PB-I-POLICY-ACCEPTED                                      
       IF PB-POLICY-IS-DECLINED  OR                              
          PB-REISSUED-CERT  OR                                   
          PB-REIN-ONLY-CERT  OR                                  
          PB-POLICY-IS-VOIDED                                    
           MOVE  ER-1891         TO  WS-ERROR                    
           PERFORM  9900-ERROR-FORMAT  THRU  9900-EXIT           
       ELSE                                                      
           MOVE SPACE            TO  PB-BATCH-ENTRY              
           MOVE '2'              TO  PB-I-ENTRY-STATUS.          
                                                                 
    IF PB-I-POLICY-DECLINED                                      
       IF PB-REISSUED-CERT  OR                                   
          PB-REIN-ONLY-CERT  OR                                  
          PB-POLICY-IS-VOIDED                                    
           MOVE  ER-1906         TO  WS-ERROR                    
           PERFORM  9900-ERROR-FORMAT  THRU  9900-EXIT           
       ELSE                                                      
           MOVE  'D'             TO  PB-BATCH-ENTRY              
                                     PB-I-ENTRY-STATUS.          
                                                                 
    IF PB-I-NEEDS-UNDERWRITING                                   
       IF PB-POLICY-IS-DECLINED OR                               
          PB-REISSUED-CERT  OR                                   
          PB-REIN-ONLY-CERT  OR                                  
          PB-POLICY-IS-VOIDED                                    
           MOVE  ER-1907         TO  WS-ERROR                    
           PERFORM  9900-ERROR-FORMAT  THRU  9900-EXIT           
       ELSE                                                      
           MOVE 'U'              TO  PB-BATCH-ENTRY              
                                     PB-I-ENTRY-STATUS.          
                                                                 
    IF PB-NEEDS-UNDERWRITING                                     
       MOVE  ER-1901             TO  WS-ERROR                    
       PERFORM  9900-ERROR-FORMAT THRU 9900-EXIT.                
                                                                 
    IF PB-COMPANY-ID = 'DCC' or 'VPP'                            
       MOVE PB-I-LF-BENEFIT-CD  TO WS-LF-BEN-CD                  
       MOVE PB-I-AH-BENEFIT-CD  TO WS-AH-BEN-CD                  
       IF (((WS-LF-BEN-CD (1:1) = 'N' OR 'M' OR 'S' OR 'P')      
          AND (NOT WS-LF-CASH))                                  
                          OR                                     
          ((WS-AH-BEN-CD (1:1) = 'N' OR 'M' OR 'S' OR 'P')       
          AND (NOT WS-AH-CASH)))                                 
                      AND                                        
          (PB-I-ENTRY-STATUS NOT = '5')                          
          MOVE 'M'              TO PB-BATCH-ENTRY                
                                   PB-I-ENTRY-STATUS             
       END-IF                                                    
    END-IF                                                       
                                                                 
    move pending-business       to ws-passed-erpndb              
    move ' '                    to ws-exceed-limit-yn            
    perform 7780-call-agg       thru 7780-exit                   
*>    display ' el050 exceed ' ws-exceed-limit-yn                  
    if ws-exceed-limit-yn = 'Y'                                  
       MOVE  ER-9813            TO WS-ERROR                      
       move 'Y'                 to ws-9813-sw                    
       PERFORM  9900-ERROR-FORMAT                                
                                THRU 9900-EXIT                   
    end-if                                                       
                                                                 
    if (PB-EXCEEDED-LIMIT-FORCED)                                
       and (ws-9813-sw = 'Y')                                    
       subtract +1 from PB-NO-OF-ERRORS                          
       move er-9823             to ws-error                      
       PERFORM  9900-ERROR-FORMAT                                
                                THRU 9900-EXIT                   
    end-if                                                       
                                                                 
    IF  PB-COMPANY-ID NOT = 'NCL'                                
        GO TO 3999-END-EDIT.                                     
                                                                 
    IF PB-REISSUED-CERT  OR                                      
       PB-POLICY-IS-DECLINED  OR                                 
       PB-POLICY-IS-VOIDED  OR                                   
       PB-I-OB                                                   
        GO TO 3999-END-EDIT.                                     
                                                                 
    IF PB-I-POLICY-FORM-NO = SPACES  OR  ZEROS                   
        MOVE  ER-2644            TO  WS-ERROR                    
        PERFORM  9900-ERROR-FORMAT THRU 9900-EXIT                
        GO TO 3999-END-EDIT.                                     
                                                                 
    MOVE 'N'                     TO  FORM-MASTER-SW.             
    PERFORM  3350-GET-ERFORM  THRU  3350-EXIT.                   
                                                                 
    IF NOT FORM-MASTER-FOUND                                     
        GO TO 3999-END-EDIT.                                     
                                                                 
    MOVE 'N'                     TO  FORM-MASTER-LIFE-SW         
                                     FORM-MASTER-AH-SW.          
                                                                 
    IF  PB-I-LIFE-BENEFIT-CD NOT = ZEROS  AND  SPACES            
        MOVE  +1                 TO SUB6                         
        PERFORM  3355-EDIT-FORM-LF-BENEFITS THRU 3355-EXIT.      
                                                                 
    IF PB-I-AH-BENEFIT-CD NOT = ZEROS  AND  SPACES               
        MOVE  +1                 TO SUB6                         
        PERFORM  3356-EDIT-FORM-AH-BENEFITS THRU 3356-EXIT.      
                                                                 
    IF LF-FORM-MASTER-FOUND  OR                                  
       AH-FORM-MASTER-FOUND                                      
        GO TO 3360-CHECK-FORM-LIMITS                             
    ELSE                                                         
        GO TO 3999-END-EDIT.                                     
                                                                 
*>*****************************************************************
*>                  3350-GET-ERFORM                               *
*>    THIS SECTION READS THE POLICY FORM MASTER FILE.  THIS FILE  *
*>    IS READ TO VERIFY POLICY FORM NUMBER AND VALID BENEFITS.    *
*>*****************************************************************
                                                                 
3350-GET-ERFORM.                                                 
                                                                 
    IF  WK-FORM-ADDR      EQUAL    ZEROS                         
        GO TO  3350-ERFORM-GETMAIN.                              
                                                                 
    MOVE  WK-FORM-ADDR           TO  ERFORM-WS-COMP.             
    SET ERFORM-FILE-ADDRESSED    TO  TRUE.                       
    PERFORM 8900-ADDRESS-FILE   THRU 8900-ADDRESS-EXIT.          
    MOVE 'Y'                     TO FORM-MASTER-SW.              
                                                                 
    IF PB-CANCELLATION                                           
        IF PB-COMPANY-CD        EQUAL   FO-COMPANY-CD  AND       
           PB-SV-STATE          EQUAL   FO-STATE       AND       
           CM-POLICY-FORM-NO    EQUAL   FO-FORM-ID               
             GO TO 3350-EXIT                                     
        ELSE                                                     
            NEXT SENTENCE                                        
    ELSE                                                         
        IF PB-COMPANY-CD        EQUAL   FO-COMPANY-CD  AND       
           PB-SV-STATE          EQUAL   FO-STATE       AND       
           PB-I-POLICY-FORM-NO  EQUAL   FO-FORM-ID               
             GO TO 3350-EXIT.                                    
                                                                 
3350-ERFORM-GETMAIN.                                             
                                                                 
    PERFORM  8400-GETMAIN-ERFORM      THRU  8400-EXIT.           
    MOVE  ERFORM-WS-COMP         TO  WK-FORM-ADDR.               
                                                                 
3350-ERFORM-READ.                                                
                                                                 
    MOVE  PB-COMPANY-CD          TO  FORM-COMPANY-CD.            
    MOVE  PB-SV-STATE            TO  FORM-STATE.                 
    IF PB-CANCELLATION                                           
        MOVE  CM-POLICY-FORM-NO      TO  FORM-FORM-ID            
    ELSE                                                         
        MOVE  PB-I-POLICY-FORM-NO    TO  FORM-FORM-ID.           
    MOVE  PB-CERT-EFF-DT         TO  FORM-FORM-EXP-DT.           
                                                                 
    PERFORM  8450-READ-FORM-MASTER  THRU  8450-EXIT.             
                                                                 
    IF FORM-MASTER-FOUND                                         
      IF PB-CANCELLATION                                         
         IF PB-COMPANY-CD        EQUAL   FO-COMPANY-CD  AND      
            PB-SV-STATE          EQUAL   FO-STATE       AND      
            CM-POLICY-FORM-NO    EQUAL   FO-FORM-ID              
             GO  TO 3350-EXIT                                    
         ELSE                                                    
             NEXT SENTENCE                                       
      ELSE                                                       
         IF PB-COMPANY-CD        EQUAL   FO-COMPANY-CD  AND      
            PB-SV-STATE          EQUAL   FO-STATE       AND      
            PB-I-POLICY-FORM-NO  EQUAL   FO-FORM-ID              
             GO  TO 3350-EXIT.                                   
                                                                 
    MOVE  'N'                    TO  FORM-MASTER-SW.             
    MOVE  ER-1895                TO  WS-ERROR.                   
    PERFORM  9900-ERROR-FORMAT  THRU  9900-EXIT.                 
                                                                 
3350-EXIT.                                                       
     EXIT.                                                       
                                                                 
*>*****************************************************************
*>                  3355-EDIT-FORM-LF-BENEFITS                    *
*>   THIS SECTION IS USED TO VERIFY THAT THE LIFE BENEFIT IS      *
*>   VALID FOR THE POLICY FORM USED.                              *
*>*****************************************************************
                                                                 
3355-EDIT-FORM-LF-BENEFITS.                                      
                                                                 
    IF PB-LIFE-OVERRIDE-L1  =  FO-PLAN-TYPE (SUB6)  AND          
       PB-I-LIFE-BENEFIT-CD =  FO-PLAN-ID (SUB6)                 
        MOVE  'Y'                TO  FORM-MASTER-LIFE-SW         
        GO TO   3355-EXIT.                                       
                                                                 
    IF  SUB6    LESS THAN   +40                                  
        ADD  +1      TO  SUB6                                    
        GO TO  3355-EDIT-FORM-LF-BENEFITS.                       
                                                                 
    MOVE  ER-1896         TO  WS-ERROR.                          
    PERFORM  9900-ERROR-FORMAT   THRU  9900-EXIT.                
                                                                 
3355-EXIT.                                                       
     EXIT.                                                       
                                                                 
*>*****************************************************************
*>                  3356-EDIT-FORM-AH-BENEFITS                    *
*>   THIS SECTION IS USED TO VERIFY THAT THE A&H BENEFIT IS       *
*>   VALID FOR THE POLICY FORM USED.                              *
*>*****************************************************************
                                                                 
3356-EDIT-FORM-AH-BENEFITS.                                      
                                                                 
    IF PB-AH-OVERRIDE-L1  =  FO-PLAN-TYPE (SUB6)  AND            
       PB-I-AH-BENEFIT-CD =  FO-PLAN-ID (SUB6)                   
        MOVE  'Y'                TO  FORM-MASTER-AH-SW           
        GO TO  3356-EXIT.                                        
                                                                 
     IF  SUB6    LESS THAN     +40                               
         ADD  +1      TO  SUB6                                   
         GO TO  3356-EDIT-FORM-AH-BENEFITS.                      
                                                                 
    MOVE  ER-1897         TO  WS-ERROR.                          
    PERFORM  9900-ERROR-FORMAT   THRU  9900-EXIT.                
                                                                 
3356-EXIT.                                                       
     EXIT.                                                       
                                                                 
*>*****************************************************************
*>                  3357-BENEFIT-SEARCH                           *
*>   THIS SECTION IS USED TO FIND A MATCHING BENEFIT CODE FOR     *
*>   CANCELLATIONS ONLY.                                          *
*>*****************************************************************
3357-BENEFIT-SEARCH.                                             
    IF PB-CI-LF-BENEFIT-CD NOT = ZEROS AND SPACES                
        MOVE +1                          TO SUB6                 
        PERFORM 3358-BENEFIT-LIFE-SEARCH THRU 3358-EXIT.         
                                                                 
    IF PB-CI-AH-BENEFIT-CD NOT = ZEROS AND SPACES                
        MOVE +1                          TO SUB6                 
        PERFORM 3359-BENEFIT-AH-SEARCH   THRU 3359-EXIT.         
                                                                 
3357-EXIT.                                                       
    EXIT.                                                        
                                                                 
*>*****************************************************************
*>                  3358-BENEFIT-LIFE-SEARCH                      *
*>   THE COMPANY OR STATE LIFE REFUND METHOD WILL BE REPLACED BY  *
*>   THE FORM BENEFIT REFUND METHOD IF THE CANCELLATIONS TERM     *
*>   IS LESS THAN OR EQUAL TO THE FORM BENEFIT PLAN TERM.         *
*>*****************************************************************
3358-BENEFIT-LIFE-SEARCH.                                        
    IF PB-LIFE-OVERRIDE-L1     EQUAL TO FO-PLAN-TYPE (SUB6)      
      IF PB-CI-LF-BENEFIT-CD EQUAL TO FO-PLAN-ID  (SUB6)         
        IF (PB-CI-LF-TERM  IS LESS THAN FO-PLAN-TERM (SUB6)      
          OR PB-CI-LF-TERM IS EQUAL TO  FO-PLAN-TERM (SUB6))     
             IF FO-PLAN-REFUND-METHOD (SUB6)                     
                    IS GREATER THAN SPACES                       
                MOVE FO-PLAN-REFUND-METHOD (SUB6) TO             
                                         WS-LF-REFUND-CALC       
                GO TO 3358-EXIT.                                 
                                                                 
    IF SUB6 LESS THAN +40                                        
        ADD +1                 TO SUB6                           
        GO TO 3358-BENEFIT-LIFE-SEARCH.                          
                                                                 
3358-EXIT.                                                       
    EXIT.                                                        
*>*****************************************************************
*>                  3359-BENEFIT-AH-SEARCH                        *
*>   THE COMPANY OR STATE A/H  REFUND METHOD WILL BE REPLACED BY  *
*>   THE FORM BENEFIT REFUND METHOD IF THE CANCELLATIONS TERM     *
*>   IS LESS THAN OR EQUAL TO THE FORM BENEFIT PLAN TERM.         *
*>*****************************************************************
3359-BENEFIT-AH-SEARCH.                                          
    IF PB-AH-OVERRIDE-L1      EQUAL TO FO-PLAN-TYPE (SUB6)       
      IF PB-CI-AH-BENEFIT-CD  EQUAL TO FO-PLAN-ID  (SUB6)        
        IF (PB-CI-AH-TERM  IS LESS THAN FO-PLAN-TERM (SUB6)      
          OR PB-CI-AH-TERM IS EQUAL TO  FO-PLAN-TERM (SUB6))     
             IF FO-PLAN-REFUND-METHOD (SUB6)                     
                     IS GREATER THAN SPACES                      
                MOVE FO-PLAN-REFUND-METHOD (SUB6) TO             
                                         WS-AH-REFUND-CALC       
                GO TO 3359-EXIT.                                 
                                                                 
    IF SUB6 LESS THAN +40                                        
        ADD +1                 TO SUB6                           
        GO TO 3359-BENEFIT-AH-SEARCH.                            
                                                                 
3359-EXIT.                                                       
    EXIT.                                                        
                                                                 
*>*****************************************************************
*>                  3360-CHECK-FORM-LIMITS                        *
*>   THIS SECTION WILL EDIT THE POLICY FORM CRITERIA FOR THE MIN  *
*>   AND MAX ISSUE AGE, ATTAINED AGE, MAX TERM AND MAX BENEFITS.  *
*>*****************************************************************
                                                                 
3360-CHECK-FORM-LIMITS.                                          
                                                                 
    IF FO-IND-GRP-CD = 'I'  OR  'G'                              
        MOVE  FO-IND-GRP-CD    TO  PB-I-INDV-GRP-CD.             
                                                                 
    IF NOT LF-FORM-MASTER-FOUND                                  
        GO TO 3360-CHECK-FORM-AH-LIMITS.                         
                                                                 
3360-CHECK-FORM-LF-MIN-ISS-AGE.                                  
                                                                 
    IF PB-I-AGE  LESS THAN  FO-LF-MIN-ISSUE-AGE  OR              
      (PB-I-JOINT-AGE  LESS THAN  FO-LF-MIN-ISSUE-AGE  AND       
       WS-LF-JOINT-INDICATOR = 'J'  AND                          
       (WS-JOINT-AGE-FLAG = '1' OR 'Y'))                         
        IF PB-COMPANY-ID = 'NCL'                                 
           MOVE  ER-1911          TO  WS-ERROR                   
           PERFORM  9900-ERROR-FORMAT  THRU  9900-EXIT           
        ELSE                                                     
           MOVE  ER-1892          TO  WS-ERROR                   
           PERFORM  9900-ERROR-FORMAT  THRU  9900-EXIT.          
                                                                 
3360-CHECK-FORM-LF-ATT-AGE.                                      
                                                                 
    COMPUTE WS-ATT-AGE = WS-EDIT-AGE + (PB-I-LF-TERM / 12).      
                                                                 
    IF  FO-MAX-ATT-AGE     NUMERIC  AND                          
        FO-MAX-ATT-AGE     GREATER THAN  ZEROS                   
        IF  WS-ATT-AGE     GREATER THAN  FO-MAX-ATT-AGE          
           IF PB-COMPANY-ID = 'NCL'                              
               MOVE  ER-1913      TO  WS-ERROR                   
               PERFORM  9900-ERROR-FORMAT  THRU  9900-EXIT       
           ELSE                                                  
               MOVE  ER-1894      TO  WS-ERROR                   
               PERFORM  9900-ERROR-FORMAT  THRU  9900-EXIT.      
                                                                 
3360-CHECK-FORM-LF-MAX-ISS-AGE.                                  
                                                                 
    IF  FO-LF-MAX-ISSUE-AGE   NUMERIC   AND                      
        FO-LF-MAX-ISSUE-AGE   GREATER THAN   ZEROS               
        IF  WS-EDIT-AGE  GREATER THAN  FO-LF-MAX-ISSUE-AGE       
           IF PB-COMPANY-ID = 'NCL'                              
               MOVE  ER-1914      TO  WS-ERROR                   
               PERFORM  9900-ERROR-FORMAT   THRU  9900-EXIT      
           ELSE                                                  
               MOVE  ER-1899      TO  WS-ERROR                   
               PERFORM  9900-ERROR-FORMAT   THRU  9900-EXIT.     
                                                                 
3360-CHECK-FORM-LF-TERM-BAMT.                                    
                                                                 
    IF  FO-LF-MAX-TERM   NUMERIC   AND                           
        FO-LF-MAX-TERM   GREATER THAN   ZEROS                    
        IF  PB-I-LF-TERM   GREATER THAN  FO-LF-MAX-TERM          
          IF  PB-COMPANY-ID = 'NCL'                              
              MOVE  ER-1916       TO  WS-ERROR                   
              PERFORM  9900-ERROR-FORMAT  THRU  9900-EXIT        
          ELSE                                                   
              MOVE  ER-1902       TO  WS-ERROR                   
              PERFORM  9900-ERROR-FORMAT  THRU  9900-EXIT.       
                                                                 
    COMPUTE  WS-LF-BENEFIT-AMT  =  PB-I-LF-BENEFIT-AMT  +        
                                   PB-I-LF-ALT-BENEFIT-AMT.      
                                                                 
    IF  FO-MAX-LF-AMT   NUMERIC   AND                            
        FO-MAX-LF-AMT   GREATER THAN   ZEROS                     
        IF  WS-LF-BENEFIT-AMT   GREATER THAN  FO-MAX-LF-AMT      
            IF  PB-COMPANY-ID = 'NCL'                            
                MOVE  ER-1918     TO  WS-ERROR                   
                PERFORM  9900-ERROR-FORMAT  THRU  9900-EXIT      
            ELSE                                                 
                MOVE  ER-1904     TO  WS-ERROR                   
                PERFORM  9900-ERROR-FORMAT  THRU  9900-EXIT.     
                                                                 
*>*****************************************************************
*>                  3360-CHECK-FORM-AH-LIMITS                     *
*>   THIS SECTION WILL EDIT THE POLICY FORM CRITERIA FOR THE MIN  *
*>   AND MAX ISSUE AGE, ATTAINED AGE, MAX TERM AND MAX BENEFITS.  *
*>*****************************************************************
                                                                 
3360-CHECK-FORM-AH-LIMITS.                                       
                                                                 
    IF NOT AH-FORM-MASTER-FOUND                                  
        GO TO 3999-END-EDIT.                                     
                                                                 
3360-CHECK-FORM-AH-MIN-ISS-AGE.                                  
                                                                 
    IF PB-I-AGE  LESS THAN  FO-AH-MIN-ISSUE-AGE  OR              
      (PB-I-JOINT-AGE  LESS THAN  FO-AH-MIN-ISSUE-AGE  AND       
       WS-AH-JOINT-INDICATOR = 'J'  AND                          
       (WS-JOINT-AGE-FLAG = '1' OR 'Y'))                         
        IF PB-COMPANY-ID = 'NCL'                                 
           MOVE  ER-1912          TO  WS-ERROR                   
           PERFORM  9900-ERROR-FORMAT  THRU  9900-EXIT           
        ELSE                                                     
           MOVE  ER-1893          TO  WS-ERROR                   
           PERFORM  9900-ERROR-FORMAT  THRU  9900-EXIT.          
                                                                 
3360-CHECK-FORM-AH-ATT-AGE.                                      
                                                                 
    COMPUTE WS-ATT-AGE = WS-AH-EDIT-AGE + (PB-I-AH-TERM / 12).   
                                                                 
    IF  FO-MAX-ATT-AGE     NUMERIC     AND                       
        FO-MAX-ATT-AGE     GREATER THAN  ZEROS                   
        IF  WS-ATT-AGE   GREATER THAN  FO-MAX-ATT-AGE            
           IF PB-COMPANY-ID = 'NCL'                              
               MOVE  ER-1913      TO  WS-ERROR                   
               PERFORM  9900-ERROR-FORMAT THRU 9900-EXIT         
           ELSE                                                  
               MOVE  ER-1894      TO  WS-ERROR                   
               PERFORM  9900-ERROR-FORMAT THRU 9900-EXIT.        
                                                                 
3360-CHECK-FORM-AH-MAX-ISS-AGE.                                  
                                                                 
    IF  FO-AH-MAX-ISSUE-AGE    NUMERIC   AND                     
        FO-AH-MAX-ISSUE-AGE    GREATER THAN  +0                  
        IF  WS-AH-EDIT-AGE  GREATER THAN  FO-AH-MAX-ISSUE-AGE    
           IF PB-COMPANY-ID = 'NCL'                              
               MOVE  ER-1915      TO  WS-ERROR                   
               PERFORM  9900-ERROR-FORMAT  THRU  9900-EXIT       
           ELSE                                                  
               MOVE  ER-1900      TO  WS-ERROR                   
               PERFORM  9900-ERROR-FORMAT  THRU  9900-EXIT.      
                                                                 
3360-CHECK-FORM-AH-TERM-BAMT.                                    
                                                                 
    IF  FO-AH-MAX-TERM    NUMERIC   AND                          
        FO-AH-MAX-TERM    GREATER THAN  +0                       
        IF  PB-I-AH-TERM      GREATER THAN   FO-AH-MAX-TERM      
            IF  PB-COMPANY-ID = 'NCL'                            
                MOVE  ER-1917     TO  WS-ERROR                   
                PERFORM  9900-ERROR-FORMAT   THRU  9900-EXIT     
            ELSE                                                 
                 MOVE  ER-1903    TO  WS-ERROR                   
                 PERFORM  9900-ERROR-FORMAT   THRU  9900-EXIT.   
                                                                 
    IF  FO-MAX-AH-AMT    NUMERIC   AND                           
        FO-MAX-AH-AMT    GREATER THAN  +0                        
        IF  PB-I-AH-BENEFIT-AMT   GREATER THAN   FO-MAX-AH-AMT   
            IF  PB-COMPANY-ID = 'NCL'                            
                MOVE  ER-1919     TO  WS-ERROR                   
                PERFORM  9900-ERROR-FORMAT  THRU  9900-EXIT      
            ELSE                                                 
                MOVE  ER-1905     TO  WS-ERROR                   
                PERFORM  9900-ERROR-FORMAT  THRU  9900-EXIT.     
                                                                 
    GO TO 3999-END-EDIT.                                         
                                                                 
