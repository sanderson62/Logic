    EJECT                                                        
3400-edit-vin.                                                   
                                                                 
    move spaces                 to hldi-pass-area                
    MOVE PB-I-VIN               TO WS-WORK-VIN                   
    MOVE +8                     TO V2                            
    MOVE ZEROS                  TO WS-VIN-TOTAL                  
    PERFORM VARYING V1 FROM +1 BY +1 UNTIL                       
     (V1 > 17)                                                   
       IF WS-WORK-VIN (V1:1) = ' ' OR 'O' OR 'I' OR 'Q'          
          MOVE ER-3825          TO WS-ERROR                      
          PERFORM 9900-ERROR-FORMAT                              
                                THRU 9900-EXIT                   
       ELSE                                                      
          IF V1 = 8                                              
             MOVE 10            TO V2                            
          END-IF                                                 
          IF V1 = 9                                              
             MOVE 0             TO V2                            
          END-IF                                                 
          IF V1 = 10                                             
             MOVE 9             TO V2                            
          END-IF                                                 
          IF WS-WORK-VIN (V1:1) NUMERIC                          
             COMPUTE WS-VIN-TOTAL = WS-VIN-TOTAL                 
              + (WS-WORK-VIN-N (V1) * V2)                        
          ELSE                                                   
             MOVE WS-WORK-VIN (V1:1)                             
                           TO WS-HEX-BYTE                        
             COMPUTE V3 = (WS-CHARCD - WS-CHARCD-A) + 1          
             COMPUTE WS-VIN-TOTAL = WS-VIN-TOTAL                 
              + (WS-NUM (V3) * V2)                               
          END-IF                                                 
          COMPUTE V2 = V2 - 1                                    
       END-IF                                                    
    END-PERFORM                                                  
    MOVE ZEROS                  TO WS-VIN-REMAINDER              
    DIVIDE WS-VIN-TOTAL BY 11 GIVING WS-VIN-FINAL                
       REMAINDER WS-VIN-REMAINDER                                
    IF ((WS-WORK-VIN (9:1) = 'X')                                
       AND (WS-VIN-REMAINDER = 10))                              
               OR                                                
       (WS-WORK-VIN-N (9) = WS-VIN-REMAINDER)                    
       if pb-company-id = 'VPP'                                  
          perform 3410-check-with-hldi                           
                                thru 3410-exit                   
       end-if                                                    
    ELSE                                                         
       IF WS-WORK-VIN (1:1) > 0 AND < 6                          
          MOVE ER-3826          TO WS-ERROR                      
          PERFORM 9900-ERROR-FORMAT                              
                                THRU 9900-EXIT                   
       ELSE                                                      
          MOVE ER-3827          TO WS-ERROR                      
          PERFORM 9900-ERROR-FORMAT                              
                                THRU 9900-EXIT                   
       END-IF                                                    
    END-IF                                                       
                                                                 
    .                                                            
3400-exit.                                                       
    exit.                                                        
                                                                 
3410-check-with-hldi.                                            
                                                                 
    move spaces                 to hldi-pass-area                
    move pb-i-vin               to pa-vin                        
    perform 7785-call-hldi      thru 7785-exit                   
                                                                 
*>    display ' back to EL050 from elhldi '                        
*>    display 'PA-VIN        ' PA-VIN                              
*>    display 'PA-ErrorCode  ' PA-ErrorCode                        
*>    display 'PA-ErrorDesc  ' PA-ErrorDesc                        
*>    display 'PA-ModelYear  ' PA-ModelYear                        
*>    display 'PA-MakeName   ' PA-MakeName                         
*>    display 'PA-ModelName  ' PA-ModelName                        
*>    display 'PA-SeriesName ' PA-SeriesName                       
                                                                 
    if pa-errorcode (1:1) not = '0'                              
       move spaces              to hldi-pass-area                
       move pb-i-vin            to pa-vin                        
*>       move pa-errordesc        to pa-seriesname                 
*>       perform 7786-call-hldim  thru 7786-exit                   
*>       display ' back to EL050 from elhldim '                    
*>       display 'PA-VIN        ' PA-VIN                           
*>       display 'PA-ErrorCode  ' PA-ErrorCode                     
*>       display 'PA-ErrorDesc  ' PA-ErrorDesc                     
*>       display 'PA-ModelYear  ' PA-ModelYear                     
*>       display 'PA-MakeName   ' PA-MakeName                      
*>       display 'PA-ModelName  ' PA-ModelName                     
*>       display 'PA-SeriesName ' PA-SeriesName                    
    end-if                                                       
                                                                 
    .                                                            
3410-exit.                                                       
    exit.                                                        
                                                                 
                                                                 
