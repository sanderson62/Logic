3700-EDIT-CANCEL.                                                
    IF PB-COMPANY-ID = 'DMD'                                     
        MOVE ZEROS                   TO WS-RES-STATE-TAX         
                                        WS-RES-MUNI-TAX          
                                        PB-CI-STATE-TAX          
                                        PB-CI-MUNI-TAX           
        PERFORM 6600-GET-STATE-MUNICIPAL-TAX THRU 6640-EXIT.     
                                                                 
*>    IF PB-COMPANY-ID  EQUAL  'NCL'                               
*>       IF PB-ENTRY-BATCH  NUMERIC                                
*>          IF PB-CANCELLATION                                     
*>             MOVE PB-ENTRY-BATCH      TO  PB-C-MICROFILM-NO      
*>          ELSE                                                   
*>             IF PB-ISSUE                                         
*>                 MOVE PB-ENTRY-BATCH  TO  PB-I-MICROFILM-NO.     
                                                                 
    IF PB-C-INT-ON-REFS NOT NUMERIC                              
       MOVE ZEROS               TO PB-C-INT-ON-REFS              
    END-IF                                                       
                                                                 
*>    IF PB-C-MICROFILM-NO  NOT NUMERIC                            
*>        MOVE ZEROS              TO  PB-C-MICROFILM-NO.           
                                                                 
    IF CM-ENTRY-STATUS = 'V'                                     
       MOVE ER-2780             TO WS-ERROR                      
       PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT                  
       GO TO 3999-END-EDIT.                                      
                                                                 
    IF CM-ENTRY-STATUS = 'D'                                     
       MOVE ER-2781             TO WS-ERROR                      
       PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT                  
       GO TO 3999-END-EDIT.                                      
                                                                 
    IF CM-ENTRY-STATUS = 'U'                                     
       MOVE ER-2782             TO WS-ERROR                      
       PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT.                 
                                                                 
*>**************************************************************   
*>                                                             *   
*>   THE FOLLOWING LOGIC ALLOWS FOR VOIDING OF CANCELS         *   
*>   FOR EITHER OR BOTH LIFE AND A/H COVERAGE, BUT DOES        *   
*>   NOT ALLOW FOR VOIDING OF ONE COVERAGE AND ADJUSTING       *   
*>   OF ANOTHER COVERAGE.                                      *   
*>                                                             *   
*>**************************************************************   
                                                                 
    MOVE SPACE                  TO PB-C-LF-CANCEL-VOID-SW        
                                   PB-C-AH-CANCEL-VOID-SW.       
                                                                 
                                                                 
    if pb-company-id = 'DCC' or 'VPP'                            
       if ((pb-ci-lf-cancel-amt <> zeros)                        
                      or                                         
           (pb-ci-ah-cancel-amt <> zeros))                       
          MOVE ER-3068          TO WS-ERROR                      
          PERFORM 9900-ERROR-FORMAT                              
                                THRU 9900-EXIT                   
          GO TO 3999-END-EDIT                                    
       end-if                                                    
    end-if                                                       
                                                                 
    IF PB-C-LF-CANCEL-AMT = ZERO                                 
       NEXT SENTENCE                                             
    ELSE                                                         
       IF (PB-C-LF-CANCEL-AMT NEGATIVE)                          
          AND                                                    
          ((PB-C-LF-CANCEL-AMT + PB-CI-LF-CANCEL-AMT) = ZERO)    
              MOVE '1'          TO PB-C-LF-CANCEL-VOID-SW        
              MOVE ER-2745           TO WS-ERROR                 
              PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT.          
                                                                 
    IF PB-C-AH-CANCEL-AMT = ZERO                                 
       NEXT SENTENCE                                             
    ELSE                                                         
       IF (PB-C-AH-CANCEL-AMT NEGATIVE)                          
          AND                                                    
          (PB-C-AH-CANCEL-AMT + PB-CI-AH-CANCEL-AMT = ZERO)      
              MOVE '1'          TO PB-C-AH-CANCEL-VOID-SW        
              MOVE ER-2745           TO WS-ERROR                 
              PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT.          
                                                                 
    IF WS-LF-EARNINGS-CALC = '4'                                 
       MOVE '4'                   TO WS-LF-REFUND-CALC.          
                                                                 
    MOVE WS-TOL-LF-REFUND         TO PB-CALC-TOLERANCE.          
    MOVE WS-REF-REJECT-SW         TO PB-TOLERANCE-REJECT-SW.     
    MOVE WS-LF-REM-TERM-CALC      TO PB-LF-TERM-CALC-METHOD.     
    MOVE WS-AH-REM-TERM-CALC      TO PB-AH-TERM-CALC-METHOD.     
    MOVE WS-LF-REFUND-CALC        TO PB-LF-REFUND-TYPE.          
    MOVE WS-AH-REFUND-CALC        TO PB-AH-REFUND-TYPE.          
                                                                 
    MOVE ZERO                     TO PB-C-LF-REF-CALC            
                                     PB-C-AH-REF-CALC.           
    MOVE WS-ACCT-REIN-TABLE       TO PB-REIN-CD.                 
                                                                 
*>*****************************************************************
*>        EDIT BOTH THE CANCEL DATE AND REFUND AMOUNT             *
*>             TO VALIDATE COVERAGE.                              *
*>*****************************************************************
                                                                 
    IF PB-C-LF-CANCEL-DT  = LOW-VALUE AND                        
       PB-C-LF-CANCEL-AMT = ZEROS                                
         MOVE 'N'                 TO WS-LF-CANCEL-SW             
     ELSE                                                        
         MOVE 'Y'                 TO WS-LF-CANCEL-SW.            
                                                                 
    IF PB-C-AH-CANCEL-DT = LOW-VALUE  AND                        
       PB-C-AH-CANCEL-AMT = ZEROS                                
         MOVE 'N'                 TO WS-AH-CANCEL-SW             
     ELSE                                                        
         MOVE 'Y'                 TO WS-AH-CANCEL-SW.            
                                                                 
    EVALUATE TRUE                                                
        WHEN PB-CANCEL-FORCE             OR                      
             PB-CANCEL-DATE-FORCED       OR                      
             PB-FORCE-OFF                                        
                 IF PB-COMPANY-ID = 'ACE' or 'DCC' or 'VPP'      
                    MOVE CM-CANCEL-FEE  TO  PB-CI-CANCEL-FEE     
                 END-IF                                          
        WHEN (PB-CANCEL-DATE-FORCED-NO-FEE  OR                   
              PB-ALL-CANCEL-FORCED-NO-FEE)  AND                  
             (PB-COMPANY-ID = 'ACE' or 'DCC' or 'VPP')           
                 MOVE ZEROES TO PB-CI-CANCEL-FEE                 
        WHEN OTHER                                               
             IF PB-COMPANY-ID = 'ACE' or 'DCC' or 'VPP'          
                MOVE ER-2794              TO WS-ERROR            
             ELSE                                                
                MOVE ER-2749              TO WS-ERROR            
             END-IF                                              
             PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT            
    END-EVALUATE.                                                
                                                                 
                                                                 
*>    IF  PB-COMPANY-ID    EQUAL    'HER'                          
*>        IF  PB-C-MICROFILM-NO     EQUAL       ZEROS              
*>            MOVE  ER-1909            TO  WS-ERROR                
*>            PERFORM  9900-ERROR-FORMAT  THRU  9900-EXIT          
*>        ELSE                                                     
*>            MOVE  PB-C-MICROFILM-NO  TO  CM-CAN-MICROFILM-NO.    
                                                                 
    MOVE WS-ST-LF-PREM-TAX      TO PB-C-LF-PREM-TAX              
    IF PB-CI-INDV-GRP-CD   = '1' OR 'I'                          
       MOVE WS-ST-AH-PREM-TAX-I TO PB-C-AH-PREM-TAX              
    ELSE                                                         
       MOVE WS-ST-AH-PREM-TAX-G TO PB-C-AH-PREM-TAX              
    END-IF                                                       
    .                                                            
3705-EDIT-CANCEL-DT.                                             
                                                                 
    IF LF-COVERAGE-CANCELLED                                     
       AND (PB-CI-LF-BENEFIT-CD NOT = ZERO)                      
        MOVE PB-C-LF-CANCEL-DT TO DC-BIN-DATE-1                  
        MOVE WS-CURRENT-MONTH-END                                
                                TO DC-BIN-DATE-2                 
        SET ELAPSED-BETWEEN-BIN TO TRUE                          
*>        MOVE 1                  TO DC-OPTION-CODE                
        PERFORM 8500-DATE-CONVERT THRU 8500-EXIT                 
        IF DATE-CONVERSION-ERROR                                 
           NEXT SENTENCE                                         
         ELSE                                                    
            IF DC-ELAPSED-DAYS GREATER +180                      
               IF PB-CANCEL-DATE-FORCED  OR                      
                  PB-CANCEL-DATE-FORCED-NO-FEE                   
                     MOVE ER-2751  TO WS-ERROR                   
                     PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT    
                ELSE                                             
                  MOVE ER-2752  TO WS-ERROR                      
                  PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT.      
                                                                 
    IF AH-COVERAGE-CANCELLED                                     
       AND (PB-CI-AH-BENEFIT-CD NOT = ZERO)                      
        MOVE PB-C-AH-CANCEL-DT TO DC-BIN-DATE-1                  
        MOVE WS-CURRENT-MONTH-END                                
                                TO DC-BIN-DATE-2                 
        SET ELAPSED-BETWEEN-BIN TO TRUE                          
*>        MOVE 1                  TO DC-OPTION-CODE                
        PERFORM 8500-DATE-CONVERT THRU 8500-EXIT                 
        IF DATE-CONVERSION-ERROR                                 
           NEXT SENTENCE                                         
         ELSE                                                    
            IF DC-ELAPSED-DAYS GREATER +180                      
               IF PB-CANCEL-DATE-FORCED OR                       
                  PB-CANCEL-DATE-FORCED-NO-FEE                   
                     MOVE ER-2751  TO WS-ERROR                   
                     PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT    
                ELSE                                             
                  MOVE ER-2752  TO WS-ERROR                      
                  PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT.      
                                                                 
    IF WS-ERROR = ER-2752                                        
       AND (PB-COMPANY-ID = 'ABL'                                
        OR  PB-COMPANY-ID = 'FGL'                                
        OR  PB-COMPANY-ID = 'IST'                                
        OR  PB-COMPANY-ID = 'JHL'                                
        OR  PB-COMPANY-ID = 'LAP'                                
        OR  PB-COMPANY-ID = 'LBL'                                
        OR  PB-COMPANY-ID = 'NCB'                                
        OR  PB-COMPANY-ID = 'NLI'                                
        OR  PB-COMPANY-ID = 'OFI'                                
        OR  PB-COMPANY-ID = 'PLI'                                
        OR  PB-COMPANY-ID = 'TAO'                                
        OR  PB-COMPANY-ID = 'TIC'                                
        OR  PB-COMPANY-ID = 'HER'                                
        OR  WS-LF-SPECIAL-CALC-CD = 'Z'                          
        OR  WS-AH-SPECIAL-CALC-CD = 'Z')                         
           SUBTRACT +1          FROM PB-NO-OF-ERRORS             
           MOVE ER-2762     TO WS-ERROR                          
           PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT.             
                                                                 
    IF LF-COVERAGE-CANCELLED                                     
       AND (PB-CI-LF-BENEFIT-CD NOT = ZERO)                      
       IF PB-C-LF-CANCEL-DT GREATER WS-CURRENT-BIN-DT            
            MOVE ER-2733         TO WS-ERROR                     
            PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT.            
                                                                 
    IF AH-COVERAGE-CANCELLED                                     
       AND (PB-CI-AH-BENEFIT-CD NOT = ZERO)                      
       IF PB-C-AH-CANCEL-DT GREATER WS-CURRENT-BIN-DT            
             MOVE ER-2733        TO WS-ERROR                     
             PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT.           
                                                                 
    IF (PB-CI-LF-BENEFIT-CD    NOT =  ZEROS)  AND                
       (PB-CI-AH-BENEFIT-CD    NOT =  ZEROS)                     
       IF (PB-C-LF-CANCEL-DT   NOT =  LOW-VALUE)  AND            
          (PB-C-AH-CANCEL-DT   NOT =  LOW-VALUE)                 
           IF PB-C-LF-CANCEL-DT  NOT = PB-C-AH-CANCEL-DT         
             IF PB-COMPANY-ID NOT = 'LAP'                        
              MOVE ER-2704            TO WS-ERROR                
              PERFORM 9900-ERROR-FORMAT  THRU  9900-EXIT.        
                                                                 
    IF CERT-WAS-NOT-FOUND                                        
        GO TO 3999-END-EDIT.                                     
                                                                 
    IF PB-COMPANY-ID EQUAL 'CDC'                                 
       IF LF-COVERAGE-CANCELLED                                  
          AND (PB-CI-LF-BENEFIT-CD NOT = ZERO)                   
           MOVE PB-CERT-EFF-DT  TO DC-BIN-DATE-1                 
           MOVE PB-C-LF-CANCEL-DT                                
                                TO DC-BIN-DATE-2                 
           SET ELAPSED-BETWEEN-BIN TO TRUE                       
*>           MOVE 1               TO DC-OPTION-CODE                
           PERFORM 8500-DATE-CONVERT THRU 8500-EXIT              
           IF DATE-CONVERSION-ERROR                              
              NEXT SENTENCE                                      
            ELSE                                                 
               IF DC-ELAPSED-DAYS GREATER +60                    
                  MOVE 'N'      TO PB-C-LF-COMM-CHARGEBACK.      
                                                                 
    IF PB-COMPANY-ID EQUAL 'CDC'                                 
       IF AH-COVERAGE-CANCELLED                                  
          AND (PB-CI-AH-BENEFIT-CD NOT = ZERO)                   
           MOVE PB-CERT-EFF-DT  TO DC-BIN-DATE-1                 
           MOVE PB-C-AH-CANCEL-DT                                
                                TO DC-BIN-DATE-2                 
           MOVE +0              TO DC-ELAPSED-DAYS               
                                   DC-ELAPSED-MONTHS             
           SET ELAPSED-BETWEEN-BIN  TO TRUE                      
*>           MOVE 1               TO DC-OPTION-CODE                
           PERFORM 8500-DATE-CONVERT THRU 8500-EXIT              
           IF DATE-CONVERSION-ERROR                              
              NEXT SENTENCE                                      
            ELSE                                                 
               IF DC-ELAPSED-DAYS GREATER +60                    
                  MOVE 'N'      TO PB-C-AH-COMM-CHARGEBACK.      
                                                                 
    MOVE +0                     TO DC-ELAPSED-DAYS               
                                   DC-ELAPSED-MONTHS.            
                                                                 
    IF (WS-LF-SPECIAL-CALC-CD = 'Z')  OR                         
       (WS-AH-SPECIAL-CALC-CD = 'Z')                             
         MOVE 'Z'                TO PB-CI-OB-FLAG                
         MOVE ER-2767            TO WS-ERROR                     
         PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT                
         MOVE PB-C-LF-CANCEL-AMT TO PB-C-LF-REF-CALC             
         MOVE PB-C-AH-CANCEL-AMT TO PB-C-AH-REF-CALC             
         GO TO 3775-OVER-REF-CHK                                 
       ELSE                                                      
         MOVE ZEROS              TO PB-CI-LIVES                  
                                    PB-C-LIVES                   
         IF PB-CI-OB-FLAG NOT = 'B'                              
            MOVE SPACE           TO PB-CI-OB-FLAG.               
                                                                 
    IF (NOT NO-LIFE-COVERAGE  AND  WS-LF-SPECIAL-CALC-CD = 'D')  
        MOVE ZEROS              TO PB-CI-EXTENTION-DAYS          
    ELSE                                                         
        MOVE ZEROS              TO PB-CI-TERM-IN-DAYS.           
                                                                 
    IF (PB-CI-ENTRY-DT GREATER THAN PB-CREDIT-SELECT-DT)         
       AND (PB-CI-CREDIT-INTERFACE-SW-1 NOT = SPACE AND '3')     
           MOVE ER-2760         TO WS-ERROR                      
           PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT.             
                                                                 
    IF LF-COVERAGE-CANCELLED                                     
       AND (PB-CI-LF-BENEFIT-CD NOT = ZERO)                      
        IF PB-C-LF-CANCEL-DT LESS PB-CERT-EFF-DT                 
           AND (PB-C-LF-CANCEL-DT NOT = LOW-VALUES)              
              MOVE ER-2774       TO WS-ERROR                     
              PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT.          
                                                                 
    IF AH-COVERAGE-CANCELLED                                     
       AND (PB-CI-AH-BENEFIT-CD NOT = ZERO)                      
        IF PB-C-AH-CANCEL-DT LESS PB-CERT-EFF-DT                 
           AND (PB-C-AH-CANCEL-DT NOT = LOW-VALUES)              
              MOVE ER-2775       TO WS-ERROR                     
              PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT.          
                                                                 
    IF LF-COVERAGE-CANCELLED                                     
       AND (PB-CI-LF-BENEFIT-CD NOT = ZERO)                      
        IF PB-C-LF-CANCEL-DT = LOW-VALUES                        
           MOVE ER-2776         TO WS-ERROR                      
           PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT.             
                                                                 
    IF AH-COVERAGE-CANCELLED                                     
       AND (PB-CI-AH-BENEFIT-CD NOT = ZERO)                      
        IF PB-C-AH-CANCEL-DT = LOW-VALUES                        
           MOVE ER-2777         TO WS-ERROR                      
           PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT.             
                                                                 
    IF LF-COVERAGE-CANCELLED                                     
       AND (PB-CI-LF-BENEFIT-CD NOT = ZERO)                      
       if pb-ci-lf-expire-dt = low-values or spaces              
          MOVE PB-CERT-EFF-DT   TO DC-BIN-DATE-1                 
          MOVE PB-CI-LF-TERM    TO DC-ELAPSED-MONTHS             
          MOVE PB-CI-EXTENTION-DAYS                              
                                TO DC-ELAPSED-DAYS               
          SET BIN-PLUS-ELAPSED  TO TRUE                          
          PERFORM 8500-DATE-CONVERT                              
                                THRU 8500-EXIT                   
          IF DATE-CONVERSION-ERROR                               
             move low-values    to dc-bin-date-2                 
          end-if                                                 
       else                                                      
          move pb-ci-lf-expire-dt                                
                                to dc-bin-date-2                 
       end-if                                                    
       IF PB-C-LF-CANCEL-DT GREATER DC-BIN-DATE-2                
          IF PB-COMPANY-ID = 'JHL' OR 'BNE' OR 'HAN'             
                            OR  'FIA'                            
             MOVE DC-BIN-DATE-2 TO PB-C-LF-CANCEL-DT             
          ELSE                                                   
             MOVE 'Y'           TO WS-FUTURE-CANCEL-DT-SW        
             MOVE ER-2731       TO WS-ERROR                      
             PERFORM 9900-ERROR-FORMAT                           
                                THRU 9900-EXIT                   
          end-if                                                 
       end-if                                                    
    end-if                                                       
                                                                 
