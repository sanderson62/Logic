                ELSE                                             
                  IF PB-C-AH-CANCEL-DT <> CL-CERT-EFF-DT         
                     IF MSTR-CERT-NO = W-CL-CERT-NO              
                        MOVE ER-2890 TO WS-ERROR                 
                        PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT 
                        GO TO 7300-NEXT-CLAIM                    
                     ELSE                                        
                        MOVE ER-2898 TO WS-ERROR                 
                        PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT 
                        GO TO 7300-NEXT-CLAIM                    
                     END-IF                                      
                  END-IF                                         
                END-IF                                           
             END-IF                                              
          ELSE                                                   
             IF CL-DENIAL-TYPE = '1'  AND                        
                CL-TOTAL-PAID-AMT = 0                            
                   GO TO 7300-NEXT-CLAIM                         
             END-IF                                              
             IF PB-C-AH-CANCEL-DT <= CL-PAID-THRU-DT             
