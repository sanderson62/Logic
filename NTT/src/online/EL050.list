*>*********************************                                
*>** UNSIGN-WRK3 SHOULD CHECK IF GREATER THAN LF-REFUND TOLERANCE  
*>** ONLY.  ADDING LF-REFUND TOLERANCE AND AH-REFUND TOLERANCE     
*>** DOUBLES THE TOLERANCE AMOUNT.                                 
*>**    CHANGED ONLY FOR MON NOW, BUT SHOULD BE IN EFFECT FOR ALL. 
*>*********************************                                
    IF PB-COMPANY-ID = 'MON' or 'CID' or 'DCC' or 'VPP'          
    IF WS-UNSIGN-WRK3 >= WS-TOL-LF-REFUND                        
        IF WS-REF-REJECT-SW = SPACE                              
            MOVE ER-2754        TO WS-ERROR                      
            PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT             
          ELSE                                                   
            MOVE ER-2736        TO WS-ERROR                      
            PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT.            
*>*********************************                                
                                                                 
3773-CONTINUE.                                                   
                                                                 
    IF PB-C-LF-CANCEL-AMT GREATER THAN +0                        
       IF PB-C-LF-CANCEL-AMT    LESS THAN WS-MINIMUM-REFUND      
          MOVE ER-2723          TO WS-ERROR                      
          PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT.              
                                                                 
    IF PB-C-AH-CANCEL-AMT GREATER THAN +0                        
       IF PB-C-AH-CANCEL-AMT    LESS THAN WS-MINIMUM-REFUND      
          MOVE ER-2724          TO WS-ERROR                      
          PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT.              
                                                                 
    IF WS-LF-REFUND-OVS-PCT IS GREATER THAN ZEROS                
        COMPUTE WS-TOL-LF-HIGH-PREM-PCT = PB-C-LF-REF-CALC *     
            (1 + WS-LF-REFUND-OVS-PCT)                           
    ELSE                                                         
        MOVE +9999999.99        TO  WS-TOL-LF-HIGH-PREM-PCT.     
                                                                 
    IF WS-TOL-LF-REFUND-PCT IS GREATER THAN ZEROS                
        COMPUTE WS-TOL-LF-LOW-PREM-PCT  = PB-C-LF-REF-CALC *     
            (1 - WS-TOL-LF-REFUND-PCT)                           
    ELSE                                                         
        MOVE +0                 TO  WS-TOL-LF-LOW-PREM-PCT.      
                                                                 
    IF (PB-COMPANY-ID = 'DCC' or 'VPP')                          
       AND (PB-CI-ENTRY-STATUS = 'M')                            
       AND (PB-C-LF-CANCEL-AMT > +0)                             
       MOVE ER-3843             TO WS-ERROR                      
       PERFORM 9900-ERROR-FORMAT                                 
                                THRU 9900-EXIT                   
    END-IF                                                       
    EVALUATE TRUE                                                
       WHEN (PB-C-LF-REF-CALC = PB-C-LF-CANCEL-AMT)              
          CONTINUE                                               
       WHEN PB-C-LF-CANCEL-AMT = +0                              
                AND PB-C-LF-REF-CALC > +0                        
          CONTINUE                                               
       WHEN WS-LF-REFUND-OVS-AMT = 0                             
                AND WS-TOL-LF-REFUND = 0                         
          CONTINUE                                               
       WHEN OTHER                                                
          IF WS-TOL-LF-REFUND  = +0                              
             MOVE PB-C-LF-REF-CALC TO WS-TOL-LF-REFUND           
          END-IF                                                 
          IF ((PB-C-LF-REF-CALC > PB-C-LF-CANCEL-AMT) AND        
               (PB-C-LF-REF-CALC - WS-TOL-LF-REFUND ) <          
                         (PB-C-LF-CANCEL-AMT))                   
             CONTINUE                                            
          ELSE                                                   
             IF WS-LF-REFUND-OVS-AMT = +0                        
                MOVE +9999999.99      TO WS-LF-REFUND-OVS-AMT    
             END-IF                                              
             IF ((PB-C-LF-REF-CALC < PB-C-LF-CANCEL-AMT) AND     
                 (PB-C-LF-REF-CALC + WS-LF-REFUND-OVS-AMT) >     
                                      PB-C-LF-CANCEL-AMT)        
                CONTINUE                                         
             ELSE                                                
                IF WS-REF-REJECT-SW IS EQUAL TO SPACE            
                  MOVE ER-2754   TO  WS-ERROR                    
                     PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT    
                ELSE                                             
                   MOVE ER-2736   TO  WS-ERROR                   
                   PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT      
                END-IF                                           
             END-IF                                              
          END-IF                                                 
    END-EVALUATE.                                                
                                                                 
    EVALUATE TRUE                                                
       WHEN (PB-C-LF-REF-CALC = PB-C-LF-CANCEL-AMT)              
          CONTINUE                                               
       WHEN PB-C-LF-CANCEL-AMT = +0                              
                AND PB-C-LF-REF-CALC > +0                        
          CONTINUE                                               
       WHEN OTHER                                                
          IF ((PB-C-LF-REF-CALC > PB-C-LF-CANCEL-AMT) AND        
              (PB-C-LF-CANCEL-AMT > WS-TOL-LF-LOW-PREM-PCT))     
             CONTINUE                                            
          ELSE                                                   
