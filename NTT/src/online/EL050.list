       AND (PB-I-LOAN-APR = ZERO)                                
       MOVE ER-2657             TO WS-ERROR                      
       PERFORM 9900-ERROR-FORMAT                                 
                                THRU 9900-EXIT                   
       GO TO 3130-EDIT-AGE                                       
    END-IF                                                       
                                                                 
*>    IF PB-I-LOAN-APR = ZERO                                      
*>       MOVE WS-DEFAULT-APR      TO PB-I-LOAN-APR                 
*>    END-IF                                                       
                                                                 
    if pb-i-loan-apr = +99.9999                                  
       move er-2656             to ws-error                      
       PERFORM 9900-ERROR-FORMAT                                 
                                THRU 9900-EXIT                   
       GO TO 3130-EDIT-AGE                                       
    end-if                                                       
                                                                 
*>    IF PB-I-LOAN-APR = ZERO                                      
*>       MOVE ER-2656             TO WS-ERROR                      
*>       PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT                  
*>       GO TO 3130-EDIT-AGE.                                      
                                                                 
*>    IF PB-I-LOAN-APR LESS +00.0001 OR GREATER +98.999            
    if pb-i-loan-apr not = +99.9999                              
       IF PB-I-LOAN-APR > +30.000                                
          MOVE ER-2657          TO WS-ERROR                      
          PERFORM 9900-ERROR-FORMAT                              
                                THRU 9900-EXIT                   
       end-if                                                    
    end-if                                                       
                                                                 
    .                                                            
3130-EDIT-AGE.                                                   
                                                                 
*>*****************************************************************
*>    ATTAINED AGE EDIT IS NOT REQUIRED FOR OUTSTANDING BUSINESS  *
*>*****************************************************************
                                                                 
    IF NOT NO-LIFE-COVERAGE                                      
       IF WS-LF-SPECIAL-CALC-CD NOT = 'O'                        
          IF (WS-EDIT-AGE + (PB-I-LF-TERM / 12)) GREATER +99     
              MOVE ER-2628             TO WS-ERROR               
              PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT.          
                                                                 
    IF NOT NO-AH-COVERAGE                                        
       IF WS-AH-SPECIAL-CALC-CD NOT = 'O'                        
          IF (WS-AH-EDIT-AGE + (PB-I-AH-TERM / 12)) GREATER +99  
              MOVE ER-2628         TO WS-ERROR                   
              PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT.          
                                                                 
