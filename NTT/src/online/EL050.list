    .                                                            
0560-EDIT-CONTINUE.                                              
                                                                 
    MOVE ZEROS                  TO WS-LF-PMF-COMM                
                                   WS-AH-PMF-COMM                
*>****   THIS ROUTINE VALIDATES THE EXISTENCE OF A COMMISSION      
*>****   TABLE FOR THE 'O', 'P' AND 'K' AGENT TYPES                
                                                                 
    PERFORM VARYING SUB1 FROM +1 BY +1 UNTIL                     
       (SUB1 > +10)                                              
       IF AM-COM-TYP (SUB1) = 'O' OR 'P' OR 'K' OR 'A'           
          MOVE WS-AH-BENEFIT-CD TO WS-AH-COMP-CD                 
          MOVE '0530'           TO CTBL-SW                       
          MOVE ' '              TO WS-CTBL-READ-SW               
          PERFORM 0561-CHECK-AH THRU 0561-EXIT                   
          MOVE WS-LF-BENEFIT-CD TO WS-LF-COMP-CD                 
          MOVE '0540'           TO CTBL-SW                       
          MOVE ' '              TO WS-CTBL-READ-SW               
          PERFORM 0563-CHECK-LF THRU 0563-EXIT                   
       END-IF                                                    
    END-PERFORM                                                  
                                                                 
    GO TO 0564-EDIT-CONTINUE                                     
                                                                 
    .                                                            
0561-CHECK-AH.                                                   
                                                                 
    IF (NO-AH-COVERAGE)                                          
       OR (NO-AH-BENEFIT-MATCH)                                  
       CONTINUE                                                  
    ELSE                                                         
       IF (AM-A-COM (SUB1) NUMERIC)                              
          AND (AM-A-COMA (SUB1) (3:1) NOT = 'L' AND 'M')         
          IF (AM-COM-TYP (SUB1) = 'A')                           
             AND (WS-AH-PMF-COMM = ZEROS)                        
             MOVE AM-A-COM (SUB1) TO WS-AH-PMF-COMM              
          END-IF                                                 
          CONTINUE                                               
       ELSE                                                      
          MOVE WS-AH-COMP-CD    TO CTBL-BEN-CODE                 
          MOVE PB-I-AH-TERM     TO WS-WORK-TERM                  
          MOVE PB-AH-OVERRIDE-L1                                 
                                TO CTBL-BEN-TYPE                 
          MOVE ZEROS            TO WS-SUB1                       
                                   WS-COMMISSION                 
          MOVE AM-A-COMA (SUB1) TO CTBL-TABLE                    
          PERFORM 8300-READ-CTBL-TABLE                           
                                THRU 8300-EXIT                   
          IF CTBL-TABLE-FOUND                                    
             IF (AM-COM-TYP (SUB1) = 'A')                        
                AND (WS-AH-PMF-COMM = ZEROS)                     
                COMPUTE WS-COMM-CK-AMT = PB-I-AH-BENEFIT-AMT *   
                                       PB-I-AH-TERM              
                PERFORM 8800-GET-COMP-RATE THRU 8800-EXIT        
                MOVE WS-WK-RATE        TO WS-AH-PMF-COMM         
             END-IF                                              
             CONTINUE                                            
          ELSE                                                   
             IF FIRST-TIME-THROUGH                               
                GO TO 0561-CHECK-AH                              
             ELSE                                                
                MOVE ER-2609    TO WS-ERROR                      
                PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT         
             END-IF                                              
          END-IF                                                 
       END-IF                                                    
    END-IF                                                       
                                                                 
    .                                                            
0561-EXIT.                                                       
    EXIT.                                                        
                                                                 
0563-CHECK-LF.                                                   
                                                                 
    IF (NO-LIFE-COVERAGE)                                        
       OR (NO-LF-BENEFIT-MATCH)                                  
       CONTINUE                                                  
    ELSE                                                         
       IF WS-LF-JOINT-INDICATOR = 'J'                            
          IF AM-J-COM (SUB1) NUMERIC                             
             AND (AM-J-COMA (SUB1) (3:1) NOT = 'L' AND 'M')      
             IF (AM-COM-TYP (SUB1) = 'A')                        
                AND (WS-LF-PMF-COMM = ZEROS)                     
                MOVE AM-J-COM (SUB1) TO WS-LF-PMF-COMM           
             END-IF                                              
             CONTINUE                                            
          ELSE                                                   
             MOVE WS-LF-COMP-CD TO CTBL-BEN-CODE                 
             MOVE PB-I-LF-TERM  TO WS-WORK-TERM                  
             MOVE PB-LIFE-OVERRIDE-L1                            
                                TO CTBL-BEN-TYPE                 
             MOVE ZEROS         TO WS-SUB1                       
                                   WS-COMMISSION                 
             MOVE AM-J-COMA (SUB1)                               
                                TO CTBL-TABLE                    
             PERFORM 8300-READ-CTBL-TABLE                        
                                THRU 8300-EXIT                   
             IF CTBL-TABLE-FOUND                                 
                IF (AM-COM-TYP (SUB1) = 'A')                     
                   AND (WS-LF-PMF-COMM = ZEROS)                  
                   MOVE PB-I-LF-BENEFIT-AMT  TO WS-COMM-CK-AMT   
                   PERFORM 8800-GET-COMP-RATE THRU 8800-EXIT     
                   MOVE WS-WK-RATE        TO WS-LF-PMF-COMM      
                END-IF                                           
                CONTINUE                                         
             ELSE                                                
                IF FIRST-TIME-THROUGH                            
                   GO TO 0563-CHECK-LF                           
                ELSE                                             
                   MOVE ER-2609 TO WS-ERROR                      
                   PERFORM 9900-ERROR-FORMAT                     
                                THRU 9900-EXIT                   
*>                   GO TO 0560-EDIT-CONTINUE                      
                END-IF                                           
             END-IF                                              
          END-IF                                                 
       ELSE                                                      
          IF AM-L-COM (SUB1) NUMERIC                             
             AND (AM-L-COMA (SUB1) (3:1) NOT = 'L' AND 'M')      
             IF (AM-COM-TYP (SUB1) = 'A')                        
                AND (WS-LF-PMF-COMM = ZEROS)                     
                MOVE AM-L-COM (SUB1) TO WS-LF-PMF-COMM           
             END-IF                                              
             CONTINUE                                            
          ELSE                                                   
             MOVE WS-LF-COMP-CD TO CTBL-BEN-CODE                 
             MOVE PB-I-LF-TERM  TO WS-WORK-TERM                  
             MOVE PB-LIFE-OVERRIDE-L1                            
                                TO CTBL-BEN-TYPE                 
             MOVE ZEROS         TO WS-SUB1                       
                                   WS-COMMISSION                 
             MOVE AM-L-COMA (SUB1)                               
                                TO CTBL-TABLE                    
             PERFORM 8300-READ-CTBL-TABLE                        
                                THRU 8300-EXIT                   
             IF CTBL-TABLE-FOUND                                 
                CONTINUE                                         
             ELSE                                                
                IF FIRST-TIME-THROUGH                            
                   GO TO 0563-CHECK-LF                           
                ELSE                                             
                   MOVE ER-2609 TO WS-ERROR                      
                   PERFORM 9900-ERROR-FORMAT                     
                                THRU 9900-EXIT                   
                END-IF                                           
             END-IF                                              
          END-IF                                                 
       END-IF                                                    
    END-IF                                                       
                                                                 
    .                                                            
0563-EXIT.                                                       
    EXIT.                                                        
                                                                 
0564-EDIT-CONTINUE.                                              
                                                                 
    IF  PB-COMPANY-ID EQUAL 'DMD'                                
        IF PB-I-NUM-BILLED EQUAL ZEROS                           
           MOVE ER-8148       TO WS-ERROR                        
           PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT.             
                                                                 
*>    IF  PB-I-BIRTHDAY GREATER THAN LOW-VALUE                     
*>         MOVE PB-I-BIRTHDAY    TO DC-BIN-DATE-1                  
*>         MOVE PB-CERT-EFF-DT   TO DC-BIN-DATE-2                  
*>         SET ELAPSED-BETWEEN-BIN TO TRUE                         
*>         PERFORM 8500-DATE-CONVERT THRU 8500-EXIT                
*>         IF NO-CONVERSION-ERROR                                  
*>             COMPUTE PB-I-AGE  =  DC-ELAPSED-MONTHS / +12        
*>         ELSE                                                    
*>             IF PB-COMPANY-ID EQUAL 'HAN'                        
*>                 MOVE ZEROS     TO PB-I-AGE.                     
                                                                 
    IF PB-I-BIRTHDAY > LOW-VALUES                                
       MOVE PB-I-BIRTHDAY       TO DC-BIN-DATE-1                 
       MOVE PB-CERT-EFF-DT      TO DC-BIN-DATE-2                 
       SET ELAPSED-BETWEEN-BIN  TO TRUE                          
       PERFORM 8500-DATE-CONVERT                                 
                                THRU 8500-EXIT                   
       IF NO-CONVERSION-ERROR                                    
          COMPUTE WS-WORK-AGE = DC-ELAPSED-MONTHS / +12          
          IF PB-I-AGE > ZEROS                                    
             IF WS-WORK-AGE NOT = PB-I-AGE                       
                MOVE ER-9501    TO WS-ERROR                      
                PERFORM 9900-ERROR-FORMAT                        
                                THRU 9900-EXIT                   
             END-IF                                              
          ELSE                                                   
             MOVE WS-WORK-AGE   TO PB-I-AGE                      
          END-IF                                                 
       END-IF                                                    
    END-IF                                                       
                                                                 
    MOVE 'N'                   TO WS-INS-AGE-DEFAULTED           
                                  WS-INS-AGE-SET                 
    IF PB-I-AGE = ZEROS                                          
       IF PB-COMPANY-ID NOT = 'HAN'  AND  'JHL'                  
          MOVE 'Y'             TO WS-INS-AGE-DEFAULTED           
                                  WS-INS-AGE-SET                 
          MOVE WS-DEFAULT-AGE  TO PB-I-AGE.                      
                                                                 
    MOVE PB-I-AGE              TO WS-EDIT-AGE                    
                                  WS-AH-EDIT-AGE.                
                                                                 
    IF PB-I-JOINT-BIRTHDAY NOT = LOW-VALUES AND SPACES           
       MOVE PB-I-JOINT-BIRTHDAY TO DC-BIN-DATE-1                 
       MOVE PB-CERT-EFF-DT      TO DC-BIN-DATE-2                 
       SET ELAPSED-BETWEEN-BIN  TO TRUE                          
       PERFORM 8500-DATE-CONVERT THRU 8500-EXIT                  
       IF NO-CONVERSION-ERROR                                    
          COMPUTE WS-WORK-AGE = DC-ELAPSED-MONTHS / +12          
          IF PB-I-JOINT-AGE > 0                                  
             IF WS-WORK-AGE NOT = PB-I-JOINT-AGE                 
                MOVE ER-9503    TO WS-ERROR                      
                PERFORM 9900-ERROR-FORMAT                        
                                THRU 9900-EXIT                   
             END-IF                                              
          ELSE                                                   
             MOVE WS-WORK-AGE   TO PB-I-JOINT-AGE                
          END-IF                                                 
       ELSE                                                      
          MOVE ZEROS            TO PB-I-JOINT-AGE                
       END-IF                                                    
    END-IF                                                       
                                                                 
    IF PB-I-JOINT-AGE NOT NUMERIC                                
       MOVE ZERO               TO PB-I-JOINT-AGE.                
                                                                 
    MOVE 'N'                   TO WS-JNT-AGE-DEFAULTED           
                                  WS-JNT-AGE-SET                 
    IF PB-COMPANY-ID = 'NCL' OR 'CID' OR 'DCC' OR 'AHL' or 'VPP' 
       IF (WS-LF-JOINT-INDICATOR = 'J')  OR                      
          (WS-AH-JOINT-INDICATOR = 'J')                          
          IF PB-I-JOINT-AGE = ZEROS                              
              MOVE 'Y'                TO WS-JNT-AGE-DEFAULTED    
                                         WS-JNT-AGE-SET          
              MOVE WS-DEFAULT-AGE     TO PB-I-JOINT-AGE          
          END-IF                                                 
       END-IF                                                    
    END-IF                                                       
                                                                 
    MOVE ' ' TO PB-I-LIFE-INDICATOR                              
    IF WS-LF-JOINT-INDICATOR = 'J'                               
       MOVE 'J'                TO PB-I-LIFE-INDICATOR            
       IF  WS-JOINT-AGE-FLAG = '1' OR 'Y'                        
           IF  PB-I-JOINT-AGE = ZERO                             
               MOVE ER-2686       TO WS-ERROR                    
               PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT          
           ELSE                                                  
               IF  PB-I-JOINT-AGE GREATER PB-I-AGE               
                   MOVE PB-I-JOINT-AGE TO WS-EDIT-AGE            
                   IF PB-COMPANY-ID NOT = 'LBL'                  
                       MOVE ER-2685    TO WS-ERROR               
                       PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT. 
                                                                 
    IF WS-AH-JOINT-INDICATOR = 'J'                               
       IF  WS-JOINT-AGE-FLAG = '1' OR 'Y'                        
           IF  PB-I-JOINT-AGE = ZERO                             
               MOVE ER-2686       TO WS-ERROR                    
               PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT          
           ELSE                                                  
               IF  PB-I-JOINT-AGE GREATER PB-I-AGE               
                   MOVE PB-I-JOINT-AGE TO WS-AH-EDIT-AGE         
                                          WS-EDIT-AGE            
                   IF PB-COMPANY-ID NOT = 'LBL'                  
                       MOVE ER-2685    TO WS-ERROR               
                       PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT. 
                                                                 
*>*****************************************************************
*>    MINIMUM AGE IS NOT REQUIRED FOR OUTSTANDING BUSINESS         
*>*****************************************************************
                                                                 
    IF (WS-LF-SPECIAL-CALC-CD = 'O') OR                          
       (WS-AH-SPECIAL-CALC-CD = 'O')                             
       NEXT SENTENCE                                             
    ELSE                                                         
       IF PB-I-AGE LESS THAN WS-MIN-AGE  OR                      
         (PB-I-JOINT-AGE LESS THAN WS-MIN-AGE  AND               
          (WS-LF-JOINT-INDICATOR = 'J'  OR                       
           WS-AH-JOINT-INDICATOR = 'J')  AND                     
          (WS-JOINT-AGE-FLAG = '1' OR 'Y'))                      
           MOVE ER-2627             TO WS-ERROR                  
           PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT.             
                                                                 
    IF (WS-LF-SPECIAL-CALC-CD = 'O') OR                          
       (WS-AH-SPECIAL-CALC-CD = 'O')                             
        NEXT SENTENCE                                            
    ELSE                                                         
        IF WS-LF-JOINT-INDICATOR = 'J'  OR                       
           WS-AH-JOINT-INDICATOR = 'J'                           
            IF PB-I-JOINT-INSURED = SPACES                       
                IF PB-COMPANY-ID NOT = 'JAL'                     
                    MOVE ER-2696    TO WS-ERROR                  
                    PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT.    
                                                                 
    IF PB-COMPANY-ID NOT = 'BOA'                                 
       IF (WS-LF-JOINT-INDICATOR NOT = 'J') AND                  
          (WS-AH-JOINT-INDICATOR NOT = 'J')                      
          IF (PB-I-JOINT-AGE NOT = ZEROS OR                      
              PB-I-JOINT-INSURED NOT = SPACES)                   
               MOVE ER-2691         TO WS-ERROR                  
               PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT.         
                                                                 
    IF PB-COMPANY-ID = 'BOA'                                     
       IF (WS-LF-JOINT-INDICATOR NOT = 'J') AND                  
          (WS-AH-JOINT-INDICATOR NOT = 'J')                      
           IF PB-I-JOINT-AGE NOT = ZEROS                         
               MOVE ER-2691         TO WS-ERROR                  
               PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT.         
                                                                 
    IF PB-COMPANY-ID = 'CRI'                                     
       IF PB-I-SIG-SW = 'J'                                      
          IF WS-LF-JOINT-INDICATOR NOT = 'J'  AND                
             WS-AH-JOINT-INDICATOR NOT = 'J'                     
             MOVE ER-2675       TO WS-ERROR                      
             PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT.           
                                                                 
    IF PB-COMPANY-ID = 'CRI'                                     
       IF PB-I-SIG-SW = 'S'                                      
          IF WS-LF-JOINT-INDICATOR = 'J'  OR                     
             WS-AH-JOINT-INDICATOR = 'J'                         
             MOVE ER-2676       TO WS-ERROR                      
             PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT.           
                                                                 
    IF PB-COMPANY-ID = 'CVL'                                     
        IF (WS-AH-JOINT-INDICATOR = 'J') AND                     
           (WS-LF-JOINT-INDICATOR NOT = 'J')                     
             MOVE ER-2783           TO WS-ERROR                  
             PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT.           
                                                                 
    IF (WS-LF-JOINT-INDICATOR = 'J')                             
       AND (WS-AH-JOINT-INDICATOR = 'J')                         
       AND (PB-I-LF-TERM > 120)                                  
       AND (PB-I-LF-TERM NOT = PB-I-AH-TERM)                     
       MOVE ER-2783             TO WS-ERROR                      
       PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT                  
    END-IF                                                       
                                                                 
    MOVE SPACE                  TO PB-I-INDV-GRP-CD.             
                                                                 
    IF PB-I-INDV-GRP-OVRD = 'I'                                  
       MOVE ER-2666             TO WS-ERROR                      
       PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT                  
       MOVE 'I'                 TO PB-I-INDV-GRP-CD.             
                                                                 
    IF PB-I-INDV-GRP-OVRD = 'G'                                  
       MOVE ER-2666             TO WS-ERROR                      
       PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT                  
       MOVE 'G'                 TO PB-I-INDV-GRP-CD.             
                                                                 
    IF PB-I-INDV-GRP-OVRD = 'I' OR 'G'                           
       NEXT SENTENCE                                             
    ELSE                                                         
       MOVE WS-IG-OVRD          TO PB-I-INDV-GRP-CD.             
                                                                 
    IF PB-I-INDV-GRP-OVRD = WS-IG-OVRD                           
       IF WS-ERROR = ER-2666                                     
          MOVE +0    TO PB-COMMON-ERROR (PB-NO-OF-ERRORS)        
          SUBTRACT +1           FROM PB-NO-OF-ERRORS.            
                                                                 
    IF PB-COMPANY-ID = 'CID'                                     
       IF PB-STATE = 'UT' OR 'MO'                                
          IF ((PB-I-LIFE-BENEFIT-CD = '45' OR '46'               
                OR '76' OR '77')                                 
                     OR                                          
              (PB-I-AH-BENEFIT-CD = '38' OR '39' OR '40'         
                  OR '41' OR '42' OR '63' OR '64' OR '65'        
                  OR '66' OR '67' OR '2H' OR '2I' OR '2J'        
                  OR '2K' OR '2L'))                              
             AND PB-I-INDV-GRP-CD = 'I'                          
             MOVE ER-2721             TO WS-ERROR                
             PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT            
          END-IF                                                 
       END-IF                                                    
    END-IF                                                       
                                                                 
    MOVE WS-ST-LF-PREM-TAX      TO PB-I-LF-PREM-TAX              
    IF PB-I-INDV-GRP-CD   = '1' OR 'I'                           
       MOVE WS-ST-AH-PREM-TAX-I TO PB-I-AH-PREM-TAX              
    ELSE                                                         
       MOVE WS-ST-AH-PREM-TAX-G TO PB-I-AH-PREM-TAX              
    END-IF                                                       
                                                                 
*>    MOVE WS-BANK-FEE            TO PB-I-BANK-FEE                 
*>    MOVE WS-BANK-NOCHRGB        TO PB-I-BANK-NOCHRGB             
    MOVE 'NO '                  TO ACCOUNT-LIFE-CK-SW            
                                   ACCOUNT-AH-CK-SW.             
                                                                 
    IF PB-REISSUED-CERT       OR                                 
       PB-REIN-ONLY-CERT      OR                                 
       PB-POLICY-IS-DECLINED  OR                                 
       PB-POLICY-IS-VOIDED                                       
*>       OR PB-MONTHLY-CERT                                        
        GO TO 3000-CONTINUE-EDIT.                                
                                                                 
    IF PB-COMPANY-ID = 'CID' OR 'AHL'                            
       IF WS-EDIT-AGE           > +65                            
          MOVE ER-2709          TO WS-ERROR                      
          PERFORM 9900-ERROR-FORMAT                              
                                THRU 9900-EXIT                   
       ELSE                                                      
          IF WS-EDIT-AGE        > +55                            
             MOVE ER-2718       TO WS-ERROR                      
             PERFORM 9900-ERROR-FORMAT                           
                                THRU 9900-EXIT                   
          END-IF                                                 
       END-IF                                                    
                                                                 
    IF PB-COMPANY-ID = 'DCC' or 'VPP'                            
       IF WS-EDIT-AGE           > +65                            
          MOVE ER-2709          TO WS-ERROR                      
          PERFORM 9900-ERROR-FORMAT                              
                                THRU 9900-EXIT                   
       END-IF                                                    
    END-IF                                                       
    .                                                            
                                                                 
0565-LIFE-PLAN-LIMIT-EDITS.                                      
                                                                 
    IF NOT LF-PLAN-MASTER-FOUND                                  
       MOVE +0                  TO EC-AM-LF-MAX-ATT-AGE          
                                   EC-AM-LF-MAX-TOT-BEN          
       GO TO 0580-AH-PLAN-LIMIT-EDITS.                           
                                                                 
    MOVE LINK-LIFE-PLAN-RECORD  TO PLAN-MASTER.                  
                                                                 
    IF PB-COMPANY-ID NOT = 'NCL' AND 'ACE'                       
        IF PL-LM-AGE (1) = ZEROS                                 
            GO TO 0580-AH-PLAN-LIMIT-EDITS.                      
                                                                 
    IF NO-LF-BENEFIT-MATCH          OR                           
       WS-LF-BENEFIT-CD = ZEROS     OR                           
       WS-LF-SPECIAL-CALC-CD = 'M'                               
       GO TO 0580-AH-PLAN-LIMIT-EDITS.                           
                                                                 
    MOVE 'YES'                  TO ACCOUNT-LIFE-CK-SW.           
    MOVE +1                     TO SUB2.                         
                                                                 
    IF PB-COMPANY-ID EQUAL 'CRI' OR 'ACE'                        
       IF PB-I-SIG-SW EQUAL 'S' OR 'J'                           
          MOVE +5   TO SUB2                                      
          IF (PL-LM-AMT (SUB2) EQUAL +0) AND                     
             (PL-LM-DUR (SUB2) EQUAL +0) AND                     
             (PL-LM-AGE (SUB2) EQUAL +0)                         
             MOVE 'NO ' TO ACCOUNT-LIFE-CK-SW                    
             GO TO 0580-AH-PLAN-LIMIT-EDITS.                     
                                                                 
    COMPUTE WS-ATT-AGE = WS-EDIT-AGE + (PB-I-LF-TERM / 12).      
                                                                 
    COMPUTE WS-LF-BENEFIT-AMT = PB-I-LF-BENEFIT-AMT              
                              + PB-I-LF-ALT-BENEFIT-AMT.         
                                                                 
0568-CHECK-LIFE-ATT-AGE.                                         
                                                                 
                                                                 
    IF PL-ATT-AGE NUMERIC AND                                    
       PL-ATT-AGE > +0                                           
       MOVE PL-ATT-AGE          TO EC-AM-LF-MAX-ATT-AGE          
    END-IF                                                       
                                                                 
                                                                 
                                                                 
*>    IF PL-ATT-AGE NUMERIC AND                                    
*>       PL-ATT-AGE > +0                                           
*>       MOVE PL-ATT-AGE          TO EC-AM-LF-MAX-ATT-AGE          
*>       IF PB-I-BIRTHDAY > LOW-VALUES                             
*>          MOVE PB-I-BIRTHDAY    TO DC-BIN-DATE-1                 
*>          COMPUTE DC-ELAPSED-MONTHS = PL-ATT-AGE * +12           
*>          MOVE ZERO             TO DC-ELAPSED-DAYS               
*>          SET BIN-PLUS-ELAPSED  TO TRUE                          
*>          PERFORM 8500-DATE-CONVERT                              
*>                                THRU 8500-EXIT                   
*>          IF NO-CONVERSION-ERROR                                 
*>             IF PB-I-LF-EXPIRE-DT > DC-BIN-DATE-2                
*>             MOVE ER-2669       TO WS-ERROR                      
*>             PERFORM 9900-ERROR-FORMAT                           
*>                                THRU 9900-EXIT                   
*>          END-IF                                                 
*>       ELSE                                                      
*>          IF WS-ATT-AGE >= PL-ATT-AGE                            
*>             MOVE ER-2669       TO WS-ERROR                      
*>             PERFORM 9900-ERROR-FORMAT                           
*>                                THRU 9900-EXIT                   
*>          END-IF                                                 
*>       END-IF                                                    
*>    END-IF                                                       
                                                                 
    .                                                            
0570-CHECK-LIFE-LIMITS.                                          
                                                                 
    IF PB-COMPANY-ID EQUAL 'CRI'                                 
       IF PB-I-SIG-SW NOT EQUAL 'S' AND 'J'                      
          IF SUB2 GREATER THAN +4                                
             MOVE ER-2637 TO WS-ERROR                            
             PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT            
             GO TO 0580-AH-PLAN-LIMIT-EDITS.                     
                                                                 
    IF PB-COMPANY-ID  =  'NCL'                                   
       IF (SUB2 GREATER THAN +4  AND  NOT APP-CERT-USED)  OR     
           SUB2 GREATER THAN +8                                  
            MOVE 'U'         TO PB-BATCH-ENTRY                   
            MOVE ER-2637     TO WS-ERROR                         
            PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT             
            GO TO 0575-CHECK-NCL-SIG-SWITCH.                     
                                                                 
    IF SUB2 > +8                                                 
       IF (PB-COMPANY-ID = 'CID' OR 'DCC' OR 'AHL' or 'VPP')     
          AND (PB-I-SIG-SW = 'Y')                                
          CONTINUE                                               
       ELSE                                                      
          MOVE ER-2637          TO WS-ERROR                      
          PERFORM 9900-ERROR-FORMAT                              
                                THRU 9900-EXIT                   
       END-IF                                                    
       GO TO 0580-AH-PLAN-LIMIT-EDITS                            
    END-IF                                                       
                                                                 
    MOVE PL-LM-AGE (SUB2)    TO EC-AM-LF-MAX-AGE.                
    MOVE PL-LM-DUR (SUB2)    TO EC-AM-LF-MAX-TERM.               
    MOVE PL-LM-AMT (SUB2)    TO EC-AM-LF-MAX-TOT-BEN.            
                                                                 
    IF WS-EDIT-AGE  GREATER THAN  PL-LM-AGE (SUB2)               
       ADD +1  TO SUB2                                           
       GO TO 0570-CHECK-LIFE-LIMITS.                             
                                                                 
    IF PB-COMPANY-ID = 'NCL'                                     
        GO TO 0570-CHECK-NCL-LIFE-LIMITS.                        
                                                                 
    IF (PB-I-LF-TERM > PL-LM-DUR (SUB2))                         
       OR (WS-LF-BENEFIT-AMT > PL-LM-AMT (SUB2))                 
       IF (PB-COMPANY-ID = 'CID' OR 'DCC' OR 'AHL' or 'VPP')     
          AND (PB-I-SIG-SW = 'Y')                                
          CONTINUE                                               
       ELSE                                                      
          MOVE ER-2637          TO WS-ERROR                      
          PERFORM 9900-ERROR-FORMAT                              
                                THRU 9900-EXIT                   
       END-IF                                                    
    END-IF                                                       
                                                                 
    GO TO 0580-AH-PLAN-LIMIT-EDITS.                              
                                                                 
                                                                 
