                        GO TO 7300-NEXT-CLAIM                    
                     END-IF                                      
                END-IF                                           
             ELSE                                                
                IF (CL-DENIAL-TYPE = '1')   *> regular denial    
                   and (CL-TOTAL-PAID-AMT = 0)                   
                   GO TO 7300-NEXT-CLAIM                         
                END-IF                                           
                IF (                                             
                     (AH-COVERAGE-CANCELLED)                     
                     AND (PB-C-AH-CANCEL-DT <> CL-INCURRED-DT)   
                     AND (WS-AH-CLAIM-RESCINDED <> 'Y')          
                     and (pb-ci-ah-cancel-amt +                  
                        pb-c-ah-cancel-amt) <> zeros             
                   )                                             
                              OR                                 
                   (                                             
                     (LF-COVERAGE-CANCELLED)                     
                     AND (PB-C-LF-CANCEL-DT <> CL-INCURRED-DT)   
                     AND (WS-ST-REFUND-CLAIM-FLAG = '4')         
                     and (pb-ci-lf-cancel-amt +                  
