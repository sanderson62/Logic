    .                                                            
3705-EDIT-AH-CANCEL-DT.                                          
                                                                 
    IF AH-COVERAGE-CANCELLED                                     
       AND (PB-CI-AH-BENEFIT-CD NOT = ZERO)                      
       if pb-ci-AH-expire-dt = low-values or spaces              
          MOVE PB-CERT-EFF-DT   TO DC-BIN-DATE-1                 
          MOVE PB-CI-AH-TERM    TO DC-ELAPSED-MONTHS             
          MOVE PB-CI-EXTENTION-DAYS                              
                                TO DC-ELAPSED-DAYS               
          SET BIN-PLUS-ELAPSED  TO TRUE                          
          PERFORM 8500-DATE-CONVERT                              
                                THRU 8500-EXIT                   
          IF DATE-CONVERSION-ERROR                               
             move low-values    to dc-bin-date-2                 
          end-if                                                 
       else                                                      
          move pb-ci-ah-expire-dt                                
                                to dc-bin-date-2                 
       end-if                                                    
       IF PB-C-AH-CANCEL-DT GREATER DC-BIN-DATE-2                
          IF PB-COMPANY-ID = 'JHL' OR 'BNE' OR 'HAN'             
                            OR  'FIA'                            
             MOVE DC-BIN-DATE-2 TO PB-C-AH-CANCEL-DT             
          ELSE                                                   
             MOVE 'Y'           TO WS-FUTURE-CANCEL-DT-SW        
             MOVE ER-2771       TO WS-ERROR                      
             PERFORM 9900-ERROR-FORMAT                           
                                THRU 9900-EXIT                   
          end-if                                                 
       end-if                                                    
    end-if                                                       
                                                                 
*>    IF AH-COVERAGE-CANCELLED                                     
*>       AND (PB-CI-AH-BENEFIT-CD NOT = ZERO)                      
*>        MOVE PB-CERT-EFF-DT       TO DC-BIN-DATE-1               
*>        MOVE PB-CI-AH-TERM        TO DC-ELAPSED-MONTHS           
*>        MOVE PB-CI-EXTENTION-DAYS TO DC-ELAPSED-DAYS             
