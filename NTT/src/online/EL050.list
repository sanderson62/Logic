    IF PB-I-INSURED-SEX = SPACE OR ZERO                          
        IF WS-DEFAULT-SEX = 'M'                                  
            MOVE WS-DEFAULT-SEX TO PB-I-INSURED-SEX              
         ELSE                                                    
            IF WS-DEFAULT-SEX = 'F'                              
            MOVE WS-DEFAULT-SEX TO PB-I-INSURED-SEX.             
                                                                 
    IF PB-I-INSURED-SEX = '1'                                    
        MOVE 'M' TO PB-I-INSURED-SEX.                            
                                                                 
    IF PB-I-INSURED-SEX = '2'                                    
        MOVE 'F' TO PB-I-INSURED-SEX.                            
                                                                 
3134-SIG-SW-EDIT.                                                
    IF PB-I-SIG-SW = SPACE                                       
        MOVE 'N'                TO  PB-I-SIG-SW.                 
                                                                 
    IF PB-COMPANY-ID = 'NCL'                                     
        IF PB-I-SIG-SW = 'S' OR 'J' OR 'N' OR 'Y' OR ' '         
            NEXT SENTENCE                                        
        ELSE                                                     
            MOVE ER-1923        TO  WS-ERROR                     
            PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT             
    ELSE                                                         
    IF PB-COMPANY-ID = 'CRI'                                     
        IF PB-I-SIG-SW = 'S' OR 'J' OR 'N' OR ' '                
            NEXT SENTENCE                                        
        ELSE                                                     
            MOVE ER-2606        TO  WS-ERROR                     
            PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT             
    ELSE                                                         
    IF PB-COMPANY-ID = 'TMS'                                     
        IF PB-I-SIG-SW = 'N' OR 'U' OR 'O'                       
            NEXT SENTENCE                                        
        ELSE                                                     
            MOVE ER-2689        TO  WS-ERROR                     
            PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT             
    ELSE                                                         
        IF PB-I-SIG-SW = 'Y' OR 'N'                              
            NEXT SENTENCE                                        
        ELSE                                                     
            MOVE ER-2651        TO  WS-ERROR                     
            PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT.            
                                                                 
    IF PB-COMPANY-ID = 'FLA'                                     
        IF (WS-EDIT-AGE GREATER 54  OR                           
            WS-AH-EDIT-AGE GREATER 54)  AND                      
           PB-I-SIG-SW = 'N'                                     
           MOVE ER-2651         TO  WS-ERROR                     
           PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT.             
                                                                 
