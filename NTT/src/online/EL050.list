*>             ELSE                                                
*>              MOVE 'Y'           TO WS-FUTURE-CANCEL-DT-SW       
*>              MOVE ER-2771       TO WS-ERROR                     
*>              PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT.          
    .                                                            
3705-EDIT-CONTINUED.                                             
                                                                 
*>    IF LF-COVERAGE-CANCELLED                                     
*>      IF PB-CI-AH-PAID-THRU-DT NOT = LOW-VALUE                   
*>         IF PB-C-LF-CANCEL-DT NOT GREATER PB-CI-AH-PAID-THRU-DT  
*>            MOVE ER-2727         TO WS-ERROR                     
*>            PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT.            
*>                                                                 
*>    IF AH-COVERAGE-CANCELLED                                     
*>      IF PB-CI-AH-PAID-THRU-DT NOT = LOW-VALUE                   
*>        IF PB-COMPANY-ID = 'ACE'                                 
*>           MOVE ER-2727        TO WS-ERROR                       
*>           PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT              
*>        ELSE                                                     
*>           IF PB-C-AH-CANCEL-DT NOT GREATER PB-CI-AH-PAID-THRU-DT
*>              MOVE ER-2727     TO WS-ERROR                       
*>              PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT.          
                                                                 
    IF PB-COMPANY-ID = 'ACE'                                     
      IF AH-COVERAGE-CANCELLED                                   
        IF PB-CI-AH-PAID-THRU-DT NOT = LOW-VALUE                 
          IF PB-C-AH-CANCEL-DT LESS THAN PB-CI-AH-PAID-THRU-DT   
             MOVE ER-2732      TO WS-ERROR                       
             PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT.           
                                                                 
    IF PB-COMPANY-ID      EQUAL   'HER'                          
       IF WS-ERROR        EQUAL   ER-2727                        
           SUBTRACT  +1           FROM  PB-NO-OF-ERRORS          
           MOVE  ER-2725          TO  WS-ERROR                   
           PERFORM  9900-ERROR-FORMAT  THRU  9900-EXIT.          
                                                                 
    IF PB-C-CLAIM-CREATED-CANCEL                                 
           MOVE ER-2748         TO WS-ERROR                      
           PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT.             
                                                                 
    IF PB-C-LAST-NAME NOT = SPACES                               
        IF PB-C-LAST-NAME NOT = PB-CI-LAST-NAME                  
           MOVE ER-2747         TO WS-ERROR                      
           PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT.             
                                                                 
3710-EDIT-LUMP-SUM-PRIOR.                                        
                                                                 
    IF AH-COVERAGE-CANCELLED                                     
        IF (PB-CI-AH-SETTLEMENT-DT NOT = LOW-VALUE)              
            IF PB-CI-AH-SETTLEMENT-DT NOT = PB-C-AH-CANCEL-DT    
               MOVE ER-2735    TO WS-ERROR                       
               PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT.         
                                                                 
    IF LF-COVERAGE-CANCELLED                                     
        IF (PB-CI-AH-SETTLEMENT-DT NOT = LOW-VALUE)              
            IF PB-CI-AH-SETTLEMENT-DT NOT = PB-C-LF-CANCEL-DT    
               MOVE ER-2735    TO WS-ERROR                       
               PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT.         
                                                                 
3710-EDIT-PRIOR-DEATH.                                           
                                                                 
*>    IF LF-COVERAGE-CANCELLED                                     
*>        IF (PB-CI-DEATH-DT NOT = LOW-VALUE) AND                  
*>           (CM-LF-STATUS-AT-DEATH NOT = SPACE) AND               
*>           (NOT PB-CI-OB)                                        
*>             IF PB-CI-DEATH-DT NOT = PB-C-LF-CANCEL-DT           
*>                IF PB-CERT-EFF-DT NOT EQUAL PB-C-LF-CANCEL-DT    
*>                   MOVE ER-2732  TO WS-ERROR                     
*>                   PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT.     
*>                                                                 
*>    IF AH-COVERAGE-CANCELLED                                     
*>        IF (PB-CI-DEATH-DT NOT = LOW-VALUE) AND                  
*>           (CM-LF-STATUS-AT-DEATH NOT = SPACE) AND               
*>           (NOT PB-CI-OB)                                        
*>             IF PB-CI-DEATH-DT NOT = PB-C-AH-CANCEL-DT           
*>                IF PB-CERT-EFF-DT NOT EQUAL PB-C-AH-CANCEL-DT    
*>                   MOVE ER-2732  TO WS-ERROR                     
