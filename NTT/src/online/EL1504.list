7100-GET-CHK-CASHED-DT.                                          
                                                                 
    move spaces                 to ws-check-cashed-dt            
    move '2'                    to ws-draft-or-check  *>  draft  
    if at-check-written-dt not = low-values and spaces           
       evaluate true                                             
          when (at-check-written-dt > X'ACFE')    *> 04/30/2015  
          and (pi-company-id not = 'DCC')                        
             move '1'           to ws-draft-or-check  *>  check  
             move '000'         to ws-check-number (1:3)         
             move at-check-no   to ws-check-number (4:7)         
          when (at-check-written-dt > X'B016')    *> 05/22/2017  
          and (pi-company-id = 'DCC')                            
             move '1'           to ws-draft-or-check  *>  check  
             move '000'         to ws-check-number (1:3)         
             move at-check-no   to ws-check-number (4:7)         
          when at-payment-type = 'I'                             
             move '000'         to ws-check-number (1:3)         
             move at-check-no   to ws-check-number (4:7)         
          when other                                             
             move '0'           to ws-check-number (1:1)         
             move at-check-no (1:1)                              
                                to ws-check-number (2:1)         
             move '00'          to ws-check-number (3:2)         
             move at-check-no (2:6)                              
                                to ws-check-number (5:6)         
       end-evaluate                                              
    else   *>  no check written date yet                         
       go to 7100-exit                                           
    end-if                                                       
                                                                 
    evaluate true                                                
       when pi-company-id = 'AHL'                                
          move '%AHL%'          to ws-check-company              
       when pi-company-id = 'DCC'                                
          move '%DCC%'          to ws-check-company              
       when pi-company-id = 'VPP'                                
          move '%VPP%'          to ws-check-company              
       when pi-company-id = 'FNL'                                
          move '%FNL%'          to ws-check-company              
       when other   *>   CID s/b   CSO                           
          move '%CSO%'          to ws-check-company              
    end-evaluate                                                 
                                                                 
    move at-payment-type        to ws-check-type                 
    move at-claim-no            to ws-claim-number               
    move at-amount-paid         to ws-check-amt-tmp              
    move ws-check-amt-tmpx      to ws-check-amount               
                                                                 
    EXEC SQL                                                     
      CALL LogicPaidBankChkCashedDt                              
               (                                                 
                @draftorcheck = :WS-DRAFT-OR-CHECK,              
                @checktype = :WS-CHECK-TYPE,                     
                @claimnumber = :WS-CLAIM-NUMBER,                 
                @checknumber = :WS-CHECK-NUMBER,                 
                @checkamount = :WS-CHECK-AMOUNT,                 
                @checkcompany = :WS-CHECK-COMPANY,               
                @checkcasheddate = :WS-CHECK-CASHED-DT           
               )                                                 
    END-EXEC                                                     
    IF SQLCODE NOT = 0                                           
       MOVE SPACES TO WS-CHECK-CASHED-DT                         
        DISPLAY "ERROR: DID NOT RETURN CHK CASHED DT "           
        DISPLAY ' SQL RETURN CODE ' SQLCODE                      
        DISPLAY ' SQL ERR MESS    ' SQLERRMC                     
       GO TO 7100-EXIT                                           
    END-IF                                                       
                                                                 
    .                                                            
7100-EXIT.                                                       
    EXIT.                                                        
                                                                 
