4000-PERFORM-CALCULATIONS.                                       
    IF WS-FREQ = 'MO'                                            
         MOVE 12 TO PPY.                                         
    IF WS-FREQ = '13'                                            
         MOVE 13 TO PPY.                                         
    IF WS-FREQ = 'SM'                                            
         MOVE 24 TO PPY.                                         
    IF WS-FREQ = 'BW'                                            
         MOVE 26 TO PPY.                                         
    IF WS-FREQ = 'WK'                                            
         MOVE 52 TO PPY.                                         
    IF WS-FREQ = 'SA'                                            
         MOVE 2 TO PPY.                                          
    IF WS-FREQ = 'AN'                                            
         MOVE 1 TO PPY.                                          
                                                                 
    IF WS-BASIS-X = 'AF'                                         
         MOVE 1 TO WS-BASIS.                                     
    IF WS-BASIS-X = 'TP'                                         
         MOVE 2 TO WS-BASIS.                                     
    IF WS-BASIS-X = 'NP'                                         
         MOVE 3 TO WS-BASIS.                                     
                                                                 
    MOVE WS-AMOUNT-REQUESTED TO OTF.                             
                                                                 
    COMPUTE I = WS-APR / (100 * PPY).                            
                                                                 
    MOVE WS-ADDTL-DAYS TO ADL-DAYS.                              
                                                                 
    COMPUTE ADL-ADJ = 1 + I * ADL-DAYS / (365 / PPY).            
                                                                 
    COMPUTE ATF = OTF * ADL-ADJ.                                 
                                                                 
    MOVE WS-TRUNC TO COV-TERM.                                   
                                                                 
    MOVE WS-PAYMENTS-COVERED TO AHN.                             
                                                                 
    MOVE WS-NUMBER-OF-PMTS TO N.                                 
                                                                 
    IF AHN LESS 1                                                
        MOVE COV-TERM TO AHN.                                    
                                                                 
    IF AHN LESS 1                                                
        MOVE N TO AHN.                                           
                                                                 
    MOVE WS-EXTRA TO EMII.                                       
                                                                 
    EJECT                                                        
                                                                 
4000-CONTINUE-CALCS-1.                                           
*>******** M USED FOR TRUNCATED NET PAY                            
    IF PI-COMPANY-ID EQUAL 'CDC' OR 'GTL' OR 'MON' OR 'CSO'      
                        OR 'LGX'                                 
       IF WS-TRUNC NOT EQUAL +0                                  
          MOVE WS-TRUNC         TO M.                            
                                                                 
    MOVE WS-LIFE-RATE TO LIFE-RATE.                              
                                                                 
    COMPUTE LR = (LIFE-RATE / 100) * 12 / PPY.                   
                                                                 
    COMPUTE LIFEDEV = WS-LIFE-DEVIATION / 100.                   
                                                                 
    COMPUTE LR = LR * LIFEDEV.                                   
                                                                 
    MOVE WS-AH-RATE TO AH-RATE.                                  
                                                                 
    COMPUTE AH = AH-RATE / 100.                                  
                                                                 
    COMPUTE AHDEV = WS-AH-DEVIATION / 100.                       
                                                                 
    COMPUTE AH = AH * AHDEV.                                     
                                                                 
*>***** BALLOON RATE                                               
    MOVE WS-BL-RATE TO BL-RATE.                                  
                                                                 
    COMPUTE BL = BL-RATE / 100.                                  
*>******************                                               
                                                                 
*>****  N1 USED FOR O/B                                            
    IF COV-TERM = ZERO                                           
       COMPUTE N1 = N * 12 / PPY                                 
      ELSE                                                       
       COMPUTE N1 = COV-TERM * 12 / PPY.                         
                                                                 
    COMPUTE K1 = 12 / PPY.                                       
                                                                 
    IF PPY = 1 OR 2                                              
        MOVE 1     TO K1.                                        
                                                                 
    COMPUTE PADJ = (N1 + K1) / (N1 + 1).                         
                                                                 
    IF WS-OBYN = 'Y'                                             
       IF PI-COMPANY-ID EQUAL 'CSO' OR 'CID' OR 'LGX' OR 'AHL'   
             or 'FNL'                                            
       COMPUTE L = LR * PADJ * M / 12 * (N1 + 1) / 20 * 12 / N1  
       ELSE                                                      
       COMPUTE L = LR * PADJ * N / 12 * (N1 + 1) / 20 * 12 / N1  
    ELSE                                                         
       IF PI-COMPANY-ID EQUAL 'CDC' OR 'GTL' OR 'MON' OR 'CSO'   
                        OR 'CID' OR 'AHL' OR 'FNL'               
                        OR 'LGX'                                 
          COMPUTE L = LR * PADJ * M / 12                         
       ELSE                                                      
*>          COMPUTE L = LR * PADJ * N / 12.                        
          COMPUTE L = LR * PADJ * AHN / 12.                      
                                                                 
    IF WS-BALLOON-AMOUNT NOT = ZERO                              
        GO TO 7000-BALLOON-CALC.                                 
                                                                 
    COMPUTE V = 1 / (1 + I).                                     
                                                                 
    COMPUTE VU = 1 - V ** N.                                     
                                                                 
    COMPUTE ANGLN = VU / I.                                      
                                                                 
    IF PI-COMPANY-ID EQUAL 'CDC' OR 'GTL' OR 'MON' OR 'CSO'      
                        OR 'CID' OR 'AHL' or 'FNL'               
                        OR 'LGX'                                 
       COMPUTE TOT-PMTS = I / VU * M                             
    ELSE                                                         
       COMPUTE TOT-PMTS = I / VU * N.                            
                                                                 
    COMPUTE ATOT-PMTS = I / VU * AHN.                            
                                                                 
    EJECT                                                        
                                                                 
4000-CONTINUE-CALCS-2.                                           
    GO TO 4000-COMM, 4000-COMM, 4000-NP                          
        DEPENDING ON WS-BASIS.                                   
                                                                 
4000-NP.                                                         
    MOVE COV-TERM TO M.                                          
    IF M = 0                                                     
       MOVE N TO M.                                              
                                                                 
    COMPUTE VU1 = 1 - V ** (N - M).                              
                                                                 
    COMPUTE ANGLM = VU1 / I.                                     
                                                                 
    COMPUTE VA = (M - ANGLN + ANGLM) * 2 * N                     
                  / (I * M * (2 * N - M + 1) * ANGLN).           
                                                                 
    IF PI-COMPANY-ID EQUAL 'CDC' OR 'GTL' OR 'MON' OR 'CSO'      
                        OR 'CID' OR 'AHL' OR 'FNL'               
                        OR 'LGX'                                 
       COMPUTE VA = VA * ((1 + N) / (1 + M)).                    
                                                                 
    COMPUTE RA = 1 - (N - M) * (N - M + 1) / (N * (N + 1)).      
                                                                 
    COMPUTE TA = VA * RA.                                        
                                                                 
    COMPUTE NPLF = TA * L * (1 + EMII * I).                      
                                                                 
    IF WS-NPBEN = 'Y'                                            
       COMPUTE NPLF = NPLF / (1 - NPLF).                         
                                                                 
4000-COMM.                                                       
    MOVE ZERO TO LIFE-PREM AH-PREM.                              
                                                                 
    PERFORM 4000-GET-PREMIUM THRU 4000-EXIT.                     
    GO TO 6000-DISPLAY-RESULTS.                                  
                                                                 
4000-GET-PREMIUM.                                                
    PERFORM 5000-COMPUTE-PREMIUMS THRU 5000-EXIT                 
        VARYING XREPT FROM +1 BY +1 UNTIL XREPT = +15.           
4000-EXIT.                                                       
    EXIT.                                                        
                                                                 
   EJECT                                                         
                                                                 
5000-COMPUTE-PREMIUMS.                                           
    GO TO 5000-AF, 5000-TP, 5000-NP                              
        DEPENDING ON WS-BASIS.                                   
                                                                 
5000-AF.                                                         
    COMPUTE LIFE-PREM = L / (1 - L) * (1 + AH-PREM).             
    GO TO 5000-GET-AH-PREM.                                      
                                                                 
5000-TP.                                                         
    COMPUTE LIFE-PREM = TOT-PMTS * L / (1 - TOT-PMTS * L)        
                            * (1 + AH-PREM).                     
    GO TO 5000-GET-AH-PREM.                                      
                                                                 
5000-NP.                                                         
    COMPUTE LIFE-PREM = NPLF * (1 + AH-PREM).                    
    GO TO 5000-GET-AH-PREM.                                      
                                                                 
5000-GET-AH-PREM.                                                
    COMPUTE AH-PREM = ATOT-PMTS * AH / (1 - ATOT-PMTS * AH)      
                            * (1 + LIFE-PREM).                   
                                                                 
    IF AH = 0 OR                                                 
       LR = 0                                                    
         GO TO 4000-EXIT.                                        
                                                                 
5000-EXIT.                                                       
    EXIT.                                                        
                                                                 
  EJECT                                                          
                                                                 
6000-DISPLAY-RESULTS.                                            
    IF CHG-LF OR CHG-LF-AH                                       
        COMPUTE L-PREMIUM = LIFE-PREM * ATF                      
      ELSE                                                       
        COMPUTE L-PREMIUM = LIFE-PREM * OTF.                     
                                                                 
    COMPUTE L-PREMIUM = (L-PREMIUM * 100 + .502) / 100.          
                                                                 
    IF CHG-AH OR CHG-LF-AH                                       
        COMPUTE A-PREMIUM = AH-PREM * ATF                        
      ELSE                                                       
        COMPUTE A-PREMIUM = AH-PREM * OTF.                       
                                                                 
    COMPUTE A-PREMIUM = (A-PREMIUM * 100 + .502) / 100.          
                                                                 
    COMPUTE PAYMENT = I / VU * (ATF + L-PREMIUM + A-PREMIUM).    
                                                                 
    COMPUTE PAYMENT = (PAYMENT * 100 + .002) / 100.              
                                                                 
    COMPUTE TOT-FIN = OTF + L-PREMIUM + A-PREMIUM.               
                                                                 
    COMPUTE TOT-PAYMT = N * PAYMENT.                             
                                                                 
    COMPUTE TOT-INTRS = TOT-PAYMT - TOT-FIN.                     
                                                                 
6500-COMMON-DISPLAY.                                             
    MOVE TOT-FIN             TO BAMOUNTO.                        
                                                                 
    MOVE L-PREMIUM           TO BLAMTO.                          
    MOVE A-PREMIUM           TO BDAMTO.                          
    MOVE PAYMENT             TO BMOPMTO.                         
    MOVE TOT-PAYMT           TO BTOTPMTO.                        
    MOVE TOT-INTRS           TO BTOTINTO.                        
    MOVE B                   TO BALPMTO.                         
                                                                 
    MOVE -1                  TO ANAMEL.                          
    GO TO 8200-SEND-DATAONLY.                                    
                                                                 
    EJECT                                                        
7000-BALLOON-CALC.                                               
    MOVE WS-BALLOON-AMOUNT   TO B.                               
*>    MOVE WS-REGULAR-PAYMENT  TO MP.                              
                                                                 
    COMPUTE V = 1 / (1 + I).                                     
                                                                 
    COMPUTE V-NTH = V ** N.                                      
                                                                 
    COMPUTE ANGLN = (1 - V-NTH) / I.                             
                                                                 
    COMPUTE ANGLN-LESS-1 = (1 - V-NTH / V) / I.                  
                                                                 
    COMPUTE NM1 = N - 1.                                         
                                                                 
    COMPUTE AEQ = N / (N + 1).                                   
                                                                 
    IF WS-BALYN = 'Y'                                            
        MOVE N TO NM1                                            
        MOVE ANGLN TO ANGLN-LESS-1                               
        MOVE 1 TO AEQ.                                           
                                                                 
    IF WS-BASIS = 2                                              
        GO TO 7500-TP-BALLOON.                                   
                                                                 
*>    IF MP NOT = ZERO                                             
*>        GO TO 7200-REGULAR-PAYMENT.                              
                                                                 
    COMPUTE MPT = (N + 1) / 2 * (ATF - B * V-NTH) + L * (1 + EMII
                 * I) * B * ANGLN.                               
                                                                 
    COMPUTE MPB = (N + 1) / 2 * (ANGLN-LESS-1 - NM1 * AH) - L    
                  * (1 + EMII * I) * ((NM1 - ANGLN-LESS-1) / I). 
                                                                 
    DIVIDE MPT BY MPB GIVING MP.                                 
                                                                 
    GO TO 8000-DISPLAY-BALLOON-RESULTS.                          
                                                                 
7200-REGULAR-PAYMENT.                                            
    COMPUTE MPB = (N + 1) / 2 * (ANGLN-LESS-1 - NM1 * AH) - L    
                   * (1 + EMII * I) * (NM1 - ANGLN-LESS-1) / I.  
                                                                 
    COMPUTE B = (MPB * MP - (N + 1) / 2 * ATF) / (L * (1 + EMII  
                  * I) * ANGLN - (N + 1) / 2 * V-NTH).           
                                                                 
    GO TO 8000-DISPLAY-BALLOON-RESULTS.                          
                                                                 
    EJECT                                                        
7500-TP-BALLOON.                                                 
*>    IF MP NOT = ZERO                                             
*>        GO TO 7600-REGULAR-PAYMENT.                              
                                                                 
    COMPUTE MPT = (ATF - B * V-NTH) + L * B * 2 * N / (N + 1).   
                                                                 
    COMPUTE MPB = (ANGLN-LESS-1 - NM1 * AH) - L * NM1 * AEQ.     
                                                                 
    COMPUTE MP = MPT / MPB.                                      
                                                                 
    GO TO 7700-SKIP.                                             
                                                                 
7600-REGULAR-PAYMENT.                                            
    COMPUTE MPB = (ANGLN-LESS-1 - NM1 * AH) - L * NM1 * AEQ.     
                                                                 
    COMPUTE B = (MPB * MP - ATF) / (L * 2 * N / (N + 1) - V-NTH).
                                                                 
7700-SKIP.                                                       
    COMPUTE IA = MP * NM1 + B.                                   
                                                                 
    IF WS-BALYN = 'Y'                                            
        COMPUTE VA = 1 + B / IA * (N - 1) / (N + 1)              
     ELSE                                                        
        COMPUTE VA = N * (IA + B) / (IA * (N + 1)).              
                                                                 
8000-DISPLAY-BALLOON-RESULTS.                                    
    COMPUTE A-PREMIUM = NM1 * MP * AH.                           
                                                                 
    IF WS-BASIS = 2                                              
       COMPUTE L-PREMIUM = L * IA * VA                           
     ELSE                                                        
       COMPUTE L-PREMIUM = MP * ANGLN-LESS-1 + B * V-NTH - ATF - 
               A-PREMIUM.                                        
                                                                 
*>* IF BALLOON RATE KEYED, ADD PREMIUM TO LIFE PREMIUM             
    IF BL GREATER ZERO                                           
        COMPUTE L-PREMIUM = L-PREMIUM + (B * BL).                
                                                                 
    COMPUTE L-PREMIUM = (L-PREMIUM * 100 + .502) / 100.          
                                                                 
    COMPUTE A-PREMIUM = (A-PREMIUM * 100 + .502) / 100.          
                                                                 
    COMPUTE TOT-FIN = OTF + L-PREMIUM + A-PREMIUM.               
                                                                 
    COMPUTE MP = (MP * 100 + .002) / 100.                        
    COMPUTE TOT-PAYMT = MP * NM1 + B.                            
    MOVE MP TO PAYMENT.                                          
                                                                 
    COMPUTE TOT-INTRS = TOT-PAYMT - TOT-FIN.                     
                                                                 
    GO TO 6500-COMMON-DISPLAY.                                   
                                                                 
    EJECT                                                        
