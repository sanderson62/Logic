    IF NOT WS-ELCERT-FULL                                        
       IF ELCERT-FINISHED                                        
          GO TO 0200-EXIT                                        
       END-IF                                                    
    END-IF                                                       
                                                                 
    IF CM-ENTRY-STATUS = 'M' OR '9' OR 'D' OR 'V' OR 'U'         
       MOVE 'CERT NOT ACTIVE '  TO WS-COMMENT                    
       MOVE 'X'                 TO WS-LF-STATUS WS-AH-STATUS     
