    if (ws-epiq-request)                                         
       and (not EPIQ-CLASS)                                      
       go to 0200-continue                                       
    end-if                                                       
                                                                 
    if ws-epiq-request                                           
       add +1 to ws-epiq-max                                     
       if ws-epiq-max > +100                                     
