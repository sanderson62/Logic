          MOVE DC-GREG-DATE-1-EDIT                               
                                TO DISPLAY-DATE                  
          MOVE ws-TRLR-SEQ-NO   TO DISPLAY-SEQ                   
          MOVE AT-INFO-LINE-1   TO DISPLAY-TEXT                  
          IF MAP-DISPLAY (1) (1:14) = 'SPECIAL REVIEW'           
              MOVE TRAILER-DISPLAY-WORK-AREA                     
                                TO MAP-DISPLAY (2)               
          ELSE                                                   
              MOVE TRAILER-DISPLAY-WORK-AREA                     
                                TO MAP-DISPLAY (1)               
          END-IF                                                 
          if direction-switch = 'F'                              
             ADD +1             TO  DISPLAY-CNT                  
          end-if                                                 
       end-if                                                    
       PERFORM 6170-READ-TRLR94 THRU 6170-EXIT                   
       if ws-resp-normal                                         
          MOVE SPACES           TO TRAILER-DISPLAY-WORK-AREA     
          MOVE 'CAUSAL STATE  ' TO DISPLAY-ACTION                
          MOVE AT-RECORDED-BY   TO DISPLAY-BY                    
          MOVE AT-RECORDED-DT   TO DC-BIN-DATE-1                 
          MOVE ' '              TO DC-OPTION-CODE                
          PERFORM 9700-LINK-DATE-CONVERT                         
                                THRU 9700-EXIT                   
          MOVE DC-GREG-DATE-1-EDIT                               
                                TO DISPLAY-DATE                  
          MOVE WS-TRLR-SEQ-NO   TO DISPLAY-SEQ                   
          MOVE AT-INFO-LINE-1   TO DISPLAY-TEXT                  
          IF MAP-DISPLAY (1) (1:14) = 'SPECIAL REVIEW'           
           OR 'VERIFY SSN    '                                   
              IF MAP-DISPLAY (2) (1:14) = 'VERIFY SSN    '       
                  MOVE TRAILER-DISPLAY-WORK-AREA                 
                                TO MAP-DISPLAY (3)               
              ELSE                                               
                 MOVE TRAILER-DISPLAY-WORK-AREA                  
                                TO MAP-DISPLAY (2)               
              END-IF                                             
          ELSE                                                   
              MOVE TRAILER-DISPLAY-WORK-AREA                     
                                TO MAP-DISPLAY (1)               
          END-IF                                                 
          IF DIRECTION-SWITCH = 'F'                              
             ADD +1             TO  DISPLAY-CNT                  
          END-IF                                                 
       END-IF                                                    
