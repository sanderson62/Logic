                   when pi-save-type = 'I'                       
                      move '  IU  '                              
                             to emi-claim-type                   
                   when pi-save-type = 'F'                       
                      move '  FL  '                              
                             to emi-claim-type                   
                   WHEN pi-save-type = 'B'                       
                      MOVE ' BR  '         TO emi-claim-type     
                   WHEN pi-save-type = 'H'                       
                      MOVE ' HS '          TO emi-claim-type     
                                                                 
                   when pi-save-type = 'O'                       
                      move '  OT  '                              
                             to emi-claim-type                   
                   when other                                    
                      move '  AH  '                              
                             to emi-claim-type                   
                end-evaluate                                     
             end-if                                              
             PERFORM 9900-ERROR-FORMAT                           
                                THRU 9900-EXIT                   
             move emi-line1 (8:64)                               
                            to TRAILER-DISPLAY-WORK-AREA (16:64) 
             move trailer-display-work-area                      
