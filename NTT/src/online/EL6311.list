*>                                                                *
*>*****************************************************************
                                                                 
6100-TERM-CONVERSION.                                            
                                                                 
    IF PI-BPMTS = ZERO                                           
        GO TO 6199-EXIT.                                         
                                                                 
    IF PI-BMODE = ' ' OR 'M'                                     
        MOVE PI-BPMTS      TO WS-CALC-TERM-WHOLE                 
        GO TO 6105-ROUND-TERM.                                   
                                                                 
    IF PI-BMODE = 'S'                                            
        COMPUTE WS-CALC-TERM = PI-BPMTS / 2                      
        GO TO 6105-ROUND-TERM.                                   
                                                                 
    IF PI-BMODE = 'W'                                            
        COMPUTE WS-CALC-TERM = PI-BPMTS / 4.33333                
        GO TO 6105-ROUND-TERM.                                   
                                                                 
    IF PI-BMODE = 'B'                                            
        COMPUTE WS-CALC-TERM = PI-BPMTS / 2.16667                
        GO TO 6105-ROUND-TERM.                                   
                                                                 
    IF PI-BMODE = 'T'                                            
        COMPUTE WS-CALC-TERM = PI-BPMTS / 1.08334.               
                                                                 
6105-ROUND-TERM.                                                 
                                                                 
    IF WS-CALC-TERM-REMAIN GREATER THAN .00000                   
        ADD +1                  TO WS-CALC-TERM-WHOLE            
        MOVE ZEROS              TO WS-CALC-TERM-REMAIN.          
                                                                 
    IF PI-COMPANY-ID = CLIENT-CRI OR CLIENT-LGX                  
        GO TO 6125-COMPUTE-TERM-BENEFIT.                         
                                                                 
    IF BTERM-LEN (WS-SUB1) NOT GREATER THAN +0                   
        MOVE WS-CALC-TERM-WHOLE   TO BTERMO      (WS-SUB1)       
                                     WS-BTERM    (WS-SUB1)       
        MOVE +3                   TO BTERM-LEN   (WS-SUB1).      
                                                                 
    IF BTERM-LEN (WS-SUB1) GREATER THAN +0                       
        IF WS-BTERM (WS-SUB1)   NOT = WS-CALC-TERM-WHOLE         
            MOVE -1               TO BMODEL                      
            MOVE ER-2593          TO EMI-ERROR                   
            MOVE AL-UNBON         TO BTERM-ATTRB (WS-SUB1)       
            PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT.            
                                                                 
6124-CHECK-BENEFIT.                                              
                                                                 
    IF BBEN-LEN (WS-SUB1) NOT GREATER THAN +0                    
       GO TO 6126-COMPUTE-BENEFIT.                               
                                                                 
    GO TO 6199-EXIT.                                             
                                                                 
EJECT                                                            
*>*****************************************************************
*>*         COMPUTE TERM AND BENEFIT                               
*>*****************************************************************
6125-COMPUTE-TERM-BENEFIT.                                       
                                                                 
    IF PI-BTYPE (WS-SUB1) = SPACES  OR                           
       PI-BPMTAMT = ZERO                                         
        GO TO 6199-EXIT.                                         
                                                                 
    MOVE WS-CALC-TERM-WHOLE     TO  BTERMI    (WS-SUB1)          
                                    WS-BTERM  (WS-SUB1).         
    MOVE +3                     TO  BTERM-LEN (WS-SUB1).         
                                                                 
6126-COMPUTE-BENEFIT.                                            
                                                                 
    IF PI-BMODE = 'M' OR  ' '                                    
        MOVE PI-BPMTAMT        TO  WS-BBEN (WS-SUB1).            
                                                                 
    IF PI-BMODE = 'W'                                            
        COMPUTE WS-BBEN (WS-SUB1) ROUNDED = PI-BPMTAMT * 4.33333.
                                                                 
    IF PI-BMODE = 'S'                                            
        COMPUTE WS-BBEN (WS-SUB1)  ROUNDED = PI-BPMTAMT * 2.     
                                                                 
    IF PI-BMODE = 'B'                                            
        COMPUTE WS-BBEN (WS-SUB1) ROUNDED = PI-BPMTAMT * 2.16667.
                                                                 
    IF PI-BMODE = 'T'                                            
        COMPUTE WS-BBEN (WS-SUB1) ROUNDED = PI-BPMTAMT * 1.08334.
                                                                 
    IF WS-SUB1 = +1                                              
       COMPUTE WS-BBEN (WS-SUB1) =                               
               WS-BBEN (WS-SUB1) * WS-CALC-TERM.                 
                                                                 
    MOVE WS-BBEN (WS-SUB1)           TO BBENO       (WS-SUB1).   
    MOVE +12                         TO BBEN-LEN    (WS-SUB1).   
    MOVE AL-UNNON                    TO BBEN-ATTRB  (WS-SUB1).   
                                                                 
6199-EXIT.                                                       
    EXIT.                                                        
    EJECT                                                        
                                                                 
