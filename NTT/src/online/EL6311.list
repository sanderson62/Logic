3030-END-BATCH.                                                  
    MOVE WS-ISS-TOTAL-CNT       TO DITOTALO.                     
    MOVE WS-ISS-FATAL-CNT       TO DIFATALO.                     
    MOVE WS-ISS-FORCE-CNT       TO DIFORCEO.                     
    MOVE WS-ISS-HOLD-CNT        TO DIHOLDO.                      
                                                                 
    COMPUTE DIAVAILO = (WS-ISS-TOTAL-CNT - WS-ISS-FATAL-CNT      
                                         - WS-ISS-FORCE-CNT      
                                         - WS-ISS-HOLD-CNT).     
                                                                 
    MOVE WS-ISS-GOOD-PREM       TO DIGPREMO.                     
    MOVE WS-ISS-BAD-PREM        TO DIBPREMO.                     
                                                                 
    MOVE WS-CAN-TOTAL-CNT       TO DCTOTALO.                     
    MOVE WS-CAN-FATAL-CNT       TO DCFATALO.                     
    MOVE WS-CAN-FORCE-CNT       TO DCFORCEO.                     
    MOVE WS-CAN-HOLD-CNT        TO DCHOLDO.                      
    COMPUTE DCAVAILO = (WS-CAN-TOTAL-CNT - WS-CAN-FATAL-CNT      
                                         - WS-CAN-FORCE-CNT      
                                         - WS-CAN-HOLD-CNT).     
    MOVE WS-CAN-GOOD-PREM       TO DCGPREMO.                     
    MOVE WS-CAN-BAD-PREM        TO DCBPREMO.                     
                                                                 
    COMPUTE WS-NET-TOTAL-CNT                                     
        = WS-ISS-TOTAL-CNT - WS-CAN-TOTAL-CNT.                   
    MOVE WS-NET-TOTAL-CNT       TO DNTOTALO.                     
    COMPUTE WS-NET-FATAL-CNT                                     
        = WS-ISS-FATAL-CNT - WS-CAN-FATAL-CNT.                   
    MOVE WS-NET-FATAL-CNT       TO DNFATALO.                     
    COMPUTE WS-NET-FORCE-CNT                                     
        = WS-ISS-FORCE-CNT - WS-CAN-FORCE-CNT.                   
    MOVE WS-NET-FORCE-CNT       TO DNFORCEO.                     
    COMPUTE WS-NET-HOLD-CNT                                      
        = WS-ISS-HOLD-CNT - WS-CAN-HOLD-CNT.                     
    MOVE WS-NET-HOLD-CNT        TO DNHOLDO.                      
                                                                 
    COMPUTE WS-NET-AVAIL-CNT                                     
         = (WS-ISS-TOTAL-CNT - WS-ISS-FATAL-CNT                  
                             - WS-ISS-FORCE-CNT                  
                             - WS-ISS-HOLD-CNT)                  
         - (WS-CAN-TOTAL-CNT - WS-CAN-FATAL-CNT                  
                             - WS-CAN-FORCE-CNT                  
                             - WS-CAN-HOLD-CNT).                 
    MOVE WS-NET-AVAIL-CNT       TO DNAVAILO.                     
                                                                 
    COMPUTE WS-NET-GOOD-PREM                                     
        = WS-ISS-GOOD-PREM - WS-CAN-GOOD-PREM.                   
    MOVE WS-NET-GOOD-PREM       TO DNGPREMO.                     
    COMPUTE WS-NET-BAD-PREM                                      
        = WS-ISS-BAD-PREM - WS-CAN-BAD-PREM.                     
    MOVE WS-NET-BAD-PREM        TO DNBPREMO.                     
                                                                 
    MOVE 0000                   TO EMI-ERROR.                    
    PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT.                    
    MOVE -1                     TO DPFENTRL.                     
    GO TO 8200-SEND-DATAONLY.                                    
                                                                 
    EJECT                                                        
                                                                 
