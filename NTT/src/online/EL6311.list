    CALL 'kxdfhei1' USING DFHEIV0,                               
          FILE-ID-ERENDT,                                        
          DFHEIV20,                                              
          DFHEIV99,                                              
          ERENDT-KEY,                                            
          DFHEIV99,                                              
          DFHEIV99,                                              
