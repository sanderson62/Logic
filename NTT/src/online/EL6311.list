                MOVE PB-SV-CARRIER          TO PI-SV-CARRIER     
                MOVE PB-SV-GROUPING         TO PI-SV-GROUPING    
                MOVE PB-SV-STATE            TO PI-SV-STATE       
                MOVE 'Y' TO WS-PB-RECORDS-FOUND-SW               
                GO TO 7100-DISPLAY-CANCELS                       
          ELSE                                                   
             GO TO 4310-BUMP-SEQ.                                
                                                                 
    IF PB-CANCELLATION                                           
       IF (CANCEL-IN-ERROR  AND                                  
           (PB-UNFORCED-ERRORS OR PB-FATAL-ERRORS)) OR           
           ALL-CANCELS                                           
           MOVE PB-CONTROL-BY-ACCOUNT  TO PI-PREV-ALT-KEY        
           MOVE PB-CONTROL-PRIMARY     TO PI-PREV-KEY            
           MOVE PB-SV-CARRIER          TO PI-SV-CARRIER          
           MOVE PB-SV-GROUPING         TO PI-SV-GROUPING         
           MOVE PB-SV-STATE            TO PI-SV-STATE            
