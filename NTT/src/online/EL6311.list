    IF NOT DISPLAY-CHANGE-RECORDS AND                            
           PB-BATCH-CHG-SEQ-NO NOT = ZEROS                       
       GO TO 4310-BUMP-SEQ.                                      
                                                                 
    IF DISPLAY-HOLD-RECORDS AND                                  
       NOT PB-RECORD-ON-HOLD                                     
         GO TO 4310-BUMP-SEQ.                                    
                                                                 
    IF PB-ISSUE                                                  
       IF DISPLAY-CHK-REQ-RECORDS                                
