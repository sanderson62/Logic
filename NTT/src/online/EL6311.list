*>**************************************************************   
*>     THIS ROUTINE DETERMINES IF EL050 ENCOUNTERED ANY        *   
*>     ERRORS.  IF ERRORS WERE DETECTED, THE ONLINE ERROR      *   
*>     PROCESSING ROUTINE IS CALLED TO FORMAT THE ERROR        *   
*>     MESSAGES AND ACCUMULATE TOTALS FOR THE DIFFERENT        *   
*>     SEVERITY LEVELS. THE SEVERITY FLAGS WITHIN THE RECORDS  *   
*>     ARE THEN SET ACCORDINGLY.                               *   
*>         ERROR NUMBERS FOR THE STANDARD ERRORS START AT 2600 *   
*>         ERROR NUMBERS FOR ISSUE TRANSACTIONS  START AT 2625 *   
*>         ERROR NUMBERS FOR CANCEL TRANSACTIONS START AT 2725 *   
*>**************************************************************   
                                                                 
*>*****************************************************************
*>                                                                *
*>            E R R O R   A N A L Y S I S                         *
*>                                                                *
*>*****************************************************************
                                                                 
*>*****************************************************************
*>                                                                *
*>   THIS ROUTINE DETERMINES IF EL050 (EDIT PROGRAM) ENCOUNTERED  *
*>   ANY ERRORS.  IF ERRORS WERE DETECTED, THE ROUTINE READS THE  *
*>   ERROR FILE TO DETERMINE THE SEVERITY OF THE ERRORS.  ONCE    *
*>   THE SEVERITY OF ERRORS IS ESTABLISHED THE APPROPRIATE ERROR  *
*>   FLAGS ARE SET (FORCIBLE, UNFORCIBLE, WARNING, ETC.).         *
*>                                                                *
*>   IF ANY UNFORCED OR FATAL ERRORS, OR RECORD IS RETURNED       *
*>   OR PLACED ON HOLD UPDATE THE CERTIFICATE ACCORDINGLY.        *
*>                                                                *
*>*****************************************************************
                                                                 
7300-FORMAT-ERRORS.                                              
                                                                 
    MOVE ' '                    TO PI-ERRORS-SW                  
                                                                 
    IF PB-COMMON-ERRORS = LOW-VALUES                             
       IF PB-FATAL-ERRORS                                        
          MOVE ER-2695          TO EMI-ERROR                     
          PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT               
          GO TO 7399-EXIT.                                       
                                                                 
    MOVE +0                     TO WS-SUB4                       
    move +0                     to pi-6311-fatal-ctr             
                                                                 
    .                                                            
7310-ERROR-LOOP.                                                 
                                                                 
    ADD +1                      TO WS-SUB4.                      
                                                                 
    IF WS-SUB4 GREATER THAN PB-NO-OF-ERRORS                      
       GO TO 7350-SET-ERROR-FLAGS.                               
                                                                 
    IF PI-COMPANY-ID = 'UCL'                                     
        IF PB-COMMON-ERROR (WS-SUB4) = ER-2799                   
            MOVE WS-CERT-EFF-DT        TO DC-BIN-DATE-1          
            MOVE ' '                   TO DC-OPTION-CODE         
            PERFORM 9700-DATE-LINK                               
            MOVE DC-GREG-DATE-1-MDY    TO EMI-AH-OVERRIDE-L6.    
                                                                 
    MOVE PB-COMMON-ERROR (WS-SUB4) TO EMI-ERROR.                 
    PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT.                    
                                                                 
    if (emi-fatal)                                               
       and (emi-error not = 1753 and 1754 and 1757 and           
                            1758 and 1759 and 1760)              
       add +1                   to pi-6311-fatal-ctr             
    end-if                                                       
    PERFORM 7400-SET-ATTRIBUTES THRU 7499-EXIT.                  
                                                                 
    GO TO 7310-ERROR-LOOP.                                       
                                                                 
                                                                 
7350-SET-ERROR-FLAGS.                                            
                                                                 
    IF EMI-FATAL-CTR NOT = ZEROS                                 
       MOVE 'Y'                 TO WS-ERRORS-PRESENT-SW          
       MOVE 'X'                 TO PB-FATAL-FLAG                 
                                   PI-ERRORS-SW                  
       GO TO 7355-SET-ERROR-FLAGS.                               
                                                                 
    move ' ' to pi-unforced-sw                                   
    IF EMI-FORCABLE-CTR NOT = ZEROS                              
       MOVE 'Y'                 TO WS-ERRORS-PRESENT-SW          
       IF PB-ISSUE                                               
          IF PB-ISSUE-FORCE                                      
             MOVE 'F'           TO PB-FORCE-ER-CD                
             MOVE ER-2600       TO EMI-ERROR                     
             PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT            
            ELSE                                                 
             MOVE 'X'           TO PB-FORCE-ER-CD                
*>                                   PI-ERRORS-SW                  
         ELSE                                                    
          IF PB-CANCEL-FORCE OR PB-ALL-CANCEL-FORCED-NO-FEE      
             MOVE 'F'           TO PB-FORCE-ER-CD                
                                   pi-unforced-sw                
             MOVE ER-2600       TO EMI-ERROR                     
             PERFORM 9900-ERROR-FORMAT THRU 9900-EXIT            
            ELSE                                                 
             MOVE 'X'           TO PB-FORCE-ER-CD                
                                   pi-unforced-sw.               
*>                                   PI-ERRORS-SW.                 
                                                                 
    MOVE PB-CONTROL-BY-ACCOUNT (1:33) TO ERENDT-KEY (1:33)       
    IF PB-ISSUE                                                  
        MOVE 'I'                TO ENDT-RECORD-TYPE              
    ELSE                                                         
        MOVE 'C'                TO ENDT-RECORD-TYPE              
    END-IF                                                       
    MOVE +0                     TO ENDT-SEQ-NO                   
                                                                 
                                                                 
