       display ' complete date is null '                         
    end-if                                                       
    if nu-complete-date = -1  *> isnull                          
       and db-key-word-type = 'Status'                           
       and obvyni <> 'Y'                                         
       and db-key-word-value = 'V'                               
                                                                 
       EXEC SQL                                                  
          DELETE                                                 
             OBKeyWordChgs                                       
          WHERE                                                  
             UniqueID = :kd-unique-id                            
