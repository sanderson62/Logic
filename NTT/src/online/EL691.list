    EXEC SQL                                                     
       SELECT                                                    
          RequestDate,                                           
          KeyWordType,                                           
          KeyWordValue,                                          
          CompletionDate                                         
       INTO                                                      
          :db-req-date,                                          
          :db-key-word-type,                                     
          :db-key-word-value,                                    
          :db-complete-dt  :nu-Complete-date                     
       FROM                                                      
          OBKeyWordChgs                                          
       where                                                     
          :kd-unique-id = UniqueID                               
    END-EXEC                                                     
                                                                 
    if sqlcode not = 0                                           
       move sqlcode             to ws-sql-code                   
       move ws-sql-code         to ws-dis-sql-code               
       display ' Error: Cannot read row ' kd-unique-id           
       display ' sql return code        ' ws-dis-sql-code        
       display ' sql err mess           ' sqlerrmc               
       go to 1610-exit                                           
    end-if                                                       
