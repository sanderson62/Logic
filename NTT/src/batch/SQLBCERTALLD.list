       S1 > +26                                                  
       IF (CR-NAME (S1:1) < 'A'                                  
          OR > 'Z')                                              
          AND (CR-NAME (S1:1) NOT = ' ' AND '-' AND ',' AND '.') 
          DISPLAY ' FIXING CR NAME ' CR-CERT-NO                  
             ' ' CR-NAME (S1:1)                                  
          MOVE SPACES             TO CR-NAME (S1:1)              
       END-IF                                                    
