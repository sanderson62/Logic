    MOVE 'E593'             TO AT-AUTO-PAY-LAST-UPDATED-BY       
    MOVE 180000             TO AT-LAST-MAINT-HHMMSS              
                                                                 
    REWRITE ACTIVITY-TRAILERS                                    
    IF WS-ELTRLR-FILE-STATUS NOT = '00'                          
       DISPLAY ' ELTRLR  WRITE  ERROR - STATUS = '               
                  WS-ELTRLR-FILE-STATUS                          
       SET AUTOPAY-MSG2 TO TRUE                                  
       GO TO 2700-EXIT                                           
    END-IF                                                       
                                                                 
    MOVE ZEROS             TO CL-AUTO-PAY-SEQ.                   
    MOVE LOW-VALUES        TO CL-NEXT-AUTO-PAY-DT.               
                                                                 
    REWRITE CLAIM-MASTER                                         
    IF WS-ELMSTR-FILE-STATUS NOT = '00'                          
        DISPLAY ' ELMSTR REWRITE ERROR - STATUS = '              
                WS-ELMSTR-FILE-STATUS                            
        GO TO 2700-EXIT                                          
    END-IF.                                                      
    .                                                            
2700-EXIT.                                                       
    EXIT.                                                        
                                                                 
                                                                 
