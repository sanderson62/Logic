    READ ELTRLR-INFILE NEXT RECORD.                              
                                                                 
    IF WS-ELTRLR-FILE-STATUS NOT = ZERO                          
        DISPLAY 'BAD READ ON TRAILER REC FOR CLAIM '             
                              CL-CLAIM-NO                        
        GO TO 2600-EXIT                                          
    END-IF.                                                      
                                                                 
    IF CL-CONTROL-PRIMARY NOT EQUAL AT-CONTROL-PRIMARY (1:20)    
         GO TO 2600-EXIT                                         
    END-IF.                                                      
                                                                 
    IF NOT GENERAL-INFO-TR                                       
        GO TO 2600-READ-LOOP                                     
    END-IF.                                                      
                                                                 
    IF AT-INFO-TRAILER-TYPE = 'T'                                
