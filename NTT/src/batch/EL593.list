    EVALUATE TRUE                                                
    WHEN P-CTL = '1'                                             
        MOVE +1                    TO WS-LINE-COUNT              
                                                                 
    WHEN P-CTL = SPACE                                           
        ADD +1                     TO WS-LINE-COUNT              
                                                                 
    WHEN P-CTL = '0'                                             
        ADD +2                     TO WS-LINE-COUNT              
                                                                 
    WHEN OTHER                                                   
        ADD +3                     TO WS-LINE-COUNT              
    END-EVALUATE                                                 
                                                                 
                                                                 
