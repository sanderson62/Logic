    MOVE '-'                    TO P-PMOD  P-PDAD.               
    MOVE DE-CLM-CAUSE           TO P-CAUSE.                      
    MOVE DE-CHECK               TO P-CHK-NO.                     
                                                                 
    MOVE ' '                    TO X.                            
    PERFORM 0300-PRT-RTN THRU 0310-EXIT.                         
                                                                 
    ADD 1 TO LN-CT.                                              
                                                                 
    IF (DE-VOIDED-PAYMENT)                                       
       or (de-claim-amt < +0)                                    
        SUBTRACT +1 FROM CNT                                     
        SUBTRACT +1 FROM A-T-CNT                                 
        SUBTRACT +1 FROM S-T-CNT                                 
    ELSE                                                         
       IF DE-CLAIM-AMT NOT EQUAL ZEROS                           
           ADD 1 TO CNT                                          
                    A-T-CNT                                      
                    S-T-CNT.                                     
                                                                 
0180-CONT.                                                       
    IF LN-CT GREATER 52                                          
        PERFORM 0210-PT-HDNG.                                    
                                                                 
    IF CAR-SW = SPACE                                            
        PERFORM 0230-WRITE-CAR THRU 0240-EXIT.                   
                                                                 
    IF CAR-SW = '9'                                              
        GO TO 0260-GET-CAR.                                      
                                                                 
    GO TO 0120-GET-CLAIM.                                        
                                                                 
0190-PRT-TOTALS.                                                 
    IF CAR-SW NOT = '9'                                          
        PERFORM 0350-ACCT-TOT  THRU 0350-EXIT                    
        PERFORM 0360-STATE-TOT THRU 0360-EXIT.                   
                                                                 
    MOVE 'TOTAL FOR CARRIER/GROUP' TO TLC.                       
    MOVE CNT                  TO TL-CNT.                         
    MOVE T-DTH                TO TL-DTH.                         
    MOVE T-DIS                TO TL-DIS.                         
    ADD T-DTH                 TO G-TOTDTH.                       
    ADD T-DIS                 TO G-TOTDIS.                       
    ADD CNT                   TO G-TOTCNT.                       
    MOVE TL-LN                TO P-DATA.                         
                                                                 
    MOVE '0' TO X.                                               
    PERFORM 0300-PRT-RTN THRU 0310-EXIT.                         
                                                                 
    MOVE SPACES               TO SAV-COMP.                       
                                                                 
0200-SET-NEW.                                                    
    IF CAR-SW = '9'                                              
        MOVE WS-DE-COMP       TO HD1B-CARR TL-CO SAV-COMP        
        MOVE HD1B             TO HD1A                            
     ELSE                                                        
        MOVE DE-CARRIER       TO  HD1A-CARR                      
        MOVE DE-GROUPING      TO  HD1A-GROUP                     
        MOVE WS-DE-COMP       TO  TL-CO SAV-COMP.                
                                                                 
    MOVE DE-ACCOUNT           TO PREV-ACT.                       
    MOVE DE-STATE             TO PREV-STATE.                     
    MOVE ZEROS                TO CNT                             
                                 T-DTH                           
                                 T-DIS.                          
    EJECT                                                        
0210-PT-HDNG.                                                    
    ADD 1     TO PG-NO.                                          
    MOVE PG-NO                TO HD3-PG                          
    MOVE HD1A                 TO P-DATA.                         
    MOVE '1'                  TO X.                              
    PERFORM 0300-PRT-RTN THRU 0310-EXIT.                         
                                                                 
    MOVE HD2                  TO P-DATA.                         
    MOVE ' '                  TO X.                              
    PERFORM 0300-PRT-RTN THRU 0310-EXIT.                         
                                                                 
    MOVE HD3                  TO P-DATA.                         
    MOVE ' '                  TO X.                              
    PERFORM 0300-PRT-RTN THRU 0310-EXIT.                         
                                                                 
    MOVE HD4                  TO P-DATA.                         
    MOVE '0'                  TO X.                              
    PERFORM 0300-PRT-RTN THRU 0310-EXIT.                         
                                                                 
    MOVE HD5                  TO P-DATA.                         
    MOVE ' '                  TO X.                              
    PERFORM 0300-PRT-RTN THRU 0310-EXIT.                         
                                                                 
    MOVE SPACES               TO P-DATA.                         
    MOVE ' '                  TO X.                              
    PERFORM 0300-PRT-RTN THRU 0310-EXIT.                         
                                                                 
    MOVE SPACES               TO P-DATA.                         
    MOVE ZERO                 TO LN-CT.                          
                                                                 
0220-P-H-X.                                                      
    GO TO 0140-MAIN-LOOP.                                        
                                                                 
    EJECT                                                        
0230-WRITE-CAR.                                                  
                                                                 
    MOVE DETAIL-EXTRACT       TO SRT-REC.                        
    MOVE DE-GROUPING          TO S-GROUP.                        
    IF DTE-FMT-OPT = 1                                           
        MOVE SPACES           TO S-CO.                           
                                                                 
    RELEASE SRT-REC.                                             
                                                                 
0240-EXIT.                                                       
    EXIT.                                                        
                                                                 
0250-PRNT-CARR.                                                  
    MOVE ZEROS                TO LN-CT                           
                                 T-DIS  T-DTH                    
                                 G-TOTDTH  G-TOTDIS  G-TOTCNT.   
    MOVE 'TOTAL FOR CARRIER'  TO TLC.                            
    MOVE HD1B                 TO HD1A.                           
    MOVE HD6                  TO HD4-1.                          
    MOVE HD5G                 TO HD5.                            
    MOVE SPACES               TO SAV-COMP WS-DE-COMP.            
                                                                 
0260-GET-CAR.                                                    
    READ CLM-CARR INTO DETAIL-EXTRACT                            
        AT END  GO TO 0270-EXIT.                                 
                                                                 
    IF NOT DE-CLAIM                                              
        GO TO 0260-GET-CAR.                                      
                                                                 
    MOVE DE-CARRIER           TO WS-DE-COMP.                     
    GO TO 0140-MAIN-LOOP.                                        
                                                                 
0270-EXIT.                                                       
    EXIT.                                                        
                                                                 
0280-NO-INPUT.                                                   
    MOVE ' NO CLAIMS INPUT RECEIVED FOR ECS031' TO P-DATA.       
    MOVE ' '                    TO X.                            
    PERFORM 0300-PRT-RTN THRU 0310-EXIT.                         
                                                                 
0290-EXIT.                                                       
    EXIT.                                                        
                                                                 
    EJECT                                                        
0300-PRT-RTN.                                                    
*>                            COPY ELCPRT2.                        
*>vvv-> file: /apps/prod/cid1p/copy/ELCPRT2
*>*****************************************************************
*>                                                                *
*>                                                                *
*>                            ELCPRT2                             *
*>           COPYBOOK REVIEWED FOR YEAR 2000 COMPLIANCE            
*>                            VMOD=2.002                          *
*>                                                                *
*>*****************************************************************
                                                                 
    IF DTE-FICH NOT = SPACE AND                                  
       FICH-OPEN    = SPACE                                      
        MOVE 'X'                TO  FICH-OPEN                    
        OPEN OUTPUT FICH.                                        
                                                                 
    IF DTE-FICH NOT = SPACE                                      
        MOVE X                  TO  P-CTL                        
        WRITE FICH-REC FROM PRT.                                 
                                                                 
    IF DTE-FICH = SPACE OR '2'                                   
      MOVE X                    TO  P-CTL                        
      IF P-CTL = ' '                                             
        WRITE PRT AFTER ADVANCING 1 LINE                         
      ELSE                                                       
        IF P-CTL = '0'                                           
          WRITE PRT AFTER ADVANCING 2 LINES                      
        ELSE                                                     
          IF P-CTL = '-'                                         
            WRITE PRT AFTER ADVANCING 3 LINES                    
          ELSE                                                   
            WRITE PRT AFTER ADVANCING PAGE.                      
                                                                 
*>*****************************************************************
*>^^^-> file: /apps/prod/cid1p/copy/ELCPRT2
0310-EXIT.                                                       
     EXIT.                                                       
                                                                 
    EJECT                                                        
                                                                 
0330-G-TOTALS.                                                   
    MOVE SPACES                 TO HD1A-COMP                     
                                   HD1B-CARR                     
                                   HD1A-CARR                     
                                   HD1A-GROUP.                   
    PERFORM 0210-PT-HDNG.                                        
                                                                 
    MOVE G-TOTCNT               TO GT-CNT.                       
    MOVE G-TOTDTH               TO GT-DTH.                       
    MOVE G-TOTDIS               TO GT-DIS.                       
    MOVE G-T-LN                 TO P-DATA.                       
                                                                 
    MOVE '0' TO X.                                               
    PERFORM 0300-PRT-RTN THRU 0310-EXIT.                         
                                                                 
0340-EXIT.                                                       
    EXIT.                                                        
                                                                 
0350-ACCT-TOT.                                                   
    MOVE 'TOTAL FOR ACCOUNT'    TO TLC.                          
    MOVE SPACE                  TO TL-CO                         
                                   P-DATA.                       
    MOVE A-T-CNT                TO TL-CNT.                       
    MOVE A-T-DTH                TO TL-DTH.                       
    MOVE A-T-DIS                TO TL-DIS.                       
    MOVE TL-LN                  TO P-DATA.                       
                                                                 
    MOVE ' '                    TO X.                            
    PERFORM 0300-PRT-RTN THRU 0310-EXIT.                         
                                                                 
    MOVE SPACE                  TO P-DATA.                       
    MOVE ' '                    TO X.                            
    PERFORM 0300-PRT-RTN THRU 0310-EXIT.                         
                                                                 
    ADD 3 TO LN-CT.                                              
    MOVE ZEROS                  TO A-T-CNT  A-T-DTH  A-T-DIS.    
                                                                 
0350-EXIT.                                                       
    EXIT.                                                        
                                                                 
0360-STATE-TOT.                                                  
    MOVE 'TOTAL FOR STATE'      TO TLC.                          
    MOVE SPACE                  TO TL-CO                         
                                   P-DATA.                       
    MOVE S-T-CNT                TO TL-CNT.                       
    MOVE S-T-DTH                TO TL-DTH.                       
    MOVE S-T-DIS                TO TL-DIS.                       
    MOVE TL-LN                  TO P-DATA.                       
                                                                 
    MOVE ' '                    TO X.                            
    PERFORM 0300-PRT-RTN THRU 0310-EXIT.                         
                                                                 
    MOVE SPACE                  TO P-DATA.                       
    MOVE ' '                    TO X.                            
    PERFORM 0300-PRT-RTN THRU 0310-EXIT.                         
                                                                 
    ADD 3                       TO LN-CT.                        
    MOVE ZERO                   TO S-T-CNT  S-T-DTH  S-T-DIS.    
    MOVE DE-STATE               TO PREV-STATE.                   
                                                                 
0360-EXIT.                                                       
    EXIT.                                                        
                                                                 
0370-END-STEP.                                                   
    IF CLM-CTR = ZERO                                            
        PERFORM 0280-NO-INPUT                                    
        MOVE ALL '9'            TO DETAIL-EXTRACT                
        PERFORM 0230-WRITE-CAR THRU 0240-EXIT                    
        GO TO 0380-STDE-ONE-X.                                   
                                                                 
    PERFORM 0190-PRT-TOTALS.                                     
    PERFORM 0330-G-TOTALS.                                       
                                                                 
    MOVE ZERO                   TO PG-NO.                        
                                                                 
0380-STDE-ONE-X.                                                 
    EXIT.                                                        
                                                                 
    EJECT                                                        
0390-STDE-TWO  SECTION.                                          
    IF CLM-CTR = ZERO                                            
        GO TO 0800-CLOSE-ALL.                                    
                                                                 
    OPEN INPUT CLM-CARR.                                         
    MOVE '9'                    TO CAR-SW.                       
    PERFORM 0250-PRNT-CARR THRU 0270-EXIT.                       
    PERFORM 0190-PRT-TOTALS.                                     
    PERFORM 0330-G-TOTALS.                                       
    GO TO 0800-CLOSE-ALL.                                        
    EJECT                                                        
0400-PRODUCE-REPORT SECTION.                                     
    IF CLM-CTR = ZERO                                            
        GO TO 0800-CLOSE-ALL.                                    
    OPEN INPUT CLM-CARR.                                         
    PERFORM 0710-RD-DETAIL.                                      
    PERFORM 0405-SET-HEADING.                                    
    GO TO 0420-CHECK-BREAKS.                                     
                                                                 
0405-SET-HEADING.                                                
    MOVE 'B'                     TO HD1A-RPT.                    
    MOVE ' : '                   TO HD1A-FILL.                   
    MOVE ' GROUP -'              TO HD1A-COMP.                   
    MOVE CLAS-REPORT-CD1-CAPTION TO HD1A-CAPT.                   
    MOVE DE-CARRIER              TO HD1A-CARR.                   
    MOVE DE-GROUPING             TO HD1A-GROUP.                  
    MOVE COMPANY-NAME            TO HD2-CO.                      
    MOVE DE-REPORT-CODE-1        TO SAV-RPT-CD-1                 
                                    HD1A-RPT-CD.                 
    PERFORM 0210-PT-HDNG.                                        
                                                                 
0410-GET-DETAIL.                                                 
    PERFORM 0710-RD-DETAIL THRU 0710-EXIT.                       
    IF DET-END = 'END'                                           
        PERFORM 0500-TOTALS THRU 0560-EXIT                       
        PERFORM 0570-FINAL-TOTAL THRU 0599-EXIT                  
        GO TO 0800-CLOSE-ALL.                                    
                                                                 
0420-CHECK-BREAKS.                                               
    IF DE-REPORT-CODE-1 =  SAV-RPT-CD-1                          
        NEXT SENTENCE                                            
    ELSE                                                         
        PERFORM 0500-TOTALS THRU 0560-EXIT.                      
                                                                 
    IF SAV-CARRIER = SPACES                                      
       MOVE DE-CARRIER          TO SAV-CARRIER                   
       MOVE DE-GROUPING         TO SAV-GROUP                     
       MOVE DE-STATE            TO SAV-STATE                     
       MOVE DE-REPORT-CODE-2    TO SAV-RPT-CD-2                  
       MOVE DE-ACCOUNT          TO SAV-ACCOUNT.                  
                                                                 
    IF DE-REPORT-CODE-1 NOT = SAV-RPT-CD-1                       
        PERFORM 0510-ACCT-BREAK THRU 0560-EXIT.                  
                                                                 
    IF DE-CARRIER NOT = SAV-CARRIER                              
        PERFORM 0510-ACCT-BREAK THRU 0550-EXIT.                  
                                                                 
    IF DE-GROUPING NOT = SAV-GROUP                               
        PERFORM 0510-ACCT-BREAK THRU 0540-EXIT.                  
                                                                 
    IF DE-REPORT-CODE-2 NOT = SAV-RPT-CD-2                       
        PERFORM 0510-ACCT-BREAK THRU 0530-EXIT.                  
                                                                 
    IF DE-STATE NOT = SAV-STATE                                  
        PERFORM 0510-ACCT-BREAK THRU 0520-EXIT.                  
                                                                 
    IF DE-ACCOUNT NOT = SAV-ACCOUNT                              
        PERFORM 0510-ACCT-BREAK THRU 0510-EXIT.                  
                                                                 
0430-PRT-DETAIL.                                                 
    PERFORM 0150-CONT-RPT THRU 0180-PT-LST                       
    IF LN-CT GREATER 52                                          
        PERFORM 0210-PT-HDNG.                                    
                                                                 
    GO TO 0410-GET-DETAIL.                                       
                                                                 
0499-EXIT.                                                       
    EXIT.                                                        
                                                                 
0500-TOTALS SECTION.                                             
                                                                 
0510-ACCT-BREAK.                                                 
    PERFORM 0350-ACCT-TOT THRU 0350-EXIT.                        
    MOVE DE-ACCOUNT             TO SAV-ACCOUNT.                  
0510-EXIT.                                                       
    EXIT.                                                        
                                                                 
0520-STATE-BREAK.                                                
    COMPUTE RPT-CD2-CNT = RPT-CD2-CNT + S-T-CNT.                 
    COMPUTE RPT-CD2-DTH = RPT-CD2-DTH + S-T-DTH.                 
    COMPUTE RPT-CD2-DIS = RPT-CD2-DIS + S-T-DIS.                 
    PERFORM 0360-STATE-TOT THRU 0360-EXIT.                       
    MOVE DE-STATE               TO SAV-STATE.                    
0520-EXIT.                                                       
    EXIT.                                                        
                                                                 
0530-RPT-CD2-BREAK.                                              
    COMPUTE GRP-CNT = GRP-CNT + RPT-CD2-CNT.                     
    COMPUTE GRP-DTH = GRP-DTH + RPT-CD2-DTH.                     
    COMPUTE GRP-DIS = GRP-DIS + RPT-CD2-DIS.                     
    MOVE 'TOTAL FOR  '          TO TL-CON.                       
    MOVE CLAS-REPORT-CD2-CAPTION TO TL-CAP.                      
    MOVE SPACE                  TO TL-FILL.                      
    MOVE TL-CONSTANT            TO TLC.                          
    MOVE SAV-RPT-CD-2           TO TL-CO.                        
    MOVE SPACE                  TO P-DATA.                       
    MOVE RPT-CD2-CNT            TO TL-CNT.                       
    MOVE RPT-CD2-DTH            TO TL-DTH.                       
    MOVE RPT-CD2-DIS            TO TL-DIS.                       
    MOVE TL-LN                  TO P-DATA.                       
    MOVE ' '                    TO X.                            
    PERFORM 0300-PRT-RTN THRU 0310-EXIT.                         
    MOVE SPACE                  TO P-DATA.                       
    MOVE ' '                    TO X.                            
    PERFORM 0300-PRT-RTN THRU 0310-EXIT.                         
    ADD 3 TO LN-CT.                                              
    MOVE ZERO                   TO RPT-CD2-CNT RPT-CD2-DTH       
                                   RPT-CD2-DIS.                  
    MOVE DE-REPORT-CODE-2 TO SAV-RPT-CD-2.                       
0530-EXIT.                                                       
    EXIT.                                                        
                                                                 
0540-GROUP-BREAK.                                                
    COMPUTE CAR-CNT = CAR-CNT + GRP-CNT.                         
    COMPUTE CAR-DTH = CAR-DTH + GRP-DTH.                         
    COMPUTE CAR-DIS = CAR-DIS + GRP-DIS.                         
    MOVE 'TOTAL FOR GROUP'      TO TLC.                          
    MOVE SPACE                  TO TL-CO                         
                                   P-DATA.                       
    MOVE GRP-CNT                TO TL-CNT.                       
    MOVE GRP-DTH                TO TL-DTH.                       
    MOVE GRP-DIS                TO TL-DIS.                       
    MOVE TL-LN                  TO P-DATA.                       
    MOVE ' '                    TO X.                            
    PERFORM 0300-PRT-RTN THRU 0310-EXIT.                         
    MOVE SPACE                  TO P-DATA.                       
    MOVE ' '                    TO X.                            
    PERFORM 0300-PRT-RTN THRU 0310-EXIT.                         
    ADD 3 TO LN-CT.                                              
    MOVE ZERO                   TO GRP-CNT GRP-DTH               
                                   GRP-DIS.                      
    MOVE DE-GROUPING            TO SAV-GROUP                     
                                   HD1A-GROUP.                   
0540-EXIT.                                                       
    EXIT.                                                        
                                                                 
0550-CARRIER-BREAK.                                              
    COMPUTE RPT-CD1-CNT = RPT-CD1-CNT + CAR-CNT.                 
    COMPUTE RPT-CD1-DTH = RPT-CD1-DTH + CAR-DTH.                 
    COMPUTE RPT-CD1-DIS = RPT-CD1-DIS + CAR-DIS.                 
    MOVE 'TOTAL FOR CARRIER'     TO TLC.                         
    MOVE SPACE                   TO P-DATA                       
                                    TL-CO.                       
    MOVE CAR-CNT                 TO TL-CNT.                      
    MOVE CAR-DTH                 TO TL-DTH.                      
    MOVE CAR-DIS                 TO TL-DIS.                      
    MOVE TL-LN                   TO P-DATA.                      
    MOVE ' '                     TO X.                           
    PERFORM 0300-PRT-RTN THRU 0310-EXIT.                         
    MOVE SPACE                   TO P-DATA.                      
    MOVE ' '                     TO X.                           
    PERFORM 0300-PRT-RTN THRU 0310-EXIT.                         
    ADD 3 TO LN-CT.                                              
    MOVE ZERO                    TO CAR-CNT CAR-DTH              
                                   CAR-DIS.                      
    IF DE-REPORT-CODE-1 = SAV-RPT-CD-1                           
        MOVE DE-CARRIER          TO HD1A-CARR                    
        PERFORM 0210-PT-HDNG                                     
    ELSE                                                         
        MOVE SPACE               TO HD1A-CARR                    
                                    HD1A-COMP                    
                                    HD1A-GROUP                   
        PERFORM 0210-PT-HDNG.                                    
    MOVE DE-CARRIER              TO HD1A-CARR                    
                                    SAV-CARRIER                  
    MOVE ' GROUP -'              TO HD1A-COMP                    
    MOVE SAV-GROUP               TO HD1A-GROUP.                  
0550-EXIT.                                                       
    EXIT.                                                        
                                                                 
0560-REPORT-CODE1-BREAK.                                         
    COMPUTE FIN-CNT = FIN-CNT + RPT-CD1-CNT.                     
    COMPUTE FIN-DTH = FIN-DTH + RPT-CD1-DTH.                     
    COMPUTE FIN-DIS = FIN-DIS + RPT-CD1-DIS.                     
    MOVE 'TOTALS FOR  '          TO TL-CON.                      
    MOVE CLAS-REPORT-CD1-CAPTION TO TL-CAP.                      
    MOVE SPACE                   TO TL-CO                        
                                    TL-FILL.                     
    MOVE TL-CONSTANT             TO TLC.                         
    MOVE SPACE                   TO P-DATA.                      
    MOVE RPT-CD1-CNT             TO TL-CNT.                      
    MOVE RPT-CD1-DTH             TO TL-DTH.                      
    MOVE RPT-CD1-DIS             TO TL-DIS.                      
    MOVE TL-LN                   TO P-DATA.                      
    MOVE ' '                     TO X.                           
    PERFORM 0300-PRT-RTN THRU 0310-EXIT.                         
    MOVE SPACE                   TO P-DATA.                      
    MOVE ' '                     TO X.                           
    PERFORM 0300-PRT-RTN THRU 0310-EXIT.                         
    ADD 3 TO LN-CT.                                              
    MOVE ZERO                    TO RPT-CD1-CNT RPT-CD1-DTH      
                                    RPT-CD1-DIS.                 
    MOVE DE-REPORT-CODE-1        TO SAV-RPT-CD-1                 
                                    HD1A-RPT-CD.                 
    MOVE CLAS-REPORT-CD1-CAPTION TO HD1A-CAPT.                   
    IF DET-END = 'END'                                           
        NEXT SENTENCE                                            
    ELSE                                                         
        PERFORM 0210-PT-HDNG.                                    
0560-EXIT.                                                       
    EXIT.                                                        
                                                                 
0570-FINAL-TOTAL.                                                
    MOVE SPACE                   TO HD1B-CARR                    
    MOVE HD1B                    TO HD1A.                        
    PERFORM 0210-PT-HDNG.                                        
    MOVE 'OVERALL TOTALS   '     TO TLC.                         
    MOVE SPACE                   TO TL-CO                        
                                    P-DATA.                      
    MOVE FIN-CNT                 TO TL-CNT.                      
    MOVE FIN-DTH                 TO TL-DTH.                      
    MOVE FIN-DIS                 TO TL-DIS.                      
    MOVE TL-LN                   TO P-DATA.                      
    MOVE ' '                     TO X.                           
    PERFORM 0300-PRT-RTN THRU 0310-EXIT.                         
    MOVE SPACE                   TO P-DATA.                      
    MOVE ' '                     TO X.                           
    PERFORM 0300-PRT-RTN THRU 0310-EXIT.                         
    ADD 3 TO LN-CT.                                              
0570-EXIT.                                                       
    EXIT.                                                        
                                                                 
0599-EXIT.                                                       
    EXIT.                                                        
                                                                 
0700-READ-SECTION.                                               
                                                                 
0710-RD-DETAIL.                                                  
                                                                 
    IF DET-END = 'END'                                           
        GO TO 0710-EXIT.                                         
                                                                 
    READ CLM-CARR INTO DETAIL-EXTRACT                            
        AT END  MOVE 'END' TO DET-END.                           
                                                                 
0710-EXIT.                                                       
    EXIT.                                                        
                                                                 
0720-RD-ACCT-MSTR.                                               
    IF ERACCT-FILE-STATUS = '10'                                 
       GO TO 0720-EXIT.                                          
                                                                 
    IF DE-CNTRL1 = AM-CONTROL-A                                  
         MOVE AM-REPORT-CODE-1 TO SAV-RPT-CD-1                   
         MOVE AM-REPORT-CODE-2 TO SAV-RPT-CD-2                   
         MOVE DE-CNTRL1        TO SAV-DE-CNTRL.                  
                                                                 
    READ ACC-MSTR.                                               
                                                                 
    IF ERACCT-FILE-STATUS = '10'                                 
       GO TO 0720-EXIT.                                          
                                                                 
    IF ERACCT-FILE-STATUS NOT EQUAL ZERO                         
        MOVE 'ERROR OCCURED READ - ERACCTT' TO WS-ABEND-MESSAGE  
        MOVE ERACCT-FILE-STATUS TO WS-ABEND-FILE-STATUS          
        PERFORM ABEND-PGM.                                       
                                                                 
     IF DE-CNTRL1 < AM-CONTROL-A                                 
         GO TO 0720-EXIT.                                        
                                                                 
     GO TO 0720-RD-ACCT-MSTR.                                    
                                                                 
0720-EXIT.                                                       
    EXIT.                                                        
                                                                 
0800-CLOSE-ALL.                                                  
