    MOVE LIFE-OVERRIDE-L6 TO WS-TA-LIFE-OVERIDE-L6.              
    MOVE AH-OVERRIDE-L6   TO WS-TA-AH-OVERIDE-L6.                
    MOVE '  IU  '         TO WS-TA-IU-OVERIDE-L6                 
    MOVE ' GAP  '         TO WS-TA-GP-OVERIDE-L6                 
    MOVE ' FAM  '         TO WS-TA-FL-OVERIDE-L6                 
    MOVE ' BRV  '         TO WS-TA-BR-OVERIDE-L6                 
    MOVE ' HOS  '         TO WS-TA-HS-OVERIDE-L6                 
    MOVE ' OTHER'         TO WS-TA-OT-OVERIDE-L6                 
                                                                 
    IF DTE-PRC-OPT = '2'                                         
        MOVE 'DAILY CHECK REGISTER' TO WS-H1-TITLE.              
                                                                 
1000-MAIN-LOGIC SECTION.                                         
                                                                 
    PERFORM OPEN-FILES.                                          
                                                                 
    PERFORM 3000-PRINT-REPORT.                                   
                                                                 
    PERFORM CLOSE-FILES.                                         
                                                                 
    GOBACK.                                                      
                                                                 
    EJECT                                                        
3000-PRINT-REPORT SECTION.                                       
                                                                 
3100-PRINT-REPORT.                                               
    READ REPORTS-EXTRACT-FILE                                    
        AT END                                                   
            MOVE HIGH-VALUES    TO  EX-SB-CARRIER                
            GO TO 3110-PRINT-REPORT.                             
                                                                 
    IF EX-POSITIONING-CODE < '9'                                 
        GO TO 3100-PRINT-REPORT.                                 
                                                                 
    IF EX-EXTRACT-CODE < 'B'                                     
        GO TO 3100-PRINT-REPORT.                                 
                                                                 
    IF EX-EXTRACT-CODE > 'B'                                     
        MOVE HIGH-VALUES  TO  EX-SB-CARRIER                      
        GO TO  3110-PRINT-REPORT.                                
                                                                 
    IF EX-COMPANY-CD < DTE-CLASIC-COMPANY-CD                     
        GO TO 3100-PRINT-REPORT.                                 
                                                                 
    IF EX-COMPANY-CD > DTE-CLASIC-COMPANY-CD                     
        MOVE HIGH-VALUES  TO  EX-SB-CARRIER                      
        GO TO  3110-PRINT-REPORT.                                
                                                                 
    IF EX-RECORD-TYPE > 'A'                                      
        MOVE HIGH-VALUES  TO  EX-SB-CARRIER                      
        GO TO 3110-PRINT-REPORT.                                 
                                                                 
    IF EX-BA-LIFE-INTEREST                                       
       GO TO 3100-PRINT-REPORT                                   
    END-IF                                                       
                                                                 
    IF DTE-PRC-OPT NOT = '2'                                     
        GO TO 3110-PRINT-REPORT.                                 
                                                                 
    IF EX-BA-PAYMENT-ORIGIN NOT = '3'                            
      IF EX-BA-CHECK-WRITTEN-DT > EX-BA-LAST-PROCESS-DT          
        GO TO 3110-PRINT-REPORT.                                 
                                                                 
    IF EX-BA-PAYMENT-ORIGIN = '3'                                
      IF EX-BA-PMT-RECORDED-DT > EX-BA-LAST-PROCESS-DT           
        GO TO 3110-PRINT-REPORT.                                 
                                                                 
    IF EX-BA-VOID-DT > EX-BA-LAST-PROCESS-DT                     
        GO TO 3110-PRINT-REPORT.                                 
                                                                 
    GO TO 3100-PRINT-REPORT.                                     
                                                                 
    EJECT                                                        
3110-PRINT-REPORT.                                               
    IF WS-FIRST-TIME                                             
        MOVE 'N'               TO  WS-FIRST-TIME-SWITCH          
        MOVE EX-SB-CARRIER     TO  WS-LAST-CARRIER               
        PERFORM 8100-GET-CARRIER-NAME.                           
                                                                 
    IF EX-SB-CARRIER = HIGH-VALUES                               
       IF WS-RECORD-COUNT NOT > ZERO                             
         MOVE WS-NO-ACTIVITY         TO  PRT                     
         PERFORM WRITE-A-LINE                                    
         GO TO 3600-EXIT.                                        
                                                                 
    ADD +1  TO  WS-RECORD-COUNT.                                 
                                                                 
    EJECT                                                        
*>    NOTE ******************************************************* 
*>         *          CONTROL BREAK LOGIC FOR CARRIER            * 
*>         *******************************************************.
                                                                 
    IF EX-SB-CARRIER = WS-LAST-CARRIER                           
       GO TO 3200-PRINT-REPORT.                                  
                                                                 
    PERFORM 4000-PRINT-TOTALS.                                   
                                                                 
    EJECT                                                        
*>    NOTE ******************************************************* 
*>         *          CONTROL BREAK LOGIC FOR FINAL              * 
*>         *******************************************************.
                                                                 
    IF EX-SB-CARRIER NOT = HIGH-VALUES                           
        MOVE EX-SB-CARRIER   TO  WS-LAST-CARRIER                 
        PERFORM 8100-GET-CARRIER-NAME                            
        GO TO 3200-PRINT-REPORT.                                 
                                                                 
