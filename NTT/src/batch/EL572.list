       IF WS-CRT-CNT = +10000                                    
          DISPLAY ' CERTS READ ' WS-CRT-CNT-TOT                  
          MOVE +0               TO WS-CRT-CNT                    
       END-IF                                                    
       IF CR-CERT-NO = '0000109489 '                             
