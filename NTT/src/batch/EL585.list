            ADD +1                       TO WS-DLY-LIFE-VOID-CNT 
            ADD WS-DRFTLST-STOP-PAY-AMT  TO WS-DLY-LIFE-NETPD-AMT
        END-IF                                                   
                                                                 
        ADD AT-AMOUNT-PAID               TO WS-TOT-LIFE-VOID-AMT 
        ADD +1                           TO WS-TOT-LIFE-VOID-CNT 
