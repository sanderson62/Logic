        ADD +1                           TO WS-TOT-IU-VOID-CNT   
        ADD WS-DRFTLST-STOP-PAY-AMT      TO WS-TOT-IU-NETPD-AMT  
                                                                 
    WHEN AT-CLAIM-TYPE = 'F'                                     
                                                                 
        IF MONTH-TO-DATE                                         
            CONTINUE                                             
        ELSE                                                     
            MOVE WS-DRFTLST-STOP-PAY-AMT  TO WS-DRFTLST-DRFTAMT  
            MOVE 'STOP-PAY'               TO                     
                                          WS-DRFTLST-ENTRY-TYPE  
            PERFORM 2400-DAILY-DRAFT-LIST THRU 2400-EXIT         
                                                                 
*>************* ACCUMULATE CURRENT CYCLE DAY TOTALS HERE AND GO ON 
*>              TO ADD THESE FIGURES TO MTD TOTALS                 
            ADD AT-AMOUNT-PAID          TO WS-DLY-FAM-VOID-AMT   
            ADD +1                      TO WS-DLY-FAM-VOID-CNT   
            ADD WS-DRFTLST-STOP-PAY-AMT TO WS-DLY-FAM-NETPD-AMT  
        END-IF                                                   
                                                                 
        ADD AT-AMOUNT-PAID               TO WS-TOT-FAM-VOID-AMT  
        ADD +1                           TO WS-TOT-FAM-VOID-CNT  
        ADD WS-DRFTLST-STOP-PAY-AMT      TO WS-TOT-FAM-NETPD-AMT 
                                                                 
    WHEN AT-CLAIM-TYPE = 'B'                                     
                                                                 
        IF MONTH-TO-DATE                                         
            CONTINUE                                             
        ELSE                                                     
            MOVE WS-DRFTLST-STOP-PAY-AMT  TO WS-DRFTLST-DRFTAMT  
            MOVE 'STOP-PAY'               TO                     
                                          WS-DRFTLST-ENTRY-TYPE  
            PERFORM 2400-DAILY-DRAFT-LIST THRU 2400-EXIT         
                                                                 
*>************* ACCUMULATE CURRENT CYCLE DAY TOTALS HERE AND GO ON 
