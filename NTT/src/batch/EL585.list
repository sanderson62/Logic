        ADD AT-AMOUNT-PAID               TO WS-TOT-HOS-VOID-AMT  
        ADD +1                           TO WS-TOT-HOS-VOID-CNT  
        ADD WS-DRFTLST-STOP-PAY-AMT      TO WS-TOT-HOS-NETPD-AMT 
                                                                 
    WHEN AT-CLAIM-TYPE = 'O'                                     
                                                                 
        IF MONTH-TO-DATE                                         
            CONTINUE                                             
        ELSE                                                     
            MOVE WS-DRFTLST-STOP-PAY-AMT  TO WS-DRFTLST-DRFTAMT  
