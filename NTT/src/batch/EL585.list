        ADD AT-AMOUNT-PAID               TO WS-TOT-BRV-VOID-AMT  
        ADD +1                           TO WS-TOT-BRV-VOID-CNT  
        ADD WS-DRFTLST-STOP-PAY-AMT      TO WS-TOT-BRV-NETPD-AMT 
                                                                 
    WHEN AT-CLAIM-TYPE = 'H'                                     
                                                                 
        IF MONTH-TO-DATE                                         
            CONTINUE                                             
        ELSE                                                     
            MOVE WS-DRFTLST-STOP-PAY-AMT  TO WS-DRFTLST-DRFTAMT  
            MOVE 'STOP-PAY'               TO                     
