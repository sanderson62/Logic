            ADD AT-AMOUNT-PAID          TO WS-DLY-IU-VOID-AMT    
            ADD +1                      TO WS-DLY-IU-VOID-CNT    
            ADD WS-DRFTLST-STOP-PAY-AMT TO WS-DLY-IU-NETPD-AMT   
