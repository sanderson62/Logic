        ADD AT-AMOUNT-PAID               TO WS-TOT-OTH-VOID-AMT  
        ADD +1                           TO WS-TOT-OTH-VOID-CNT  
        ADD WS-DRFTLST-STOP-PAY-AMT      TO WS-TOT-OTH-NETPD-AMT 
                                                                 
    WHEN AT-CLAIM-TYPE = 'L'                                     
                                                                 
        IF MONTH-TO-DATE                                         
            CONTINUE                                             
        ELSE                                                     
            MOVE WS-DRFTLST-STOP-PAY-AMT  TO WS-DRFTLST-DRFTAMT  
            MOVE 'STOP-PAY'               TO                     
                                          WS-DRFTLST-ENTRY-TYPE  
