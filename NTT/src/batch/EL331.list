       ((CR-AH-TERM GREATER CR-LF-TERM) OR                       
        (WS-WORK-TERM EQUAL ZERO))                               
          MOVE CR-AH-TERM       TO WS-WORK-TERM.                 
                                                                 
    IF (CR-DTH-DT NOT EQUAL ZERO)                                
       or (CR-DIS-DT NOT EQUAL ZERO)                             
           COMPUTE WS-EXPIRE-MOS EQUAL                           
                  (CR-CCYY * 12) + CR-MO + WS-WORK-TERM + 36     
      ELSE                                                       
           COMPUTE WS-EXPIRE-MOS EQUAL                           
                  (CR-CCYY * 12) + CR-MO + WS-WORK-TERM + 12.    
                                                                 
    IF WS-WORK-TERM EQUAL +1                                     
           COMPUTE WS-EXPIRE-MOS EQUAL                           
                  (CR-CCYY * 12) + CR-MO + WS-WORK-TERM + 36.    
                                                                 
    IF DTE-CLIENT = 'LAP'  OR  'RMC'                             
           COMPUTE WS-EXPIRE-MOS EQUAL                           
                  (CR-CCYY * 12) + CR-MO + WS-WORK-TERM + 36.    
                                                                 
    IF DTE-CLIENT = 'CID'                                        
       IF (CR-ENTRY-BATCH (1:2) = 'BL' OR 'VA')                  
          OR (CR-LF-EXIT-BATCH (1:2) = 'BL' OR 'VA')             
          OR (CR-AH-EXIT-BATCH (1:2) = 'BL' OR 'VA')             
          COMPUTE WS-EXPIRE-MOS =                                
             (CR-CCYY * 12) + CR-MO + WS-WORK-TERM + 300         
       END-IF                                                    
    END-IF                                                       
                                                                 
    IF DTE-CLIENT = 'AHL' or 'FNL'                               
       COMPUTE WS-EXPIRE-MOS =                                   
          (CR-CCYY * 12) + CR-MO + WS-WORK-TERM + 300            
    END-IF                                                       
                                                                 
    IF WS-EXPIRE-MOS < WS-CURRENT-MOS                            
       IF (CR-ENTRY-CCYY = RUN-CCYY)                             
          and (CR-ENTRY-MO = RUN-MO)                             
          continue                                               
       ELSE                                                      
          set do-not-load        to true                         
       end-if                                                    
    end-if                                                       
                                                                 
    IF WS-EXPIRE-MOS LESS WS-CURRENT-MOS                         
      IF DTE-CLIENT NOT = 'LGX' and 'CID' and 'DCC' and 'VPP'    
          IF CR-ENTRY-CCYY = RUN-CCYY AND                        
             CR-ENTRY-MO = RUN-MO                                
              NEXT SENTENCE                                      
          ELSE                                                   
              ADD 1 TO WS-INACTIVE-CERTS                         
              GO TO 2100-READ-ECS-RECORDS.                       
                                                                 
    PERFORM 2120-SAVE-ECS-KEY  THRU 2120-EXIT.                   
                                                                 
    MOVE CLAS-STARTA            TO CLAS-INDEXA.                  
    MOVE CLAS-STARTL            TO CLAS-INDEXL.                  
                                                                 
2100-LF-TYPE.                                                    
                                                                 
    IF CR-LFTYP EQUAL ZERO                                       
        GO TO 2100-AH-TYPE.                                      
                                                                 
    IF CLAS-INDEXL GREATER CLAS-MAXL                             
        DISPLAY LIFE-OVERRIDE-L6                                 
                ' BENEFIT TYPE NOT FOUND IN TABLE EQUAL ',       
                CR-LFTYP                                         
        MOVE 'BENEFIT TYPE NOT FOUND (SEE DISPLAY)'              
                                TO WS-ABEND-MESSAGE              
        GO TO ABEND-PGM.                                         
                                                                 
    IF CR-LFTYP EQUAL CLAS-I-BEN (CLAS-INDEXL)                   
        GO TO 2100-AH-TYPE.                                      
                                                                 
    ADD +1                      TO CLAS-INDEXL.                  
    GO TO 2100-LF-TYPE.                                          
                                                                 
2100-AH-TYPE.                                                    
                                                                 
    IF CR-AHTYP EQUAL ZERO                                       
       GO TO 2100-EXIT.                                          
                                                                 
    IF CLAS-INDEXA GREATER CLAS-MAXA                             
        DISPLAY AH-OVERRIDE-L6                                   
                ' BENEFIT TYPE NOT FOUND IN TABLE EQUAL ',       
                CR-AHTYP                                         
        MOVE 'BENEFIT TYPE NOT FOUND (SEE DISPLAY)'              
                                TO WS-ABEND-MESSAGE              
        GO TO ABEND-PGM.                                         
                                                                 
    IF CR-AHTYP EQUAL CLAS-I-BEN (CLAS-INDEXA)                   
           GO TO 2100-EXIT.                                      
                                                                 
    ADD +1                      TO CLAS-INDEXA.                  
    GO TO 2100-AH-TYPE.                                          
                                                                 
2100-EXIT.                                                       
     EXIT.                                                       
                                                                 
    EJECT                                                        
*>***************************************************              
*>  2120-SAVE-ECS-KEY                               *              
*>***************************************************              
*>  SAVE BATCH CERT MASTER KEY IN WS-SAVED-ECS-KEY  *              
*>***************************************************              
                                                                 
2120-SAVE-ECS-KEY.                                               
                                                                 
    MOVE DTE-CLASIC-COMPANY-CD  TO WS-SAVED-B-COMP.              
    MOVE CR-CARRIER             TO WS-SAVED-B-CARR.              
    MOVE CR-GROUPING            TO WS-SAVED-B-GRPG.              
    MOVE CR-STATE               TO WS-SAVED-B-STAT.              
    MOVE CR-ACCOUNT             TO WS-SAVED-B-ACCT.              
    MOVE CR-CERT-NO             TO WS-SAVED-B-CERT.              
    MOVE CR-DT                  TO WS-SAVED-B-DATE               
                                   DC-GREG-DATE-CYMD.            
                                                                 
    MOVE 'L'                    TO DC-OPTION-CODE.               
    PERFORM 7100-DATE-RTN  THRU   7100-EXIT.                     
    MOVE DC-BIN-DATE-1          TO BIN-CR-DT.                    
                                                                 
2120-EXIT.                                                       
     EXIT.                                                       
                                                                 
    EJECT                                                        
*>***************************************************              
*>  2200-MATCH-ACCT                                 *              
*>***************************************************              
*>  IF ONLINE BILLING OPTION IS USED RETRIEVE THE   *              
*>  ACCOUNT MASTER FILE RECORD ASSOCIATED WITH THE  *              
*>  CERTIFICATE BEING PROCESSED. IF A MATCH OCCURS, *              
*>  THE COMPENSATION STRUCTURES BETWEEN THE TWO     *              
*>  RECORDS ARE COMPARED, IF ANY DIFFERENCES ARE    *              
*>  DETECTED A COMPENSATION EXCEPTION RECORD IS     *              
*>  CREATED. THIS RECORD WILL CONTAIN THE ORIGINAL  *              
*>  COMPENSATION STRUCTURE THE CERTIFICATE WAS      *              
*>  ISSUED UNDER.                                   *              
*>***************************************************              
                                                                 
2200-MATCH-ACCT.                                                 
    IF DTE-SYS-D-DEMAND-BILL EQUAL '1' OR                        
       DTE-SYS-G-AR-USED     EQUAL '1'                           
        NEXT SENTENCE                                            
    ELSE                                                         
        GO TO 2200-EXIT.                                         
                                                                 
    IF NO-MORE-ACT-RECORDS                                       
        GO TO 2200-EXIT.                                         
                                                                 
    IF CR-ACCT-CONTROL GREATER AM-CONTROL-A                      
       PERFORM 2260-READ-ACCT THRU 2260-EXIT                     
       GO TO 2200-MATCH-ACCT.                                    
                                                                 
    IF CR-ACCT-CONTROL LESS AM-CONTROL-A                         
       GO TO 2200-EXIT.                                          
                                                                 
    IF BIN-CR-DT LESS AM-EFFECTIVE-DT  OR                        
       BIN-CR-DT NOT LESS AM-EXPIRATION-DT                       
          PERFORM 2260-READ-ACCT THRU 2260-EXIT                  
          GO TO 2200-MATCH-ACCT.                                 
                                                                 
*>***************************************************              
*>  MATCHED  *  CHECK COMPENSATION STRUCTURE        *              
*>***************************************************              
                                                                 
    IF NOT CR-POLICY-IS-DECLINED  AND                            
       NOT CR-POLICY-IS-VOID      AND                            
       NOT CR-POLICY-IS-REIN-ONLY                                
        ADD 1 TO WS-ACCT-MATCH                                   
        PERFORM 2210-MATCH-LEVELS THRU 2250-EXIT.                
                                                                 
2200-EXIT.                                                       
     EXIT.                                                       
                                                                 
    EJECT                                                        
*>***************************************************              
*>  2210-MATCH-LEVELS.                              *              
*>***************************************************              
*>  THE COMPENSATION STRUCTURES BETWEEN THE TWO     *              
*>  RECORDS ARE COMPARED, IF ANY DIFFERENCES ARE    *              
*>  DETECTED A COMPENSATION EXCEPTION RECORD        *              
*>  IS CREATED. THIS RECORD WILL CONTAIN THE ORIG-  *              
*>  INAL COMPENSATION STRUCTURE THE CERTIFICATE WAS *              
*>  ISSUED UNDER.                                   *              
*>                                                  *              
*>  AM-COM-TYP =   O (OVERWRITE COMMISSION ON GROSS *              
*>                  BUSINESS ONLY)                  *              
*>                                                  *              
*>  AM-COM-TYP =   P (OVERWRITE COMMISSION ON GROSS *              
*>                  AND REINSURANCE BUSINESS)       *              
*>***************************************************              
                                                                 
2210-MATCH-LEVELS.                                               
                                                                 
    MOVE 1                      TO NDX.                          
    MOVE CLAS-STARTA            TO CLAS-INDEXA.                  
    MOVE CLAS-STARTL            TO CLAS-INDEXL.                  
    PERFORM 2100-LF-TYPE  THRU  2100-EXIT.                       
                                                                 
2220-GET-NEXT-LEVEL.                                             
                                                                 
    IF CR-AGT-TYPE (NDX)  =  AM-COM-TYP (NDX)                    
        IF (AM-COM-TYP (NDX) NOT EQUAL 'O' AND 'P'               
             AND 'G' AND 'B' AND 'S')                            
            GO TO 2230-GET-NEXT.                                 
                                                                 
    IF (AM-J-COM (NDX) NOT NUMERIC)   OR                         
       (AM-L-COM (NDX) NOT NUMERIC)   OR                         
       (AM-A-COM (NDX) NOT NUMERIC)                              
         NEXT SENTENCE                                           
    ELSE                                                         
         MOVE AM-L-COM (NDX)    TO  WS-L-COM                     
         MOVE AM-J-COM (NDX)    TO  WS-J-COM                     
         MOVE AM-A-COM (NDX)    TO  WS-A-COM                     
         GO TO 2225-CHECK-NUMERIC.                               
                                                                 
    IF CR-LFTYP = ZERO                                           
        GO TO 2224-CHECK-AH-TABLE.                               
                                                                 
    IF CLAS-I-JOINT (CLAS-INDEXL) = 'J'                          
        GO TO 2221-JOINT-LIFE-COMM.                              
                                                                 
    IF AM-L-COM (NDX) NUMERIC                                    
        MOVE AM-L-COM (NDX)    TO  WS-L-COM                      
        GO TO 2224-CHECK-AH-TABLE.                               
                                                                 
    MOVE AM-L-COMA (NDX)        TO  WS-CTBL-TABLE.               
    MOVE LIFE-OVERRIDE-L1       TO  WS-CTBL-BEN-TYPE.            
    MOVE CR-LFTYP               TO  WS-CTBL-BEN-CODE.            
    MOVE CR-LFAMT               TO  WS-COMM-CK-AMT.              
    MOVE CR-LF-TERM             TO  WS-CK-TERM.                  
    MOVE CR-AGE                 TO  WS-CK-AGE.                   
    PERFORM  2249-CTBL-PROCESS  THRU  2249-READ-CTBL-EXIT.       
    MOVE CT-RT (CINDX)          TO  WS-L-COM.                    
    GO TO 2224-CHECK-AH-TABLE.                                   
                                                                 
2221-JOINT-LIFE-COMM.                                            
                                                                 
    IF AM-J-COM (NDX) NUMERIC                                    
        MOVE AM-J-COM (NDX)    TO  WS-J-COM                      
        GO TO 2224-CHECK-AH-TABLE.                               
                                                                 
    MOVE AM-J-COMA (NDX)        TO  WS-CTBL-TABLE.               
    MOVE LIFE-OVERRIDE-L1       TO  WS-CTBL-BEN-TYPE.            
    MOVE CR-LFTYP               TO  WS-CTBL-BEN-CODE.            
    MOVE CR-LFAMT               TO  WS-COMM-CK-AMT.              
    MOVE CR-LF-TERM             TO  WS-CK-TERM.                  
    MOVE CR-AGE                 TO  WS-CK-AGE.                   
    PERFORM  2249-CTBL-PROCESS  THRU  2249-READ-CTBL-EXIT.       
    MOVE CT-RT (CINDX)          TO  WS-J-COM.                    
                                                                 
2224-CHECK-AH-TABLE.                                             
                                                                 
    IF CR-AHTYP = ZERO                                           
        GO TO 2225-CHECK-NUMERIC.                                
                                                                 
    IF AM-A-COM (NDX) NUMERIC                                    
        MOVE AM-A-COM (NDX)     TO  WS-A-COM                     
        GO TO 2225-CHECK-NUMERIC.                                
                                                                 
    MOVE AM-A-COMA (NDX)        TO  WS-CTBL-TABLE.               
    MOVE AH-OVERRIDE-L1         TO  WS-CTBL-BEN-TYPE.            
    MOVE CR-AHTYP               TO  WS-CTBL-BEN-CODE.            
    MOVE CR-AHAMT               TO  WS-COMM-CK-AMT.              
    MOVE CR-AH-TERM             TO  WS-CK-TERM.                  
    MOVE CR-AGE                 TO  WS-CK-AGE.                   
    PERFORM  2249-CTBL-PROCESS  THRU  2249-READ-CTBL-EXIT.       
    MOVE CT-RT (CINDX)          TO  WS-A-COM.                    
                                                                 
2225-CHECK-NUMERIC.                                              
                                                                 
    IF DTE-CLIENT EQUAL 'WDS'                                    
       IF CR-COM-AGT (NDX) EQUAL '0000000001'                    
          GO TO 2230-GET-NEXT.                                   
                                                                 
    IF (CR-COM-AGT  (NDX) NOT EQUAL AM-AGT (NDX)) OR             
       (CR-AGT-TYPE (NDX) NOT EQUAL AM-COM-TYP (NDX))            
          GO TO 2235-NO-MATCH.                                   
                                                                 
    IF CR-LFTYP EQUAL ZERO                                       
        NEXT SENTENCE                                            
    ELSE                                                         
        IF ((CLAS-I-JOINT (CLAS-INDEXL) EQUAL 'J') AND           
            (CR-LCOM-L  (NDX) NOT EQUAL WS-J-COM ))              
        OR                                                       
           ((CLAS-I-JOINT (CLAS-INDEXL) NOT EQUAL 'J') AND       
            (CR-LCOM-L  (NDX) NOT EQUAL WS-L-COM ))              
                GO TO 2235-NO-MATCH.                             
                                                                 
    IF CR-AHTYP EQUAL ZERO                                       
        NEXT SENTENCE                                            
    ELSE                                                         
        IF CR-LCOM-AH (NDX) NOT EQUAL WS-A-COM                   
              GO TO 2235-NO-MATCH.                               
                                                                 
2230-GET-NEXT.                                                   
                                                                 
    ADD 1                       TO NDX.                          
                                                                 
    IF NDX GREATER 10                                            
       GO TO 2250-EXIT.                                          
                                                                 
    GO TO 2220-GET-NEXT-LEVEL.                                   
                                                                 
2235-NO-MATCH.                                                   
                                                                 
    MOVE SPACES                 TO COMMISSION-EXCEPTIONS.        
    MOVE 'CE'                   TO CE-RECORD-ID.                 
    MOVE DTE-CLASIC-COMPANY-CD  TO CE-COMPANY-CD.                
    MOVE CR-CARRIER             TO CE-CARRIER.                   
    MOVE CR-GROUPING            TO CE-GROUPING.                  
    MOVE CR-STATE               TO CE-STATE.                     
    MOVE CR-ACCOUNT             TO CE-ACCOUNT.                   
                                                                 
    MOVE BIN-CR-DT              TO CE-CERT-EFF-DT.               
                                                                 
    MOVE CR-CERT-NO             TO CE-CERT-NO.                   
    MOVE CR-COMPENSATION-LEVELS TO CE-COMP-STRUCTURE.            
                                                                 
    MOVE CR-LF-EXPIRE-DATE      TO DC-GREG-DATE-CYMD.            
                                                                 
    IF CR-LF-EXPIRE-DATE EQUAL ZERO                              
         MOVE CR-AH-EXPIRE-DATE TO DC-GREG-DATE-CYMD.            
                                                                 
    MOVE 'L'                    TO DC-OPTION-CODE.               
    PERFORM 7100-DATE-RTN  THRU   7100-EXIT.                     
    MOVE DC-BIN-DATE-1          TO CE-CERT-EXPIRATION-DT.        
                                                                 
    WRITE COMMISSION-EXCEPTIONS.                                 
                                                                 
    IF CE-STATUS EQUAL '22'                                      
        GO TO 2245-SKIP-DUP.                                     
                                                                 
    IF CE-STAT-1  NOT EQUAL ZERO                                 
        MOVE CE-STATUS          TO  WS-ABEND-FILE-STATUS         
        MOVE 'ERROR OCCURED WRITE - ERCOMM'                      
                                TO  WS-ABEND-MESSAGE             
        GO TO ABEND-PGM.                                         
                                                                 
2245-SKIP-DUP.                                                   
                                                                 
    MOVE '1'                    TO CM-COMP-EXCP-SW.              
    ADD 1                       TO WS-EXCP-ADD.                  
    GO TO 2250-EXIT.                                             
                                                                 
2249-CTBL-PROCESS.                                               
                                                                 
    IF WS-CTBL-KEY = WS-LAST-CTBL-KEY                            
        GO TO 2249-SEARCH-TABLE.                                 
                                                                 
    MOVE DTE-CLASIC-COMPANY-CD  TO  WS-CTBL-COMP-CD.             
                                                                 
2249-READ-CTBL.                                                  
                                                                 
    MOVE WS-CTBL-KEY            TO  CT-CONTROL-PRIMARY           
                                    WS-LAST-CTBL-KEY.            
                                                                 
    READ ERCTBL.                                                 
                                                                 
    IF ERCTBL-FILE-STATUS     = '97'                             
       MOVE '00' TO ERCTBL-FILE-STATUS.                          
                                                                 
    IF ERCTBL-FILE-STATUS NOT = '00'                             
       ADD  +1  TO  ERROR-COUNT                                  
*>**1**                                                            
       MOVE 'ERCTBL-FILE-STATUS IS = ' TO DIS-LINE-REASON        
       MOVE ERCTBL-FILE-STATUS         TO DIS-LINE-REC           
       PERFORM 8600-DISPLAY-PRT THRU                             
             8600-DISPLAY-EXIT                                   
*>**2**                                                            
       MOVE 'WS-CTBL-KEY        IS = ' TO DIS-LINE-REASON        
       MOVE  WS-CTBL-KEY               TO DIS-LINE-REC           
       PERFORM 8600-DISPLAY-PRT THRU                             
             8600-DISPLAY-EXIT                                   
*>**3**                                                            
       MOVE 'CM-CONTROL-PRIMARY IS = ' TO DIS-LINE-REASON        
       MOVE  CM-CONTROL-PRIMARY        TO DIS-LINE-REC           
       PERFORM 8600-DISPLAY-PRT THRU                             
             8600-DISPLAY-EXIT                                   
*>**4**                                                            
       MOVE 'CE-CONTROL-PRIMARY IS = ' TO DIS-LINE-REASON        
       MOVE  CE-CONTROL-PRIMARY        TO DIS-LINE-REC           
       PERFORM 8600-DISPLAY-PRT THRU                             
             8600-DISPLAY-EXIT                                   
*>**5**                                                            
       MOVE 'AM-CONTROL-PRIMARY IS = ' TO DIS-LINE-REASON        
       MOVE  AM-CONTROL-PRIMARY        TO DIS-LINE-REC           
       PERFORM 8600-DISPLAY-PRT THRU                             
             8600-DISPLAY-EXIT                                   
*>**6**                                                            
       MOVE 'CL-CONTROL-PRIMARY IS = ' TO DIS-LINE-REASON        
       MOVE  CL-CONTROL-PRIMARY        TO DIS-LINE-REC           
       PERFORM 8600-DISPLAY-PRT THRU                             
             8600-DISPLAY-EXIT                                   
*>**7**                                                            
       MOVE  SPACES                    TO DIS-LINE-REASON        
       MOVE  SPACES                    TO DIS-LINE-REC           
       PERFORM 8600-DISPLAY-PRT THRU                             
             8600-DISPLAY-EXIT                                   
                                                                 
        MOVE 'ERROR OCCURRED READ - ERCTBL'                      
                                TO  WS-ABEND-MESSAGE             
        MOVE ERCTBL-FILE-STATUS TO  WS-ABEND-FILE-STATUS         
        DISPLAY 'WS-CTBL-KEY = ' WS-CTBL-KEY                     
        GO TO 2100-READ-ECS-RECORDS.                             
*>        GO TO ABEND-PGM.                                         
                                                                 
2249-SEARCH-TABLE.                                               
                                                                 
     MOVE +1                    TO  CINDX.                       
                                                                 
     IF  WS-COMM-CK-AMT GREATER THAN CT-TBF (1)                  
         ADD +9 TO CINDX                                         
         IF WS-COMM-CK-AMT GREATER THAN CT-TBF (2)               
             ADD +9 TO CINDX.                                    
                                                                 
     IF WS-CK-AGE GREATER THAN CT-AGE (1)                        
         ADD +3 TO CINDX                                         
         IF WS-CK-AGE GREATER THAN CT-AGE (2)                    
             ADD +3 TO CINDX.                                    
                                                                 
     IF WS-CK-TERM GREATER THAN CT-TRM (1)                       
         ADD +1 TO CINDX                                         
         IF WS-CK-TERM GREATER THAN CT-TRM (2)                   
             ADD +1 TO CINDX.                                    
                                                                 
     IF CT-RT (CINDX) NOT NUMERIC                                
         MOVE WS-LAST-CTBL-KEY  TO  CT-CONTROL-PRIMARY           
         MOVE CT-RT-R (CINDX)   TO  WS-CTBL-TABLE                
         GO TO 2249-READ-CTBL.                                   
                                                                 
2249-READ-CTBL-EXIT.                                             
     EXIT.                                                       
                                                                 
2250-EXIT.                                                       
     EXIT.                                                       
                                                                 
    EJECT                                                        
*>***************************************************              
*>  2260-READ-ACCT                                  *              
*>***************************************************              
*>  READ ACCOUNT MASTER FILE RECORD MATCHING THE    *              
*>  ECS CERTIFICATE RECORD BEING PROCESSED.         *              
*>***************************************************              
                                                                 
2260-READ-ACCT.                                                  
                                                                 
    IF NO-MORE-ACT-RECORDS                                       
        GO TO 2260-EXIT.                                         
                                                                 
    READ ERACCT NEXT RECORD.                                     
                                                                 
    ADD 1 TO WS-ACT-READ.                                        
                                                                 
*>**  DISPLAY '*** ERACCT READ: ' AM-CARRIER '-' AM-GROUPING '-'   
*>**      AM-STATE '-' AM-ACCOUNT ' EXP: ' AM-EXPIRE-DT ' EFF: '   
*>**      AM-EFFECTIVE-DT '-' AM-EFFECT-DT '  CERT: ' CR-CARRIER   
*>**      '-' CR-GROUPING '-' CR-STATE '-' CR-ACCOUNT ' EFF: '     
*>**      CR-DT.                                                   
                                                                 
    IF AM-STAT-1 EQUAL '1'                                       
        MOVE 'X'   TO WS-ACT-END-SW                              
        GO TO 2260-EXIT.                                         
                                                                 
    IF AM-STAT-1 NOT EQUAL ZERO                                  
        MOVE 'ERROR OCCURED READ  - ERACCT'                      
                               TO  WS-ABEND-MESSAGE              
        MOVE AM-STATUSF        TO  WS-ABEND-FILE-STATUS          
        GO TO ABEND-PGM.                                         
                                                                 
    IF AM-COMPANY-CD NOT EQUAL DTE-CLASIC-COMPANY-CD             
        MOVE 'X'               TO WS-ACT-END-SW                  
        GO TO 2260-EXIT.                                         
                                                                 
2260-EXIT.                                                       
     EXIT.                                                       
                                                                 
    EJECT                                                        
*>***************************************************              
*>  2270-DELETE-ERCOMM                              *              
*>***************************************************              
*>  DELETE COMPENSATION EXCEPTION RECORDS FOR CERT- *              
*>  IFICATE MASTER RECORDS THAT HAVE EXPIRED.       *              
*>***************************************************              
                                                                 
2270-DELETE-ERCOMM.                                              
                                                                 
    IF DTE-SYS-D-DEMAND-BILL EQUAL '1' OR                        
       DTE-SYS-G-AR-USED     EQUAL '1'                           
         NEXT SENTENCE                                           
    ELSE                                                         
       GO TO 2270-EXIT.                                          
                                                                 
    IF THIS-CERT-HAS-ERCOMM-ENTRY                                
        MOVE CM-CONTROL-PRIMARY TO CE-CONTROL-PRIMARY            
    ELSE                                                         
        GO TO 2270-EXIT.                                         
                                                                 
    READ ERCOMM.                                                 
                                                                 
    IF CE-STATUS  EQUAL '23'                                     
        GO TO 2270-EXIT.                                         
                                                                 
    IF CE-STAT-1  NOT EQUAL ZERO                                 
       MOVE CE-STATUS           TO  WS-ABEND-FILE-STATUS         
       MOVE 'ERROR OCCURED READ - ERCOMM'                        
                                TO  WS-ABEND-MESSAGE             
       GO TO ABEND-PGM.                                          
                                                                 
    DELETE ERCOMM.                                               
                                                                 
    IF CE-STAT-1  NOT EQUAL ZERO                                 
       MOVE CE-STATUS           TO  WS-ABEND-FILE-STATUS         
       MOVE 'ERROR OCCURED DELETE - ERCOMM'                      
                                TO  WS-ABEND-MESSAGE             
       GO TO ABEND-PGM.                                          
                                                                 
    ADD 1                       TO WS-EXCP-DEL.                  
                                                                 
2270-EXIT.                                                       
    EXIT.                                                        
                                                                 
    EJECT                                                        
*>****************************************************             
*>  2280-DELETE-ERMAIL                               *             
*>****************************************************             
*>  DELETE NAME AND ADDRESS RECORDS FOR CERTIFICATE  *             
*>  MASTER RECORDS THAT HAVE EXPIRED.                *             
*>****************************************************             
                                                                 
2280-DELETE-ERMAIL.                                              
                                                                 
    IF INSURED-ADDR-PRESENT                                      
        MOVE CM-CONTROL-PRIMARY TO MA-CONTROL-PRIMARY            
    ELSE                                                         
        GO TO 2280-EXIT.                                         
                                                                 
    READ ERMAIL.                                                 
                                                                 
    IF ERMAIL-FILE-STATUS EQUAL '23'                             
        GO TO 2280-EXIT.                                         
                                                                 
    IF CFS-STAT-1 NOT EQUAL ZERO                                 
       MOVE ERMAIL-FILE-STATUS  TO  WS-ABEND-FILE-STATUS         
       MOVE 'ERROR OCCURED READ - ERMAIL'                        
                                TO  WS-ABEND-MESSAGE             
       GO TO ABEND-PGM.                                          
                                                                 
    DELETE ERMAIL.                                               
                                                                 
    IF CFS-STAT-1 NOT EQUAL ZERO                                 
       MOVE ERMAIL-FILE-STATUS  TO  WS-ABEND-FILE-STATUS         
       MOVE 'ERROR OCCURED DELETE - ERMAIL'                      
                                TO  WS-ABEND-MESSAGE             
       GO TO ABEND-PGM.                                          
                                                                 
    ADD 1                       TO WS-MAIL-DEL.                  
                                                                 
2280-EXIT.                                                       
    EXIT.                                                        
                                                                 
    EJECT                                                        
*>****************************************************             
*>  2300-PROCESS-N-READ-ONL.                         *             
*>****************************************************             
*>  THIS ROUTINE IS EXECUTED WHEN THE BATCH CERT     *             
*>  MASTER CONTROL IS GREATER THAN THE ONLINE CERT   *             
*>  MASTER CONTROL. THE ONLINE CERT MASTER WILL BE   *             
*>  DELETED UNLESS IT IS AN OPEN-END CERT OR CLAIM   *             
*>  PAYMENT ACTIVITY IS ASSOCIATED WITH THE CERT.    *             
*>  DELETE ANY ASSOCIATED COMPENSATION EXCEPTION     *             
*>  RECORDS(ERCOMM) AND NAME AND ADDRESS RECORDS     *             
*>  (ERMAIL) ASSOCIATED WITH THE EXPIRED CERT MASTER *             
*>  RECORD.                                          *             
*>****************************************************             
                                                                 
2300-PROCESS-N-READ-ONL.                                         
                                                                 
    MOVE ZEROS                  TO CM-STATUS.                    
                                                                 
    IF DTE-CLIENT = 'MON'                                        
        GO TO 2300-PROCESS-N-READ-MON.                           
                                                                 
*>****COPY ELCCRTM2.                                               
                                                                 
