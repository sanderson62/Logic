    MOVE CM-CERT-PRIME         TO RL-CERT-A4-PRIME               
                                  WS-S-CL-PRIM.                  
    MOVE CM-CERT-SFX           TO RL-CERT-A4-SFX                 
                                  WS-S-CL-SFX.                   
    MOVE CM-GROUPING           TO WS-CL-GRPG.                    
    MOVE CM-STATE              TO WS-CL-STAT.                    
    MOVE CM-ACCOUNT            TO WS-CL-ACCT.                    
    MOVE CM-CARRIER            TO WS-CL-CARR.                    
    MOVE CM-CERT-EFF-DT        TO WS-CL-EFDT.                    
                                                                 
    MOVE ZERO                  TO WS-CLM-CNT.                    
                                                                 
    START ELRETR5                                                
               KEY EQUAL RL-CONTROL-BY-CERT-NO.                  
                                                                 
    IF ELRETR5-FILE-STATUS = '23'                                
        GO TO 2330-EXIT.                                         
                                                                 
    IF ELRETR5-FILE-STATUS = ZERO                                
        MOVE SPACE              TO WS-CLM-END-SW                 
        PERFORM 2355-READNEXT-CLAIM  THRU 2355-EXIT              
            UNTIL NO-MORE-CLAIM-MATCH                            
    ELSE                                                         
        MOVE 'ERROR OCCURRED START - ELRETR'                     
                                 TO WS-ABEND-MESSAGE             
