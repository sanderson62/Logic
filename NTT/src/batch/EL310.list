    DISPLAY 'EL310 BEGAN PROCESSING OF ' WS-H2-COMPANY-NAME      
       ' AT ' WS-DISPLAY-TIME.                                   
                                                                 
    MOVE CF-CLAIM-CUTOFF-DATE TO DC-BIN-DATE-1.                  
    MOVE ' '                  TO DC-OPTION-CODE.                 
    PERFORM 8500-DATE-CONVERSION.                                
                                                                 
    IF (CF-CLAIM-CUTOFF-DATE = SPACES OR ZEROS OR HIGH-VALUES)   
                 OR                                              
       (DATE-CONVERSION-ERROR)                                   
           MOVE LOW-VALUES      TO CF-CLAIM-CUTOFF-DATE.         
                                                                 
1016-DO-DMD-CID.                                                 
    IF (BIN-RUN-DATE NOT LESS THAN CF-CURRENT-MONTH-END)         
                           OR                                    
       ((CF-CLAIM-CUTOFF-DATE NOT EQUAL LOW-VALUES)     AND      
        (BIN-RUN-DATE EQUAL TO CF-CLAIM-CUTOFF-DATE))            
                           OR                                    
       ((CF-CLAIM-CUTOFF-DATE NOT EQUAL LOW-VALUES)  AND         
        (BIN-RUN-DATE NOT LESS THAN CF-CLAIM-CUTOFF-DATE))       
           DISPLAY 'THIS IS A MONTH END'                         
           MOVE '-'                     TO  WS-DETAIL1           
           MOVE 'THIS IS MONTH END'     TO  WS-D1-MESSAGE        
           MOVE WS-DETAIL1              TO  PRT                  
           PERFORM WRITE-A-LINE                                  
           MOVE +1                      TO  WS-MONTH-END         
        ELSE                                                     
           MOVE '-'                     TO  WS-DETAIL1           
           MOVE 'THIS IS NOT MONTH END' TO  WS-D1-MESSAGE        
           MOVE WS-DETAIL1              TO  PRT                  
           PERFORM WRITE-A-LINE                                  
           MOVE ZERO                    TO  WS-MONTH-END.        
                                                                 
1017-CONTINUE.                                                   
    IF (CF-CLAIM-CUTOFF-DATE NOT EQUAL LOW-VALUES  AND           
        BIN-RUN-DATE NOT LESS THAN CF-CLAIM-CUTOFF-DATE)         
           MOVE ' '                       TO  WS-DETAIL1         
           MOVE 'CLAIMS WILL BE CUT OFF'  TO  WS-D1-MESSAGE      
           MOVE WS-DETAIL1                TO  PRT                
           PERFORM WRITE-A-LINE.                                 
                                                                 
    MOVE CF-CURRENT-MONTH-END   TO  DC-BIN-DATE-1.               
    MOVE -6                     TO  DC-ELAPSED-MONTHS.           
    MOVE ZERO                   TO  DC-ELAPSED-DAYS.             
    MOVE '6'                    TO  DC-OPTION-CODE.              
    PERFORM 8500-DATE-CONVERSION.                                
    MOVE DC-BIN-DATE-2          TO  WS-SIX-MONTHS-AGO.           
                                                                 
    MOVE SPACES                 TO  WS-DETAIL1.                  
    MOVE 'SIX MONTHS AGO'       TO  WS-D1-MESSAGE.               
    MOVE DC-GREG-DATE-1-EDIT    TO  WS-D1-BIRTH-DATE.            
    MOVE WS-DETAIL1             TO  PRT.                         
    PERFORM WRITE-A-LINE.                                        
                                                                 
    MOVE CF-PAYMENT-APPROVAL-SW     TO WS-PMT-APPROVAL.          
    MOVE CF-CERT-ACCESS-CONTROL     TO WS-CERT-ACCESS-CONTROL.   
    MOVE CF-CURRENT-MONTH-END       TO WS-MONTH-END-DATE.        
    MOVE CF-CR-REM-TERM-CALC        TO WS-CR-REM-TERM-CALC.      
    MOVE CF-CARRIER-CONTROL-LEVEL   TO WS-CARRIER-CONTROL.       
    MOVE CF-CO-CHECK-COUNTER        TO WS-CHECK-COUNTER          
                                       WS-BEGIN-CHECK-COUNTER.   
    MOVE CF-CO-ARCHIVE-COUNTER      TO WS-ARCHIVE-COUNTER        
                                       WS-ORIG-ARCH-COUNTER.     
    MOVE CF-CLAIMS-LAST-PROCESS-DT  TO WS-LAST-PROCESS-DT.       
                                                                 
    MOVE SPACES                 TO  WS-COMPANY-NAME2.            
    SET CN1 TO +30.                                              
                                                                 
    MOVE CF-CO-RESERVE-OPTION-SWITCH                             
                                TO WS-RESERVE-OPTION-SWITCH.     
    DISPLAY 'CF-CO-RESERVE-OPTION-SWITCH - '                     
        CF-CO-RESERVE-OPTION-SWITCH.                             
                                                                 
    IF OPT-RESERVE-METHOD-NOT-AUTH                               
        GO TO 1020-SIP.                                          
                                                                 
    IF CF-CO-CIDA-TABLE-DISCOUNT-PCT GREATER THAN ZERO           
        MOVE CF-CO-CIDA-TABLE-DISCOUNT-PCT                       
                                TO WS-CIDA-DISCOUNT              
    ELSE                                                         
        MOVE +1.0               TO WS-CIDA-DISCOUNT.             
                                                                 
    IF CF-CO-CALCULATION-INTEREST NUMERIC                        
             AND                                                 
       CF-CO-CALCULATION-INTEREST GREATER THAN ZERO              
          MOVE CF-CO-CALCULATION-INTEREST                        
                                TO WS-CO-CALC-INTEREST           
       ELSE                                                      
          MOVE +.03             TO WS-CO-CALC-INTEREST.          
                                                                 
1020-SIP.                                                        
                                                                 
    IF WS-CN-CHAR (CN1) = SPACES                                 
        IF CN1 GREATER THAN +1                                   
            SET CN1 DOWN BY +1                                   
            GO TO 1020-SIP                                       
          ELSE                                                   
            GO TO 1040-SIP.                                      
                                                                 
    SET WS-LENGTH TO CN1.                                        
                                                                 
    SUBTRACT WS-LENGTH FROM +30 GIVING WS-LENGTH.                
    DIVIDE +2 INTO WS-LENGTH ROUNDED.                            
                                                                 
    IF WS-LENGTH NOT GREATER THAN ZERO                           
        GO TO 1040-SIP.                                          
                                                                 
    SET CN2 TO CN1.                                              
    SET CN2 UP BY WS-LENGTH.                                     
                                                                 
1030-SIP.                                                        
                                                                 
    MOVE WS-CN-CHAR (CN1) TO WS-CN2-CHAR (CN2).                  
                                                                 
    IF CN1 GREATER THAN +1                                       
        SET CN1                                                  
            CN2 DOWN BY +1                                       
        GO TO 1030-SIP.                                          
                                                                 
