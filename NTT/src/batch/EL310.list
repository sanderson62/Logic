          MOVE WS-123183     TO AT-VOID-ACCEPT-DT                
          MOVE +1            TO WS-UPDATE-TRAILER                
          MOVE 'VOID NOT ACCEPTED, PRIOR TO 01/01/84'            
                             TO WS-D1-MESSAGE                    
          MOVE WS-DETAIL1    TO PRT                              
          PERFORM WRITE-A-LINE                                   
          MOVE SPACES TO WS-DETAIL1.                             
                                                                 
2900-EXIT.                                                       
    EXIT.                                                        
                                EJECT                            
2950-VERIFY-ELARCH-RECORD SECTION.                               
                                                                 
    MOVE LOW-VALUES             TO  LA-CONTROL-PRIMARY.          
    MOVE WS-COMPANY-CODE        TO  LA-COMPANY-CD.               
    MOVE '1'                    TO  LA-RECORD-TYPE.              
    MOVE AT-LETTER-ARCHIVE-NO   TO  LA-ARCHIVE-NO.               
                                                                 
    START ELARCH                                                 
        KEY IS NOT LESS THAN LA-CONTROL-PRIMARY.                 
                                                                 
    IF ELARCH-FILE-STATUS = '23'                                 
       MOVE ' ELARCH REC NOT FOUND ' TO WS-D1-MESSAGE            
        GO TO 2955-PRINT-ERROR-DATA.                             
                                                                 
    IF ELARCH-FILE-STATUS NOT = ZERO                             
        MOVE 'ERROR OCCURRED START - ELARCH'                     
                                TO  WS-ABEND-MESSAGE             
        MOVE ELARCH-FILE-STATUS TO  WS-ABEND-FILE-STATUS         
        GO TO ABEND-PGM.                                         
                                                                 
    READ ELARCH NEXT RECORD.                                     
                                                                 
    IF ELARCH-FILE-STATUS = '10'                                 
       MOVE ' ELARCH REC NOT FOUND ' TO WS-D1-MESSAGE            
       GO TO 2955-PRINT-ERROR-DATA.                              
                                                                 
    IF ELARCH-FILE-STATUS NOT = ZERO                             
        MOVE 'ERROR OCCURRED READNEXT - ELARCH'                  
                                TO  WS-ABEND-MESSAGE             
        MOVE ELARCH-FILE-STATUS TO  WS-ABEND-FILE-STATUS         
        GO TO ABEND-PGM.                                         
                                                                 
    IF LA-COMPANY-CD NOT = WS-COMPANY-CODE                       
      OR AT-LETTER-ARCHIVE-NO NOT = LA-ARCHIVE-NO                
      OR LA-RECORD-TYPE NOT = '1'                                
         PERFORM 2970-SEARCH-LETTERS THRU 2999-EXIT              
         GO TO 2955-PRINT-ERROR-DATA.                            
                                                                 
    MOVE SPACES                 TO  WS-D1-MESSAGE.               
                                                                 
    IF (LA4-RESEND-DATE  NOT = AT-AUTO-RE-SEND-DT AND            
       LA4-RESEND-DATE   NOT = LOW-VALUES AND SPACES)            
        MOVE ' ARCH RSND DT NOT = TRLR, SEQ='                    
                                TO  WS-D1-MESSAGE                
        MOVE AT-SEQUENCE-NO     TO  WS-D1-TRLR-SEQ               
        GO TO 2955-PRINT-ERROR-DATA.                             
                                                                 
    IF LA4-CREATION-DT NOT = AT-LETTER-SENT-DT                   
        MOVE 'ARCH CREAT DT NOT = TRLR, SEQ='                    
                                TO  WS-D1-MESSAGE                
        MOVE AT-SEQUENCE-NO     TO  WS-D1-TRLR-SEQ               
        GO TO 2955-PRINT-ERROR-DATA.                             
                                                                 
    IF LA4-INITIAL-PRINT-DATE NOT = AT-INITIAL-PRINT-DATE        
        MOVE ' ARCH IPRT DT NOT = TRLR, SEQ='                    
                                TO  WS-D1-MESSAGE                
        MOVE AT-SEQUENCE-NO     TO  WS-D1-TRLR-SEQ               
        GO TO 2955-PRINT-ERROR-DATA.                             
                                                                 
    IF LA4-RESEND-PRINT-DATE NOT = AT-RESEND-PRINT-DATE          
