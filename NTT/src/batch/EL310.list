*>***********************************************                  
*>    IF WS-COMPANY-ID = 'CID' OR 'DCC' or 'VPP'                   
*>       MOVE 'P'                 TO  WS-CSO-ACTION-TYPE           
*>       PERFORM 9300-CSO-SPECIAL-PROCESSING                       
*>                                THRU 9300-EXIT                   
*>    END-IF                                                       
*>                                                                 
*>*****************************************************************
*>***   THE FOLLOWING STATEMENT PRODUCES A CONTINUING FORM IF THE  
*>***   LAST PAYMENT OF THE SCHEDULE WAS MADE AND IF THE LAST      
*>***   PAYMENT WAS A PARTIAL PAYMENT.                             
*>*****************************************************************
                                                                 
*>    IF WS-COMPANY-ID = 'LGX' OR 'CRI' OR 'LAP' OR 'RMC'          
*>       IF CL-AUTO-PAY-SEQ = +0                                   
*>          IF EX-DA-PAYMENT-TYPE = '1'                            
*>             PERFORM 2582-BUILD-FORM-TRAILER                     
*>             IF WS-COMPANY-ID NOT = 'LAP' AND 'RMC'              
*>                 PERFORM 2587-BUILD-ARCHIVE-HEADER.              
                                                                 
    IF EX-DA-PAYMENT-TYPE = '2'                                  
*>*       PERFORM 7000-RELEASE-RECORD                              
*>*       ADD +1  TO  WS-EXTRACT-DA-COUNT                          
        IF CLAIM-IS-OPEN                                         
           PERFORM 2000-CLOSE-CLAIM                              
           GO TO 2590-EXIT                                       
        ELSE                                                     
           GO TO 2590-EXIT.                                      
                                                                 
2580-AUTO-PAYMENTS.                                              
                                                                 
*>*   PERFORM 7000-RELEASE-RECORD.                                 
                                                                 
*>*   ADD +1                      TO  WS-EXTRACT-DA-COUNT.         
    GO TO 2590-EXIT.                                             
                                                                 
2582-BUILD-FORM-TRAILER.                                         
                                                                 
    SUBTRACT +1  FROM CL-TRAILER-SEQ-CNT.                        
                                                                 
    MOVE SPACES                 TO ACTIVITY-TRAILERS.            
                                                                 
    MOVE CL-CONTROL-PRIMARY     TO AT-CONTROL-PRIMARY.           
    MOVE 'AT'                   TO AT-RECORD-ID.                 
    MOVE CL-TRAILER-SEQ-CNT     TO AT-SEQUENCE-NO.               
    MOVE 'A'                    TO AT-TRAILER-TYPE.              
    MOVE BIN-RUN-DATE           TO AT-RECORDED-DT                
                                   AT-FORM-LAST-MAINT-DT         
                                   AT-FORM-SEND-ON-DT.           
    MOVE 'EL310'                TO AT-RECORDED-BY                
                                   AT-FORM-LAST-UPDATED-BY.      
    MOVE WS-CURRENT-TIME        TO AT-LAST-MAINT-HHMMSS.         
    MOVE WS-CURRENT-DATE-PLUS-31 TO AT-FORM-FOLLOW-UP-DT.        
                                                                 
    MOVE LOW-VALUES             TO AT-FORM-ANSWERED-DT           
                                   AT-FORM-RE-SEND-DT            
                                   AT-EMP-FORM-ANSWERED-DT       
                                   AT-PHY-FORM-ANSWERED-DT       
                                   AT-EMP-FORM-SEND-ON-DT        
                                   AT-PHY-FORM-SEND-ON-DT        
                                   AT-FORM-PRINTED-DT            
                                   AT-FORM-REPRINT-DT            
                                   AT-FORM-REM-PRINT-DT.         
                                                                 
    MOVE '2'                    TO AT-FORM-TYPE.                 
                                                                 
    MOVE CL-INSURED-ADDR-CNT    TO AT-FORM-ADDR-SEQ-NO.          
    MOVE 'I'                    TO AT-FORM-ADDRESS.              
                                                                 
    IF CL-PROG-FORM-TYPE NOT = 'S'                               
       MOVE BIN-RUN-DATE        TO AT-PHY-FORM-SEND-ON-DT.       
                                                                 
    PERFORM 8700-WRITE-TRAILER.                                  
                                                                 
2587-BUILD-ARCHIVE-HEADER.                                       
                                                                 
    ADD +1                      TO WS-ARCHIVE-COUNTER.           
    MOVE 'LA'                   TO LA-RECORD-ID.                 
    MOVE WS-ARCHIVE-COUNTER     TO LA-ARCHIVE-NO                 
                                   LA-ARCHIVE-NO-A1.             
    MOVE '4'                    TO LA-RECORD-TYPE                
                                   LA-RECORD-TYPE-A1.            
    MOVE +0                     TO LA-LINE-SEQ-NO                
                                   LA-LINE-SEQ-NO-A1.            
    MOVE WS-COMPANY-CODE        TO LA-COMPANY-CD                 
                                   LA-COMPANY-CD-A1.             
    MOVE CL-CARRIER             TO LA4-CARRIER.                  
    MOVE CL-CLAIM-NO            TO LA4-CLAIM-NO.                 
    MOVE CL-CERT-NO             TO LA4-CERT-NO.                  
    MOVE +0                     TO LA4-NO-OF-COPIES.             
    MOVE 'EL310'                TO LA4-PROCESSOR-CD.             
    MOVE BIN-RUN-DATE           TO LA4-CREATION-DT.              
    MOVE LOW-VALUES             TO LA4-RESEND-DATE               
                                   LA4-INITIAL-PRINT-DATE        
                                   LA4-RESEND-PRINT-DATE         
                                   LA4-FORM-REM-PRINT-DT.        
    MOVE CL-TRAILER-SEQ-CNT     TO LA4-FORM-TRLR-SEQ.            
    MOVE '2'                    TO LA4-FORM-TYPE.                
                                                                 
    WRITE LETTER-ARCHIVE.                                        
                                                                 
    IF ELARCH-FILE-STATUS NOT = '00'                             
       MOVE 'ERROR OCCURRED WRITE - ELARCH'                      
                         TO WS-ABEND-MESSAGE                     
       MOVE ELARCH-FILE-STATUS TO WS-ABEND-FILE-STATUS           
       GO TO ABEND-PGM.                                          
                                                                 
2590-EXIT.                                                       
    EXIT.                                                        
                                EJECT                            
2600-GET-BENEFIT SECTION.                                        
*>    NOTE ******************************************************* 
*>         *      THIS SECTION SEARCHES THE BENEFIT TABLES TO    * 
*>         *  GET THE BENEFIT TABLE ENTRY FOR THE CLAIM BEING    * 
*>         *  PROCESSED.                                         * 
*>         *******************************************************.
                                                                 
    MOVE ZERO                   TO  WS-REM-TERM-METHOD           
                                    WS-EARNING-METHOD            
                                    WS-NO-BENEFIT.               
    MOVE SPACES                 TO  WS-COVERAGE-TYPE             
                                    WS-SPECIAL-CALC-CODE.        
                                                                 
    IF CL-CLAIM-TYPE = WS-AH-OVERRIDE-L1 OR 'I' OR 'G' OR 'F'    
        OR 'B' OR 'H'                                            
       SET BENEFIT-INDEX1 TO +2                                  
    ELSE                                                         
       SET BENEFIT-INDEX1 TO +1.                                 
                                                                 
    SET BENEFIT-INDEX2 TO +1.                                    
                                                                 
2620-GET-BENEFIT.                                                
                                                                 
    IF ((CL-CLAIM-TYPE = WS-AH-OVERRIDE-L1 OR 'I' OR 'G'         
         OR 'F' OR 'B' OR 'H') AND                               
        CM-AH-BENEFIT-CD =                                       
            WS-BENEFIT-CODE (BENEFIT-INDEX1, BENEFIT-INDEX2))    
                       OR                                        
       ((CL-CLAIM-TYPE = WS-LIFE-OVERRIDE-L1 OR 'O') AND         
        CM-LF-BENEFIT-CD =                                       
            WS-BENEFIT-CODE (BENEFIT-INDEX1, BENEFIT-INDEX2))    
         MOVE WS-CO-REM-TERM-CALC (BENEFIT-INDEX1 BENEFIT-INDEX2)
                                TO  WS-REM-TERM-METHOD           
         MOVE WS-CO-EARNINGS-CALC (BENEFIT-INDEX1 BENEFIT-INDEX2)
                                TO  WS-EARNING-METHOD            
         MOVE WS-LF-COVERAGE-TYPE (BENEFIT-INDEX1 BENEFIT-INDEX2)
                                TO  WS-COVERAGE-TYPE             
         MOVE WS-SPECIAL-CALC-CD  (BENEFIT-INDEX1 BENEFIT-INDEX2)
                                TO  WS-SPECIAL-CALC-CODE         
         GO TO 2690-EXIT.                                        
                                                                 
    IF BENEFIT-INDEX2 LESS THAN WS-BENEFIT-MAX (BENEFIT-INDEX1)  
        SET BENEFIT-INDEX2 UP BY +1                              
        GO TO 2620-GET-BENEFIT.                                  
                                                                 
    MOVE +1                     TO  WS-NO-BENEFIT.               
                                                                 
    MOVE 'BENEFIT NOT FOUND'    TO  WS-D1-MESSAGE.               
    MOVE WS-DETAIL1             TO  PRT.                         
    PERFORM WRITE-A-LINE.                                        
    MOVE SPACES                 TO  WS-DETAIL1.                  
                                                                 
2690-EXIT.                                                       
    EXIT.                                                        
                                EJECT                            
2700-PROCESS-CHECK-QUEUE SECTION.                                
*>    NOTE ******************************************************* 
*>         *      THIS SECTION PROCESSES THE CHECK QUEUE TO      * 
*>         *  PRODUCE THE CHECKS TO BE PRINTED REPORT AND PURGE  * 
*>         *  THE EXPIRED RECORDS FROM THE CHECK QUEUE.          * 
*>         *******************************************************.
                                                                 
    MOVE +1                     TO  WS-ELCHKQ-SW.                
                                                                 
    MOVE LOW-VALUES             TO  CQ-CONTROL-PRIMARY           
    MOVE WS-COMPANY-CODE        TO  CQ-COMPANY-CD.               
                                                                 
    START ELCHKQ                                                 
        KEY IS NOT LESS THAN CQ-CONTROL-PRIMARY                  
                                                                 
    IF ELCHKQ-FILE-STATUS = ('23' OR '10')                       
        GO TO 2790-EXIT.                                         
                                                                 
    IF ELCHKQ-FILE-STATUS NOT = ZERO                             
        MOVE 'ERROR OCCURRED START - ELCHKQ'                     
                                TO  WS-ABEND-MESSAGE             
        MOVE ELCHKQ-FILE-STATUS TO  WS-ABEND-FILE-STATUS         
        GO TO ABEND-PGM.                                         
                                                                 
2720-PROCESS-CHECK-QUEUE.                                        
                                                                 
    READ ELCHKQ NEXT INTO WS-SAVE-CHECK-QUEUE.                   
                                                                 
    IF ELCHKQ-FILE-STATUS = '10'                                 
        GO TO 2790-EXIT.                                         
                                                                 
    IF ELCHKQ-FILE-STATUS NOT = ZERO                             
        MOVE 'ERROR OCCURRED READ NEXT - ELCHKQ'                 
                                TO  WS-ABEND-MESSAGE             
        MOVE ELCHKQ-FILE-STATUS TO  WS-ABEND-FILE-STATUS         
        GO TO ABEND-PGM.                                         
                                                                 
    IF CQ-COMPANY-CD NOT = WS-COMPANY-CODE                       
        GO TO 2790-EXIT.                                         
                                                                 
    MOVE +0             TO  WS-VOID-PAYMENT-SW.                  
                                                                 
    IF CQ-ENTRY-TYPE = ('A' OR 'X' OR 'S')                       
        PERFORM 3300-BUILD-EXTRACT-C-RECORD-A                    
        IF THIS-IS-MONTH-END                                     
          AND CQ-CHECK-WRITTEN-DT NOT = LOW-VALUES               
          AND CQ-CHECK-WRITTEN-DT LESS THAN WS-CHKQ-PURGE-DATE   
            PERFORM 7200-PURGE-CHECK-QUEUE                       
            GO TO 2720-PROCESS-CHECK-QUEUE                       
          ELSE                                                   
            GO TO 2720-PROCESS-CHECK-QUEUE.                      
                                                                 
*>*****************************************************************
*>*****     BYPASS CHECK QUE RECORDS THAT WERE NOT          *******
*>*****     PROCESSED DURING THE CURRENT MONTH              *******
*>*****************************************************************
                                                                 
    IF THIS-IS-NOT-MONTH-END                                     
       IF (CQ-CHECK-WRITTEN-DT NOT = LOW-VALUES)                 
         AND                                                     
          (CQ-CHECK-WRITTEN-DT NOT GREATER THAN WS-PRIOR-MONTH   
               OR                                                
           CQ-CHECK-WRITTEN-DT GREATER THAN BIN-RUN-DATE)        
             GO TO 2720-PROCESS-CHECK-QUEUE.                     
                                                                 
    MOVE '0'                    TO  WS-DETAIL1.                  
                                                                 
    MOVE CQ-COMPANY-CD          TO  CL-COMPANY-CD.               
    MOVE CQ-CARRIER             TO  CL-CARRIER                   
                                    WS-D1-CARRIER.               
    MOVE CQ-CLAIM-NO            TO  CL-CLAIM-NO                  
                                    WS-D1-CLAIM-NO.              
    MOVE CQ-CERT-NO             TO  CL-CERT-NO                   
                                    WS-D1-CERT-NO.               
                                                                 
    READ ELMSTR INTO WS-SAVE-CLAIM-MASTER.                       
                                                                 
    IF ELMSTR-FILE-STATUS = '23'                                 
        MOVE 'CLAIM NOT FOUND - PROCESS CHECK QUEUE'             
                                TO  WS-D1-MESSAGE                
        MOVE WS-DETAIL1         TO  PRT                          
        PERFORM WRITE-A-LINE                                     
        MOVE SPACES             TO  WS-DETAIL1                   
        GO TO 2720-PROCESS-CHECK-QUEUE.                          
                                                                 
    IF ELMSTR-FILE-STATUS NOT = ZERO                             
        MOVE 'ERROR OCCURRED READ - ELMSTR'                      
                                TO  WS-ABEND-MESSAGE             
        MOVE ELMSTR-FILE-STATUS TO  WS-ABEND-FILE-STATUS         
        GO TO ABEND-PGM.                                         
                                                                 
    PERFORM 5000-MOVE-NAME.                                      
                                                                 
*>    IF CL-SYSTEM-IDENTIFIER = 'CV'                               
*>        PERFORM 2210-GET-POLICY-MASTER                           
*>        IF WS-EMPLCY-RETURN-CODE NOT = '00'                      
*>            MOVE 'POLICY RECORD NOT FOUND '                      
*>                                TO  WS-D1-MESSAGE                
*>            MOVE WS-DETAIL1     TO  PRT                          
*>            PERFORM WRITE-A-LINE                                 
*>        ELSE                                                     
*>            NEXT SENTENCE                                        
*>    ELSE                                                         
        PERFORM 2200-GET-CERTIFICATE.                            
        IF ELCERT-FILE-STATUS NOT = ZERO                         
            MOVE 'CERTIFICATE NOT FOUND - CREATED'               
                                TO  WS-D1-MESSAGE                
            MOVE WS-DETAIL1     TO  PRT                          
            PERFORM WRITE-A-LINE                                 
            PERFORM 9100-CREATE-CERTIFICATE                      
            PERFORM 8800-WRITE-CERTIFICATE.                      
                                                                 
    MOVE CL-CONTROL-PRIMARY     TO  AT-CONTROL-PRIMARY.          
    MOVE CQ-PMT-TRLR-SEQUENCE   TO  AT-SEQUENCE-NO.              
                                                                 
    PERFORM 8300-READ-TRAILER.                                   
                                                                 
    IF ELTRLR-FILE-STATUS = '23'                                 
        MOVE 'PAYMENT TRAILER NOT FOUND - PROCESS CHECK QUEUE'   
                                TO  WS-D1-MESSAGE                
        MOVE WS-DETAIL1         TO  PRT                          
        PERFORM WRITE-A-LINE                                     
        MOVE SPACES             TO  WS-DETAIL1                   
        GO TO 2720-PROCESS-CHECK-QUEUE.                          
                                                                 
    MOVE +2                     TO  WS-ELCHKQ-SW.                
                                                                 
    IF CQ-CHECK-WRITTEN-DT = SPACES OR LOW-VALUES                
        MOVE AT-CHECK-WRITTEN-DT  TO  CQ-CHECK-WRITTEN-DT.       
                                                                 
    IF AT-PMT-SELECT-DT = WS-MONTH-END-DATE                      
        PERFORM 3300-BUILD-EXTRACT-C-RECORD-A.                   
                                                                 
    IF CQ-TIMES-PRINTED = ZERO                                   
        PERFORM 3700-BUILD-EXTRACT-D-RECORD-E.                   
                                                                 
    IF THIS-IS-MONTH-END                                         
      AND CQ-CHECK-WRITTEN-DT NOT = LOW-VALUES                   
      AND CQ-CHECK-WRITTEN-DT LESS THAN WS-CHKQ-PURGE-DATE       
        PERFORM 7200-PURGE-CHECK-QUEUE                           
        MOVE +99999999          TO  AT-CHECK-QUE-CONTROL         
        MOVE ZERO               TO  AT-CHECK-QUE-SEQUENCE        
        PERFORM 8100-REWRITE-TRAILER                             
        GO TO 2720-PROCESS-CHECK-QUEUE.                          
                                                                 
    IF AT-CHECK-QUE-CONTROL NOT = +99999999                      
        IF AT-CHECK-QUE-CONTROL = CQ-CONTROL-NUMBER              
          AND AT-CHECK-QUE-SEQUENCE = CQ-SEQUENCE-NUMBER         
            NEXT SENTENCE                                        
          ELSE                                                   
            MOVE 'CORRECTING CHECK QUEUE POINTER'                
                                       TO  WS-D1-MESSAGE         
            MOVE AT-SEQUENCE-NO        TO  WS-D1-SEQ             
            MOVE AT-CHECK-QUE-CONTROL  TO  WS-D1-QUE             
            MOVE AT-CHECK-QUE-SEQUENCE TO  WS-D1-QUE-SEQ         
            MOVE WS-DETAIL1            TO  PRT                   
            PERFORM WRITE-A-LINE                                 
            MOVE SPACES                TO  WS-DETAIL1            
            MOVE CQ-CONTROL-NUMBER     TO  AT-CHECK-QUE-CONTROL  
            MOVE CQ-SEQUENCE-NUMBER    TO  AT-CHECK-QUE-SEQUENCE 
            PERFORM 8100-REWRITE-TRAILER                         
            GO TO 2720-PROCESS-CHECK-QUEUE.                      
                                                                 
    GO TO 2720-PROCESS-CHECK-QUEUE.                              
                                                                 
2790-EXIT.                                                       
    EXIT.                                                        
                                EJECT                            
2800-VERIFY-ACTIVITY-QUEUE SECTION.                              
*>    NOTE ******************************************************* 
*>         *      THIS SECTION VERIFIES THAT THE PAYMENT THAT    * 
*>         *  HAS NOT BEEN RELEASED FOR PRINTING HAS AN ACTIVITY * 
*>         *  QUEUE RECORD AND THERE ARE PAYMENTS PENDING.       * 
*>         *******************************************************.
                                                                 
    MOVE CL-CONTROL-PRIMARY     TO  AQ-CONTROL-PRIMARY.          
                                                                 
    READ ELACTQ INTO WS-SAVE-ACTIVITY-QUEUE.                     
                                                                 
    IF ELACTQ-FILE-STATUS = '23'                                 
        GO TO 2830-VERIFY-ACTIVITY-QUEUE.                        
                                                                 
    IF ELACTQ-FILE-STATUS NOT = ZERO                             
        MOVE ELACTQ-FILE-STATUS TO  WS-ABEND-FILE-STATUS         
        MOVE 'ERROR OCCURRED READ - ELACTQ' TO  WS-ABEND-MESSAGE 
        GO TO ABEND-PGM.                                         
                                                                 
    IF PENDING-PAYMENTS                                          
      AND AQ-PAYMENT-COUNTER GREATER THAN ZERO                   
        GO TO 2899-EXIT.                                         
                                                                 
    MOVE 'PAYMENT NOT RELEASED & PENDING PMT FLAG OFF - CORRECTED
         ' '                    TO  WS-D1-MESSAGE.               
    MOVE WS-DETAIL1             TO  PRT.                         
    PERFORM WRITE-A-LINE.                                        
    MOVE SPACES             TO  WS-DETAIL1.                      
                                                                 
    MOVE '1'                    TO  AQ-PENDING-PAYMENT-FLAG.     
                                                                 
    MOVE +1                     TO  AQ-PAYMENT-COUNTER.          
                                                                 
    MOVE +3100                  TO  AQ-LAST-UPDATED-BY.          
                                                                 
    MOVE +20                    TO SLR-KEY-LENGTH.               
    MOVE +60                    TO SLR-RECORD-LENGTH.            
    MOVE AQ-CONTROL-PRIMARY     TO SLR-KEY.                      
    MOVE ACTIVITY-QUE           TO SLR-RECORD-IMAGE.             
    MOVE 'ELACTQ  '             TO SLR-DSID.                     
    MOVE 'C'                    TO SLR-ACTION.                   
                                                                 
    REWRITE ACTIVITY-QUE.                                        
                                                                 
    IF ELACTQ-FILE-STATUS NOT = ZERO                             
        MOVE ELACTQ-FILE-STATUS TO  WS-ABEND-FILE-STATUS         
        MOVE 'ERROR OCCURRED REWRITE -ELACTQ' TO WS-ABEND-MESSAGE
        GO TO ABEND-PGM.                                         
                                                                 
*>    PERFORM LOG-JOURNAL-RECORD.                                  
                                                                 
    GO TO 2899-EXIT.                                             
                                                                 
2830-VERIFY-ACTIVITY-QUEUE.                                      
                                                                 
    MOVE 'PAYMENT NOT RELEASED & NO ACTIVITY QUE RECORD - CORRECT
         'ED'                   TO  WS-D1-MESSAGE.               
    MOVE WS-DETAIL1             TO  PRT.                         
    PERFORM WRITE-A-LINE.                                        
    MOVE SPACES                 TO  WS-DETAIL1                   
                                                                 
    MOVE 'AQ'                   TO  ACTIVITY-QUE.                
                                                                 
    MOVE CL-CONTROL-PRIMARY     TO  AQ-CONTROL-PRIMARY.          
                                                                 
    MOVE '1'                    TO  AQ-PENDING-PAYMENT-FLAG.     
    MOVE +1                     TO  AQ-PAYMENT-COUNTER.          
                                                                 
    MOVE +3100                  TO  AQ-LAST-UPDATED-BY.          
                                                                 
    MOVE +20                    TO SLR-KEY-LENGTH.               
    MOVE +60                    TO SLR-RECORD-LENGTH.            
    MOVE AQ-CONTROL-PRIMARY     TO SLR-KEY.                      
    MOVE ACTIVITY-QUE           TO SLR-RECORD-IMAGE.             
    MOVE 'ELACTQ  '             TO SLR-DSID.                     
    MOVE 'A'                    TO SLR-ACTION.                   
                                                                 
    WRITE ACTIVITY-QUE.                                          
                                                                 
    IF ELACTQ-FILE-STATUS NOT = ZERO                             
        MOVE ELACTQ-FILE-STATUS TO  WS-ABEND-FILE-STATUS         
        MOVE 'ERROR OCCURRED WRITE - ELACTQ' TO WS-ABEND-MESSAGE 
        GO TO ABEND-PGM.                                         
                                                                 
*>    PERFORM LOG-JOURNAL-RECORD.                                  
                                                                 
2899-EXIT.                                                       
    EXIT.                                                        
                                EJECT                            
2900-OLD-PAYMENTS SECTION.                                       
    IF (AT-PMT-ACCEPT-DT = LOW-VALUES AND                        
        AT-CHECK-WRITTEN-DT LESS THAN WS-010184 AND              
        AT-CHECK-WRITTEN-DT NOT = LOW-VALUES)                    
           MOVE WS-123183          TO AT-PMT-ACCEPT-DT           
           MOVE +1                 TO WS-UPDATE-TRAILER          
           MOVE 'PAYMENT NOT ACCEPTED, PRIOR TO 01/01/84'        
