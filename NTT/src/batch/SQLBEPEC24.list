    IF NOT END-OF-ERACCT                                         
       READ ERACCTT                                              
    END-IF                                                       
                                                                 
    IF ERACCT-FILE-STATUS = '10' OR '23'                         
       SET END-OF-ERACCT        TO TRUE                          
       MOVE HIGH-VALUES         TO AM-CONTROL-A                  
    ELSE                                                         
       IF ERACCT-FILE-STATUS = '00'                              
          CONTINUE                                               
       ELSE                                                      
          DISPLAY ' BAD READ ON ERACCTT ' ERACCT-FILE-STATUS     
          PERFORM ABEND-PGM                                      
       END-IF                                                    
    END-IF                                                       
                                                                 
