    EVALUATE TRUE                                                
    WHEN WS-ELMSTR-FILE-STATUS = '00'                            
        CONTINUE                                                 
                                                                 
    WHEN WS-ELMSTR-FILE-STATUS = '23'                            
        SET END-OF-ELMSTR TO TRUE                                
        MOVE '0NO ACTIVITY FOR EL585 REPORT SERIES'              
                                       TO PRT                    
        PERFORM 3900-WRITE             THRU 3900-EXIT            
                                                                 
