        ADD +1                       TO WS-TOT-FAM-VOID-CNT      
        ADD WS-STOP-PAY-AMT          TO WS-TOT-FAM-NETPD-AMT     
                                                                 
                                                                 
    WHEN AT-CLAIM-TYPE = 'B'                                     
                                                                 
        IF MONTH-TO-DATE                                         
            CONTINUE                                             
        ELSE                                                     
*>************* ACCUMULATE CURRENT CYCLE DAY TOTALS HERE AND GO ON 
*>              TO ADD THESE FIGURES TO MTD TOTALS                 
            ADD AT-AMOUNT-PAID       TO WS-DLY-BRV-VOID-AMT      
            ADD +1                   TO WS-DLY-BRV-VOID-CNT      
            ADD WS-STOP-PAY-AMT      TO WS-DLY-BRV-NETPD-AMT     
