    if dte-client not = 'CID'                                    
       go to 0650-exit                                           
    end-if                                                       
                                                                 
    EXEC SQL                                                     
       insert into NAPERSOFT_EL513A (                            
          MOEDate          ,                                     
          Carrier          ,                                     
          Grouping         ,                                     
          State            ,                                     
          Account          ,                                     
          Cert_Eff_Dt      ,                                     
          Cert_No          ,                                     
          Batch_No         ,                                     
          Insured_Name     ,                                     
          Lf_Cancel_Date   ,                                     
          Lf_Cancel_Amt    ,                                     
          Ah_Cancel_Date   ,                                     
          Ah_Cancel_Amt)                                         
       values (                                                  
          :TB-MOE-DATE         ,                                 
          :TB-CARRIER          ,                                 
          :TB-GROUPING         ,                                 
          :TB-STATE            ,                                 
          :TB-ACCOUNT          ,                                 
          :TB-CERT-EFF-DT      ,                                 
          :TB-CERT-NO          ,                                 
          :TB-BATCH-NO         ,                                 
          :TB-INSURED-NAME     ,                                 
          :TB-LF-CANCEL-DATE   :nu-lf-cancel-date,               
          :TB-LF-CANCEL-AMT-a  ,                                 
          :TB-AH-CANCEL-DATE   :nu-ah-cancel-date,               
          :TB-AH-CANCEL-AMT-a)                                   
       end-exec                                                  
                                                                 
    if sqlcode not = 0                                           
       display "Error: cannot insert row "                       
       move sqlcode             to ws-sql-code                   
       move ws-sql-code         to ws-dis-sql-code               
       display ' sqlcode ' ws-dis-sql-code                       
       display ' sql err mess    ' sqlerrmc                      
       display ' in out cnt ' ws-recs-in ' ' rec-cnt             
       display ' offending rec ' ns-el513-table-record           
    else                                                         
       add 1 to ws-recs-in                                       
    end-if                                                       
                                                                 
    .                                                            
