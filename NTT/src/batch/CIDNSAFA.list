0600-UPDATE-PEND-ISSUE.                                          
                                                                 
   MOVE 'N'              TO WS-PEND-FORCED                       
   MOVE 'N'              TO WS-PEND-FATAL                        
   MOVE CM-CONTROL-PRIMARY TO PB-CONTROL-BY-ACCOUNT              
   MOVE +0               TO PB-ALT-CHG-SEQ-NO                    
   MOVE '1'              TO PB-RECORD-TYPE                       
                                                                 
   READ ERPNDB                                                   
                                                                 
   IF ERPNDB-FILE-STATUS = '00'                                  
      IF PB-FATAL-ERRORS                                         
          MOVE 'Y'  TO WS-PEND-FATAL                             
      ELSE                                                       
        IF PB-UNFORCED-ERRORS                                    
          MOVE 'Y'  TO WS-PEND-FORCED                            
          MOVE 'A'  TO PB-FORCE-CODE                             
          REWRITE PENDING-BUSINESS                               
          IF ERPNDB-FILE-STATUS = '00'                           
             PERFORM 0610-ADD-CERT-NOTE THRU 0610-EXIT           
          ELSE                                                   
             DISPLAY ' ERROR ON ERPNDB - REWRITE - FORCE CD '    
                ERPNDB-FILE-STATUS  '  CERT '  PB-CERT-NO        
          END-IF                                                 
        END-IF                                                   
      END-IF                                                     
   END-IF                                                        
                                                                 
