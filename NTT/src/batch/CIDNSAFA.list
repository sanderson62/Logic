    exit.                                                        
                                                                 
0700-WRITE-CERT-NOTE.                                            
                                                                 
    SET TB-INDX         DOWN BY 1.                               
    SET TB-INDX1        TO TB-INDX.                              
                                                                 
    SET TB-INDX TO 1.                                            
    MOVE +0             TO WS-NOTE-SEQ                           
                                                                 
    PERFORM VARYING TB-INDX FROM 1 BY 1                          
            UNTIL TB-INDX > TB-INDX1                             
       MOVE SPACES                 TO  CERT-NOTE-FILE            
       MOVE  'CZ'                  TO  CZ-RECORD-ID              
       MOVE CM-CONTROL-PRIMARY     TO  CZ-CONTROL-PRIMARY        
       MOVE '1'                    TO  CZ-RECORD-TYPE            
       ADD 1                       TO  WS-NOTE-SEQ               
       MOVE WS-NOTE-SEQ            TO  CZ-NOTE-SEQUENCE          
       MOVE CERT-NT-TEXT (TB-INDX) TO  CZ-NOTE                   
       MOVE CERT-NT-LAST-MAINT-BY (TB-INDX)                      
                                   TO  CZ-LAST-MAINT-USER        
       MOVE CERT-NT-LAST-MAINT-HHMMSS (TB-INDX)                  
                                   TO  CZ-LAST-MAINT-HHMMSS      
       MOVE CERT-NT-LAST-MAINT-DT (TB-INDX)                      
                                   TO  CZ-LAST-MAINT-DT          
                                                                 
       WRITE CERT-NOTE-FILE                                      
       IF ERCNOT-FILE-STATUS NOT = '00'                          
            DISPLAY ' ERROR - ERCNOT - WRITE '                   
                 ERCNOT-FILE-STATUS ' - ' CZ-CERT-NO             
       END-IF                                                    
    END-PERFORM                                                  
                                                                 
*>**UPDATE CERT NOTE FLAG ON CERT                                  
    MOVE SPACES TO WS-CERT-UPDATE-SW                             
    EVALUATE CM-NOTE-SW                                          
       WHEN '1'                                                  
       WHEN '3'                                                  
       WHEN '5'                                                  
       WHEN '7'                                                  
          SET NO-CERT-RW     TO TRUE                             
       WHEN ' '                                                  
          MOVE '1'           TO CM-NOTE-SW                       
       WHEN '2'                                                  
          MOVE '3'           TO CM-NOTE-SW                       
       WHEN '4'                                                  
          MOVE '5'           TO CM-NOTE-SW                       
       WHEN '6'                                                  
          MOVE '7'           TO CM-NOTE-SW                       
    END-EVALUATE                                                 
                                                                 
    IF NOT NO-CERT-RW                                            
        REWRITE CERTIFICATE-MASTER                               
    END-IF                                                       
                                                                 
