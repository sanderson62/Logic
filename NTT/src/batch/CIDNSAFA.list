0610-ADD-CERT-NOTE.                                              
                                                                 
    MOVE LOW-VALUES TO CERT-NOTE-ENTRIES                         
    SET TB-INDX TO 1                                             
                                                                 
    MOVE 'FORCED FOR AUTO CANCEL' TO   CERT-NT-TEXT (TB-INDX)    
    MOVE 'SYST'            TO CERT-NT-LAST-MAINT-BY (TB-INDX)    
    MOVE WS-CURRENT-BIN-DT TO CERT-NT-LAST-MAINT-DT (TB-INDX)    
    MOVE WS-CURRENT-TIME   TO                                    
                          CERT-NT-LAST-MAINT-HHMMSS (TB-INDX)    
    SET TB-INDX UP BY +1                                         
                                                                 
    MOVE CM-CONTROL-PRIMARY TO CZ-CONTROL-PRIMARY                
    MOVE '1'                TO CZ-RECORD-TYPE                    
    MOVE +0                 TO CZ-NOTE-SEQUENCE                  
                                                                 
    START ERCNOT KEY >= CZ-CONTROL-PRIMARY                       
    IF ERCNOT-FILE-STATUS NOT = '00'                             
       DISPLAY ' ERROR - ERCNOT - START ' ERCNOT-FILE-STATUS     
          ' ' CM-CONTROL-PRIMARY (2:19)                          
       GO TO 0610-TABLE-LOADED                                   
    END-IF                                                       
                                                                 
    IF CM-CONTROL-PRIMARY NOT = CZ-CONTROL-PRIMARY (1:33)        
       GO TO 0610-TABLE-LOADED                                   
    END-IF.                                                      
                                                                 
0610-READ-LOOP.                                                  
                                                                 
    READ ERCNOT NEXT RECORD                                      
    IF ERCNOT-FILE-STATUS = '00' AND                             
       CM-CONTROL-PRIMARY = CZ-CONTROL-PRIMARY (1:33) AND        
       CZ-RECORD-TYPE = '1'                                      
           MOVE CZ-NOTE TO CERT-NT-TEXT (TB-INDX)                
           MOVE CZ-LAST-MAINT-USER TO                            
                   CERT-NT-LAST-MAINT-BY (TB-INDX)               
           MOVE CZ-LAST-MAINT-DT TO                              
                   CERT-NT-LAST-MAINT-DT (TB-INDX)               
           MOVE CZ-LAST-MAINT-HHMMSS TO                          
                  CERT-NT-LAST-MAINT-HHMMSS (TB-INDX)            
           SET TB-INDX UP BY 1                                   
                                                                 
           DELETE ERCNOT                                         
           IF ERCNOT-FILE-STATUS NOT = '00'                      
               DISPLAY ' ERROR - ERCNOT - DELETE '               
                 ERCNOT-FILE-STATUS ' - ' CZ-CERT-NO             
               GO TO 0610-EXIT                                   
           END-IF                                                
           GO TO 0610-READ-LOOP                                  
    END-IF.                                                      
                                                                 
