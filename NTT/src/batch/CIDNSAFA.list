    IF ELCERT-FILE-STATUS = '00'                                 
       MOVE 'Y'                 TO WS-CERT-FOUND-SW              
    ELSE                                                         
       DISPLAY ' ERROR ON ELCERT - READ - 0250 '                 
          ELCERT-FILE-STATUS                                     
    END-IF.                                                      
                                                                 
