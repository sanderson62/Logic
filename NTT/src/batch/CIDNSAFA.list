    MOVE WS-CURRENT-DATE TO RPT-DATE.                            
    MOVE +1 TO WS-PAGE-CNT.                                      
    MOVE WS-PAGE-CNT TO RPT-PAGE.                                
    WRITE REPORT-RECORD FROM REPORT-HEAD1.                       
    WRITE REPORT-RECORD FROM REPORT-HEAD2.                       
    WRITE REPORT-RECORD FROM BLANK-LINE.                         
