       AND (CP-STATE-STD-ABBRV EQUAL 'SC')                       
       AND (CP-BENEFIT-CD = '2I' OR '2J' OR '2K' OR '2L')        
       AND (CP-EARN-AS-NET-PAY)                                  
       GO TO 1075-BYPASS-REGULAR-RATE                            
    END-IF                                                       
                                                                 
*>*****************************************************            
*>    IF CP-COMPANY-ID NOT = 'LBL'                                 
    IF (CP-COMPANY-ID NOT = 'LBL' AND 'CID')                     
                  or                                             
       ((cp-company-id = 'CID')                                  
       and (cp-earn-as-reg-balloon)                              
       and (cp-ben-category = 'P' or 'I'))                       
        GO TO 1075-BYPASS-REGULAR-RATE.                          
                                                                 
    MOVE CP-CERT-EFF-DT         TO DC-BIN-DATE-1.                
    MOVE CP-FIRST-PAY-DATE      TO DC-BIN-DATE-2.                
    MOVE '1'                    TO DC-OPTION-CODE.               
    PERFORM 9100-CONVERT-DATE THRU 9100-EXIT.                    
    IF DATE-CONVERSION-ERROR                                     
        MOVE '2'                TO CP-RETURN-CODE                
        GO TO 9999-CALC-RATE-PREM-X                              
      ELSE                                                       
        MOVE DC-ELAPSED-MONTHS  TO CP-MNTHS-TO-FIRST-PMT         
        MOVE DC-ODD-DAYS-OVER   TO CP-ODD-DAYS-TO-PMT.           
                                                                 
    IF CP-MNTHS-TO-FIRST-PMT GREATER THAN 1                      
       COMPUTE CP-ODD-DAYS-TO-PMT EQUAL CP-ODD-DAYS-TO-PMT +     
       ((CP-MNTHS-TO-FIRST-PMT - 1) * +30).                      
                                                                 
    IF (CP-EXT-CHG-LF OR CP-EXT-CHG-LF-AH)                       
       IF (CP-ODD-DAYS-TO-PMT NOT = ZEROS)                       
          NEXT SENTENCE                                          
        ELSE                                                     
          GO TO 1075-BYPASS-REGULAR-RATE                         
      ELSE                                                       
       IF (CP-MNTHS-TO-FIRST-PMT = ZERO) AND                     
          (CP-ODD-DAYS-TO-PMT NOT = ZEROS)                       
          NEXT SENTENCE                                          
        ELSE                                                     
          GO TO 1075-BYPASS-REGULAR-RATE.                        
                                                                 
    IF CP-MNTHS-TO-FIRST-PMT GREATER ZERO                        
       COMPUTE CP-PREMIUM-RATE =                                 
       ((CP-PREMIUM-RATE / CP-ORIGINAL-TERM                      
        * (CP-ORIGINAL-TERM + 1)) - CP-PREMIUM-RATE)             
        * (CP-ODD-DAYS-TO-PMT * 12 / 365) + CP-PREMIUM-RATE.     
                                                                 
    IF CP-MNTHS-TO-FIRST-PMT = ZEROS                             
       COMPUTE CP-PREMIUM-RATE =                                 
       (CP-PREMIUM-RATE - (CP-PREMIUM-RATE /                     
       CP-ORIGINAL-TERM * (CP-ORIGINAL-TERM - 1))) *             
       (CP-ODD-DAYS-TO-PMT * 12 / 365) +                         
       (CP-PREMIUM-RATE / CP-ORIGINAL-TERM                       
        * (CP-ORIGINAL-TERM - 1)).                               
*>*****************************************************            
                                                                 
1075-BYPASS-REGULAR-RATE.                                        
                                                                 
    MOVE CP-LOAN-TERM              TO WS-RATE-TERM.              
                                                                 
    MOVE +1 TO LINDX.                                            
                                                                 
    IF CP-COMPANY-ID EQUAL 'CRI'                                 
       IF CP-SIG-SWITCH EQUAL 'S' OR 'J'                         
          MOVE +5               TO LINDX                         
          IF RT-L-EX-AGE (LINDX) EQUAL ZERO                      
             MOVE 'F'           TO CP-RETURN-CODE                
             GO TO 1200-CALCULATE-PREMIUMS.                      
                                                                 
    IF RT-L-EX-AGE (LINDX) = ZERO                                
         GO TO 1200-CALCULATE-PREMIUMS.                          
                                                                 
1100-SEARCH-LIFE-LIMITS.                                         
                                                                 
    IF CP-COMPANY-ID EQUAL 'CRI'                                 
       IF CP-SIG-SWITCH NOT EQUAL 'S' AND 'J'                    
          IF LINDX GREATER THAN +4                               
             MOVE 'A'           TO CP-RETURN-CODE                
             GO TO 1200-CALCULATE-PREMIUMS.                      
                                                                 
    IF LINDX GREATER +8                                          
        MOVE 'A'                TO CP-RETURN-CODE                
        GO TO 1200-CALCULATE-PREMIUMS.                           
                                                                 
    if rt-l-min-term(lindx) not numeric                          
       move +0                  to rt-l-min-term(lindx)          
    end-if                                                       
                                                                 
    IF CP-ISSUE-AGE GREATER RT-L-EX-AGE (LINDX)                  
        IF RT-L-EX-AGE (LINDX) = ZEROS                           
            ADD +1              TO LINDX                         
            GO TO 1100-SEARCH-LIFE-LIMITS                        
        ELSE                                                     
            MOVE RT-L-EX-AGE (LINDX)                             
                                TO CP-R-MAX-AGE                  
            MOVE RT-L-EX-TERM (LINDX)                            
                                TO CP-R-MAX-TERM                 
            MOVE RT-L-EX-FACE (LINDX)                            
                                TO CP-R-MAX-TOT-BEN              
            ADD +1              TO LINDX                         
            GO TO 1100-SEARCH-LIFE-LIMITS                        
    ELSE                                                         
        MOVE RT-L-EX-AGE (LINDX)                                 
                                TO CP-R-MAX-AGE                  
        MOVE RT-L-EX-TERM (LINDX)                                
                                TO CP-R-MAX-TERM                 
        MOVE RT-L-EX-FACE (LINDX)                                
                                TO CP-R-MAX-TOT-BEN.             
                                                                 
    IF CP-ORIGINAL-TERM GREATER RT-L-EX-TERM (LINDX)             
        MOVE '9'                TO CP-RETURN-CODE.               
                                                                 
    IF CP-ORIGINAL-BENEFIT GREATER RT-L-EX-FACE (LINDX)          
        MOVE '8'                TO CP-RETURN-CODE.               
                                                                 
    if rt-l-min-term(lindx) <> zeros                             
       and CP-loan-term     < RT-L-min-TERM (LINDX)              
       MOVE 'I'                 TO CP-RETURN-CODE                
    end-if                                                       
                                                                 
    .                                                            
1200-CALCULATE-PREMIUMS.                                         
                                                                 
    IF CP-COMPANY-ID = 'CSL'                                     
       IF CP-CSL-VALID-NP-BENEFIT-CD                             
          GO TO 1240-NET-PAY-CONT                                
       ELSE                                                      
          GO TO 1250-CSL-CALC.                                   
                                                                 
    IF CP-COMPANY-ID = 'WDS'                                     
       IF CP-STATE-STD-ABBRV = 'MN' OR 'NC'                      
          IF CP-EARN-AS-REG-BALLOON                              
             GO TO 1240-NET-PAY-CONT.                            
                                                                 
    IF (CP-COMPANY-ID = 'CID')                                   
       AND (CP-STATE-STD-ABBRV = 'MN')                           
       AND (CP-EARN-AS-REG-BALLOON OR CP-EARN-AS-FARM-PLAN)      
       AND (CP-CERT-EFF-DT > X'A4FF')                            
*>       display ' made it to mn 2 '                               
       GO TO 1240-NET-PAY-CONT                                   
    END-IF                                                       
                                                                 
    IF (CP-COMPANY-ID = 'CID')                                   
       AND (CP-STATE-STD-ABBRV = 'WA' or 'AL')                   
       AND (CP-FARM-PLAN)                                        
       GO TO 1240-NET-PAY-CONT                                   
    END-IF                                                       
                                                                 
                                                                 
    if (cp-company-id = 'CID')                                   
       and (cp-earn-as-reg-balloon)                              
       and (cp-ben-category = 'P' or 'I')                        
       display ' found balloon p or i ' rt-discount-ob-rate      
       evaluate true                                             
          when (cp-net-only-state = 'Y')                         
             and (rt-discount-ob-rate > zeros)                   
             compute cp-premium-rate = rt-discount-ob-rate / 10  
          when (cp-net-only-state = 'Y')                         
             and (rt-discount-ob-rate = zeros)                   
             display ' not ob rate     ' cp-premium-rate         
             compute cp-premium-rate =                           
                cp-premium-rate / cp-original-term * 12          
             display ' new rate ' cp-premium-rate                
             display ' net only state ' cp-premium-rate          
             compute cp-premium-rate rounded =                   
               (cp-premium-rate *                                
                  (cp-original-term + 1) / 12) /                 
               ((cp-original-term + 2) / 2)                      
             display ' new OB rate ' cp-premium-rate             
          when (cp-net-only-state <> 'Y')                        
             and (rt-discount-ob-rate = zeros)                   
             display ' not net only st ' cp-premium-rate         
             compute cp-premium-rate =                           
                cp-premium-rate / cp-original-term * 12          
             display ' new rate ' cp-premium-rate                
       end-evaluate                                              
       go to 1240-net-pay-cont                                   
    end-if                                                       
                                                                 
    IF (CP-COMPANY-ID = 'CID')                                   
       AND (CP-STATE-STD-ABBRV = 'ND' OR 'MT' OR 'NJ'            
          OR 'IN' OR 'AZ')                                       
       AND (CP-FARM-PLAN)                                        
*>**  OK, I DID THIS SO ERCNETP WOULDN'T ERROR OUT                 
*>**  THE APR ISN'T EVEN USED IN THE ROUTINE, SO SUE ME :)         
       MOVE +1.0                TO CP-LOAN-APR                   
       GO TO 1240-NET-PAY-CONT                                   
    END-IF                                                       
                                                                 
    IF CP-RATE-AS-STANDARD                                       
        GO TO 1210-REGULAR-RATE.                                 
                                                                 
    IF CP-CRITICAL-PERIOD                                        
        GO TO 1220-CRITICAL-PERIOD-LF-CONT.                      
                                                                 
    IF CP-EARN-AS-TEXAS                                          
        GO TO 1230-TEXAS-CONT.                                   
                                                                 
    IF CP-EARN-AS-NET-PAY                                        
       IF CP-STATE-STD-ABBRV = 'MD'                              
         IF CP-COMPANY-ID = 'MON'                                
           IF CP-BENEFIT-CD = '11' OR '12' OR '13' OR            
                              '14' OR '26' OR '47'               
             GO TO 1210-REGULAR-RATE.                            
                                                                 
    IF CP-SPECIAL-CALC-CD EQUAL 'H'                              
       IF CP-MONTHLY-PAYMENT EQUAL +0                            
          COMPUTE CP-MONTHLY-PAYMENT EQUAL                       
               CP-ORIGINAL-BENEFIT / CP-ORIGINAL-TERM.           
                                                                 
    IF CP-COMPANY-ID EQUAL 'GTL'                                 
       NEXT SENTENCE                                             
    ELSE                                                         
    IF CP-SPECIAL-CALC-CD EQUAL 'H'                              
       IF CP-STATE-STD-ABBRV NOT EQUAL 'NY'                      
          COMPUTE WK3 ROUNDED EQUAL                              
          CP-ORIGINAL-TERM * (CP-ORIGINAL-TERM - +1) *           
          CP-MONTHLY-PAYMENT                                     
          COMPUTE WK3 ROUNDED EQUAL WK3 / 2                      
          COMPUTE WK3 ROUNDED EQUAL WK3 + (CP-ORIGINAL-TERM *    
          CP-ALTERNATE-BENEFIT)                                  
          COMPUTE CP-CALC-PREMIUM ROUNDED EQUAL                  
          WK3 * WK5                                              
          GO TO 9999-CALC-RATE-PREM-X                            
       ELSE                                                      
          COMPUTE WK3 ROUNDED EQUAL (CP-MONTHLY-PAYMENT *        
          (CP-ORIGINAL-TERM - 1 - WK1)) /                        
          (WS-WORK-DIS-RATE / +12)                               
          COMPUTE WK3 EQUAL WK3 + (CP-ALTERNATE-BENEFIT * WK2)   
          COMPUTE CP-CALC-PREMIUM ROUNDED EQUAL WK3 *            
                              WK5                                
          GO TO 9999-CALC-RATE-PREM-X.                           
                                                                 
    IF CP-COMPANY-ID EQUAL 'GTL' AND                             
       CP-STATE-STD-ABBRV EQUAL 'RI' AND                         
       CP-SPECIAL-CALC-CD EQUAL 'H' AND                          
       CP-ORIGINAL-TERM LESS THAN +61                            
       COMPUTE CP-CALC-PREMIUM EQUAL                             
                       (CP-ORIGINAL-BENEFIT *                    
          (CP-ORIGINAL-TERM + 1) * CP-PREMIUM-RATE) /            
          (20 * (1 + .0019 * CP-ORIGINAL-TERM) * 100)            
       COMPUTE CP-CALC-PREMIUM EQUAL                             
               CP-CALC-PREMIUM +                                 
             (CP-ALTERNATE-BENEFIT * CP-PREMIUM-RATE *           
       (CP-ORIGINAL-TERM + 1)) /                                 
       (10 * (1 + .0027 * (CP-ORIGINAL-TERM + 1)) * 100)         
       GO TO 9999-CALC-RATE-PREM-X.                              
                                                                 
    IF CP-COMPANY-ID = 'GTL'     AND                             
       CP-STATE-STD-ABBRV = 'PA' AND                             
       CP-SPECIAL-CALC-CD = 'H'                                  
        GO TO 1260-GTL-PA-LEASE.                                 
                                                                 
    IF (CP-EARN-AS-NET-PAY)                                      
*>       AND (CP-LOAN-APR > ZEROS)                                 
       GO TO 1240-NET-PAY-CONT                                   
    END-IF                                                       
                                                                 
    IF CP-LIFE-OVERRIDE-CODE NOT = 'L'                           
        GO TO 1210-REGULAR-RATE.                                 
                                                                 
*>*** CHECK TO SEE IF THE LEVEL PORTION OF A BALLOON IS BEING      
*>*** RATED AND BYPASS NET-PAY CHECK IF NO APR HAS BEEN ENTERED.   
*>***                                                              
    IF CP-DEVIATION-CODE = 'LEV' AND                             
       CP-LOAN-APR = ZEROS                                       
        GO TO 1210-REGULAR-RATE.                                 
                                                                 
    IF (CP-STATE-STD-ABBRV = 'OH')                               
       AND (CP-CLASS-CODE NOT = 'L ')                            
       AND (CP-ORIGINAL-TERM > 60)                               
       AND (DC-GREG-DATE-CYMD > 19831031)                        
       AND (CP-LOAN-APR > ZEROS)                                 
       GO TO 1240-NET-PAY-CONT                                   
    END-IF                                                       
                                                                 
    IF (CP-STATE-STD-ABBRV = 'UT')                               
       AND (CP-ORIGINAL-TERM > 62)                               
       AND (DC-GREG-DATE-CYMD > 19810831)                        
       AND (DC-GREG-DATE-CYMD < 19830901)                        
       AND (CP-LOAN-APR > ZEROS)                                 
       GO TO 1240-NET-PAY-CONT                                   
    END-IF                                                       
                                                                 
    IF (CP-STATE-STD-ABBRV = 'RI')                               
       AND (CP-ORIGINAL-TERM > 60)                               
       AND (DC-GREG-DATE-CYMD > 19831231)                        
       AND (CP-LOAN-APR > ZEROS)                                 
       GO TO 1240-NET-PAY-CONT                                   
    END-IF                                                       
                                                                 
    .                                                            
1210-REGULAR-RATE.                                               
                                                                 
    IF (CP-COMPANY-ID = 'CID')                                   
       AND (CP-EARN-AS-NET-PAY)                                  
       AND (CP-LOAN-APR = 0)                                     
       IF RT-L-RATE (WS-RATE-TERM) = 0                           
          COMPUTE WS-SIN-PREM-RATE =                             
             (RT-DISCOUNT-OB-RATE / +1000) *                     
             ((WS-RATE-TERM + +1) / +2)                          
          COMPUTE CP-PREMIUM-RATE =                              
             WS-SIN-PREM-RATE /                                  
             (+1 + (((WS-WORK-DIS-RATE / +100) *                 
             WS-RATE-TERM) / +24))                               
       ELSE                                                      
          MOVE RT-L-RATE (WS-RATE-TERM) TO CP-PREMIUM-RATE       
       END-IF                                                    
    END-IF                                                       
                                                                 
*>    IF CP-SPECIAL-CALC-CD EQUAL 'H'                              
*>       IF CP-MONTHLY-PAYMENT EQUAL +0                            
*>          COMPUTE CP-MONTHLY-PAYMENT EQUAL                       
*>               CP-ORIGINAL-BENEFIT / CP-ORIGINAL-TERM.           
*>                                                                 
*>    IF CP-SPECIAL-CALC-CD EQUAL 'H'                              
*>       IF CP-STATE-STD-ABBRV NOT EQUAL 'NY'                      
*>          COMPUTE WK3 ROUNDED EQUAL                              
*>          CP-ORIGINAL-TERM * (CP-ORIGINAL-TERM - +1) *           
*>          CP-MONTHLY-PAYMENT                                     
*>          COMPUTE WK3 ROUNDED EQUAL WK3 / 2                      
*>          COMPUTE WK3 ROUNDED EQUAL WK3 + (CP-ORIGINAL-TERM *    
*>          CP-ALTERNATE-BENEFIT)                                  
*>          COMPUTE CP-CALC-PREMIUM ROUNDED EQUAL                  
*>          WK3 * WK5                                              
*>          GO TO 9999-CALC-RATE-PREM-X                            
*>       ELSE                                                      
*>          COMPUTE WK3 ROUNDED EQUAL (CP-MONTHLY-PAYMENT *        
*>          (CP-ORIGINAL-TERM - 1 - WK1)) /                        
*>          (WS-WORK-DIS-RATE / +12)                               
*>          COMPUTE WK3 EQUAL WK3 + (CP-ALTERNATE-BENEFIT * WK2)   
*>          COMPUTE CP-CALC-PREMIUM ROUNDED EQUAL WK3 *            
*>                              WK5                                
*>          GO TO 9999-CALC-RATE-PREM-X.                           
                                                                 
    IF CP-TERM-IS-DAYS                                           
        COMPUTE CP-PREMIUM-RATE ROUNDED =                        
            ((CP-PREMIUM-RATE / CP-ORIGINAL-TERM) * 12) / +365   
        COMPUTE CP-CALC-PREMIUM ROUNDED =                        
            ((CP-ORIGINAL-BENEFIT / 100) * CP-PREMIUM-RATE)      
                         * CP-TERM-OR-EXT-DAYS                   
          GO TO 9999-CALC-RATE-PREM-X.                           
                                                                 
    if (cp-company-id = 'AHL')                                   
       and (cp-loan-term > cp-original-term)                     
       continue                                                  
    else                                                         
       IF NOT CP-TRUNCATED-LIFE                                  
          GO TO 1212-GROSS-NOT-TRUNCATED                         
       end-if                                                    
    end-if                                                       
                                                                 
*>***  GROSS TRUNCATED CALCULATION                                 
                                                                 
    IF CP-LOAN-TERM NOT GREATER THAN CP-ORIGINAL-TERM            
        MOVE 'H'                TO  CP-RETURN-CODE               
        GO TO 9999-CALC-RATE-PREM-X.                             
                                                                 
    if cp-company-id = 'AHL'                                     
       if (cp-state = 'AZ' OR 'GA' OR 'IN' OR 'MT' OR 'NH'       
          OR 'NJ' OR 'OH' OR 'PA' OR 'TX' OR 'VA')               
          compute cp-calc-premium rounded = (1 - (cp-loan-term - 
          cp-original-term) * (cp-loan-term - cp-original-term   
           + 1) / cp-loan-term / (cp-loan-term + 1)) *           
           cp-premium-rate / 100 * cp-original-term /            
           cp-loan-term * cp-original-benefit                    
       else                                                      
          compute cp-calc-premium rounded = (1 - (cp-loan-term - 
          cp-original-term) * (cp-loan-term - cp-original-term   
           + 1) / cp-loan-term / (cp-loan-term + 1)) *           
           cp-premium-rate * cp-original-benefit / 100           
       end-if                                                    
       go to 9999-calc-rate-prem-x                               
    end-if                                                       
                                                                 
                                                                 
    IF CP-COMPANY-ID = 'NCL'                                     
        COMPUTE WK-MONTHLY-RATE =                                
              ((((CP-LOAN-TERM * (CP-LOAN-TERM + 1) -            
                 (CP-LOAN-TERM - CP-ORIGINAL-TERM) *             
                 (CP-LOAN-TERM - CP-ORIGINAL-TERM + 1))          
                                /                                
                 (CP-LOAN-TERM * (CP-LOAN-TERM + 1))))           
                                *                                
                 ((RT-L-RATE (12) * CP-LOAN-TERM / 12)))         
        COMPUTE CP-CALC-PREMIUM =                                
                WK-MONTHLY-RATE * CP-ORIGINAL-BENEFIT / 100      
    ELSE                                                         
        COMPUTE CP-R-MAX-TERM = CP-LOAN-TERM -                   
                                CP-ORIGINAL-TERM                 
        COMPUTE CP-CALC-PREMIUM = CP-ORIGINAL-BENEFIT *          
                    ((RT-L-RATE (CP-LOAN-TERM)  -                
                    (1 - (CP-ORIGINAL-TERM / CP-LOAN-TERM)) *    
                    RT-L-RATE (CP-R-MAX-TERM)) / 100)            
    END-IF                                                       
                                                                 
    IF CP-EXT-CHG-LF OR CP-EXT-CHG-LF-AH                         
       COMPUTE CP-CALC-PREMIUM ROUNDED = CP-CALC-PREMIUM +       
          (((CP-ORIGINAL-BENEFIT / 100) * ((CP-PREMIUM-RATE /    
          CP-ORIGINAL-TERM) / 30)) * CP-TERM-OR-EXT-DAYS)        
    END-IF                                                       
                                                                 
    GO TO 9999-CALC-RATE-PREM-X                                  
                                                                 
    .                                                            
1212-GROSS-NOT-TRUNCATED.                                        
*>    DISPLAY ' ELCRATPD '                                         
*>    DISPLAY ' MADE IT TO 1212-GROSS-NOT '                        
*>    display ' premium rate ' cp-premium-rate                     
                                                                 
    IF (CP-COMPANY-ID = 'CID')                                   
       AND (CP-STATE-STD-ABBRV = 'VT' OR 'MA'                    
          OR 'IN')                                               
       AND (CP-BENEFIT-CD = '55' OR '56')                        
       COMPUTE CP-CALC-PREMIUM ROUNDED =                         
         (CP-ORIGINAL-BENEFIT / (CP-ORIGINAL-TERM)) *            
         (CP-ORIGINAL-TERM) *                                    
         ((CP-ORIGINAL-TERM) / 12) *                             
         ((6.5 * RT-DISCOUNT-OB-RATE) / 1000)                    
         GO TO 9999-CALC-RATE-PREM-X                             
    END-IF                                                       
                                                                 
    IF (CP-COMPANY-ID = 'CID')                                   
       AND (CP-STATE-STD-ABBRV = 'GA')                           
       AND (CP-BENEFIT-CD = '55' OR '56' OR '2M' OR '2N')        
       COMPUTE WK3 ROUNDED =                                     
         CP-ORIGINAL-BENEFIT *                                   
         ((CP-ORIGINAL-TERM + 1) / 2) *                          
         (RT-DISCOUNT-OB-RATE / 1000)                            
         MOVE WK3 TO CP-CALC-PREMIUM                             
         GO TO 9999-CALC-RATE-PREM-X                             
    END-IF                                                       
                                                                 
    IF (CP-COMPANY-ID EQUAL 'CID')                               
       AND (CP-STATE-STD-ABBRV = 'MN')                           
       AND (CP-LEVEL-LIFE)                                       
       AND (CP-CERT-EFF-DT > X'A4FF')                            
*>       display ' made it to mn level '                           
       MOVE CP-CERT-EFF-DT      TO DC-BIN-DATE-1                 
       MOVE CP-EXPIRE-DT        TO DC-BIN-DATE-2                 
       MOVE '1'                 TO DC-OPTION-CODE                
       PERFORM 9100-CONVERT-DATE                                 
                                THRU 9100-EXIT                   
       IF DATE-CONVERSION-ERROR                                  
          MOVE '2'              TO CP-RETURN-CODE                
          MOVE ZEROS            TO FACTOR                        
                                CP-CALC-PREMIUM                  
          GO TO 9999-CALC-RATE-PREM-X                            
       END-IF                                                    
       MOVE DC-ELAPSED-DAYS     TO D                             
       COMPUTE wk-pay-amt = CP-ORIGINAL-BENEFIT /                
          (1 / (1 + (CP-LOAN-APR / 100) * D / 360) -             
          (CP-PREMIUM-RATE * 12 / 365 * D / 1000))               
       COMPUTE CP-CALC-PREMIUM = wk-pay-amt *                    
          CP-PREMIUM-RATE * 12 / 365 * D / 1000                  
       GO TO 9999-CALC-RATE-PREM-X                               
    END-IF                                                       
                                                                 
    IF (CP-COMPANY-ID = 'CID')                                   
       AND (CP-BENEFIT-CD (1:1) = 'M' OR 'N' OR 'H')             
       COMPUTE CP-CALC-PREMIUM ROUNDED =                         
          (CP-ORIGINAL-BENEFIT / 1000) * CP-PREMIUM-RATE         
    ELSE                                                         
       COMPUTE CP-CALC-PREMIUM ROUNDED =                         
                (CP-ORIGINAL-BENEFIT / 100) * CP-PREMIUM-RATE    
    END-IF                                                       
*>    display ' calc premium ' cp-calc-premium                     
                                                                 
*>    IF CP-COMPANY-ID = 'LBL'                                     
    IF (CP-COMPANY-ID = 'LBL' OR 'CID')                          
       AND (NOT CP-EARN-AS-NET-PAY)                              
       GO TO 9999-CALC-RATE-PREM-X                               
    END-IF                                                       
                                                                 
*>    display ' fell through to ext days '                         
    IF CP-TERM-OR-EXT-DAYS = ZEROS                               
        GO TO 9999-CALC-RATE-PREM-X.                             
                                                                 
    IF CP-EXT-CHG-LF OR CP-EXT-CHG-LF-AH                         
        COMPUTE CP-CALC-PREMIUM ROUNDED = CP-CALC-PREMIUM +      
           (((CP-ORIGINAL-BENEFIT / 100) * ((CP-PREMIUM-RATE /   
              CP-ORIGINAL-TERM) / 30)) * CP-TERM-OR-EXT-DAYS).   
                                                                 
    GO TO 9999-CALC-RATE-PREM-X.                                 
                                                                 
1220-CRITICAL-PERIOD-LF-CONT.                                    
    COMPUTE CP-CALC-PREMIUM ROUNDED =                            
              (CP-ORIGINAL-BENEFIT / +100) * CP-PREMIUM-RATE.    
                                                                 
    GO TO 9999-CALC-RATE-PREM-X.                                 
                                                                 
1230-TEXAS-CONT.                                                 
    IF CP-FARM-PLAN                                              
        GO TO 1235-TEXAS-FARM-CONT.                              
                                                                 
    COMPUTE CP-CALC-PREMIUM ROUNDED =                            
              (CP-ORIGINAL-BENEFIT / 100) * CP-PREMIUM-RATE.     
                                                                 
    COMPUTE TEX-FACT-9 =                                         
                (CP-ORIGINAL-TERM + CP-PAY-FREQUENCY) /          
                (CP-ORIGINAL-TERM + 1).                          
                                                                 
    COMPUTE CP-CALC-PREMIUM ROUNDED =                            
                CP-CALC-PREMIUM * TEX-FACT-9.                    
                                                                 
    GO TO 9999-CALC-RATE-PREM-X.                                 
                                                                 
1235-TEXAS-FARM-CONT.                                            
    COMPUTE WS-FARM-TERM =                                       
              (CP-ORIGINAL-TERM + CP-PAY-FREQUENCY).             
                                                                 
    COMPUTE WS-PREMIUM-RATE = RT-L-RATE (WS-FARM-TERM) / 2.      
    MOVE WS-PREMIUM-RATE         TO CP-PREMIUM-RATE.             
                                                                 
                                                                 
    COMPUTE CP-CALC-PREMIUM ROUNDED =                            
             (CP-ORIGINAL-BENEFIT / 100) * CP-PREMIUM-RATE.      
                                                                 
