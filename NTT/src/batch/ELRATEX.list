1250-CSL-CALC.                                                   
    MOVE CP-CERT-EFF-DT TO DC-BIN-DATE-1.                        
    MOVE CP-FIRST-PAY-DATE TO DC-BIN-DATE-2.                     
    MOVE '1' TO DC-OPTION-CODE.                                  
    PERFORM 9100-CONVERT-DATE THRU 9100-EXIT.                    
                                                                 
    IF DATE-CONVERSION-ERROR                                     
        MOVE '2' TO CP-RETURN-CODE                               
        GO TO 9999-CALC-RATE-PREM-X.                             
                                                                 
    IF CP-JOINT-INDICATOR = 'J'                                  
        MOVE RT-L-RATE (3) TO WK-RED-RATE                        
    ELSE                                                         
        MOVE RT-L-RATE (1) TO WK-RED-RATE.                       
                                                                 
    MOVE WK-RED-RATE TO CP-PREMIUM-RATE.                         
    MOVE CP-REMAINING-BENEFIT TO WK-PAY-AMT.                     
                                                                 
    IF CP-PAY-FREQUENCY = 12                                     
        MOVE 01               TO WS-PAY-SCH.                     
    IF CP-PAY-FREQUENCY = 04                                     
        MOVE 03               TO WS-PAY-SCH.                     
    IF CP-PAY-FREQUENCY = 02                                     
        MOVE 06               TO WS-PAY-SCH.                     
    IF CP-PAY-FREQUENCY = 01                                     
        MOVE 12               TO WS-PAY-SCH.                     
                                                                 
    COMPUTE WS-NUM-PAYMTS = CP-ORIGINAL-TERM / WS-PAY-SCH.       
    IF CP-PAY-FREQUENCY NOT = 12                                 
        COMPUTE WK-PAY-AMT = CP-REMAINING-BENEFIT /              
                               WS-NUM-PAYMTS.                    
                                                                 
    IF CP-STATE = 'SC'  AND                                      
      (CP-BENEFIT-CD = '07'  OR  '08')                           
        GO TO 1250-CSL-CALC-LIFE-6.                              
                                                                 
    IF CP-CSL-METH-1                                             
        GO TO 1250-CSL-CALC-LIFE-1.                              
                                                                 
    IF CP-CSL-METH-2                                             
        IF (WK-PAY-AMT * CP-LOAN-TERM) GREATER THAN              
                         WS-MAX-TOT-BEN                          
            GO TO 1250-CSL-CALC-LIFE-2                           
        ELSE                                                     
            GO TO 1250-CSL-CALC-LIFE-1.                          
                                                                 
    IF CP-CSL-METH-3                                             
        IF WK-PAY-AMT GREATER THAN WS-MAX-MON-BEN                
            GO TO 1250-CSL-CALC-LIFE-2                           
        ELSE                                                     
            GO TO 1250-CSL-CALC-LIFE-3.                          
                                                                 
    IF CP-CSL-METH-4                                             
        IF (WK-PAY-AMT * WS-NUM-PAYMTS) GREATER THAN             
                         WS-MAX-TOT-BEN                          
            GO TO 1250-CSL-CALC-LIFE-5                           
        ELSE                                                     
            GO TO 1250-CSL-CALC-LIFE-4.                          
                                                                 
1250-CSL-CALC-LIFE-1.                                            
    IF CP-JOINT-INDICATOR = 'J'                                  
        MOVE RT-L-RATE (4) TO WK-LEV-RATE                        
    ELSE                                                         
        MOVE RT-L-RATE (2) TO WK-LEV-RATE.                       
                                                                 
    COMPUTE WK-D = (((DC-ELAPSED-DAYS * CP-PAY-FREQUENCY) /      
                   365) - 1) * 365 / 12.                         
    COMPUTE WK-S ROUNDED = ((144 * WK-D * WK-LEV-RATE) +         
                   (365 * WS-NUM-PAYMTS * WK-RED-RATE)) /        
                   4380.                                         
    COMPUTE CP-CALC-PREMIUM ROUNDED = WK-PAY-AMT *               
                   WS-NUM-PAYMTS * WK-S.                         
                                                                 
    GO TO 9999-CALC-RATE-PREM-X.                                 
                                                                 
1250-CSL-CALC-LIFE-2.                                            
    IF CP-JOINT-INDICATOR = 'J'                                  
        MOVE RT-L-RATE (4) TO WK-LEV-RATE                        
    ELSE                                                         
        MOVE RT-L-RATE (2) TO WK-LEV-RATE.                       
                                                                 
    COMPUTE WK-K = WS-MAX-TOT-BEN / WK-PAY-AMT.                  
    COMPUTE WK-S ROUNDED = (WK-K * WK-RED-RATE) / 12.            
    COMPUTE WK-D = (((DC-ELAPSED-DAYS * CP-PAY-FREQUENCY / 365)  
                   - 1) + WS-NUM-PAYMTS - WK-K) *                
                   (WK-LEV-RATE * WS-MAX-TOT-BEN).               
    COMPUTE CP-CALC-PREMIUM ROUNDED = (WK-PAY-AMT * WK-K *       
                   WK-S) + WK-D.                                 
                                                                 
    GO TO 9999-CALC-RATE-PREM-X.                                 
                                                                 
1250-CSL-CALC-LIFE-3.                                            
    IF CP-JOINT-INDICATOR = 'J'                                  
        MOVE RT-L-RATE (4) TO WK-LEV-RATE                        
    ELSE                                                         
        MOVE RT-L-RATE (2) TO WK-LEV-RATE.                       
                                                                 
    COMPUTE WK-K = WS-MAX-TOT-BEN / WK-PAY-AMT.                  
    COMPUTE WK-S ROUNDED = (WK-K * WK-RED-RATE) / 12.            
    COMPUTE WK-D = (((DC-ELAPSED-DAYS * CP-PAY-FREQUENCY / 365)  
                   - 1) + WS-NUM-PAYMTS - WK-K) *                
                   (WK-LEV-RATE * WS-MAX-TOT-BEN).               
    COMPUTE CP-CALC-PREMIUM ROUNDED = (WK-PAY-AMT * WK-K *       
                   WK-S) + WK-D.                                 
                                                                 
    GO TO 9999-CALC-RATE-PREM-X.                                 
                                                                 
1250-CSL-CALC-LIFE-4.                                            
    IF CP-JOINT-INDICATOR = 'J'                                  
        MOVE RT-L-RATE (4) TO WK-LEV-RATE                        
    ELSE                                                         
        MOVE RT-L-RATE (2) TO WK-LEV-RATE.                       
                                                                 
    COMPUTE WK-D = DC-ELAPSED-DAYS * 12 / 365.                   
    COMPUTE WK-S ROUNDED =                                       
                   (((6 * (WS-NUM-PAYMTS - 1)) + (WK-D *         
                   CP-PAY-FREQUENCY)) * WK-LEV-RATE) /           
                   CP-PAY-FREQUENCY.                             
    COMPUTE CP-CALC-PREMIUM ROUNDED =  WK-PAY-AMT *              
                   WS-NUM-PAYMTS * WK-S.                         
                                                                 
    GO TO 9999-CALC-RATE-PREM-X.                                 
                                                                 
1250-CSL-CALC-LIFE-5.                                            
    IF CP-JOINT-INDICATOR = 'J'                                  
        MOVE RT-L-RATE (4) TO WK-LEV-RATE                        
    ELSE                                                         
        MOVE RT-L-RATE (2) TO WK-LEV-RATE.                       
                                                                 
    COMPUTE WK-K = WS-MAX-TOT-BEN / WK-PAY-AMT.                  
    COMPUTE WK-S ROUNDED = (6 * (WK-K + 1) * WK-LEV-RATE) /      
                   CP-PAY-FREQUENCY.                             
    COMPUTE WK-U = ((((DC-ELAPSED-DAYS * CP-PAY-FREQUENCY / 365) 
                   - 1) + WS-NUM-PAYMTS) * 12) /                 
                   CP-PAY-FREQUENCY.                             
    COMPUTE WK-L = WK-U - (12 * WK-K / CP-PAY-FREQUENCY).        
    COMPUTE CP-CALC-PREMIUM ROUNDED = (WK-L * WK-LEV-RATE *      
                   WS-MAX-TOT-BEN) + (WK-K * WK-PAY-AMT * WK-S). 
                                                                 
    GO TO 9999-CALC-RATE-PREM-X.                                 
                                                                 
1250-CSL-CALC-LIFE-6.                                            
                                                                 
    COMPUTE CP-PREMIUM-RATE ROUNDED =                            
               (CP-PREMIUM-RATE * CP-ORIGINAL-TERM) / 12.        
                                                                 
    COMPUTE CP-CALC-PREMIUM ROUNDED =                            
                   CP-ORIGINAL-BENEFIT * CP-PREMIUM-RATE.        
                                                                 
    GO TO 9999-CALC-RATE-PREM-X.                                 
                                                                 
