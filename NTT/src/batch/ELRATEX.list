    MOVE CP-LOAN-APR             TO N-P-APR.                     
    MOVE CP-ORIGINAL-TERM        TO N-P-ORIG                     
                                    N-P-REM                      
                                    N-P-LOAN.                    
                                                                 
    MOVE CP-SPECIAL-CALC-CD      TO N-P-OPT.                     
                                                                 
    IF CP-TRUNCATED-LIFE                                         
        MOVE CP-LOAN-TERM        TO N-P-LOAN.                    
                                                                 
    IF CP-COMPANY-ID EQUAL 'NCL'                                 
       NEXT SENTENCE                                             
    ELSE                                                         
       GO TO 1241-NET-PAY-CONT.                                  
                                                                 
    MOVE CP-CERT-EFF-DT         TO DC-BIN-DATE-1                 
    MOVE CP-FIRST-PAY-DATE      TO DC-BIN-DATE-2                 
    MOVE '1'                    TO DC-OPTION-CODE                
    PERFORM 9100-CONVERT-DATE THRU 9100-EXIT.                    
    IF DATE-CONVERSION-ERROR                                     
        MOVE '2'                TO CP-RETURN-CODE                
        GO TO 9999-CALC-RATE-PREM-X                              
    ELSE                                                         
       MOVE DC-ELAPSED-MONTHS   TO CP-MNTHS-TO-FIRST-PMT         
       MOVE DC-ODD-DAYS-OVER    TO CP-ODD-DAYS-TO-PMT.           
                                                                 
    IF (CP-EXT-CHG-LF OR CP-EXT-CHG-LF-AH) AND                   
       (CP-ODD-DAYS-TO-PMT NOT EQUAL ZEROS)                      
       NEXT SENTENCE                                             
    ELSE                                                         
       MOVE +1                  TO ODF                           
       GO TO 1241-NET-PAY-CONT.                                  
                                                                 
    IF CP-MNTHS-TO-FIRST-PMT GREATER THAN ZERO                   
       COMPUTE CP-PREMIUM-RATE EQUAL                             
       ((CP-PREMIUM-RATE / N-P-LOAN * (N-P-LOAN + 1)) -          
       CP-PREMIUM-RATE) *                                        
       (CP-ODD-DAYS-TO-PMT * 12 / 365) + CP-PREMIUM-RATE.        
                                                                 
    IF CP-MNTHS-TO-FIRST-PMT EQUAL ZEROS                         
       COMPUTE CP-PREMIUM-RATE EQUAL                             
       (CP-PREMIUM-RATE - (CP-PREMIUM-RATE /                     
       N-P-LOAN * (N-P-LOAN - 1))) *                             
       (CP-ODD-DAYS-TO-PMT * 12 / 365) +                         
       (CP-PREMIUM-RATE / N-P-LOAN * (N-P-LOAN - 1)).            
                                                                 
    COMPUTE ODF EQUAL                                            
    ((1 + (N-P-APR / +1200)) ** (1 - CP-MNTHS-TO-FIRST-PMT)) /   
    ((1 + (CP-ODD-DAYS-TO-PMT * 12 / 36500))).                   
                                                                 
