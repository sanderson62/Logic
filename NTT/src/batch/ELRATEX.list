    COMPUTE WK4 = WS-WORK-DIS-RATE / 12                          
    COMPUTE WK7 = (1 / (1 + WK4)) ** N                           
    COMPUTE WK8 = (1 / (1 + WK4)) ** (N - 1)                     
    COMPUTE ANGLEN-1 = (1 - WK8) / WK4                           
    COMPUTE CP-CALC-PREMIUM =                                    
         (CP-ORIGINAL-BENEFIT * (1 + ANGLEN-1) *                 
         (CP-PREMIUM-RATE / 1000)) /                             
         (1 - ((1 + ANGLEN-1) * (CP-PREMIUM-RATE / 1000)))       
                                                                 
