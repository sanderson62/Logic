    GO TO 9999-CALC-RATE-PREM-X.                                 
                                                                 
2250-CSL-CALC-AH-3.                                              
                                                                 
    COMPUTE WK-K = WS-MAX-TOT-BEN / WK-PAY-AMT.                  
    COMPUTE WK-T = WK-K + 1.                                     
    COMPUTE WK-MONTHLY-RATE-T =                                  
         RT-AH-RATE (WK-T) / 100.                                
    COMPUTE WK-C = (CP-ORIGINAL-TERM - (WK-T * 2) + 1).          
                                                                 
    COMPUTE WK-T1 = WK-K * (CP-ORIGINAL-TERM - WK-T).            
    IF WK-T1 EQUAL ZEROS                                         
        MOVE ZEROS TO CP-CALC-PREMIUM                            
        MOVE 'G' TO CP-RETURN-CODE                               
        GO TO 9999-CALC-RATE-PREM-X.                             
                                                                 
    COMPUTE WK-C = ((WK-C * WK-MONTHLY-RATE-T) + ((WK-T - 1) *   
                   WK-MONTHLY-RATE)) / (WK-K * (CP-ORIGINAL-TERM 
                   - WK-T)).                                     
    COMPUTE WK-D = (DC-ELAPSED-DAYS * CP-PAY-FREQUENCY / 365)    
    COMPUTE WK-PREM = ((CP-ORIGINAL-TERM -                       
                      (WK-K + 1) + (WK-D - 1))) * (WK-C *        
                      WS-MAX-TOT-BEN).                           
    COMPUTE CP-CALC-PREMIUM ROUNDED = (WK-PAY-AMT * (WK-K + 1) * 
                   WK-MONTHLY-RATE-T) + WK-PREM.                 
                                                                 
    GO TO 9999-CALC-RATE-PREM-X.                                 
                                                                 
2250-CSL-CALC-AH-4.                                              
