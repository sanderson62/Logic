12001-CONTINUE-NCL-TRUNC.                                        
                                                                 
    IF (CP-COMPANY-ID EQUAL 'NCL') AND                           
       (CP-EARN-AS-NET-PAY) AND                                  
       (CP-TRUNCATED-LIFE)                                       
       NEXT SENTENCE                                             
    ELSE                                                         
       GO TO 12001-CONTINUE-NCL.                                 
                                                                 
     COMPUTE ANGLEN EQUAL WK1 / I.                               
     COMPUTE ANGLEN-M EQUAL WK5 / I.                             
                                                                 
     COMPUTE ANGLEN-1 EQUAL ANGLEN * ODF.                        
                                                                 
     COMPUTE WK3 EQUAL (CP-ORIGINAL-BENEFIT * 2 *                
     (M - (ANGLEN - ANGLEN-M)) * (CP-PREMIUM-RATE / +100)) /     
     (ANGLEN-1 * (N + 1) * I).                                   
                                                                 
     MOVE WK3 TO CP-CALC-PREMIUM.                                
                                                                 
    GO TO 99999-RETURN.                                          
                                                                 
12001-CONTINUE-NCL.                                              
                                                                 
    IF (CP-COMPANY-ID EQUAL 'NCL') AND                           
       (CP-STATE-STD-ABBRV EQUAL 'NC') AND                       
       (CP-EARN-AS-NET-PAY)                                      
       GO TO 12001-CHECK-NC-NET-PLUS3.                           
                                                                 
    IF (CP-COMPANY-ID EQUAL 'NCL') AND                           
       (CP-STATE-STD-ABBRV EQUAL 'CA') AND                       
       (CP-EARN-AS-NET-PAY)                                      
       GO TO 12001-CHECK-CA.                                     
                                                                 
    IF (CP-COMPANY-ID EQUAL 'NCL') AND                           
       (CP-EARN-AS-NET-PAY)                                      
       NEXT SENTENCE                                             
    ELSE                                                         
       GO TO 12000-CONTINUE-NET.                                 
                                                                 
     COMPUTE ANGLEN EQUAL WK1 / I.                               
     MOVE ANGLEN                TO ANGLEN-1.                     
                                                                 
     COMPUTE ANGLEN-1 EQUAL ANGLEN * ODF.                        
                                                                 
     COMPUTE WK3 EQUAL (CP-ORIGINAL-BENEFIT *                    
      (CP-PREMIUM-RATE / +100) * 2) / (ANGLEN-1 * (N + 1)) *     
      ((N - ANGLEN) / I).                                        
                                                                 
     MOVE WK3 TO CP-CALC-PREMIUM.                                
                                                                 
    GO TO 99999-RETURN.                                          
                                                                 
12000-CONTINUE-NET.                                              
                                                                 
    IF (CP-COMPANY-ID EQUAL 'TMS') AND                           
       (CP-STATE-STD-ABBRV EQUAL 'MN' OR 'CA' OR 'WA') AND       
       (CP-EARN-AS-NET-PAY)                                      
       NEXT SENTENCE                                             
    ELSE                                                         
       GO TO 12001-CONTINUE-MINN.                                
                                                                 
     COMPUTE ANGLEN EQUAL WK6 / I.                               
     COMPUTE ODF EQUAL 1 + (12 * CP-TERM-OR-EXT-DAYS / +365).    
                                                                 
     COMPUTE WK7 EQUAL (1 + ODF * I) / (1 + ANGLEN).             
     COMPUTE WK3 EQUAL (M  * WK7) - 1.                           
     COMPUTE WK3 EQUAL (WK3 / I) *                               
      ((M * (CP-PREMIUM-RATE / +100)) / (+6 * (M + 1))).         
                                                                 
     IF CP-R-MAX-TOT-BEN NOT NUMERIC                             
        MOVE +0 TO CP-R-MAX-TOT-BEN.                             
                                                                 
     COMPUTE WK3 EQUAL (WK3 * CP-ORIGINAL-BENEFIT)               
                                                                 
     IF (CP-ORIGINAL-BENEFIT GREATER THAN CP-R-MAX-TOT-BEN) AND  
        (CP-R-MAX-TOT-BEN NOT EQUAL +0)                          
        COMPUTE WK3 EQUAL (WK3 * CP-R-MAX-TOT-BEN).              
                                                                 
     MOVE WK3 TO CP-CALC-PREMIUM.                                
                                                                 
    GO TO 99999-RETURN.                                          
                                                                 
12001-CONTINUE-MINN.                                             
                                                                 
    IF (CP-COMPANY-ID EQUAL 'GTL') AND                           
       (CP-STATE-STD-ABBRV EQUAL 'NJ') AND                       
       (CP-TRUNCATED-LIFE)                                       
       NEXT SENTENCE                                             
    ELSE                                                         
       GO TO 12001-CHECK-RI-GTL.                                 
                                                                 
     COMPUTE ANGLEN EQUAL WK1 / I.                               
     COMPUTE ANGLEM EQUAL WK2 / I.                               
                                                                 
     COMPUTE WK3 EQUAL ((M - ANGLEM * SX) /                      
      (I * ANGLEN) * CP-PREMIUM-RATE / (1 + WS-WORK-DIS-RATE     
      * M / 2400) * 1000 + .5) / 1000000.                        
                                                                 
     COMPUTE CP-CALC-PREMIUM EQUAL WK3 * (CP-ORIGINAL-BENEFIT    
      - CP-ORIGINAL-PREMIUM) / (1 - WK3).                        
                                                                 
    GO TO 99999-RETURN.                                          
                                                                 
12001-CHECK-RI-GTL.                                              
                                                                 
    IF (CP-COMPANY-ID EQUAL 'GTL' ) AND                          
       (CP-STATE-STD-ABBRV EQUAL 'RI') AND                       
       (CP-EARN-AS-NET-PAY)                                      
       NEXT SENTENCE                                             
    ELSE                                                         
       GO TO 12001-CHECK-RI-LEASE-GTL.                           
                                                                 
     COMPUTE ANGLEN EQUAL WK1 / I.                               
*>     COMPUTE ANGLEM EQUAL WK2 / I.                               
                                                                 
     COMPUTE WK3 EQUAL (1000 * I * ANGLEN) *                     
              (1 + WS-WORK-DIS-RATE * N).                        
     COMPUTE CP-CALC-PREMIUM EQUAL CP-ORIGINAL-BENEFIT *         
        ((N - ANGLEN) * (1 + 2 * I) * CP-PREMIUM-RATE) / WK3.    
                                                                 
                                                                 
    GO TO 99999-RETURN.                                          
                                                                 
12001-CHECK-RI-LEASE-GTL.                                        
                                                                 
    IF (CP-COMPANY-ID EQUAL 'GTL' ) AND                          
       (CP-STATE-STD-ABBRV EQUAL 'RI') AND                       
       (CP-SPECIAL-CALC-CD EQUAL 'H')                            
       NEXT SENTENCE                                             
    ELSE                                                         
       GO TO 12001-CHECK-OR-GTL.                                 
                                                                 
    COMPUTE ANGLEN EQUAL WK1 / I.                                
                                                                 
    COMPUTE WK3 EQUAL (CP-ORIGINAL-BENEFIT / ANGLEN *            
       (N - ANGLEN) * CP-PREMIUM-RATE * (1 + 2 * I))             
                       /                                         
       (1000 * I * (1 + .0021 * N)).                             
    COMPUTE CP-CALC-PREMIUM EQUAL WK3 +                          
      (CP-ALTERNATE-BENEFIT * CP-PREMIUM-RATE * (N + 1))         
                           /                                     
      (1000 * (1 + .0027 * (N + 1))).                            
                                                                 
    GO TO 99999-RETURN.                                          
                                                                 
12001-CHECK-OR-GTL.                                              
                                                                 
    IF (CP-COMPANY-ID EQUAL 'GTL' ) AND                          
       (CP-STATE-STD-ABBRV EQUAL 'OR') AND                       
       (CP-EARN-AS-NET-PAY)                                      
       NEXT SENTENCE                                             
    ELSE                                                         
       GO TO 12001-CHECK-GA-GTL.                                 
                                                                 
    COMPUTE ANGLEN-M EQUAL WK5 / I.                              
    COMPUTE ANGLEN EQUAL WK1 / I.                                
                                                                 
    IF M LESS THAN +64                                           
       COMPUTE WK3 EQUAL (((N - (N - M)) - (ANGLEN - ANGLEN-M)) *
        (1 + I) * CP-PREMIUM-RATE * (N - (N - M)))               
                  /                                              
        (I * ANGLEN * (N - (N - M) + 1) * 600)                   
       COMPUTE CP-CALC-PREMIUM EQUAL WK3 * CP-ORIGINAL-BENEFIT   
    ELSE                                                         
       COMPUTE WK3 EQUAL (((N - (N - M)) - (ANGLEN - ANGLEN-M)) *
       CP-PREMIUM-RATE * (1 + I))                                
                 /                                               
       (+1000 * I * ANGLEN)                                      
       COMPUTE CP-CALC-PREMIUM EQUAL WK3 * CP-ORIGINAL-BENEFIT.  
*>    MOVE WK3 TO CP-CALC-PREMIUM.                                 
    GO TO 99999-RETURN.                                          
                                                                 
12001-CHECK-GA-GTL.                                              
                                                                 
    IF (CP-COMPANY-ID EQUAL 'GTL') AND                           
       (CP-STATE-STD-ABBRV EQUAL 'GA') AND                       
       (CP-EARN-AS-NET-PAY)                                      
*>       (CP-TRUNCATED-LIFE)                                       
       NEXT SENTENCE                                             
    ELSE                                                         
       GO TO 12001-CHECK-MA-GTL.                                 
                                                                 
    COMPUTE ANGLEN-M EQUAL WK5 / I.                              
    COMPUTE ANGLEN EQUAL WK1 / I.                                
                                                                 
    COMPUTE WK3 EQUAL CP-ORIGINAL-BENEFIT / ANGLEN.              
    COMPUTE WK3 EQUAL (CP-PREMIUM-RATE / +1000) * WK3 *          
     ((N - (ANGLEN - ANGLEN-M)) / I) * (1 + 2 * I).              
    MOVE WK3 TO CP-CALC-PREMIUM.                                 
                                                                 
    GO TO 99999-RETURN.                                          
                                                                 
12001-CHECK-MA-GTL.                                              
                                                                 
    IF (CP-COMPANY-ID EQUAL 'GTL') AND                           
       (CP-STATE-STD-ABBRV EQUAL 'MA') AND                       
       (CP-EARN-AS-NET-PAY) AND                                  
       (CP-SPECIAL-CALC-CD EQUAL 'H')                            
       NEXT SENTENCE                                             
    ELSE                                                         
       GO TO 12001-CHECK-NV-NET.                                 
                                                                 
    COMPUTE ANGLEN EQUAL WK1 / I.                                
                                                                 
    COMPUTE WK3 EQUAL (CP-PREMIUM-RATE / +1000) *                
     CP-ALTERNATE-BENEFIT * N.                                   
                                                                 
    COMPUTE WK7 EQUAL (CP-ORIGINAL-BENEFIT + WK3) /              
    (ANGLEN - (CP-PREMIUM-RATE / +1000) * (N - ANGLEN) / I).     
                                                                 
    COMPUTE WK3 EQUAL WK3 + (CP-PREMIUM-RATE / +1000) *          
    WK7 * (N - ANGLEN) / I.                                      
    MOVE WK3 TO CP-CALC-PREMIUM.                                 
                                                                 
    GO TO 99999-RETURN.                                          
                                                                 
12001-CHECK-NV-NET.                                              
                                                                 
    IF (CP-COMPANY-ID EQUAL 'CID')                               
       AND (CP-STATE-STD-ABBRV = 'NV' OR 'WA')                   
       AND (CP-EARN-AS-NET-PAY)                                  
       CONTINUE                                                  
    ELSE                                                         
       GO TO 12001-CHECK-MN-NET                                  
    END-IF                                                       
                                                                 
    COMPUTE ANGLEN = (1 - (1 / (1 + I)) ** N) / I                
    COMPUTE ANGLEN-M = (1 - (1 / (1 + I)) ** (N - M)) / I        
                                                                 
    COMPUTE WK8 = (1 + ((CP-TERM-OR-EXT-DAYS + +30) * I) / +30)  
                  / (1 + I)                                      
                                                                 
    COMPUTE ANGLEN-1 = ANGLEN / WK8                              
                                                                 
    COMPUTE WK4 = CP-ORIGINAL-BENEFIT / ANGLEN-1                 
                                                                 
    COMPUTE WK3 = WK4 * ((M - (ANGLEN - ANGLEN-M)) / I) *        
       (CP-PREMIUM-RATE * K-I / 1000)                            
                                                                 
    MOVE WK3                    TO CP-CALC-PREMIUM               
                                                                 
    GO TO 99999-RETURN                                           
                                                                 
     .                                                           
12001-CHECK-MN-NET.                                              
                                                                 
    IF (CP-COMPANY-ID EQUAL 'CID')                               
       AND (CP-STATE-STD-ABBRV = 'MN')                           
       AND (CP-EARN-AS-NET-PAY)                                  
       AND (NOT CP-LEVEL-LIFE)                                   
       AND (WS-DISCOUNT-OPTION = '1')                            
       CONTINUE                                                  
    ELSE                                                         
       GO TO 12001-CHECK-NC-NET                                  
    END-IF                                                       
                                                                 
    if i = .0000000001                                           
       compute wk4 = cp-original-benefit / n                     
       compute wk3 = wk4 * ((n * (n + 1) / 2) - ((n - m) *       
          (n - m + 1) / 2))*(cp-premium-rate / 1000)             
       move wk3 to cp-calc-premium                               
       go to 99999-return                                        
    end-if                                                       
                                                                 
    COMPUTE ANGLEN = (1 - (1 / (1 + I)) ** N) / I                
    COMPUTE ANGLEN-M = (1 - (1 / (1 + I)) ** (N - M)) / I        
                                                                 
    COMPUTE WK8 = (1 + ((CP-TERM-OR-EXT-DAYS + +30) * I) / +30)  
                  / (1 + I)                                      
                                                                 
    COMPUTE ANGLEN-1 = ANGLEN / WK8                              
                                                                 
    COMPUTE WK4 = CP-ORIGINAL-BENEFIT / ANGLEN-1                 
                                                                 
    COMPUTE WK3 = WK4 * ((M - (ANGLEN - ANGLEN-M)) / I) *        
       (CP-PREMIUM-RATE / 1000) * (1 + I)                        
                                                                 
    MOVE WK3                    TO CP-CALC-PREMIUM               
                                                                 
    GO TO 99999-RETURN                                           
                                                                 
     .                                                           
12001-CHECK-NC-NET.                                              
                                                                 
    IF (CP-COMPANY-ID EQUAL 'NCB' ) AND                          
       (CP-STATE-STD-ABBRV EQUAL 'NC') AND                       
       (CP-EARN-AS-NET-PAY)                                      
       NEXT SENTENCE                                             
    ELSE                                                         
       GO TO 12001-CHECK-NC-NET-PLUS3.                           
                                                                 
     COMPUTE ANGLEN EQUAL WK1 / I.                               
     COMPUTE ANGLEN-M EQUAL WK5 / I.                             
                                                                 
     COMPUTE WK6 ROUNDED = CP-ORIGINAL-BENEFIT /                 
         (ANGLEN - (CP-PREMIUM-RATE / 1000) *                    
         ((N - (ANGLEN - ANGLEN-M)) / I + 3 * N) -               
         N * (CP-PREMIUM-RATE / 1000)).                          
                                                                 
     COMPUTE WK3 ROUNDED = (CP-PREMIUM-RATE / 1000) * WK6 *      
         ((N - (ANGLEN - ANGLEN-M)) / I + 3 * N).                
                                                                 
     COMPUTE WK7 ROUNDED = (1 - (CP-PREMIUM-RATE / 1000) *       
         ((N - ANGLEN) / I + 3) / ANGLEN) /                      
         (1 - (CP-PREMIUM-RATE / 1000) * N * (N + 1) /           
         (ANGLEN * 2))                                           
                                                                 
     COMPUTE WK7 ROUNDED = WK7 * N - 3.                          
                                                                 
     IF ANGLEN IS NOT LESS THAN WK7                              
         COMPUTE CP-PREMIUM-RATE ROUNDED =                       
             (CP-PREMIUM-RATE / 1000) * +6.5 * (N / 12)          
         COMPUTE WK3 ROUNDED =                                   
             (CP-ORIGINAL-BENEFIT * CP-PREMIUM-RATE).            
                                                                 
    MOVE WK3 TO CP-CALC-PREMIUM.                                 
                                                                 
    GO TO 99999-RETURN.                                          
                                                                 
12001-CHECK-NC-NET-PLUS3.                                        
                                                                 
    IF (CP-STATE-STD-ABBRV EQUAL 'NC') AND                       
       (CP-EARN-AS-NET-PAY) AND                                  
       (CP-COMPANY-ID NOT = 'CID' AND 'DCC' AND 'AHL' AND 'VPP') 
       NEXT SENTENCE                                             
    ELSE                                                         
       GO TO 12001-CHECK-CA.                                     
                                                                 
    MOVE CP-CERT-EFF-DT TO DC-BIN-DATE-1.                        
    MOVE ' ' TO DC-OPTION-CODE.                                  
    PERFORM 9100-CONVERT-DATE THRU 9100-EXIT.                    
                                                                 
    IF DATE-CONVERSION-ERROR                                     
       MOVE '2'                 TO CP-RETURN-CODE                
       MOVE ZEROS               TO FACTOR                        
                                   CP-CALC-PREMIUM               
        GO TO 99999-RETURN.                                      
                                                                 
    IF DC-GREG-DATE-CYMD GREATER THAN 19931231                   
       NEXT SENTENCE                                             
    ELSE                                                         
       GO TO 12001-CONTINUE-NET.                                 
                                                                 
    COMPUTE ANGLEN EQUAL WK1 / I.                                
    COMPUTE ANGLEN-M EQUAL WK5 / I.                              
                                                                 
    COMPUTE NC-LR = CP-PREMIUM-RATE / ( ORIGINAL-TERM / 12).     
                                                                 
    COMPUTE NC-R = 20 *                                          
                   ((NC-LR * ORIGINAL-TERM) / 12)                
                         / (ORIGINAL-TERM + 1).                  
                                                                 
    COMPUTE NC-OB = NC-R / 1000.                                 
                                                                 
    COMPUTE NC-P = (((ORIGINAL-TERM - ANGLEN) / I) * NC-OB) +    
                   (NC-OB * ORIGINAL-TERM * 3).                  
                                                                 
    IF CP-EXT-CHG-LF                                             
        COMPUTE NC-BIG-D = CP-TERM-OR-EXT-DAYS + 30              
    ELSE                                                         
        MOVE 30                 TO  NC-BIG-D.                    
                                                                 
    COMPUTE NC-LITTLE-D = (1 + I) /                              
            ((( 1 + ((NC-BIG-D / 30.4166666)) * I))).            
                                                                 
    COMPUTE NC-MP ROUNDED = CP-ORIGINAL-BENEFIT  /               
            (ANGLEN * NC-LITTLE-D ).                             
                                                                 
    COMPUTE CP-CALC-PREMIUM ROUNDED = NC-MP * NC-P.              
                                                                 
    GO TO 99999-RETURN.                                          
                                                                 
12001-CHECK-CA.                                                  
                                                                 
    IF (CP-STATE-STD-ABBRV EQUAL 'CA') AND                       
       (WS-DISCOUNT-OPTION = '6')                                
       NEXT SENTENCE                                             
    ELSE                                                         
       GO TO 12001-CHECK-GTL-IA.                                 
                                                                 
    COMPUTE CA-J = CA-DISCOUNT / 12.                             
    COMPUTE ANGLEN EQUAL WK1 / I.                                
    COMPUTE CA-MP = CP-ORIGINAL-BENEFIT  / ANGLEN.               
    COMPUTE CA-API = (1 - (1 + I)** - ORIGINAL-TERM) / I.        
    COMPUTE CA-APJ = (1 - (1 + CA-J)** - ORIGINAL-TERM) / CA-J.  
    COMPUTE CA-VI = (1 + I)** -1.                                
    COMPUTE CP-CALC-PREMIUM =                                    
        ((CP-PREMIUM-RATE * 2) / (100 * (ORIGINAL-TERM + 1)) *   
        ((1 + CA-J) / I) *                                       
        (CA-APJ + (CA-J * CA-APJ - I * CA-API) *                 
        ((CA-VI ** (LOAN-TERM - ORIGINAL-TERM)) /                
        (I - CA-J))) * (1 + CP-ODD-DAYS-TO-PMT * I) * CA-MP).    
                                                                 
    GO TO 99999-RETURN.                                          
                                                                 
12001-CHECK-GTL-IA.                                              
                                                                 
    IF (CP-COMPANY-ID EQUAL 'GTL')     AND                       
       (CP-STATE-STD-ABBRV EQUAL 'IA') AND                       
       (CP-EARN-AS-NET-PAY)                                      
       NEXT SENTENCE                                             
    ELSE                                                         
       GO TO 12001-CHECK-CID-TRN-VA.                             
                                                                 
*>**************************************************************   
                                                                 
    COMPUTE GTL-IA-ODD-DAYS = CP-ODD-DAYS-TO-PMT - 30.           
                                                                 
    IF GTL-IA-ODD-DAYS = ZEROS                                   
        MOVE ZERO               TO  GTL-IA-SGN                   
    ELSE                                                         
        IF GTL-IA-ODD-DAYS GREATER THAN ZERO                     
            MOVE +1             TO  GTL-IA-SGN                   
        ELSE                                                     
            MOVE -1             TO  GTL-IA-SGN.                  
                                                                 
    MOVE GTL-IA-ODD-DAYS        TO  GTL-IA-ABS.                  
    COMPUTE GTL-IA-ABS = GTL-IA-ABS - 30.                        
                                                                 
    COMPUTE GTL-IA-ODF = (1 + I * GTL-IA-ABS / 30) **            
                         GTL-IA-SGN.                             
                                                                 
    COMPUTE ANGLEN EQUAL WK1 / I.                                
    COMPUTE ANGLEN-M EQUAL WK5 / I.                              
                                                                 
    IF GTL-IA-ODF NOT = ZERO                                     
        COMPUTE ANGLEN EQUAL ANGLEN / GTL-IA-ODF                 
        COMPUTE ANGLEN-M EQUAL ANGLEN-M / GTL-IA-ODF.            
                                                                 
    COMPUTE GTL-IA-DIVIDEND =                                    
          (((N-P-LOAN - (N-P-LOAN - N-P-ORIG)) -                 
                            (ANGLEN - ANGLEN-M))) *              
          ((K1 + I) * CP-PREMIUM-RATE *                          
                    (N-P-LOAN - (N-P-LOAN - N-P-ORIG))).         
                                                                 
    COMPUTE GTL-IA-DIVISOR =                                     
     I * ANGLEN * (N-P-LOAN - (N-P-LOAN - N-P-ORIG) + 1) * +600. 
                                                                 
    COMPUTE GTL-IA-FACTOR = GTL-IA-DIVIDEND / GTL-IA-DIVISOR.    
                                                                 
    COMPUTE CP-CALC-PREMIUM =                                    
                          GTL-IA-FACTOR * CP-ORIGINAL-BENEFIT.   
    GO TO 99999-RETURN.                                          
                                                                 
                                                                 
12001-CHECK-CID-TRN-VA.                                          
                                                                 
    IF (CP-COMPANY-ID EQUAL 'CID')     AND                       
       (CP-STATE-STD-ABBRV EQUAL 'VA') AND                       
       (CP-EARN-AS-NET-PAY) AND                                  
       (CP-TRUNCATED-LIFE) AND                                   
       (CP-STATE-STD-ABBRV = CP-STATE)                           
       CONTINUE                                                  
    ELSE                                                         
       GO TO 12001-CHECK-CID-TX                                  
    END-IF                                                       
                                                                 
    COMPUTE ANGLEN EQUAL WK1 / I                                 
    COMPUTE WS-WORK-DIS-RATE = WS-WORK-DIS-RATE / +12            
    IF WS-WORK-DIS-RATE = ZEROS                                  
       CONTINUE                                                  
    ELSE                                                         
       COMPUTE WK4 = 1 / (1 + WS-WORK-DIS-RATE)                  
       COMPUTE WK7 = (1 / I) *                                   
             (((WK4 ** M - 1) / (WK4 - 1)) -                     
             ((WK4 ** M * SX - VX) /                             
             (WK4 * (1 + I) - 1))) *                             
             (CP-PREMIUM-RATE / 1000)                            
       COMPUTE WK4 = CP-ORIGINAL-BENEFIT / ANGLEN                
       COMPUTE WK8 = WK7 * 100 / ANGLEN                          
       COMPUTE CP-CALC-PREMIUM =                                 
          WK8 *  ((WK4 * ANGLEN) / 100)                          
    END-IF                                                       
                                                                 
    GO TO 99999-RETURN                                           
                                                                 
    .                                                            
                                                                 
12001-CHECK-CID-TX.                                              
                                                                 
    IF (CP-COMPANY-ID EQUAL 'CID')     AND                       
       (CP-STATE-STD-ABBRV EQUAL 'TX') AND                       
       (CP-EARN-AS-NET-PAY) AND                                  
       (WS-WORK-DIS-RATE > ZEROS) AND                            
       (CP-STATE-STD-ABBRV = CP-STATE)                           
       CONTINUE                                                  
    ELSE                                                         
       GO TO 12001-CHECK-CID-LEV-MN-NEW                          
    END-IF                                                       
*> THE FOLLOWING STMT FORCES ONE MONTH EXTRA INT PERIOD            
    COMPUTE K-I = 1 + (1 * I)                                    
    COMPUTE ANGLEm EQUAL WK1 / I   *>   Loan term                
    COMPUTE ANGLEn EQUAL WK2 / I   *>   Ins  term                
                                                                 
    COMPUTE D = CP-TERM-OR-EXT-DAYS + +30                        
                                                                 
    COMPUTE Y = (1 + (D * I) / 30) / (1 + I)                     
    COMPUTE ANGLEmy = ANGLEm / Y                                 
                                                                 
    COMPUTE WS-WORK-DIS-RATE = WS-WORK-DIS-RATE / +12            
    IF WS-WORK-DIS-RATE = ZEROS                                  
       CONTINUE                                                  
    ELSE                                                         
       COMPUTE WK4 = 1 / (1 + WS-WORK-DIS-RATE)                  
       COMPUTE WK7 = (K-I / I) *                                 
             (((WK4 ** M - 1) / (WK4 - 1)) -                     
*>             ((WK4 ** M * SX - VX) /                             
             ((WK4 ** M * SX) - VX) /                            
*>             (WK4 * (1 + I) - 1))) *                             
             (WK4 * (1 + I) - 1)) *                              
             (CP-PREMIUM-RATE / 1000)                            
       COMPUTE WK4 = CP-ORIGINAL-BENEFIT / ANGLEmy               
       COMPUTE WK8 = (WK7 * 100) / (ANGLEmy * K-I)               
       COMPUTE CP-CALC-PREMIUM =                                 
          WK8 *  ((WK4 * ANGLEmy * K-I) / 100)                   
    END-IF                                                       
                                                                 
    GO TO 99999-RETURN                                           
                                                                 
    .                                                            
12001-CHECK-CID-LEV-MN-NEW.                                      
                                                                 
*>    IF (CP-COMPANY-ID EQUAL 'CID')                               
*>       AND (CP-STATE-STD-ABBRV = 'MN')                           
*>       AND (CP-EARN-AS-NET-PAY)                                  
*>       AND (CP-LEVEL-LIFE)                                       
*>       AND (CP-CERT-EFF-DT > X'A4FF')                            
*>       CONTINUE                                                  
*>    ELSE                                                         
*>       GO TO 12001-CHECK-CID-LEV-MN                              
*>    END-IF                                                       
*>                                                                 
*>    MOVE CP-CERT-EFF-DT         TO DC-BIN-DATE-1                 
*>    MOVE CP-EXPIRE-DT           TO DC-BIN-DATE-2                 
*>    MOVE '1'                    TO DC-OPTION-CODE                
*>    PERFORM 9100-CONVERT-DATE THRU 9100-EXIT                     
*>                                                                 
*>    IF DATE-CONVERSION-ERROR                                     
*>       MOVE '2'                 TO CP-RETURN-CODE                
*>       MOVE ZEROS               TO FACTOR                        
*>                                   CP-CALC-PREMIUM               
*>       GO TO 99999-RETURN                                        
*>    END-IF                                                       
