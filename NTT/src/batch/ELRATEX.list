    COMPUTE WK8 = CP-PREMIUM-RATE *                              
       ((M + ((CP-TERM-OR-EXT-DAYS - 30) / 30)) / 12)            
    COMPUTE WK8 = WK8 / ((M + (12 / 12)) / 2)                    
    COMPUTE WK4 = CP-ORIGINAL-BENEFIT / GR                       
    COMPUTE WK3 = WK4 * ((M - (ANGLEN - ANGLEN-M)) / I) *        
