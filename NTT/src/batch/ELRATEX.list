    COMPUTE WK-MONTHLY-RATE-J =                                  
         RT-AH-RATE (WK-J) / 100.                                
    COMPUTE WK-T = WK-J + 1.                                     
    COMPUTE WK-MONTHLY-RATE-T =                                  
         RT-AH-RATE (WK-T) / 100.                                
    COMPUTE WK-C = (CP-ORIGINAL-TERM - (WK-T * 2) + 1).          
    COMPUTE WK-C = ((WK-C * WK-MONTHLY-RATE-T) + ((WK-T - 1) *   
                   WK-MONTHLY-RATE)) / ((WK-T - 1) *             
                   (CP-ORIGINAL-TERM - WK-T)).                   
    COMPUTE WK-D = (DC-ELAPSED-DAYS * CP-PAY-FREQUENCY / 365)    
    COMPUTE WK-PREM = (CP-ORIGINAL-TERM -                        
                      (WK-J + 1) + (WK-D - 1)) * (WK-C *         
                      WS-MAX-TOT-BEN).                           
    COMPUTE CP-CALC-PREMIUM ROUNDED = WK-PREM + ((WK-J + 1) *    
                  (WS-MAX-MON-BEN * WK-MONTHLY-RATE-T)).         
                                                                 
    GO TO 9999-CALC-RATE-PREM-X.                                 
                                                                 
                                                                 
