           OR                                                    
       WRK-GR-MORT-CODE LESS THAN CLAS-MORT-CODE (CLAS-INDEXM)   
       GO TO 0320-FIND-MORT-DEFAULT.                             
                                                                 
    IF WRK-GR-MORT-CODE NOT = CLAS-MORT-CODE (CLAS-INDEXM)       
        ADD +1 TO CLAS-INDEXM                                    
        GO TO 0310-FIND-MORT-TYPE.                               
                                                                 
    IF CLAS-I-JOINT (CLAS-INDEXL) = 'J'                          
       IF CLAS-MORT-J-CODE (CLAS-INDEXM) = 'J'                   
              OR                                                 
          CLAS-MORT-J-FACT (CLAS-INDEXM) GREATER THAN +1.0000    
          GO TO 0330-FOUND-MORT                                  
       ELSE                                                      
          ADD +1 TO CLAS-INDEXM                                  
          GO TO 0310-FIND-MORT-TYPE                              
    ELSE                                                         
       IF CLAS-MORT-J-CODE (CLAS-INDEXM) = 'S'                   
          GO TO 0330-FOUND-MORT                                  
       ELSE                                                      
          ADD +1 TO CLAS-INDEXM                                  
          GO TO 0310-FIND-MORT-TYPE.                             
                                                                 
    GO TO 0330-FOUND-MORT.                                       
                                                                 
0320-FIND-MORT-DEFAULT.                                          
                                                                 
    IF WRK-GR-MORT-CODE = 'Z000' OR 'ZERO' OR 'ZER0'             
       NEXT SENTENCE                                             
    ELSE                                                         
       MOVE 'A' TO GR-FLAG.                                      
                                                                 
    GO TO 0399-XIT.                                              
                                                                 
0330-FOUND-MORT.                                                 
                                                                 
    IF WRK-GR-MORT-CODE = 'Z000' OR 'ZERO' OR 'ZER0'             
        GO TO 0399-XIT.                                          
                                                                 
    IF CLAS-I-JOINT (CLAS-INDEXL) = 'J'                          
           AND                                                   
       CLAS-MORT-J-CODE (CLAS-INDEXM) = 'J'                      
        MOVE 'J'                TO WRK-GR-MORT-TYP               
    ELSE                                                         
        MOVE 'S'                TO WRK-GR-MORT-TYP.              
                                                                 
                                                                 
    PERFORM 0800-GET-RATE THRU 0899-GET-RATE-X.                  
                                                                 
    MOVE GR-FLAG TO SAVE-GR-FLAG.                                
                                                                 
    MOVE CLAS-MORT-J-FACT (CLAS-INDEXM) TO WS-FACT.              
                                                                 
    IF GR-FLAG = 'D'                                             
        GO TO 0399-XIT.                                          
                                                                 
    IF GR-LFTYP = 'QD'                                           
       GO TO 0350-CALC-NET-PAY.                                  
                                                                 
    IF CLAS-I-EP (CLAS-INDEXL) = 'N'                             
       and gr-apr > zeros                                        
        GO TO 0350-CALC-NET-PAY                                  
    END-IF.                                                      
                                                                 
    PERFORM 0400-STD-MORTALITY THRU 0449-STD-EXIT.               
                                                                 
    MOVE WS-MORT-FACTOR TO GR-MORT-FACT.                         
                                                                 
    IF CLAS-I-EP (CLAS-INDEXL) = 'T'                             
        COMPUTE TEX-FACT-9 =                                     
            (GR-LF-TERM + GR-PMT-FREQ) / (GR-LF-TERM + +1)       
        COMPUTE GR-MORT-FACT ROUNDED =                           
            GR-MORT-FACT * TEX-FACT-9.                           
                                                                 
    IF NOT GR-SUMMARY-REC                                        
        MOVE +0 TO GR-APR.                                       
                                                                 
    IF CLAS-I-JOINT (CLAS-INDEXL) = 'J'                          
        IF CLAS-I-RL-AH (CLAS-INDEXL) = 'R'                      
            COMPUTE WRK-GR-RESV ROUNDED =                        
                (GR-MO-DEC / +100) * (GR-MORT-FACT * WS-FACT)    
        ELSE                                                     
            COMPUTE WRK-GR-RESV ROUNDED =                        
                (GR-REM-AMT / +1000) * (GR-MORT-FACT * WS-FACT)  
    ELSE                                                         
        IF CLAS-I-RL-AH (CLAS-INDEXL) = 'R'                      
            COMPUTE WRK-GR-RESV ROUNDED =                        
                (GR-MO-DEC / +100) * GR-MORT-FACT                
        ELSE                                                     
            COMPUTE WRK-GR-RESV ROUNDED =                        
                (GR-REM-AMT / +1000) * GR-MORT-FACT.             
                                                                 
    IF CLAS-I-EP (CLAS-INDEXL) = 'B' OR 'K' OR 'L'               
        IF NOT BALLOON-PASS                                      
            MOVE 'Y'              TO BALLOON-SW                  
            PERFORM 0400-STD-MORTALITY THRU 0449-STD-EXIT        
            COMPUTE WS-ALT-BENEFIT-AMT = GR-REM-AMT -            
                                     (GR-LF-REMTERM * GR-MO-DEC) 
            IF CLAS-I-JOINT (CLAS-INDEXL) = 'J'                  
              COMPUTE WRK-GR-RESV ROUNDED = WRK-GR-RESV +        
                    (WS-ALT-BENEFIT-AMT / +1000) *               
                    (WS-MORT-FACTOR * WS-FACT)                   
             ELSE                                                
              COMPUTE WRK-GR-RESV ROUNDED = WRK-GR-RESV +        
                 (WS-ALT-BENEFIT-AMT / +1000) * WS-MORT-FACTOR.  
                                                                 
    GO TO 0399-XIT.                                              
                                                                 
0350-CALC-NET-PAY.                                               
                                                                 
    ADD +1                      TO W-NUMBER-NET-PAY.             
                                                                 
    IF GR-LFTYP = 'QD'                                           
       MOVE +15.0               TO GR-APR.                       
                                                                 
    IF GR-APR = ZERO                                             
        MOVE 'Z'                TO GR-FLAG                       
        GO TO 0399-XIT.                                          
                                                                 
    IF  GR-APR LESS THAN +1.0                                    
        DISPLAY GR-CONTROL ' - NET PAY GAAP RECORD WITH APR '    
            'LESS THAN +1.0'                                     
        ADD +1                  TO W-APR-LESS-THAN-1.            
                                                                 
    MOVE +0                     TO WK-CX    WK-DX                
                                   WK-RESV  WK-RSV               
                                   NP-MORT-TOT                   
                                   NP-AVG-INFORCE                
                                                                 
    MOVE 'C'                    TO WRK-GR-MORT-TYP.              
                                                                 
    MOVE WRK-GR-MORT-AGE        TO X2.                           
                                                                 
*>*****  NOTE - AGE ADJUSTED BECAUSE TABLE STARTS AT ZERO NOT      
*>*****         BECAUSE AN AGE OF 1 GREATER IS NEEDED IN CALC.     
    COMPUTE X2 = X2 + +1.                                        
    MOVE GR-LF-REMTERM             TO X3.                        
    DIVIDE GR-LF-REMTERM BY +12 GIVING                           
                  NP-INT-1 REMAINDER NP-INT-2.                   
                                                                 
    MOVE GR-APR                 TO NP-APR.                       
    MOVE GR-LF-TERM             TO NP-ORIG  NP-CAP.              
    MOVE CLAS-I-CALC-TYPE (CLAS-INDEXL)                          
                                TO NP-OPT.                       
                                                                 
    MOVE GR-LF-EXPIRE-DATE      TO  DC-GREG-DATE-CYMD.           
    MOVE 'L'                    TO  DC-OPTION-CODE.              
    PERFORM 2900-DATE-CONVERSION THRU 2900-EXIT.                 
    IF NO-CONVERSION-ERROR                                       
        MOVE DC-BIN-DATE-1      TO  WS-EXPIRE-DT                 
    ELSE                                                         
        MOVE LOW-VALUES         TO  WS-EXPIRE-DT.                
                                                                 
    MOVE GR-EFF                 TO  DC-GREG-DATE-CYMD.           
    MOVE 'L'                    TO  DC-OPTION-CODE.              
    PERFORM 2900-DATE-CONVERSION THRU 2900-EXIT.                 
    IF NO-CONVERSION-ERROR                                       
        MOVE DC-BIN-DATE-1      TO  CP-CERT-EFF-DT               
    ELSE                                                         
        MOVE LOW-VALUES         TO  CP-CERT-EFF-DT.              
                                                                 
    MOVE WS-EXPIRE-DT           TO  DC-BIN-DATE-1.               
    MOVE '6'                    TO  DC-OPTION-CODE.              
    MOVE +0                     TO  DC-ELAPSED-DAYS              
                                    DC-ODD-DAYS-OVER.            
    COMPUTE DC-ELAPSED-MONTHS = (GR-LF-TERM - 1) * -1.           
    PERFORM 2900-DATE-CONVERSION THRU 2900-EXIT.                 
    IF NO-CONVERSION-ERROR                                       
        MOVE DC-BIN-DATE-2      TO  CP-FIRST-PAY-DATE            
    ELSE                                                         
        MOVE LOW-VALUES         TO  CP-FIRST-PAY-DATE.           
                                                                 
    MOVE GR-APR                 TO  CP-LOAN-APR.                 
    MOVE GR-LF-TERM             TO  CP-ORIGINAL-TERM.            
    MOVE GR-LOAN-TERM           TO  CP-LOAN-TERM.                
    MOVE STATE-ABBR       (CLAS-INDEXS)                          
                                TO  CP-STATE-STD-ABBRV.          
    MOVE CLAS-I-CALC-TYPE (CLAS-INDEXL)                          
                                TO  CP-SPECIAL-CALC-CD.          
    MOVE CLAS-I-RL-AH     (CLAS-INDEXL)                          
                                TO  CP-BENEFIT-TYPE.             
    MOVE CLAS-I-EP        (CLAS-INDEXL)                          
                                TO  CP-EARNING-METHOD.           
                                                                 
    PERFORM 2300-MATCH-NET-PAY THRU 2300-EXIT.                   
                                                                 
    IF NP-INT-2 = ZERO                                           
        GO TO 0355-CX-TOTALS.                                    
                                                                 
    COMPUTE NP-LOOP-END = (12 * NP-INT-1) + 1.                   
    GO TO 0355-12-MONTH-LOOP-START.                              
                                                                 
                                                                 
0355-CX-TOTALS.                                                  
    IF X3 GREATER 12                                             
         COMPUTE NP-LOOP-END = X3 - 11                           
      ELSE                                                       
          MOVE +1 TO NP-LOOP-END.                                
                                                                 
0355-12-MONTH-LOOP-START.                                        
    MOVE +0                     TO WK-RESV.                      
                                                                 
    PERFORM 0360-1-YEAR-LOOP THRU 0360-LOOP-END                  
        VARYING X3 FROM X3 BY -1                                 
            UNTIL X3 LESS NP-LOOP-END.                           
                                                                 
    COMPUTE NP-AVG-INFORCE ROUNDED =                             
                                WK-RESV / +12.                   
    MOVE RF-CX-FACTOR (X2)      TO WK-CX.                        
                                                                 
    COMPUTE NP-MORT-TOT = NP-MORT-TOT +                          
                          (NP-AVG-INFORCE * WK-CX).              
                                                                 
    COMPUTE X2 = X2 + 1.                                         
                                                                 
    IF NP-LOOP-END = +1                                          
        GO TO 0370-DX-DIVIDE                                     
     ELSE                                                        
         GO TO 0355-CX-TOTALS.                                   
                                                                 
0360-1-YEAR-LOOP.                                                
                                                                 
    MOVE X3                     TO  CP-REMAINING-TERM.           
    MOVE +0                     TO  CP-REMAMT-FACTOR.            
                                                                 
    IF  W-NET-PAY-MATCHED                                        
            AND                                                  
        W-GNPT-FACTOR (X3) NUMERIC                               
        MOVE W-GNPT-FACTOR (X3) TO  CP-REMAMT-FACTOR             
    ELSE                                                         
        MOVE GR-LFBEN           TO  CP-ORIGINAL-BENEFIT          
        CALL 'ELRAMTX' USING CALCULATION-PASS-AREA               
        MOVE CP-REMAMT-FACTOR   TO  W-GNPT-FACTOR (X3).          
                                                                 
    IF CP-STATE-STD-ABBRV EQUAL 'NC'                             
              AND                                                
       (DTE-CLIENT NOT = 'CID')                                  
              AND                                                
       CP-EARN-AS-NET-PAY                                        
        IF GR-EFF GREATER THAN 19931231                          
            COMPUTE NP-NC-TOTAL-BEN =                            
                    GR-LFBEN + (GR-MO-DEC * 3)                   
            COMPUTE WK-RSV ROUNDED =                             
                (NP-NC-TOTAL-BEN / 1000) * CP-REMAMT-FACTOR      
        ELSE                                                     
            COMPUTE WK-RSV ROUNDED =                             
                (GR-LFBEN / 1000) * CP-REMAMT-FACTOR             
    ELSE                                                         
        COMPUTE WK-RSV ROUNDED =                                 
            (GR-LFBEN / 1000) * CP-REMAMT-FACTOR.                
                                                                 
    COMPUTE WK-RESV = WK-RESV + WK-RSV.                          
                                                                 
    IF  W-CONTROL-OVERRIDE GREATER THAN SPACES                   
            AND                                                  
        W-CO-ACCOUNT EQUAL GR-ACCOUNT                            
            AND                                                  
        W-CO-CERT EQUAL GR-CERT                                  
        MOVE WK-RSV             TO W-EDIT-10                     
        DISPLAY 'RESERVE-LET - ' W-EDIT-10                       
        MOVE NP-APR             TO W-EDIT-5                      
        DISPLAY 'APR         - ' W-EDIT-5                        
        MOVE NP-ORIG            TO W-EDIT                        
        DISPLAY 'ORG-TERM    - ' W-EDIT                          
        MOVE NP-CAP             TO W-EDIT                        
        DISPLAY 'CAP-TERM    - ' W-EDIT                          
        MOVE NP-REM             TO W-EDIT                        
        DISPLAY 'REM-TERM    - ' W-EDIT                          
        DISPLAY 'OPT         - ' NP-OPT                          
        MOVE NP-FACTOR          TO W-EDIT-5                      
        DISPLAY 'FACTOR      -'  W-EDIT-5.                       
                                                                 
0360-LOOP-END.                                                   
    EXIT.                                                        
                                                                 
0370-DX-DIVIDE.                                                  
    MOVE WRK-GR-MORT-AGE        TO X2.                           
    COMPUTE X2 = X2 + +1.                                        
    MOVE RF-DX-FACTOR (X2) TO WK-DX.                             
                                                                 
    IF WK-DX = ZERO                                              
        GO TO 0399-XIT.                                          
                                                                 
    MOVE WRK-GR-MORT-AGE        TO X2.                           
    COMPUTE X2 = X2 + +2.                                        
    MOVE RF-DX-FACTOR (X2) TO WK-DX2.                            
    IF WK-DX2 = ZERO                                             
        GO TO 0399-XIT.                                          
                                                                 
    COMPUTE NP-INT-3 = GR-LF-REMTERM - (12 * NP-INT-1)           
                                                                 
    IF NP-INT-3 = +0                                             
       MOVE +12 TO NP-INT-3.                                     
                                                                 
    IF  REGULAR-MORT                                             
        COMPUTE W-PVFB ROUNDED = NP-MORT-TOT *                   
            (((NP-INT-3 - .50000) / (+12.000 * WK-DX)) +         
            ((+12.50000 - NP-INT-3) / (+12.000 * WK-DX2)))       
    ELSE                                                         
        COMPUTE W-PVFB-ALTERNATE ROUNDED = NP-MORT-TOT *         
            (((NP-INT-3 - .50000) / (+12.000 * WK-DX)) +         
            ((+12.50000 - NP-INT-3) / (+12.000 * WK-DX2))).      
                                                                 
    IF  REGULAR-MORT                                             
        IF  CLAS-I-JOINT (CLAS-INDEXL) = 'J'                     
            COMPUTE W-WORK-AREA ROUNDED                          
                = W-PVFB * RF-RESV-ADJ * WS-FACT                 
        ELSE                                                     
            COMPUTE W-WORK-AREA ROUNDED                          
                = W-PVFB * RF-RESV-ADJ                           
    ELSE                                                         
        IF  CLAS-I-JOINT (CLAS-INDEXL) = 'J'                     
            COMPUTE W-WORK-AREA ROUNDED                          
                = W-PVFB-ALTERNATE * RF-RESV-ADJ * WS-FACT       
        ELSE                                                     
            COMPUTE W-WORK-AREA ROUNDED                          
                = W-PVFB-ALTERNATE * RF-RESV-ADJ.                
                                                                 
    COMPUTE WRK-GR-RESV ROUNDED = W-WORK-AREA.                   
    MOVE SPACE TO GR-FLAG.                                       
                                                                 
0399-XIT.                                                        
    EXIT.                                                        
EJECT                                                            
0400-STD-MORTALITY.                                              
    MOVE +0                     TO RSVR.                         
    MOVE GR-LF-REMTERM          TO CTA.                          
    MOVE WRK-GR-MORT-AGE        TO CTT.                          
                                                                 
    COMPUTE CTX = WRK-GR-MORT-AGE + (GR-LF-REMTERM / 12).        
                                                                 
    IF CTX GREATER 98                                            
         MOVE ZERO              TO WS-MORT-FACTOR                
         GO TO 0449-STD-EXIT.                                    
                                                                 
    MOVE GR-LF-REMTERM          TO P.                            
                                                                 
    IF GR-LF-REMTERM GREATER 12                                  
        PERFORM 0445-FIND-P THRU 0445-EXIT.                      
                                                                 
    COMPUTE R = GR-LF-REMTERM / 12.                              
                                                                 
    IF P = 12                                                    
        SUBTRACT 2 FROM R                                        
      ELSE                                                       
        SUBTRACT 1 FROM R.                                       
                                                                 
    COMPUTE V = 1.0 / RF-INT-ADJ.                                
                                                                 
    MOVE WRK-GR-MORT-AGE        TO XX.                           
                                                                 
    COMPUTE TX = WRK-GR-MORT-AGE + +1.                           
                                                                 
    MOVE RF-DX-FACTOR (TX)      TO DX.                           
                                                                 
    COMPUTE CTS = WRK-GR-MORT-AGE + +2.                          
                                                                 
    COMPUTE CX = (V * DX) - RF-DX-FACTOR (CTS).                  
                                                                 
    IF (CLAS-I-RL-AH (CLAS-INDEXL) = 'L' OR 'P') OR              
       (BALLOON-PASS)                                            
          GO TO 0430-LEVEL-RESERVE.                              
                                                                 
EJECT                                                            
0410-DECREASING-RESERVE.                                         
    COMPUTE ANSB = (100 * GR-LF-REMTERM) * P.                    
    COMPUTE ANSD = CX / (DX * 12).                               
    COMPUTE ANSC = P * 50.                                       
                                                                 
    IF LX-PLUS                                                   
        COMPUTE CTR = P + 1                                      
      ELSE                                                       
        COMPUTE CTR = P - 1.                                     
                                                                 
    COMPUTE ANSC = ANSC * CTR.                                   
    COMPUTE ANSB = ANSB - ANSC.                                  
    COMPUTE RSVR ROUNDED = ANSB * ANSD.                          
                                                                 
    IF GR-LF-REMTERM LESS THAN 13                                
        GO TO 0440-END-RSVR-CALC.                                
                                                                 
    MOVE +0 TO T.                                                
                                                                 
0415-RSVR-ADD-LOOP.                                              
    COMPUTE CTR = XX + T + 2.                                    
    COMPUTE CTS = XX + T + 3.                                    
    COMPUTE ANSB = ((V * RF-DX-FACTOR (CTR)) -                   
                     RF-DX-FACTOR (CTS)).                        
    COMPUTE ANSB = ((V * RF-DX-FACTOR (CTR)) -                   
                     RF-DX-FACTOR (CTS)) / DX.                   
0415-LOOP-END.                                                   
    COMPUTE CTR = T * 12.                                        
    COMPUTE ANSD = ((GR-LF-REMTERM * 100) -                      
                   ((P + CTR + 5.50) * 100)) * ANSB.             
    COMPUTE RSVR = RSVR + ANSD.                                  
                                                                 
    ADD 1 TO T.                                                  
                                                                 
    IF T NOT GREATER R                                           
        GO TO 0415-RSVR-ADD-LOOP.                                
                                                                 
    GO TO 0440-END-RSVR-CALC.                                    
                                                                 
EJECT                                                            
0430-LEVEL-RESERVE.                                              
    COMPUTE ANSB = CX / DX.                                      
    COMPUTE ANSC = 1000 * P.                                     
    COMPUTE ANSD = ANSC / 12.                                    
    COMPUTE RSVR ROUNDED = ANSB * ANSD.                          
                                                                 
    IF GR-LF-REMTERM LESS THAN 13                                
        GO TO 0440-END-RSVR-CALC.                                
                                                                 
    MOVE +0 TO T.                                                
                                                                 
0435-LEV-RSVR-LOOP.                                              
    COMPUTE CTR = XX + T + 2.                                    
    COMPUTE CTS = XX + T + 3.                                    
    COMPUTE ANSB = ((V * RF-DX-FACTOR (CTR)) -                   
                     RF-DX-FACTOR (CTS)) / DX.                   
                                                                 
    COMPUTE RSVR = RSVR + (1000 * ANSB).                         
                                                                 
    ADD 1 TO T.                                                  
                                                                 
    IF T NOT GREATER R                                           
        GO TO 0435-LEV-RSVR-LOOP.                                
                                                                 
    GO TO 0440-END-RSVR-CALC.                                    
EJECT                                                            
0440-END-RSVR-CALC.                                              
    IF CLAS-I-JOINT (CLAS-INDEXL) = 'J'                          
        COMPUTE WS-MORT-FACTOR  ROUNDED = (RSVR * RF-RESV-ADJ)   
                                          * RF-JOINT-ADJ         
      ELSE                                                       
        COMPUTE WS-MORT-FACTOR ROUNDED = RSVR * RF-RESV-ADJ.     
                                                                 
    GO TO 0449-STD-EXIT.                                         
                                                                 
0445-FIND-P.                                                     
    SUBTRACT 12 FROM P.                                          
    IF P GREATER 12                                              
        GO TO 0445-FIND-P.                                       
0445-EXIT.                                                       
    EXIT.                                                        
                                                                 
0449-STD-EXIT.                                                   
    EXIT.                                                        
                            EJECT                                
                                                                 
0450-ALT-MORTALITY.                                              
    MOVE +0                     TO RSVR.                         
    MOVE GR-LF-REMTERM          TO CTA.                          
    MOVE WRK-GR-MORT-AGE        TO CTT.                          
                                                                 
    COMPUTE TX = WRK-GR-MORT-AGE + +1.                           
                                                                 
*>*                                                                
*>*   DR = DX VALUE AT VALUATION AGE                               
*>*   MR = MX VALUE AT VALUATION AGE                               
*>*                                                                
*>*   ** NOTE **                                                   
*>*     +1 IS ADDED TO AGE ONLY TO ADJUST FOR TABLE, NOT TO INCREAS
*>*     THE AGE.                                                   
*>*                                                                
    MOVE RF-DX-FACTOR (TX)      TO DR.                           
    MOVE MX-FACTORS (TX)        TO MR.                           
*>*                                                                
*>*   IF AGE AT TERMINATION IS NOT EVEN THEN THE MX AT TERMINATION 
*>*   (MY) MUST BE INTERPOLATED. I.E. IF AGE IS 35 WITH 5 MONTHS   
*>*   REMAINING (EX. AGE 34 WITH REMAINING TERM OF 17), THEN 5/12TH
*>*   OF THE AGE 36 MX IS USED PLUS 7/12THS OF THE AGE 35 MX TO    
*>*   DERIVE THE MX IN THE FORMULA                                 
*>*                                                                
*>*   X2 WILL BE THE NUMBER OF 12THS OF THE HIGH AGE (WK-MY2)      
*>*   X3 WILL BE THE NUMBER OF 12THS OF THE LOW AGE (WK-MY1)       
*>*                                                                
    IF CTA GREATER +11                                           
        DIVIDE CTA BY +12 GIVING X1 REMAINDER X2                 
        IF X2 = ZERO                                             
            MOVE +12            TO X3                            
          ELSE                                                   
            SUBTRACT X2 FROM 12 GIVING X3                        
     ELSE                                                        
        SUBTRACT CTA FROM +12 GIVING X3                          
        MOVE CTA                TO X2.                           
                                                                 
    IF CTX GREATER 98                                            
         MOVE ZERO              TO WS-MORT-FACTOR                
         GO TO 0459-ALT-EXIT.                                    
                                                                 
    COMPUTE CTX = WRK-GR-MORT-AGE + (GR-LF-REMTERM / +12) + +1.  
    MOVE MX-FACTORS (CTX)       TO WK-MY1.                       
    ADD +1 TO CTX.                                               
    MOVE MX-FACTORS (CTX)       TO WK-MY2.                       
                                                                 
    COMPUTE MY = ((WK-MY1 / +12) * X3) + ((WK-MY2 / +12) * X2).  
                                                                 
    COMPUTE WS-MORT-FACTOR-ALT ROUNDED = (MR - MY) / DR.         
                                                                 
    IF CLAS-I-JOINT (CLAS-INDEXL) = 'J'                          
        COMPUTE WS-MORT-FACTOR-ALT ROUNDED = RF-JOINT-ADJ *      
                      (WS-MORT-FACTOR-ALT * RF-RESV-ADJ)         
      ELSE                                                       
        COMPUTE WS-MORT-FACTOR-ALT ROUNDED =                     
                      (WS-MORT-FACTOR-ALT * RF-RESV-ADJ).        
                                                                 
    MOVE WS-MORT-FACTOR-ALT          TO WS-MORT-FACTOR.          
                                                                 
0459-ALT-EXIT.                                                   
    EXIT.                                                        
                            EJECT                                
0460-ALT-MORTALITY-2.                                            
                                                                 
    MOVE +0                     TO WK-CX  WK-DX.                 
    MOVE +0                     TO WK-RESV  WK-RSV.              
    MOVE +0                     TO X4.                           
    MOVE GR-LF-REMTERM          TO X3.                           
                                                                 
0461-MONTH-LOOP.                                                 
    ADD +1                      TO X4.                           
                                                                 
    IF X4 GREATER GR-LF-REMTERM                                  
        GO TO 0463-ADJUST-TOT.                                   
                                                                 
    IF X4 = 1                                                    
        COMPUTE CTX = GR-AGE +                                   
           ((X4 + (GR-LF-TERM - (GR-LF-REMTERM + 1))) / 12) + 1  
        MOVE CTX                TO CTX2                          
    ELSE                                                         
        COMPUTE CTX2 = GR-AGE +                                  
           ((X4 + (GR-LF-TERM - (GR-LF-REMTERM + 1))) / 12) + 1. 
                                                                 
    IF CLAS-I-RL-AH (CLAS-INDEXL) = 'L' OR 'P'                   
        MOVE GR-LFBEN           TO WK-RSV                        
    ELSE                                                         
        COMPUTE WK-RSV = GR-MO-DEC * X3.                         
                                                                 
    IF WK-RSV GREATER WS-LIFE-LIMIT                              
        MOVE WS-LIFE-LIMIT      TO WK-RSV.                       
                                                                 
    COMPUTE QX ROUNDED = (RF-CX-FACTOR (CTX2) /                  
                          RF-DX-FACTOR (CTX)) * RF-RESV-ADJ.     
                                                                 
    COMPUTE WK-RESV = WK-RESV + (WK-RSV * QX)                    
                                                                 
    SUBTRACT +1               FROM X3.                           
                                                                 
    GO TO 0461-MONTH-LOOP.                                       
                                                                 
0463-ADJUST-TOT.                                                 
                                                                 
    COMPUTE WRK-GR-RESV = WK-RESV / +12.                         
                                                                 
    IF CLAS-I-JOINT (CLAS-INDEXL) = 'J'                          
        COMPUTE WRK-GR-RESV ROUNDED =                            
                         WRK-GR-RESV * WS-FACT.                  
                                                                 
