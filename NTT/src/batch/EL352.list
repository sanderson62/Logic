             & ' CONTROL GROUP ' TO WS-ABEND-MESSAGE             
          PERFORM ABEND-PGM                                      
        END-IF                                                   
                                                                 
        IF CQ-ENTRY-TYPE NOT = 'Q'                               
            EXIT PERFORM CYCLE                                   
        END-IF                                                   
                                                                 
        IF OPTION = '2'                                          
          AND CQ-TIMES-PRINTED NOT GREATER THAN ZERO             
            EXIT PERFORM                                         
        END-IF                                                   
                                                                 
        IF OPTION = '3'                                          
          AND CQ-TIMES-PRINTED GREATER THAN ZERO                 
