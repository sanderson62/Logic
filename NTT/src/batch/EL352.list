    IF PI-VALID-RCD-SW NOT = '1'                                 
        MOVE 'THERE ARE NO CHECKS ON THE FILE TO BE RELEASED - ' 
           & 'NOTIFY LOGIC' TO WS-ABEND-MESSAGE                  
        display 'THERE ARE NO CHECKS ON  FILE TO BE RELEASED - ' 
