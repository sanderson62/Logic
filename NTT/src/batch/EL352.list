    END-EVALUATE                                                 
                                                                 
                                                                 
    MOVE BE-MAIL-TO-NAME        TO  NLR-BENE-NAME.               
    MOVE BE-ADDRESS-LINE-1      TO  NLR-BENE-ADDR1.              
    MOVE BE-ADDRESS-LINE-2      TO  NLR-BENE-ADDR2.              
    MOVE BE-CITY                TO  NLR-BENE-CITY.               
    MOVE BE-STATE               TO  NLR-BENE-STATE               
    MOVE BE-ZIP-CODE            TO  NLR-BENE-ZIP.                
                                                                 
    .                                                            
4050-EXIT.                                                       
    EXIT.                                                        
                                                                 
4100-MASK-LOAN-NO SECTION.                                       
                                                                 
     MOVE 'N'                 TO WRK-END-FOUND.                  
     MOVE +0                  TO SUB1 SUB2                       
     PERFORM VARYING SUB FROM 25 BY -1                           
             UNTIL END-FOUND OR SUB = 0                          
         IF WRK-LOAN-NO-CHAR (SUB) NOT EQUAL SPACE               
             MOVE 'Y'         TO WRK-END-FOUND                   
             IF SUB > 15                                         
                COMPUTE SUB1 = SUB - 15                          
             ELSE                                                
                MOVE +1      TO SUB1                             
             END-IF                                              
             COMPUTE SUB2 = SUB - 4                              
         END-IF                                                  
     END-PERFORM.                                                
                                                                 
     IF SUB2 <= 0                                                
         GO TO 4100-EXIT                                         
     END-IF.                                                     
                                                                 
     INSPECT WRK-LOAN-NUMBER (1:SUB2) CONVERTING                 
         'ABCDEFGHIJKLMNOPQRSTUVWYZ' TO                          
         'XXXXXXXXXXXXXXXXXXXXXXXXX'.                            
     INSPECT WRK-LOAN-NUMBER (1:SUB2) CONVERTING                 
         '0123456789' TO 'XXXXXXXXXX'.                           
                                                                 
4100-EXIT.                                                       
    EXIT.                                                        
                                                                 
4200-TOTAL-PAYMENTS.                                             
                                                                 
      MOVE SPACES                 TO WRK-PAYEE-NAME-1            
                                     WRK-PAYEE-NAME-2            
                                     WRK-PAYEE-NAME-3            
                                     WRK-PAYEE-NAME-4.           
      MOVE +0                     TO WRK-TOTAL-AMT-PAID-1        
                                     WRK-TOTAL-AMT-PAID-2        
                                     WRK-TOTAL-AMT-PAID-3        
                                     WRK-TOTAL-AMT-PAID-4.       
      MOVE LOW-VALUES             TO WRK-SAVE-THRU-1             
                                     WRK-SAVE-THRU-2             
                                     WRK-SAVE-THRU-3             
                                     WRK-SAVE-THRU-4.            
      MOVE HIGH-VALUES            TO WRK-SAVE-FROM-1             
                                     WRK-SAVE-FROM-2             
                                     WRK-SAVE-FROM-3             
                                     WRK-SAVE-FROM-4.            
      MOVE SPACES                 TO WRK-PAID-FROM-1             
                                     WRK-PAID-THRU-1             
                                     WRK-PAID-FROM-2             
                                     WRK-PAID-THRU-2             
                                     WRK-PAID-FROM-3             
                                     WRK-PAID-THRU-3             
                                     WRK-PAID-FROM-4             
                                     WRK-PAID-THRU-4.            
                                                                 
      MOVE CL-CONTROL-PRIMARY     TO  AT-CONTROL-PRIMARY.        
      MOVE +100   TO  AT-SEQUENCE-NO.                            
                                                                 
      START ELTRLR KEY IS NOT < AT-CONTROL-PRIMARY.              
      IF TRLR-STATUS = '00'                                      
         CONTINUE                                                
      ELSE                                                       
         MOVE TRLR-STATUS TO WS-ABEND-FILE-STATUS                
         MOVE 'START ERROR ON ELTRLR FILE' TO WS-ABEND-MESSAGE   
         PERFORM ABEND-PGM                                       
      END-IF.                                                    
                                                                 
4200-TRLR-READ-LOOP.                                             
                                                                 
      READ ELTRLR NEXT RECORD.                                   
      IF TRLR-STATUS = '00'                                      
         CONTINUE                                                
      ELSE                                                       
        IF TRLR-STATUS = '10'                                    
           GO TO 4200-EXIT                                       
        ELSE                                                     
           MOVE TRLR-STATUS TO WS-ABEND-FILE-STATUS              
           MOVE 'READ ERROR ON ELTRLR FILE' TO WS-ABEND-MESSAGE  
           PERFORM ABEND-PGM                                     
        END-IF                                                   
      END-IF.                                                    
                                                                 
      IF CL-CONTROL-PRIMARY NOT EQUAL AT-CONTROL-PRIMARY (1:20)  
          GO TO 4200-EXIT                                        
      END-IF.                                                    
                                                                 
      IF CORRESPONDENCE-TR AND                                   
          AT-STD-LETTER-FORM = 'CI05' AND                        
         (AT-STOP-LETTER-DT = LOW-VALUES OR SPACES)              
             MOVE 'CI05' TO WRK-EOB-LU-CODE (WRK-EOB-SUB)        
             ADD +1 TO WRK-EOB-SUB                               
      END-IF                                                     
                                                                 
      IF NOT PAYMENT-TR                                          
          GO TO 4200-TRLR-READ-LOOP                              
      END-IF.                                                    
                                                                 
      IF AT-VOID-DT NOT EQUAL LOW-VALUES                         
         GO TO 4200-TRLR-READ-LOOP                               
      END-IF.                                                    
                                                                 
      IF AT-TO-BE-WRITTEN-DT > LOW-VALUES                        
        AND AT-CHECK-WRITTEN-DT = LOW-VALUES                     
        AND AT-VOID-DT = LOW-VALUES                              
        AND AT-PAID-THRU-DT > WS-HOLD-UNTIL-DT                   
         MOVE AT-PAID-THRU-DT TO WS-HOLD-UNTIL-DT                
      END-IF                                                     
                                                                 
      IF AT-PAID-THRU-DT GREATER THAN CPA-PAID-THRU-DT           
         GO TO 4200-TRLR-READ-LOOP                               
      END-IF.                                                    
                                                                 
      IF AT-AMOUNT-PAID NOT EQUAL ZERO AND                       
        (AT-PAYEES-NAME EQUAL SPACES OR LOW-VALUES)              
           MOVE 'Adjustment to Claim' TO AT-PAYEES-NAME          
      END-IF                                                     
                                                                 
*>****SUMMARIZE AHL PAYMENTS PRIOR TO 4/1/2012 TOGETHER            
      IF DTE-CLIENT = 'AHL'                                      
         IF AT-CHECK-WRITTEN-DT < X'A861'                        
             MOVE 'AHL SUMMARY' TO AT-PAYEES-NAME                
         END-IF                                                  
      END-IF                                                     
                                                                 
      IF WRK-PAYEE-NAME-1 EQUAL SPACES OR                        
         WRK-PAYEE-NAME-1 EQUAL AT-PAYEES-NAME                   
          MOVE AT-PAYEES-NAME  TO  WRK-PAYEE-NAME-1              
          ADD  AT-AMOUNT-PAID  TO  WRK-TOTAL-AMT-PAID-1          
          IF AT-PAID-FROM-DT LESS THAN WRK-SAVE-FROM-1           
             MOVE AT-PAID-FROM-DT   TO  WRK-SAVE-FROM-1          
             MOVE AT-PAID-FROM-DT   TO  DC-BIN-DATE-1            
             MOVE SPACES            TO  DC-OPTION-CODE           
             PERFORM 8500-DATE-CONVERSION                        
             IF DC-ERROR-CODE EQUAL TO SPACES                    
                MOVE DC-GREG-DATE-A-EDIT  TO  WRK-PAID-FROM-1    
             END-IF                                              
          END-IF                                                 
          IF AT-PAID-THRU-DT GREATER THAN WRK-SAVE-THRU-1        
             MOVE AT-PAID-THRU-DT   TO  WRK-SAVE-THRU-1          
             MOVE AT-PAID-THRU-DT   TO  DC-BIN-DATE-1            
             MOVE SPACES            TO  DC-OPTION-CODE           
             PERFORM 8500-DATE-CONVERSION                        
             IF DC-ERROR-CODE EQUAL TO SPACES                    
                MOVE DC-GREG-DATE-A-EDIT  TO  WRK-PAID-THRU-1    
             END-IF                                              
          END-IF                                                 
          GO TO 4200-TRLR-READ-LOOP                              
      END-IF.                                                    
                                                                 
      IF WRK-PAYEE-NAME-2 EQUAL SPACES OR                        
         WRK-PAYEE-NAME-2 EQUAL AT-PAYEES-NAME                   
          MOVE AT-PAYEES-NAME  TO  WRK-PAYEE-NAME-2              
          ADD  AT-AMOUNT-PAID  TO  WRK-TOTAL-AMT-PAID-2          
          IF AT-PAID-FROM-DT LESS THAN WRK-SAVE-FROM-2           
             MOVE AT-PAID-FROM-DT   TO  WRK-SAVE-FROM-2          
             MOVE AT-PAID-FROM-DT   TO  DC-BIN-DATE-1            
             MOVE SPACES            TO  DC-OPTION-CODE           
             PERFORM 8500-DATE-CONVERSION                        
             IF DC-ERROR-CODE EQUAL TO SPACES                    
                MOVE DC-GREG-DATE-A-EDIT  TO  WRK-PAID-FROM-2    
             END-IF                                              
          END-IF                                                 
          IF AT-PAID-THRU-DT GREATER THAN WRK-SAVE-THRU-2        
             MOVE AT-PAID-THRU-DT   TO  WRK-SAVE-THRU-2          
             MOVE AT-PAID-THRU-DT   TO  DC-BIN-DATE-1            
             MOVE SPACES            TO  DC-OPTION-CODE           
             PERFORM 8500-DATE-CONVERSION                        
             IF DC-ERROR-CODE EQUAL TO SPACES                    
                MOVE DC-GREG-DATE-A-EDIT  TO  WRK-PAID-THRU-2    
             END-IF                                              
          END-IF                                                 
          GO TO 4200-TRLR-READ-LOOP                              
      END-IF.                                                    
                                                                 
      IF WRK-PAYEE-NAME-3 EQUAL SPACES OR                        
         WRK-PAYEE-NAME-3 EQUAL AT-PAYEES-NAME                   
          MOVE AT-PAYEES-NAME  TO  WRK-PAYEE-NAME-3              
          ADD  AT-AMOUNT-PAID  TO  WRK-TOTAL-AMT-PAID-3          
          IF AT-PAID-FROM-DT LESS THAN WRK-SAVE-FROM-3           
             MOVE AT-PAID-FROM-DT   TO  WRK-SAVE-FROM-3          
             MOVE AT-PAID-FROM-DT   TO  DC-BIN-DATE-1            
             MOVE SPACES            TO  DC-OPTION-CODE           
             PERFORM 8500-DATE-CONVERSION                        
             IF DC-ERROR-CODE EQUAL TO SPACES                    
                MOVE DC-GREG-DATE-A-EDIT  TO  WRK-PAID-FROM-3    
             END-IF                                              
          END-IF                                                 
          IF AT-PAID-THRU-DT GREATER THAN WRK-SAVE-THRU-3        
             MOVE AT-PAID-THRU-DT   TO  WRK-SAVE-THRU-3          
             MOVE AT-PAID-THRU-DT   TO  DC-BIN-DATE-1            
             MOVE SPACES            TO  DC-OPTION-CODE           
             PERFORM 8500-DATE-CONVERSION                        
             IF DC-ERROR-CODE EQUAL TO SPACES                    
                MOVE DC-GREG-DATE-A-EDIT  TO  WRK-PAID-THRU-3    
             END-IF                                              
          END-IF                                                 
          GO TO 4200-TRLR-READ-LOOP                              
      END-IF.                                                    
                                                                 
      MOVE 'OTHERS PAID'       TO WRK-PAYEE-NAME-4.              
      ADD  AT-AMOUNT-PAID      TO WRK-TOTAL-AMT-PAID-4.          
      IF AT-PAID-FROM-DT LESS THAN WRK-SAVE-FROM-4               
         MOVE AT-PAID-FROM-DT   TO  WRK-SAVE-FROM-4              
         MOVE AT-PAID-FROM-DT   TO  DC-BIN-DATE-1                
         MOVE SPACES            TO  DC-OPTION-CODE               
         PERFORM 8500-DATE-CONVERSION                            
         IF DC-ERROR-CODE EQUAL TO SPACES                        
            MOVE DC-GREG-DATE-A-EDIT  TO  WRK-PAID-FROM-4        
         END-IF                                                  
