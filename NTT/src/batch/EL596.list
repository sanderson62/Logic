*>    display ' group   ' ch-grouping                              
*>    display ' state   ' ch-state                                 
*>    display ' acct    ' ch-account                               
*>    display ' cert  **' ch-cert-no '**'                          
*>    display ' seq     ' ch-sequence-no                           
    read erchek                                                  
                                                                 
    if erchek-file-status = '00'                                 
