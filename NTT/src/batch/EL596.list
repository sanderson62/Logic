          perform 0055-update-erchek                             
                                thru 0055-exit                   
          move '**  D E N I E D   **'                            
                                to ws-d1-message                 
       end-if                                                    
       if db-app-status = '1' or '5'                             
          perform 0500-BUILD-ERPYAJ-RECORD                       
                                thru 0500-exit                   
          move 'I N T E R F A C E D '                            
                                to ws-d1-message                 
       end-if                                                    
    else                                                         
       move '**ERROR** NOT FOUND '                               
                                to ws-d1-message                 
    end-if                                                       
                                                                 
    perform 0200-create-rpt     thru 0200-exit                   
                                                                 
    .                                                            
