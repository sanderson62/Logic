    IF  CL-CLAIM-TYPE = AH-OVERRIDE-L1 OR 'I' OR 'G' OR 'F'      
        OR 'B' OR 'H'                                            
      IF CLAIM-WITHIN-SPECIFIED-RANGE                            
       ADD CM-AH-BENEFIT-AMT TO TB-BENEFIT (REPORT-INDEX)        
       ADD CM-AH-ORIG-TERM   TO TB-TERM    (REPORT-INDEX).       
                                                                 
    IF CLAIM-WITHIN-SPECIFIED-RANGE                              
       IF CM-INSURED-ISSUE-AGE GREATER THAN ZEROS                
          ADD CM-INSURED-ISSUE-AGE TO TB-AGE (REPORT-INDEX)      
       ELSE                                                      
          ADD DTE-DEFAULT-AGE TO TB-AGE (REPORT-INDEX).          
                                                                 
    GO TO 1010-RETURN-FILE.                                      
                                                                 
    EJECT                                                        
1030-PROCESS-EMPLCY.                                             
                                                                 
    IF (CL-CLAIM-TYPE IS EQUAL TO LIFE-OVERRIDE-L1 OR            
        CL-CLAIM-TYPE IS EQUAL TO AH-OVERRIDE-L1 OR              
        CL-CLAIM-TYPE IS EQUAL TO 'I' OR 'G' OR 'F' OR 'O'       
        OR 'B' OR 'H') AND                                       
        CLAIM-IS-OPEN                                            
        IF PM-INS-PLAN-CD IS EQUAL TO ZEROS                      
            ADD +1              TO TB-OPEN-NO-COV (REPORT-INDEX).
                                                                 
    IF CL-CLAIM-TYPE IS EQUAL TO LIFE-OVERRIDE-L1 OR 'O'         
        IF CLAIM-WITHIN-SPECIFIED-RANGE                          
            ADD PM-INS-TOTAL-BENEFIT    TO                       
                                    TB-BENEFIT (REPORT-INDEX)    
            ADD PM-LOAN-TERM            TO                       
                                    TB-TERM    (REPORT-INDEX).   
                                                                 
    IF CL-CLAIM-TYPE IS EQUAL TO AH-OVERRIDE-L1                  
      OR 'I' OR 'G' OR 'F' OR 'B' OR 'H'                         
        IF CLAIM-WITHIN-SPECIFIED-RANGE                          
            ADD PM-INS-TOTAL-BENEFIT    TO                       
                                    TB-BENEFIT (REPORT-INDEX)    
            ADD PM-LOAN-TERM            TO                       
                                    TB-TERM    (REPORT-INDEX).   
                                                                 
    IF CLAIM-WITHIN-SPECIFIED-RANGE                              
        IF PM-INSURED-ISSUE-AGE IS GREATER THAN ZEROS            
