        ELSE                                                     
            MOVE CM-CERT-EFF-DT TO DC-BIN-DATE-1.                
                                                                 
    MOVE CL-INCURRED-DT         TO DC-BIN-DATE-2.                
    MOVE '1'                    TO DC-OPTION-CODE.               
    PERFORM 8500-DATE-CONVERSION THRU 8590-EXIT.                 
                                                                 
    IF NO-CONVERSION-ERROR                                       
       ADD +1 TO TB-CNT-EFF-INC (REPORT-INDEX)                   
       ADD DC-ELAPSED-MONTHS TO TB-EFF-INC (REPORT-INDEX)        
       IF DC-ODD-DAYS-OVER GREATER THAN +0                       
          COMPUTE HOLD-ODD-DAYS-OVER =                           
                  (DC-ODD-DAYS-OVER / +30)                       
          ADD HOLD-ODD-DAYS-OVER TO TB-EFF-INC (REPORT-INDEX).   
                                                                 
*> FILE FIXES DUE TO CLAIM HISTORY ARCHIVE LOGIC MODIFICATIONS.    
                                                                 
1020-CONTINUE.                                                   
                                                                 
    IF  DTE-CLIENT = 'DMD'                                       
            AND                                                  
        CLAIM-IS-CLOSED                                          
        IF  BENEFITS-CHANGED                                     
            ADD +1 TO TB-BENEFITS-CHANGE (REPORT-INDEX)          
        ELSE                                                     
            IF  SETUP-ERRORS                                     
                ADD +1 TO TB-SETUP-ERROR (REPORT-INDEX).         
                                                                 
    IF CLAIM-IS-CLOSED                                           
      IF CL-LAST-CLOSE-DT NOT = LOW-VALUES AND SPACES            
        IF (CL-LAST-CLOSE-DT GREATER THAN BEGIN-BIN-DATE         
          AND CL-LAST-CLOSE-DT NOT GREATER THAN END-BIN-DATE)    
              IF  ESTABLISHED-WITHIN-PERIOD                      
                  ADD +1 TO TB-CLO-CLAIMS (REPORT-INDEX)         
              ELSE                                               
                 IF CLAIM-WITHIN-SPECIFIED-RANGE                 
                      ADD +1 TO TB-CLO-CLAIMS (REPORT-INDEX - 2) 
                 ELSE                                            
                      NEXT SENTENCE                              
        ELSE                                                     
            IF  ESTABLISHED-WITHIN-PERIOD                        
                ADD +1 TO TB-CLO-CLAIMS (REPORT-INDEX + 2)       
            ELSE                                                 
                IF CLAIM-WITHIN-SPECIFIED-RANGE                  
                    ADD +1 TO TB-CLO-CLAIMS (REPORT-INDEX).      
                                                                 
    IF (CL-LAST-REOPEN-DT NOT = LOW-VALUES AND SPACES)           
       IF (CL-LAST-REOPEN-DT GREATER THAN BEGIN-BIN-DATE  AND    
           CL-LAST-REOPEN-DT NOT GREATER THAN END-BIN-DATE)      
           IF ESTABLISHED-WITHIN-PERIOD                          
              ADD +1 TO TB-REOPENED (REPORT-INDEX)               
           ELSE                                                  
              IF CLAIM-WITHIN-SPECIFIED-RANGE                    
                  ADD +1 TO TB-REOPENED (REPORT-INDEX - 2)       
