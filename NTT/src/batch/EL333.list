        MOVE 'CM'               TO  LAST-RECORD-TYPE.            
                                                                 
    IF  CL-CLAIM-TYPE = LIFE-OVERRIDE-L1 OR 'O'                  
        MOVE LIFE-OVERRIDE-L1 TO LAS-CLM-TYPE                    
        SET REPORT-INDEX TO +1                                   
    ELSE                                                         
        MOVE AH-OVERRIDE-L1   TO LAS-CLM-TYPE                    
        SET REPORT-INDEX TO +2.                                  
                                                                 
    IF DTE-TOT-OPT NOT = '2'                                     
        IF CL-FILE-ESTABLISH-DT NOT GREATER THAN BEGIN-BIN-DATE  
           MOVE 'Y'             TO CRITERIA-SW                   
           MOVE 'B'             TO ESTABLISH-SW                  
        END-IF                                                   
        IF CL-FILE-ESTABLISH-DT GREATER THAN END-BIN-DATE        
            MOVE ' '            TO CRITERIA-SW                   
            MOVE 'A'            TO ESTABLISH-SW                  
            ADD 1               TO WS-FUTURE-CLAIMS              
        END-IF                                                   
        IF (CL-FILE-ESTABLISH-DT GREATER BEGIN-BIN-DATE AND      
            CL-FILE-ESTABLISH-DT NOT GREATER END-BIN-DATE)       
            MOVE 'Y'            TO CRITERIA-SW                   
            MOVE 'W'            TO ESTABLISH-SW                  
        ELSE                                                     
            SET REPORT-INDEX UP BY +2                            
        END-IF                                                   
    ELSE                                                         
        IF CL-FILE-ESTABLISH-DT NOT GREATER THAN                 
                                PERIOD-BEGIN-BIN-DATE            
                                                                 
                MOVE 'B'      TO ESTABLISH-SW                    
                MOVE 'Y'      TO CRITERIA-SW                     
        END-IF                                                   
        IF CL-FILE-ESTABLISH-DT GREATER THAN                     
                                END-BIN-DATE                     
                                                                 
            MOVE 'A'          TO ESTABLISH-SW                    
            MOVE ' '          TO CRITERIA-SW                     
            ADD +1 TO TB-NEW-CLAIMS (REPORT-INDEX)               
        END-IF                                                   
        IF (CL-FILE-ESTABLISH-DT GREATER THAN                    
                                 PERIOD-BEGIN-BIN-DATE AND       
            CL-FILE-ESTABLISH-DT NOT GREATER THAN END-BIN-DATE)  
            MOVE 'Y'          TO CRITERIA-SW                     
            MOVE 'W'          TO ESTABLISH-SW                    
        ELSE                                                     
            SET REPORT-INDEX UP BY +2.                           
                                                                 
