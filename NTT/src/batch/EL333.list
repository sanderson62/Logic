            ELSE                                                 
                IF CLAIM-WITHIN-SPECIFIED-RANGE                  
                    ADD +1 TO TB-FRST-PMTS (REPORT-INDEX - 2)    
                ELSE                                             
                    NEXT SENTENCE                                
        ELSE                                                     
            IF ESTABLISHED-WITHIN-PERIOD                         
                ADD WS-CURRENT-PMT-CNTR         TO               
                                TB-CONT-PMTS (REPORT-INDEX)      
            ELSE                                                 
                ADD WS-CURRENT-PMT-CNTR     TO                   
                                TB-CONT-PMTS (REPORT-INDEX - 2). 
                                                                 
    IF WS-CURRENT-PMT-CNTR GREATER THAN +1                       
        IF (HOLD-FIRST-PMT-DATE GREATER BEGIN-BIN-DATE AND       
            HOLD-FIRST-PMT-DATE NOT GREATER END-BIN-DATE)        
            IF ESTABLISHED-WITHIN-PERIOD                         
                ADD +1 TO TB-FRST-PMTS (REPORT-INDEX)            
                SUBTRACT +1 FROM WS-CURRENT-PMT-CNTR             
                ADD WS-CURRENT-PMT-CNTR TO                       
                    TB-CONT-PMTS (REPORT-INDEX)                  
             ELSE                                                
                 IF CLAIM-WITHIN-SPECIFIED-RANGE                 
                     ADD +1 TO TB-FRST-PMTS (REPORT-INDEX - 2)   
                     SUBTRACT +1 FROM WS-CURRENT-PMT-CNTR        
                     ADD WS-CURRENT-PMT-CNTR TO                  
                         TB-CONT-PMTS (REPORT-INDEX - 2)         
                 ELSE                                            
                     NEXT SENTENCE                               
        ELSE                                                     
            IF ESTABLISHED-WITHIN-PERIOD                         
                ADD WS-CURRENT-PMT-CNTR     TO                   
                                TB-CONT-PMTS (REPORT-INDEX)      
            ELSE                                                 
                ADD WS-CURRENT-PMT-CNTR  TO                      
                                TB-CONT-PMTS (REPORT-INDEX - 2). 
                                                                 
    IF WS-ITD-PMT-CNTR = +1                                      
          IF ESTABLISHED-WITHIN-PERIOD                           
             ADD +1 TO TB-FRST-PMTS (REPORT-INDEX + 2)           
          ELSE                                                   
             IF CLAIM-WITHIN-SPECIFIED-RANGE                     
                 ADD +1 TO TB-FRST-PMTS (REPORT-INDEX).          
                                                                 
    IF WS-ITD-PMT-CNTR GREATER THAN +1                           
          IF ESTABLISHED-WITHIN-PERIOD                           
             ADD +1 TO TB-FRST-PMTS (REPORT-INDEX + 2)           
             SUBTRACT +1 FROM WS-ITD-PMT-CNTR                    
             ADD WS-ITD-PMT-CNTR TO                              
                 TB-CONT-PMTS (REPORT-INDEX + 2)                 
          ELSE                                                   
             IF CLAIM-WITHIN-SPECIFIED-RANGE                     
                 ADD +1 TO TB-FRST-PMTS (REPORT-INDEX)           
                 SUBTRACT +1 FROM WS-ITD-PMT-CNTR                
                 ADD WS-ITD-PMT-CNTR TO                          
                     TB-CONT-PMTS (REPORT-INDEX).                
                                                                 
    MOVE +0 TO WS-CURRENT-PMT-CNTR                               
               WS-ITD-PMT-CNTR.                                  
                                                                 
1015-CHECK-FOR-BREAK.                                            
    MOVE LOW-VALUES TO HOLD-FIRST-ACTION-DATE                    
                       HOLD-FIRST-PMT-DATE.                      
                                                                 
    IF  SR-KEY NOT = LAST-SR-KEY                                 
            AND                                                  
        NOT FIRST-RECORD                                         
        PERFORM 7000-REPORT-BREAK-ROUTINE THRU 7099-EXIT         
        MOVE BLANK-RECORD  TO REPORT-TABLE-RECORD.               
                                                                 
    MOVE 'Y'               TO FIRST-RECORD-SW.                   
                                                                 
    IF SR-CONTROL = HIGH-VALUES                                  
       MOVE SPACES         TO TOTAL-VALUE                        
                              TOTAL-DESC                         
