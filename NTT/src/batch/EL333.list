           IF ESTABLISHED-WITHIN-PERIOD                          
              ADD +1 TO TB-AUTO-CLOSE (REPORT-INDEX + 2)         
           ELSE                                                  
             IF CLAIM-WITHIN-SPECIFIED-RANGE                     
                 ADD +1 TO TB-AUTO-CLOSE (REPORT-INDEX).         
                                                                 
    IF (CLAIM-IS-CLOSED AND                                      
       CL-TOTAL-PAID-AMT = +0)                                   
       IF (CL-LAST-CLOSE-DT GREATER THAN BEGIN-BIN-DATE  AND     
           CL-LAST-CLOSE-DT NOT GREATER THAN END-BIN-DATE)       
           IF ESTABLISHED-WITHIN-PERIOD                          
              ADD +1 TO TB-UNPAID-CLOSE (REPORT-INDEX)           
           ELSE                                                  
             IF CLAIM-WITHIN-SPECIFIED-RANGE                     
                  ADD +1 TO TB-UNPAID-CLOSE (REPORT-INDEX - 2)   
             ELSE                                                
                 NEXT SENTENCE                                   
       ELSE                                                      
           IF ESTABLISHED-WITHIN-PERIOD                          
              ADD +1 TO TB-UNPAID-CLOSE (REPORT-INDEX + 2)       
           ELSE                                                  
              IF CLAIM-WITHIN-SPECIFIED-RANGE                    
                  ADD +1 TO TB-UNPAID-CLOSE (REPORT-INDEX).      
                                                                 
    IF (CLAIM-IS-CLOSED AND                                      
       CL-TOTAL-PAID-AMT GREATER THAN +0)                        
       IF (CL-LAST-CLOSE-DT GREATER THAN BEGIN-BIN-DATE  AND     
           CL-LAST-CLOSE-DT NOT GREATER THAN END-BIN-DATE)       
           IF ESTABLISHED-WITHIN-PERIOD                          
              ADD +1 TO TB-PAID-CLOSE (REPORT-INDEX)             
           ELSE                                                  
              IF CLAIM-WITHIN-SPECIFIED-RANGE                    
                  ADD +1 TO TB-PAID-CLOSE (REPORT-INDEX - 2)     
              ELSE                                               
                  NEXT SENTENCE                                  
       ELSE                                                      
           IF ESTABLISHED-WITHIN-PERIOD                          
              ADD +1 TO TB-PAID-CLOSE (REPORT-INDEX + 2)         
           ELSE                                                  
              IF CLAIM-WITHIN-SPECIFIED-RANGE                    
                  ADD +1 TO TB-PAID-CLOSE (REPORT-INDEX).        
                                                                 
    IF LAST-RECORD-TYPE IS EQUAL TO 'PM'                         
        GO TO 1030-PROCESS-EMPLCY.                               
                                                                 
    IF (CL-CLAIM-TYPE = LIFE-OVERRIDE-L1 OR 'O')                 
