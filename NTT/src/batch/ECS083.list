    ADD GR-ALT-RESV  TO X-ALTRSV.                                
                                                                 
    IF GR-IG = '1'                                               
        GO TO 0190-LIFE-IND.                                     
                                                                 
0180-LIFE-GRP.                                                   
    IF GR-LF-TERM LESS THAN +121                                 
       ADD GR-RESV TO X-GUNDR                                    
    ELSE                                                         
       ADD GR-RESV TO X-GOVER.                                   
                                                                 
    GO TO 0200-END-LIFE.                                         
                                                                 
0190-LIFE-IND.                                                   
    IF GR-LF-TERM LESS THAN +121                                 
       ADD GR-RESV TO X-IUNDR                                    
    ELSE                                                         
       ADD GR-RESV TO X-IOVER.                                   
                                                                 
0200-END-LIFE.                                                   
    IF GR-AHTYP NOT = ZEROS                                      
        IF GR-SUMMARY-REC                                        
            NEXT SENTENCE                                        
         ELSE                                                    
            ADD +1 TO X-DUP.                                     
                                                                 
    IF X1 = +1                                                   
        MOVE X-DETL   TO X-AMTS (X2 X3)                          
        MOVE X-M-DETL TO X-M-AMTS (X2 X3)                        
    ELSE                                                         
        MOVE X-DETL   TO Y-AMTS (X2 X3)                          
        MOVE X-M-DETL TO Y-M-AMTS (X2 X3).                       
                                                                 
0210-FIND-AH.                                                    
    IF GR-AHTYP = ZEROS                                          
        GO TO 0130-READ-RTN.                                     
                                                                 
    MOVE '2'      TO X-LAH.                                      
    MOVE GR-AHTYP TO X-BEN.                                      
    MOVE ZEROS    TO X-SEQ2.                                     
                                                                 
    IF GR-REIN = 'R'                                             
        MOVE GR-REIN-COMP TO X-REIN                              
    ELSE                                                         
        MOVE LOW-VALUE    TO X-REIN.                             
                                                                 
    PERFORM 0300-LOCATE-X3 THRU 0320-LOCATE-X3-XIT.              
                                                                 
    IF X1 = +1                                                   
        MOVE X-AMTS (X2 X3)   TO X-DETL                          
        MOVE X-M-AMTS (X2 X3) TO X-M-DETL                        
    ELSE                                                         
        MOVE Y-AMTS (X2 X3)   TO X-DETL                          
        MOVE Y-M-AMTS (X2 X3) TO X-M-DETL.                       
                                                                 
    IF GR-SUMMARY-REC                                            
       ADD GR-CNT-AH TO X-COUNT                                  
     ELSE                                                        
       ADD +1        TO X-COUNT.                                 
                                                                 
    ADD GR-AHPRM     TO X-WRITTEN.                               
    ADD GRR-AHPRM    TO X-P78.                                   
    ADD GRP-AHPRM    TO X-PRATA.                                 
    IF DTE-CLIENT = 'DCC' or 'VPP'                               
       ADD GRD-AHPRM    TO X-DOMICILE                            
    ELSE                                                         
       COMPUTE WRK-DOMI ROUNDED = (GRR-AHPRM + GRP-AHPRM) / 2    
       ADD WRK-DOMI     TO X-DOMICILE                            
    END-IF.                                                      
    compute x-state = x-state +                                  
       grs-ahprm + gr-loaded-stat-uep                            
    ADD GR-AHCOM     TO X-PAID.                                  
    ADD GRR-AHCOM    TO X-C78.                                   
    ADD GRP-AHCOM    TO X-CRATA.                                 
    COMPUTE WRK-DOMI ROUNDED = (GRR-AHCOM + GRP-AHCOM) / 2.      
