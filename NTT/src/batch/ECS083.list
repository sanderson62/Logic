    IF X1 = +2                                                   
      IF YT-SEQ (X2 X3) = HIGH-VALUE  OR                         
         YT-SEQ (X2 X3) = X-SEQ                                  
          MOVE X-SEQ TO YT-SEQ (X2 X3)                           
          GO TO 0320-LOCATE-X3-XIT.                              
                                                                 
    GO TO 0310-LOOP-LX3.                                         
                                                                 
0320-LOCATE-X3-XIT.                                              
    EXIT.                                                        
                                                                 
0330-PRESS-RTN.                                                  
    MOVE +0 TO X1                                                
               X-DUP                                             
               X-COUNT                                           
               X-WRITTEN                                         
               X-P78                                             
               X-PRATA                                           
               X-DOMICILE                                        
               X-STATE                                           
               X-RESERV                                          
               X-ALTRSV                                          
               X-REMAIN                                          
               X-PAID                                            
               X-C78                                             
               X-CRATA                                           
               X-CDOMI                                           
               X-TAX                                             
               X-T78                                             
               X-TRATA                                           
               X-TDOMI                                           
               X-IUNDR                                           
               X-IOVER                                           
               X-GUNDR                                           
               X-GOVER.                                          
                                                                 
