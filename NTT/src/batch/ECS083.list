    MOVE +1 TO X3.                                               
                                                                 
0420-PRESS-ONE-XIT.                                              
    EXIT.                                                        
                                                                 
                                                                 
0430-GEN-RTN.                                                    
*>*   COMPUTE W-YR = (RUN-CC - W-YR) + 1.                          
    COMPUTE WORK-YR = (RUN-CCYY - W-YR) + 1.                     
                                                                 
*>*   IF W-YR < 0                                                  
*>*      ADD 100 TO W-YR.                                          
    IF WORK-YR < 0                                               
       ADD 100 TO WORK-YR.                                       
    MOVE WORK-YR TO W-YR.                                        
                                                                 
0440-GEN-LOOP.                                                   
    IF DTE-PGM-OPT NOT GREATER THAN 1                            
        PERFORM 0460-RLS-RTN THRU 0470-RLS-XIT.                  
                                                                 
    MOVE HIGH-VALUE TO W-ST.                                     
                                                                 
    IF DTE-PGM-OPT NOT GREATER THAN 2                            
        PERFORM 0460-RLS-RTN THRU 0470-RLS-XIT.                  
                                                                 
    MOVE HIGH-VALUE TO W-CO.                                     
                                                                 
    IF DTE-PGM-OPT NOT GREATER THAN 4                            
        PERFORM 0460-RLS-RTN THRU 0470-RLS-XIT.                  
                                                                 
    MOVE HIGH-VALUE TO W-CARR.                                   
                                                                 
    IF DTE-PGM-OPT NOT GREATER THAN 6                            
        PERFORM 0460-RLS-RTN THRU 0470-RLS-XIT.                  
                                                                 
    MOVE PRE-SEQ    TO W-SEQ1.                                   
    MOVE HIGH-VALUE TO W-CO.                                     
                                                                 
    IF DTE-PGM-OPT NOT GREATER THAN 3                            
        PERFORM 0460-RLS-RTN THRU 0470-RLS-XIT.                  
                                                                 
    MOVE HIGH-VALUE TO W-CARR.                                   
                                                                 
    IF DTE-PGM-OPT NOT GREATER THAN 5                            
        PERFORM 0460-RLS-RTN THRU 0470-RLS-XIT.                  
                                                                 
    IF W-REIN NOT = PRE-REIN                                     
        MOVE PRE-SEQ  TO W-SEQ1                                  
        MOVE PRE-REIN TO W-REIN                                  
        GO TO 0440-GEN-LOOP.                                     
                                                                 
0450-GEN-XIT.                                                    
    EXIT.                                                        
                                                                 
