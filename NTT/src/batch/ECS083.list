    ADD +1 TO X3.                                                
                                                                 
    IF X3 GREATER THAN MAX-BEN                                   
        PERFORM 0390-PRESS-ONE-RTN THRU 0420-PRESS-ONE-XIT.      
                                                                 
    IF X1 = +1                                                   
      IF XT-SEQ (X2 X3) = HIGH-VALUE  OR                         
