*>                                                                 
*>    CARD INPUT 02 EDIT - CLPS PROGRAM SWITCH OVERRIDE            
*>                       - PRTO PRINT SWITCH OVERRIDE              
*>                       - PRCO PROCESS SWITCH OVERRIDE            
*>                       - FMTO FORMAT SWITCH OVERRIDE             
*>                       - TOTO TOTAL SWITCH OVERRIDE              
                                                                 
*>                                                                 
C02-START.                                                       
    MOVE ZERO                    TO X1.                          
                                                                 
C02-LOOP-1.                                                      
    ADD 1 TO X1.                                                 
                                                                 
    IF X1 IS GREATER THAN 12                                     
        GO TO C02-TEST.                                          
                                                                 
    IF CLPS-SWITCH (X1) = SPACE                                  
        GO TO C02-TEST.                                          
                                                                 
    MOVE CLPS-SWITCH (X1)       TO ERR-DETAIL.                   
                                                                 
    IF CLPS-PROG (X1) IS NOT NUMERIC                             
        MOVE ZERO               TO CLPS-PROG (X1)                
        MOVE 'X'                TO CLPS-SW (X1)                  
        MOVE 92                 TO ERR-NBR                       
        PERFORM LOAD-START THRU LOAD-EXIT.                       
                                                                 
    MOVE CLPS-SW (X1)           TO TEST-OPT.                     
                                                                 
    INSPECT TEST-OPT CONVERTING ALPH-TBL TO SPACE.               
                                                                 
    IF TEST-OPT = 'S' OR 'T'                                     
        NEXT SENTENCE                                            
     ELSE                                                        
        IF TEST-OPT NOT = SPACE                                  
           MOVE 'X'             TO CLPS-SW (X1)                  
           MOVE 93              TO ERR-NBR                       
           PERFORM LOAD-START THRU LOAD-EXIT.                    
                                                                 
    MOVE CLPS-SW (X1)           TO TEST-OPT.                     
                                                                 
    IF (TEST-OPT IS NOT NUMERIC) AND                             
        (CLPS-ID = 'CLPS' OR 'PRCO') AND                         
        (TEST-OPT NOT = 'X')                                     
        MOVE 1                  TO FICHE-SW.                     
                                                                 
    IF CLPS-ID = 'PRTO'                                          
        IF TEST-OPT = 'B' OR 'F'                                 
           MOVE 1               TO FICHE-SW.                     
                                                                 
    INSPECT TEST-OPT REPLACING ALL ALPH-TBL BY NUM-TBL.          
                                                                 
    MOVE ZERO                   TO X2.                           
                                                                 
C02-LOOP-2.                                                      
    ADD 1 TO X2.                                                 
                                                                 
    IF (PROG-NM-TB (X2) = HIGH-VALUES) OR                        
       (X2 = +1502)                                              
          MOVE 94               TO ERR-NBR                       
          PERFORM LOAD-START THRU LOAD-EXIT                      
          GO TO C02-LOOP-1.                                      
                                                                 
    IF PNT-PROG (X2) IS NOT NUMERIC                              
        GO TO C02-LOOP-2.                                        
                                                                 
    IF PNT-PROG (X2) = CLPS-PROG (X1)                            
        GO TO C02-LOOP-1.                                        
                                                                 
    GO TO C02-LOOP-2.                                            
                                                                 
C02-TEST.                                                        
    PERFORM ERROR-START THRU ERROR-EXIT.                         
                                                                 
    IF ERROR-SW = 1                                              
        GO TO C02-EXIT.                                          
                                                                 
    MOVE ZERO TO X1.                                             
                                                                 
C02-LOOP-4.                                                      
    ADD 1 TO X1.                                                 
                                                                 
    IF CLPS-SWITCH (X1) = SPACE                                  
        GO TO C02-SET-PRINT.                                     
                                                                 
    MOVE CLPS-PROG (X1)         TO X2.                           
                                                                 
    IF CLAS-ID = 'PRTO'                                          
      GO TO C02-ADD-PRT-SW.                                      
                                                                 
    IF CLAS-ID = 'FMTO'                                          
      GO TO C02-ADD-FMT-SW.                                      
                                                                 
    IF CLAS-ID = 'PRCO' OR 'CLPS'                                
      GO TO C02-ADD-PRC-SW.                                      
                                                                 
    GO TO C02-ADD-TOT-SW.                                        
                                                                 
C02-ADD-PRT-SW.                                                  
    IF PROG-OR-PRT (X2) = 'O'                                    
        MOVE CLPS-SWITCH (X1)   TO ERR-DETAIL                    
        MOVE 96                 TO ERR-NBR                       
        PERFORM LOAD-START THRU LOAD-EXIT                        
        GO TO C02-EXIT.                                          
                                                                 
    MOVE CLPS-SW (X1)           TO PROG-TBL-PRT (X2).            
    MOVE 'O'                    TO PROG-OR-PRT  (X2).            
    GO TO C02-LOOP-4.                                            
                                                                 
C02-ADD-FMT-SW.                                                  
    IF PROG-OR-FMT (X2) = 'O'                                    
        MOVE CLPS-SWITCH (X1)   TO ERR-DETAIL                    
        MOVE 96                 TO ERR-NBR                       
        PERFORM LOAD-START THRU LOAD-EXIT                        
        GO TO C02-EXIT.                                          
                                                                 
    MOVE CLPS-SW (X1)           TO PROG-TBL-FMT (X2).            
    MOVE 'O'                    TO PROG-OR-FMT  (X2).            
    GO TO C02-LOOP-4.                                            
                                                                 
C02-ADD-PRC-SW.                                                  
    IF PROG-OR-PRC (X2) = 'O'                                    
        MOVE CLPS-SWITCH (X1)   TO ERR-DETAIL                    
        MOVE 96                 TO ERR-NBR                       
        PERFORM LOAD-START THRU LOAD-EXIT                        
        GO TO C02-EXIT.                                          
                                                                 
    MOVE CLPS-SW (X1)           TO PROG-TBL-PRC (X2).            
    MOVE 'O'                    TO PROG-OR-PRC  (X2).            
    GO TO C02-LOOP-4.                                            
                                                                 
C02-ADD-TOT-SW.                                                  
    IF PROG-OR-TOT (X2) = 'O'                                    
        MOVE CLPS-SWITCH (X1)   TO ERR-DETAIL                    
        MOVE 96                 TO ERR-NBR                       
        PERFORM LOAD-START THRU LOAD-EXIT                        
        GO TO C02-EXIT.                                          
                                                                 
    MOVE CLPS-SW (X1)           TO PROG-TBL-TOT (X2).            
    MOVE 'O'                    TO PROG-OR-TOT  (X2).            
    GO TO C02-LOOP-4.                                            
                                                                 
C02-SET-PRINT.                                                   
    PERFORM C02-TRANSFORM THRU C02-TRANS-EXIT                    
      VARYING X1 FROM +1 BY +1 UNTIL X1 GREATER +1500.           
                                                                 
    GO TO C02-EXIT.                                              
                                                                 
C02-TRANSFORM.                                                   
    IF PROG-SYS-NAME (X1) NOT = 'EL' OR 'EC' OR 'GL'             
        GO TO C02-TRANS-EXIT.                                    
                                                                 
    IF PROG-TBL (X1) NOT NUMERIC                                 
        GO TO C02-TRANS-EXIT.                                    
                                                                 
    IF PROG-TBL-PRT (X1) = '9' OR SPACE                          
        MOVE 'P'                TO PROG-TBL-PRT (X1).            
                                                                 
    IF PROG-TBL-PRC (X1) NUMERIC                                 
        GO TO C02-TRANS-EXIT.                                    
                                                                 
    IF PROG-TBL-PRC (X1) NOT LESS 'A' AND                        
       PROG-TBL-PRC (X1) NOT GREATER 'I'                         
        MOVE 'F'                TO PROG-TBL-PRT (X1).            
                                                                 
    IF PROG-TBL-PRC (X1) NOT LESS 'J' AND                        
       PROG-TBL-PRC (X1) NOT GREATER 'R'                         
        MOVE 'B'                TO PROG-TBL-PRT (X1).            
                                                                 
    INSPECT PROG-TBL-PRC (X1) REPLACING ALL ALPH-TBL             
        BY NUM-TBL.                                              
                                                                 
C02-TRANS-EXIT.                                                  
    EXIT.                                                        
                                                                 
                                                                 
C02-EXIT.                                                        
    EXIT.                                                        
                                                                 
    EJECT                                                        
