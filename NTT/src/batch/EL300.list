*>    RECORD 03 WRITE - CLAX - INDEX RECORD                        
*>                                                                 
W03-START.                                                       
                                                                 
    MOVE INDEX-TBL TO DATE-DISK.                                 
                                                                 
    PERFORM WRITE-START THRU WRITE-EXIT.                         
                                                                 
W03-EXIT.                                                        
    EXIT.                                                        
    EJECT                                                        
*>                                                                 
*>    RECORD 04 WRITE - CLAC - COMPANY RECORDS                     
*>                                                                 
W04-START.                                                       
                                                                 
    MOVE ZERO                   TO X1.                           
                                                                 
W04-LOOP.                                                        
                                                                 
    ADD 1 TO X1.                                                 
                                                                 
    IF X1 IS GREATER THAN CLAS-MAXC                              
        GO TO W04-EXIT.                                          
                                                                 
    MOVE SPACE                  TO DATE-DISK.                    
                                                                 
    MOVE 'CLAC'                 TO DD-ID.                        
    MOVE X1                     TO DD-CR1.                       
    MOVE COMPANY-LINE (X1)      TO DD-CR1N.                      
                                                                 
    ADD 1 TO X1.                                                 
                                                                 
    IF X1 IS GREATER THAN CLAS-MAXC                              
        PERFORM WRITE-START THRU WRITE-EXIT                      
        GO TO W04-EXIT.                                          
                                                                 
    MOVE X1 TO DD-CR2.                                           
    MOVE COMPANY-LINE (X1)      TO DD-CR2N.                      
                                                                 
    PERFORM WRITE-START THRU WRITE-EXIT.                         
                                                                 
    GO TO W04-LOOP.                                              
                                                                 
W04-EXIT.                                                        
    EXIT.                                                        
    EJECT                                                        
*>                                                                 
*>    RECORD 05 WRITE - CLAF - FACTOR RECORD                       
*>                                                                 
W05-START.                                                       
                                                                 
    MOVE 'CLAF'                 TO FACT-ID.                      
    MOVE FACTOR-TBL             TO DATE-DISK.                    
                                                                 
    PERFORM WRITE-START THRU WRITE-EXIT.                         
                                                                 
W05-EXIT.                                                        
    EXIT.                                                        
    EJECT                                                        
*>                                                                 
*>    RECORD 06 WRITE - STAT - STATE RECORDS                       
*>                                                                 
W06-START.                                                       
                                                                 
    MOVE 1                      TO X1.                           
                                                                 
W06-LOOP-1.                                                      
                                                                 
    MOVE SPACE                  TO DATE-DISK.                    
                                                                 
    MOVE 'STAT'                 TO DD-ID.                        
    MOVE ZERO                   TO X2.                           
                                                                 
W06-LOOP-2.                                                      
                                                                 
    ADD 1                       TO X2.                           
                                                                 
    IF X2 IS GREATER THAN 3                                      
        PERFORM WRITE-START THRU WRITE-EXIT                      
        GO TO W06-LOOP-1.                                        
                                                                 
    MOVE STATE-LINE (X1)        TO DD-ST (X2).                   
                                                                 
    ADD 1 TO X1.                                                 
                                                                 
    IF X1 IS GREATER THAN CLAS-MAXS                              
        PERFORM WRITE-START THRU WRITE-EXIT                      
        GO TO W06-EXIT.                                          
                                                                 
    GO TO W06-LOOP-2.                                            
                                                                 
W06-EXIT.                                                        
    EXIT.                                                        
    EJECT                                                        
*>                                                                 
*>    RECORD 07 WRITE - BUSC - BUSINESS RECORDS                    
*>                                                                 
W07-START.                                                       
                                                                 
    MOVE 1                     TO X1.                            
                                                                 
    IF CLAS-MAXB = ZERO                                          
        GO TO W07-EXIT.                                          
                                                                 
W07-LOOP-1.                                                      
                                                                 
    MOVE SPACE                  TO DATE-DISK.                    
                                                                 
    MOVE 'BUSC'                 TO DD-ID.                        
    MOVE ZERO                   TO X2.                           
                                                                 
W07-LOOP-2.                                                      
                                                                 
    ADD 1                       TO X2.                           
                                                                 
    IF X2 IS GREATER THAN 3                                      
        PERFORM WRITE-START THRU WRITE-EXIT                      
        GO TO W07-LOOP-1.                                        
                                                                 
    MOVE BUSINESS-LINE (X1)     TO DD-ST (X2).                   
                                                                 
    ADD 1                       TO X1.                           
                                                                 
    IF X1 IS GREATER THAN CLAS-MAXB                              
        PERFORM WRITE-START THRU WRITE-EXIT                      
        GO TO W07-EXIT.                                          
                                                                 
    GO TO W07-LOOP-2.                                            
                                                                 
W07-EXIT.                                                        
    EXIT.                                                        
    EJECT                                                        
*>                                                                 
*>    RECORD 08 WRITE - MORT - MORTALITY RECORDS                   
*>                                                                 
W08-START.                                                       
                                                                 
    IF CLAS-MAXM = ZERO                                          
        GO TO W08-EXIT.                                          
                                                                 
    MOVE 1                      TO X1.                           
                                                                 
W08-LOOP-1.                                                      
                                                                 
    MOVE SPACE                  TO DATE-DISK.                    
    MOVE 'MORT'                 TO DD-ID.                        
                                                                 
    MOVE ZERO                   TO X2.                           
                                                                 
W08-LOOP-2.                                                      
                                                                 
    ADD 1 TO X2.                                                 
                                                                 
    IF X2 IS GREATER THAN 2                                      
        PERFORM WRITE-START THRU WRITE-EXIT                      
        GO TO W08-LOOP-1.                                        
                                                                 
    MOVE MORTALITY-LINE (X1)    TO DD-MORT (X2).                 
                                                                 
    ADD 1 TO X1.                                                 
                                                                 
    IF X1 IS GREATER THAN CLAS-MAXM                              
        PERFORM WRITE-START THRU WRITE-EXIT                      
        GO TO W08-EXIT.                                          
                                                                 
    GO TO W08-LOOP-2.                                            
                                                                 
W08-EXIT.                                                        
    EXIT.                                                        
    EJECT                                                        
*>                                                                 
*>    RECORD 09 WRITE - CLAI - INSURANCE RECORDS                   
*>                                                                 
W09-START.                                                       
                                                                 
    MOVE 1 TO X1.                                                
                                                                 
*>******** IF NO A&H, SET WRITE-LOOP TO MAXL                       
    IF CLAS-MAXL GREATER CLAS-MAXA                               
       MOVE CLAS-MAXL           TO CLAS-MAXA.                    
                                                                 
W09-LOOP-1.                                                      
                                                                 
    MOVE SPACE                  TO DATE-DISK.                    
    MOVE 'CLAI'                 TO DD-ID.                        
                                                                 
    MOVE ZERO                   TO X2.                           
                                                                 
W09-LOOP-2.                                                      
                                                                 
    ADD 1 TO X2.                                                 
                                                                 
    IF X2 IS GREATER THAN 2                                      
        PERFORM WRITE-START THRU WRITE-EXIT                      
        GO TO W09-LOOP-1.                                        
                                                                 
    MOVE INSURANCE-LINE (X1) TO DD-TYPE (X2).                    
                                                                 
    ADD 1 TO X1.                                                 
                                                                 
    IF X1 IS GREATER THAN CLAS-MAXA                              
        PERFORM WRITE-START THRU WRITE-EXIT                      
        GO TO W09-EXIT.                                          
                                                                 
    GO TO W09-LOOP-2.                                            
                                                                 
W09-EXIT.                                                        
    EXIT.                                                        
    EJECT                                                        
*>                                                                 
*>    RECORD 10 WRITE - CLXX - PROGRAM SWITCH RECORDS              
*>                                                                 
W10-START.                                                       
                                                                 
    MOVE ZERO                   TO X1 CARD-CNT OPTION-CNT.       
                                                                 
W10-LOOP-1.                                                      
                                                                 
    PERFORM W10-LOOP-2 VARYING CARD-CNT FROM +1 BY +1            
            UNTIL CARD-CNT GREATER +50.                          
                                                                 
    GO TO W10-START-CL.                                          
                                                                 
W10-LOOP-2.                                                      
                                                                 
    PERFORM W10-LOOP-3 VARYING X1 FROM +1 BY +1                  
            UNTIL X1 GREATER +20.                                
                                                                 
    MOVE SPACE                  TO DATE-DISK.                    
                                                                 
    MOVE CARD-CNT               TO DD-IDX.                       
    MOVE 'PO'                   TO DD-IDA.                       
    MOVE PROG-SET TO DD-DATA.                                    
                                                                 
    INSPECT DATE-DISK REPLACING ALL SPACE BY '9'.                
    PERFORM WRITE-START THRU WRITE-EXIT.                         
                                                                 
W10-LOOP-3.                                                      
                                                                 
    ADD 1 TO OPTION-CNT.                                         
                                                                 
    MOVE PROG-TBL-PRT (OPTION-CNT) TO SW-SET-PRT (X1).           
    MOVE PROG-TBL-FMT (OPTION-CNT) TO SW-SET-FMT (X1).           
    MOVE PROG-TBL-PRC (OPTION-CNT) TO SW-SET-PRC (X1).           
    MOVE PROG-TBL-TOT (OPTION-CNT) TO SW-SET-TOT (X1).           
    IF PROG-TBL-PRT (OPTION-CNT) = 'P' OR 'F' OR 'B' OR          
                                   'S' OR 'T'                    
        NEXT SENTENCE                                            
     ELSE                                                        
      MOVE 'P' TO SW-SET-PRT (X1).                               
                                                                 
W10-START-CL.                                                    
                                                                 
    MOVE ZERO                   TO X1 CARD-CNT OPTION-CNT.       
                                                                 
W10-CL-LOOP-1.                                                   
                                                                 
    PERFORM W10-CL-LOOP-2 VARYING CARD-CNT FROM +1 BY +1         
            UNTIL CARD-CNT GREATER +20.                          
                                                                 
    GO TO W10-EXIT.                                              
                                                                 
W10-CL-LOOP-2.                                                   
                                                                 
    PERFORM W10-CL-LOOP-3 VARYING X1 FROM +1 BY +1               
            UNTIL X1 GREATER +50.                                
                                                                 
    MOVE SPACE                 TO DATE-DISK.                     
                                                                 
    MOVE CARD-CNT              TO DD-IDX.                        
    MOVE 'CL'                  TO DD-IDA.                        
    MOVE PROG-SET-2            TO DD-DATA.                       
                                                                 
    INSPECT DATE-DISK REPLACING ALL SPACE BY '9'.                
    PERFORM WRITE-START THRU WRITE-EXIT.                         
                                                                 
W10-CL-LOOP-3.                                                   
                                                                 
    ADD 1 TO OPTION-CNT.                                         
                                                                 
    MOVE PROG-TBL-PRC (OPTION-CNT) TO SW-SET-PRC-2 (X1).         
                                                                 
    IF PROG-TBL-PRT (OPTION-CNT) = 'F'                           
       INSPECT SW-SET-PRC-2 (X1) CONVERTING '123456789' TO       
       'ABCDEFGHI'.                                              
                                                                 
    IF PROG-TBL-PRT (OPTION-CNT) = 'B'                           
       INSPECT SW-SET-PRC-2 (X1) CONVERTING '123456789' TO       
       'JKLMNOPQR'.                                              
                                                                 
W10-EXIT.                                                        
    EXIT.                                                        
    EJECT                                                        
                                                                 
*>                                                                 
*>    RECORD 12 WRITE - CARR - CARRIER NAME TABLE                  
*>                                                                 
W12-START.                                                       
                                                                 
    IF CLAS-MAXCN = ZERO                                         
        GO TO W12-EXIT.                                          
                                                                 
    MOVE ZERO TO X1.                                             
                                                                 
W12-LOOP.                                                        
                                                                 
    ADD +1 TO X1.                                                
                                                                 
    IF X1 GREATER THAN CLAS-MAXCN                                
        GO TO W12-EXIT.                                          
                                                                 
    MOVE SPACE                  TO DATE-DISK.                    
                                                                 
    MOVE 'CARR'                 TO DD-ID.                        
    MOVE CN-CODE (X1)           TO DD-CARR1.                     
    MOVE CN-DOM-ST (X1)         TO DD-CARR1S.                    
    MOVE CN-NAME (X1)           TO DD-CARR1N.                    
    MOVE CN-IBNR-UEP-PCT (X1)   TO DD-CARR1-UEP-PCT.             
    MOVE CN-IBNR-R78-PCT (X1)   TO DD-CARR1-R78-PCT.             
    MOVE CN-IBNR-PRO-PCT (X1)   TO DD-CARR1-PRO-PCT.             
    MOVE CN-SECPAY-SWITCH (X1)  TO DD-CARR1-SECPAY-SW            
                                                                 
    ADD 1 TO X1.                                                 
                                                                 
    MOVE ZEROS                  TO DD-CARR2-UEP-PCT              
                                   DD-CARR2-R78-PCT              
                                   DD-CARR2-PRO-PCT.             
                                                                 
    IF X1 GREATER THAN CLAS-MAXCN                                
        PERFORM WRITE-START THRU WRITE-EXIT                      
        GO TO W12-EXIT.                                          
                                                                 
    MOVE CN-CODE (X1)           TO DD-CARR2.                     
    MOVE CN-DOM-ST (X1)         TO DD-CARR2S.                    
    MOVE CN-NAME (X1)           TO DD-CARR2N.                    
    MOVE CN-IBNR-UEP-PCT (X1)   TO DD-CARR2-UEP-PCT.             
    MOVE CN-IBNR-R78-PCT (X1)   TO DD-CARR2-R78-PCT.             
    MOVE CN-IBNR-PRO-PCT (X1)   TO DD-CARR2-PRO-PCT.             
    MOVE CN-SECPAY-SWITCH (X1)  TO DD-CARR2-SECPAY-SW            
                                                                 
    PERFORM WRITE-START THRU WRITE-EXIT.                         
                                                                 
    GO TO W12-LOOP.                                              
                                                                 
W12-EXIT.                                                        
    EXIT.                                                        
    EJECT                                                        
*>                                                                 
*>    RECORD 13 WRITE - COLC - CLASIC COMPATIBILITY PARAMETERS     
*>                                                                 
W13-START.                                                       
                                                                 
    MOVE CLASIC-TBL             TO DATE-DISK.                    
    PERFORM WRITE-START THRU WRITE-EXIT.                         
                                                                 
W13-EXIT.                                                        
    EXIT.                                                        
    EJECT                                                        
*>                                                                 
*>    RECORD 14 WRITE - OPCA - OPTIONAL CLAIM RESERVE CARRIER DATA 
*>                                                                 
W14-START.                                                       
                                                                 
    IF  NOT OT-OPT-RESERVE-METHOD-AUTH                           
        GO TO W14-EXIT.                                          
                                                                 
    MOVE +0                     TO X2.                           
    MOVE SPACE                  TO DATE-DISK.                    
    MOVE 'OPCA'                 TO DD-ID.                        
                                                                 
    PERFORM W14-LOOP THRU W14-LOOP-EXIT                          
            VARYING                                              
        X1 FROM 1 BY 1                                           
            UNTIL                                                
        X1 GREATER THAN +13                                      
            OR                                                   
        X1 GREATER THAN CLAS-MAXCN.                              
                                                                 
    PERFORM WRITE-START THRU WRITE-EXIT.                         
                                                                 
    IF  X1 GREATER THAN CLAS-MAXCN                               
        GO TO W14-EXIT.                                          
                                                                 
    MOVE +0                     TO X2.                           
    MOVE SPACE                  TO DATE-DISK.                    
    MOVE 'OPCA'                 TO DD-ID.                        
                                                                 
    PERFORM W14-LOOP THRU W14-LOOP-EXIT                          
            VARYING                                              
        X1 FROM X1 BY 1                                          
            UNTIL                                                
        X1 GREATER THAN +25                                      
            OR                                                   
        X1 GREATER THAN CLAS-MAXCN.                              
                                                                 
    PERFORM WRITE-START THRU WRITE-EXIT.                         
    MOVE X1                     TO INDEXCA.                      
                                                                 
    GO TO W14-EXIT.                                              
                                                                 
W14-LOOP.                                                        
                                                                 
    ADD +1                      TO X2.                           
    MOVE CARR-OPT-CLM-FLDS (X1) TO DD-CARR-FLDS (X2).            
                                                                 
W14-LOOP-EXIT.                                                   
    EXIT.                                                        
                                                                 
W14-EXIT.                                                        
    EXIT.                                                        
    EJECT                                                        
*>                                                                 
*>    RECORD 15 WRITE - OPST - OPTIONAL CLAIM RESERVE STATE DATA   
*>                                                                 
W15-START.                                                       
                                                                 
    IF  NOT OT-OPT-RESERVE-METHOD-AUTH                           
        GO TO W15-EXIT.                                          
                                                                 
    MOVE +1                     TO X1.                           
                                                                 
    PERFORM W15-LOOP-A THRU W15-LOOP-A-EXIT                      
            UNTIL                                                
        X1 GREATER THAN +75                                      
            OR                                                   
        X1 GREATER THAN CLAS-MAXS.                               
                                                                 
    MOVE X1                     TO INDEXST.                      
    GO TO W15-EXIT.                                              
                                                                 
W15-LOOP-A.                                                      
                                                                 
    MOVE SPACE                  TO DATE-DISK.                    
    MOVE 'OPST'                 TO DD-ID.                        
                                                                 
    PERFORM W15-LOOP-B THRU W15-LOOP-B-EXIT                      
            VARYING                                              
        X2 FROM 1 BY 1                                           
            UNTIL                                                
        X2 GREATER THAN +16                                      
            OR                                                   
        X1 GREATER THAN CLAS-MAXS                                
            OR                                                   
        X1 GREATER +75.                                          
                                                                 
    PERFORM WRITE-START THRU WRITE-EXIT.                         
                                                                 
W15-LOOP-A-EXIT.                                                 
    EXIT.                                                        
                                                                 
W15-LOOP-B.                                                      
                                                                 
    MOVE STATE-TLR-FLD (X1)     TO DD-STATE-FLDS (X2).           
    ADD +1                      TO X1.                           
                                                                 
W15-LOOP-B-EXIT.                                                 
    EXIT.                                                        
                                                                 
W15-EXIT.                                                        
    EXIT.                                                        
    EJECT                                                        
*>                                                                 
*>    RECORD 16 WRITE - OPBS - OPTIONAL CLAIM RESERVE BUSINESS DATA
*>                                                                 
W16-START.                                                       
                                                                 
    IF  NOT OT-OPT-RESERVE-METHOD-AUTH                           
        GO TO W16-EXIT.                                          
                                                                 
    MOVE +0                     TO X2.                           
    MOVE SPACE                  TO DATE-DISK.                    
    MOVE 'OPBS'                 TO DD-ID.                        
                                                                 
    PERFORM W16-LOOP THRU W16-LOOP-EXIT                          
            VARYING                                              
        X1 FROM 1 BY 1                                           
            UNTIL                                                
        X1 GREATER THAN +32.                                     
                                                                 
    PERFORM WRITE-START THRU WRITE-EXIT.                         
                                                                 
    MOVE +0                     TO X2.                           
    MOVE SPACE                  TO DATE-DISK.                    
    MOVE 'OPBS'                 TO DD-ID.                        
                                                                 
    PERFORM W16-LOOP THRU W16-LOOP-EXIT                          
            VARYING                                              
        X1 FROM X1 BY 1                                          
            UNTIL                                                
        X1 GREATER THAN +64.                                     
                                                                 
    PERFORM WRITE-START THRU WRITE-EXIT.                         
                                                                 
    MOVE +0                     TO X2.                           
    MOVE SPACE                  TO DATE-DISK.                    
    MOVE 'OPBS'                 TO DD-ID.                        
                                                                 
    PERFORM W16-LOOP THRU W16-LOOP-EXIT                          
            VARYING                                              
        X1 FROM X1 BY 1                                          
            UNTIL                                                
        X1 GREATER THAN +96.                                     
                                                                 
    PERFORM WRITE-START THRU WRITE-EXIT.                         
                                                                 
    MOVE +0                     TO X2.                           
    MOVE SPACE                  TO DATE-DISK.                    
    MOVE 'OPBS'                 TO DD-ID.                        
                                                                 
    PERFORM W16-LOOP THRU W16-LOOP-EXIT                          
            VARYING                                              
        X1 FROM X1 BY 1                                          
            UNTIL                                                
        X1 GREATER THAN +99.                                     
                                                                 
    PERFORM WRITE-START THRU WRITE-EXIT.                         
    MOVE X1                     TO INDEXBS.                      
                                                                 
    GO TO W16-EXIT.                                              
                                                                 
W16-LOOP.                                                        
                                                                 
    ADD +1                      TO X2.                           
    MOVE BUS-TLRM-FLD (X1)      TO DD-TLR (X2).                  
                                                                 
W16-LOOP-EXIT.                                                   
    EXIT.                                                        
                                                                 
W16-EXIT.                                                        
    EXIT.                                                        
    EJECT                                                        
*>                                                                 
*>    RECORD 17 WRITE - OPMS - OPTIONAL CLAIM RESERVE MISC DATA    
*>                                                                 
W17-START.                                                       
                                                                 
    IF  NOT OT-OPT-RESERVE-METHOD-AUTH                           
        GO TO W17-EXIT.                                          
                                                                 
    MOVE +0                     TO X2.                           
    MOVE SPACE                  TO DATE-DISK.                    
    MOVE 'OPMS'                 TO DD-ID.                        
    MOVE MISC-OPTIONAL-CLAIM-DATA                                
                                TO DD-MISC-DATA.                 
                                                                 
    PERFORM WRITE-START THRU WRITE-EXIT.                         
                                                                 
W17-EXIT.                                                        
    EXIT.                                                        
    EJECT                                                        
*>                                                                 
*>    SECTION 01 PRINT - COMPANY NAME AND ADDRESS                  
*>                                                                 
P01-START.                                                       
                                                                 
    MOVE ZERO TO X1.                                             
                                                                 
    IF LINE-CNT IS GREATER THAN NEW-PAGE                         
        PERFORM HEAD-START THRU HEAD-EXIT.                       
                                                                 
    MOVE S01-H01                TO SAVE-HEAD-1.                  
                                                                 
    MOVE SPACE                  TO SAVE-HEAD-2.                  
                                                                 
    PERFORM SECT-START THRU SECT-EXIT.                           
                                                                 
    MOVE '(CONTINUED)'          TO S01-C.                        
    MOVE S01-H01                TO SAVE-HEAD-1.                  
                                                                 
P01-LOOP.                                                        
                                                                 
    ADD 1 TO X1.                                                 
                                                                 
    IF X1 IS GREATER THAN CLAS-MAXC                              
        GO TO P01-EXIT.                                          
                                                                 
    IF LINE-CNT IS GREATER THAN END-PAGE                         
        PERFORM HEAD-START THRU HEAD-EXIT                        
        PERFORM SECT-START THRU SECT-EXIT.                       
                                                                 
    MOVE COMPANY-LINE (X1)      TO S01-NAME.                     
                                                                 
    PERFORM PRINT-START THRU PRINT-EXIT.                         
                                                                 
    GO TO P01-LOOP.                                              
                                                                 
P01-EXIT.                                                        
    EXIT.                                                        
    EJECT                                                        
*>                                                                 
*>                                                                 
*>    SECTION 02 PRINT - SELECTED PROGRAM OPTIONS                  
*>                                                                 
P02-START.                                                       
                                                                 
    MOVE ZERO                  TO X1.                            
    MOVE 1                     TO X2 X3.                         
                                                                 
    IF LINE-CNT IS GREATER THAN NEW-PAGE                         
        PERFORM HEAD-START THRU HEAD-EXIT.                       
                                                                 
    MOVE S13-H01                TO SAVE-HEAD-1.                  
    MOVE S13-H02                TO SAVE-HEAD-2.                  
    PERFORM SECT-START THRU SECT-EXIT.                           
                                                                 
    MOVE '(CONTINUED)' TO S13-C.                                 
    MOVE S13-H01                TO SAVE-HEAD-1.                  
                                                                 
P02-LOOP-1.                                                      
                                                                 
    ADD 1 TO X1.                                                 
                                                                 
    IF X1 IS GREATER THAN 1500                                   
        GO TO P02-EXIT.                                          
                                                                 
    IF PROG-TBL-FMT (X1) = '9' AND                               
       PROG-TBL-PRC (X1) = '9' AND                               
       PROG-TBL-TOT (X1) = '9'                                   
        GO TO P02-LOOP-1.                                        
                                                                 
    MOVE PROG-TBL-PRC (X1)      TO TEST-OPT.                     
                                                                 
    MOVE +1 TO X2.                                               
                                                                 
                                                                 
P02-LOOP-2.                                                      
                                                                 
    IF LINE-CNT IS GREATER THAN END-PAGE                         
        PERFORM HEAD-START THRU HEAD-EXIT                        
        PERFORM SECT-START THRU SECT-EXIT.                       
                                                                 
    IF (X2 GREATER +250) OR                                      
*>       (PROG-NM-TB (X2) = HIGH-VALUE)                            
       (Pnt-prog (X2) = HIGH-VALUE)                              
           MOVE '???-'         TO S13-CLAS                       
           MOVE X1             TO S13-PROGRAM                    
           MOVE ALL '*'        TO S13-NAME                       
           GO TO P02-LOOP-2-PRT.                                 
                                                                 
    IF PNT-PROG (X2) NOT = X1                                    
        ADD 1 TO X2                                              
        GO TO P02-LOOP-2.                                        
                                                                 
    MOVE 2                     TO PRT-CTL.                       
                                                                 
    IF PNT-SYSTEM (X2) = 'S'                                     
        MOVE 'ECS-'             TO S13-CLAS                      
     ELSE                                                        
        IF PNT-SYSTEM (X2) = 'G'                                 
            MOVE ' GL-'         TO S13-CLAS                      
        ELSE                                                     
            MOVE ' EL-'         TO S13-CLAS.                     
                                                                 
    MOVE X1                     TO S13-PROGRAM.                  
                                                                 
    MOVE PNT-MESSAGE (X2)       TO S13-NAME.                     
                                                                 
P02-LOOP-2-PRT.                                                  
                                                                 
    PERFORM PRINT-START THRU PRINT-EXIT.                         
                                                                 
    MOVE TEST-OPT               TO S13-OPTION.                   
                                                                 
    IF TEST-OPT = 'X'                                            
        MOVE 'PROGRAM IS NOT TO BE RUN' TO S13-DESC              
     ELSE                                                        
         GO TO P02-RESOLVE-DESCRIP.                              
                                                                 
    PERFORM PRINT-START THRU PRINT-EXIT.                         
                                                                 
    GO TO P02-LOOP-1.                                            
                                                                 
P02-RESOLVE-DESCRIP.                                             
                                                                 
    IF PNT-SYSTEM (X2) = 'E'                                     
        MOVE 'EC'               TO PO-SYSTEM-CODE                
     ELSE                                                        
        IF PNT-SYSTEM (X2) = 'G'                                 
            MOVE 'GL'           TO PO-SYSTEM-CODE                
        ELSE                                                     
        MOVE 'EL'               TO PO-SYSTEM-CODE.               
    MOVE X1                     TO PO-PROGRAM-SEQUENCE.          
                                                                 
P02-FMT-DESCRIP.                                                 
                                                                 
    MOVE 'FORMAT'               TO S13-OPT-TYPE.                 
    MOVE SPACE                  TO S13-MESSAGE.                  
    MOVE 'F'                    TO PO-OPTION-TYPE.               
                                                                 
    MOVE PROG-TBL-FMT (X1)      TO PO-PGM-OPTION-CD, S13-OPTION. 
    IF PNT-SYSTEM (X2) = 'S'                                     
        MOVE 'EC'               TO PO-SYSTEM-CODE                
     ELSE                                                        
        IF PNT-SYSTEM (X2) = 'G'                                 
            MOVE 'GL'           TO PO-SYSTEM-CODE                
        ELSE                                                     
            MOVE 'EL'           TO PO-SYSTEM-CODE.               
                                                                 
    MOVE X1                     TO PO-PROGRAM-SEQUENCE.          
                                                                 
    IF (PROG-TBL-FMT (X1) = '9') OR                              
       (PROG-TBL-FMT (X1) NOT NUMERIC)                           
        MOVE 'NO FORMAT OPTION AVAILABLE' TO S13-DESC            
        PERFORM PRINT-START THRU PRINT-EXIT                      
        GO TO P02-PRC-DESCRIP.                                   
                                                                 
    PERFORM P02-READ-OPTION THRU P02-READ-EXIT.                  
                                                                 
    IF PROG-OR-FMT (X1) = 'D'                                    
        MOVE 'DEFAULT'          TO S13-MESSAGE.                  
                                                                 
    IF PROG-OR-FMT (X1) = 'O'                                    
        MOVE 'OVERRIDE'         TO S13-MESSAGE.                  
                                                                 
    PERFORM PRINT-START THRU PRINT-EXIT.                         
                                                                 
P02-PRC-DESCRIP.                                                 
                                                                 
    MOVE 'PROCESS'              TO S13-OPT-TYPE.                 
    MOVE SPACE                  TO S13-MESSAGE.                  
    MOVE 'P'                    TO PO-OPTION-TYPE.               
    MOVE PROG-TBL-PRC (X1)      TO PO-PGM-OPTION-CD, S13-OPTION. 
    IF PNT-SYSTEM (X2) = 'S'                                     
        MOVE 'EC'               TO PO-SYSTEM-CODE                
     ELSE                                                        
        IF PNT-SYSTEM (X2) = 'G'                                 
            MOVE 'GL'           TO PO-SYSTEM-CODE                
        ELSE                                                     
            MOVE 'EL'           TO PO-SYSTEM-CODE.               
                                                                 
    MOVE X1                     TO PO-PROGRAM-SEQUENCE.          
                                                                 
    IF (PROG-TBL-PRC (X1) = '9') OR                              
       (PROG-TBL-PRC (X1) NOT NUMERIC)                           
        MOVE 'NO PROCESS OPTION AVAILABLE' TO S13-DESC           
        PERFORM PRINT-START THRU PRINT-EXIT                      
        GO TO P02-TOT-DESCRIP.                                   
                                                                 
    PERFORM P02-READ-OPTION THRU P02-READ-EXIT.                  
                                                                 
    IF PROG-OR-PRC (X1) = 'D'                                    
        MOVE 'DEFAULT'          TO S13-MESSAGE.                  
                                                                 
    IF PROG-OR-PRC (X1) = 'O'                                    
        MOVE 'OVERRIDE'         TO S13-MESSAGE.                  
                                                                 
    PERFORM PRINT-START THRU PRINT-EXIT.                         
                                                                 
P02-TOT-DESCRIP.                                                 
                                                                 
    MOVE 'TOTAL'                TO  S13-OPT-TYPE.                
    MOVE SPACE                  TO  S13-MESSAGE.                 
    MOVE 'T'                    TO PO-OPTION-TYPE.               
    MOVE PROG-TBL-TOT (X1)      TO PO-PGM-OPTION-CD, S13-OPTION. 
    IF PNT-SYSTEM (X2) = 'S'                                     
        MOVE 'EC'               TO PO-SYSTEM-CODE                
     ELSE                                                        
        IF PNT-SYSTEM (X2) = 'G'                                 
            MOVE 'GL'           TO PO-SYSTEM-CODE                
        ELSE                                                     
            MOVE 'EL'           TO PO-SYSTEM-CODE.               
    MOVE X1                     TO PO-PROGRAM-SEQUENCE.          
                                                                 
    IF (PROG-TBL-TOT (X1) = '9') OR                              
       (PROG-TBL-TOT (X1) NOT NUMERIC)                           
        MOVE 'NO TOTAL OPTION AVAILABLE' TO S13-DESC             
        PERFORM PRINT-START THRU PRINT-EXIT                      
        GO TO P02-LOOP-1.                                        
                                                                 
    PERFORM P02-READ-OPTION THRU P02-READ-EXIT.                  
                                                                 
    IF PROG-OR-TOT (X1) = 'D'                                    
        MOVE 'DEFAULT'          TO S13-MESSAGE.                  
                                                                 
    IF PROG-OR-TOT (X1) = 'O'                                    
        MOVE 'OVERRIDE'         TO S13-MESSAGE.                  
                                                                 
    PERFORM PRINT-START THRU PRINT-EXIT.                         
                                                                 
    GO TO P02-LOOP-1.                                            
                                                                 
P02-READ-OPTION.                                                 
                                                                 
    MOVE 'ELPGMN'                TO VSAM-PGM.                    
                                                                 
    READ ELPGMO RECORD                                           
        INVALID KEY                                              
            MOVE '* PROGRAM OPTION NOT IN FILE *' TO S13-DESC    
            GO TO P02-READ-EXIT.                                 
                                                                 
    MOVE PO-OPTION-DESCRIPTION TO S13-DESC.                      
                                                                 
P02-READ-EXIT.                                                   
    EXIT.                                                        
                                                                 
                                                                 
P02-EXIT.                                                        
    EXIT.                                                        
    EJECT                                                        
*>                                                                 
*>    SECTION 03 PRINT - SELECTED PRINT OVERRIDES                  
*>                                                                 
P03-START.                                                       
                                                                 
    MOVE ZERO TO X1.                                             
    MOVE 1 TO X2.                                                
                                                                 
    IF LINE-CNT IS GREATER THAN NEW-PAGE                         
        PERFORM HEAD-START THRU HEAD-EXIT.                       
                                                                 
    MOVE S14-H01                TO SAVE-HEAD-1.                  
    MOVE S14-H02                TO SAVE-HEAD-2.                  
    PERFORM SECT-START THRU SECT-EXIT.                           
                                                                 
    IF FICHE-SW = ZERO                                           
        MOVE 'NONE GIVEN'       TO POS-06                        
        PERFORM PRINT-START THRU PRINT-EXIT                      
        GO TO P03-EXIT.                                          
                                                                 
    MOVE '(CONTINUED)'          TO S14-C.                        
    MOVE S14-H01                TO SAVE-HEAD-1.                  
                                                                 
P03-LOOP-1.                                                      
                                                                 
    ADD 1 TO X1.                                                 
    MOVE 1 TO X2.                                                
                                                                 
    IF X1 IS GREATER THAN 1500                                   
        GO TO P03-EXIT.                                          
                                                                 
    IF PROG-TBL-PRC (X1) = 'X'                                   
        GO TO P03-LOOP-1.                                        
                                                                 
    IF PROG-TBL-PRT (X1) = 'P' OR '9'                            
        GO TO P03-LOOP-1.                                        
                                                                 
P03-LOOP-2.                                                      
                                                                 
    IF (X2 = +250) OR                                            
*>       (PROG-NM-TB (X2) = HIGH-VALUE)                            
       (Pnt-prog (X2) = HIGH-VALUE)                              
           GO TO P03-LOOP-1.                                     
                                                                 
    IF PNT-PROG (X2) IS NOT = X1                                 
        ADD 1 TO X2                                              
        GO TO P03-LOOP-2.                                        
                                                                 
    IF LINE-CNT IS GREATER THAN END-PAGE                         
        PERFORM HEAD-START THRU HEAD-EXIT                        
        PERFORM SECT-START THRU SECT-EXIT.                       
                                                                 
    IF PNT-SYSTEM (X2) = 'S'                                     
        MOVE 'ECS-' TO S14-CLAS                                  
     ELSE                                                        
        IF PNT-SYSTEM (X2) = 'G'                                 
            MOVE ' GL-' TO S14-CLAS                              
     ELSE                                                        
        IF PNT-SYSTEM (X2) = 'L'                                 
            MOVE ' LF-' TO S14-CLAS                              
        ELSE                                                     
            MOVE ' EL-' TO S14-CLAS.                             
                                                                 
    MOVE X1 TO S14-PROGRAM.                                      
    MOVE PNT-MESSAGE (X2) TO S14-NAME.                           
                                                                 
    IF PROG-TBL-PRT (X1) = 'B'                                   
        MOVE 'FICHE AND PRINT'  TO S14-MESSAGE.                  
                                                                 
    IF PROG-TBL-PRT (X1) = 'F'                                   
        MOVE 'FICHE ONLY'       TO S14-MESSAGE.                  
                                                                 
    IF PROG-TBL-PRT (X1) = 'S'                                   
        MOVE 'SAVE ONLINE'      TO S14-MESSAGE.                  
                                                                 
    IF PROG-TBL-PRT (X1) = 'T'                                   
        MOVE 'SAVE AND PRINT'   TO S14-MESSAGE.                  
                                                                 
    PERFORM PRINT-START THRU PRINT-EXIT.                         
                                                                 
    GO TO P03-LOOP-1.                                            
                                                                 
P03-EXIT.                                                        
    EXIT.                                                        
    EJECT                                                        
*>                                                                 
*>    SECTION 04 PRINT - COMPANY FACTOR TABLE                      
*>                                                                 
P04-START.                                                       
                                                                 
    MOVE ZERO TO X1.                                             
                                                                 
    IF LINE-CNT IS GREATER THAN NEW-PAGE                         
        PERFORM HEAD-START THRU HEAD-EXIT.                       
                                                                 
    MOVE S15-H01                TO SAVE-HEAD-1.                  
    MOVE S15-H02                TO SAVE-HEAD-2.                  
    PERFORM SECT-START THRU SECT-EXIT.                           
                                                                 
    IF CLAF-CNT = ZERO                                           
        MOVE 'NONE GIVEN' TO POS-06                              
        PERFORM PRINT-START THRU PRINT-EXIT                      
        GO TO P04-EXIT.                                          
                                                                 
    MOVE '(CONTINUED)'          TO S15-C.                        
    MOVE S15-H01                TO SAVE-HEAD-1.                  
                                                                 
P04-LOOP.                                                        
    ADD 1                       TO X1.                           
                                                                 
    IF X1 IS GREATER THAN 6                                      
        GO TO P04-EXIT.                                          
                                                                 
    IF LINE-CNT IS GREATER THAN END-PAGE                         
        PERFORM HEAD-START THRU HEAD-EXIT                        
        PERFORM SECT-START THRU SECT-EXIT.                       
                                                                 
    MOVE X1 TO S15-CODE.                                         
    MOVE FACT-FACTOR (X1)      TO S15-FACTOR.                    
                                                                 
    PERFORM PRINT-START THRU PRINT-EXIT.                         
                                                                 
    GO TO P04-LOOP.                                              
                                                                 
P04-EXIT.                                                        
    EXIT.                                                        
    EJECT                                                        
*>                                                                 
*>    SECTION 16 PRINT - ERROR LISTING                             
*>                                                                 
P16-START.                                                       
                                                                 
    MOVE ZERO TO X1.                                             
                                                                 
    IF LINE-CNT IS GREATER THAN NEW-PAGE                         
        PERFORM HEAD-START THRU HEAD-EXIT.                       
                                                                 
    MOVE S16-H01                TO SAVE-HEAD-1.                  
    MOVE SPACE                  TO SAVE-HEAD-2.                  
    PERFORM SECT-START THRU SECT-EXIT.                           
                                                                 
P16-LOOP-1.                                                      
                                                                 
    ADD 1 TO X1.                                                 
                                                                 
    IF ERR-MSG (X1) = HIGH-VALUES                                
        GO TO P16-EXIT.                                          
                                                                 
    IF ERR-LIST (X1) = ZERO                                      
        GO TO P16-LOOP-1.                                        
                                                                 
    IF LINE-CNT IS GREATER THAN END-PAGE                         
        PERFORM HEAD-START THRU HEAD-EXIT                        
        PERFORM SECT-START THRU SECT-EXIT.                       
                                                                 
    MOVE X1                     TO S16-CODE.                     
    MOVE ERR-MSG (X1)           TO S16-MESSAGE.                  
                                                                 
    PERFORM PRINT-START THRU PRINT-EXIT.                         
    MOVE ZERO                   TO X2.                           
                                                                 
P16-LOOP-2.                                                      
                                                                 
    ADD 1 TO X2.                                                 
                                                                 
    IF X2 IS GREATER THAN 50                                     
        GO TO P16-LOOP-1.                                        
                                                                 
    IF ERR-NBR-H (X2) = 999                                      
        GO TO P16-LOOP-1.                                        
                                                                 
    IF ERR-NBR-H (X2) NOT = X1                                   
        GO TO P16-LOOP-2.                                        
                                                                 
    IF LINE-CNT IS GREATER THAN END-PAGE                         
        PERFORM HEAD-START THRU HEAD-EXIT                        
        PERFORM SECT-START THRU SECT-EXIT                        
        MOVE X1                 TO S16-CODE                      
        MOVE ERR-MSG (X1)       TO S16-MESSAGE                   
        MOVE ' (CONT)'          TO S16-CONT                      
        PERFORM PRINT-START THRU PRINT-EXIT.                     
                                                                 
    MOVE ERR-DETAIL-H (X2)      TO S16-ERR-DETAIL.               
    PERFORM PRINT-START THRU PRINT-EXIT.                         
                                                                 
    GO TO P16-LOOP-2.                                            
                                                                 
P16-EXIT.                                                        
    EXIT.                                                        
    EJECT                                                        
*>                                                                 
*>                                                                 
*>    LOAD DETAIL ERROR TABLE                                      
*>                                                                 
LOAD-START.                                                      
                                                                 
    IF ERR-X IS GREATER THAN 50                                  
        GO TO LOAD-EXIT.                                         
                                                                 
    ADD 1 TO ERR-X.                                              
    MOVE ERR-NBR                TO ERR-NBR-H (ERR-X).            
    MOVE ERR-DETAIL             TO ERR-DETAIL-H (ERR-X).         
    MOVE 1                      TO ERR-LIST (ERR-NBR).           
                                                                 
LOAD-EXIT.                                                       
    EXIT.                                                        
*>                                                                 
*>    TEST FOR ERRORS OCCURD                                       
*>                                                                 
ERROR-START.                                                     
                                                                 
    IF ERROR-SW = 1                                              
        GO TO ERROR-EXIT.                                        
                                                                 
    MOVE ZERO                   TO X1.                           
                                                                 
ERROR-LOOP.                                                      
                                                                 
    ADD 1 TO X1.                                                 
                                                                 
    IF ERR-MSG (X1) = HIGH-VALUES                                
        GO TO ERROR-EXIT.                                        
                                                                 
    IF ERR-LIST (X1) = ZERO                                      
        GO TO ERROR-LOOP.                                        
                                                                 
    MOVE 1                      TO ERROR-SW.                     
                                                                 
ERROR-EXIT.                                                      
    EXIT.                                                        
*>                                                                 
*>    DATE EDIT                                                    
*>                                                                 
8500-DATE-CONVERT.                                               
    CALL 'ELDATCX' USING DATE-CONVERSION-DATA.                   
                                                                 
8500-EXIT.                                                       
    EXIT.                                                        
                                                                 
DATE-START.                                                      
                                                                 
    IF DATE-TEST IS NOT NUMERIC                                  
        GO TO DATE-ERR.                                          
                                                                 
    IF DATE-MO IS LESS THAN 1                                    
        GO TO DATE-ERR.                                          
                                                                 
    IF DATE-MO IS GREATER THAN 12                                
        GO TO DATE-ERR.                                          
                                                                 
    MOVE DATE-DA      TO HOLD-CEN-1-DA.                          
    MOVE DATE-MO      TO HOLD-CEN-1-MO.                          
    MOVE DATE-YY      TO HOLD-CEN-1-YY.                          
    MOVE DATE-CC      TO HOLD-CEN-1-CC.                          
                                                                 
    MOVE 'H'          TO DC-OPTION-CODE.                         
    PERFORM 8500-DATE-CONVERT THRU 8500-EXIT.                    
    IF DATE-CONVERSION-ERROR                                     
       IF ONLY-LEAP-YEAR                                         
          MOVE HOLD-CEN-1-DA     TO MONTH-D (2)                  
       ELSE                                                      
          MOVE 99 TO DATE-DA                                     
       END-IF                                                    
       MOVE SPACE TO DC-ERROR-CODE.                              
                                                                 
    IF DATE-DA IS GREATER THAN MONTH-D (DATE-MO)                 
        GO TO DATE-ERR.                                          
                                                                 
    IF DATE-DA IS GREATER THAN ZERO                              
        GO TO DATE-EXIT.                                         
                                                                 
DATE-ERR.                                                        
                                                                 
    MOVE SPACE TO DATE-TEST.                                     
                                                                 
DATE-EXIT.                                                       
    EXIT.                                                        
*>                                                                 
*>    DATE FILE WRITE                                              
*>                                                                 
WRITE-START.                                                     
    WRITE DATE-DISK                                              
        INVALID KEY MOVE 1 TO ERR-LIST (99).                     
WRITE-EXIT.                                                      
    EXIT.                                                        
*>                                                                 
*>    PAGE HEADING PRINT                                           
*>                                                                 
HEAD-START.                                                      
                                                                 
    ADD 1 TO PAGE-CNT.                                           
                                                                 
    MOVE PAGE-CNT               TO T03-P.                        
                                                                 
    MOVE T01                    TO D-LINE.                       
    MOVE ZERO                   TO PRT-CTL.                      
    PERFORM PRINT-START THRU PRINT-EXIT.                         
                                                                 
    MOVE T02                    TO D-LINE.                       
    PERFORM PRINT-START THRU PRINT-EXIT.                         
                                                                 
    MOVE T03                    TO D-LINE.                       
    PERFORM PRINT-START THRU PRINT-EXIT.                         
                                                                 
HEAD-EXIT.                                                       
    EXIT.                                                        
    EJECT                                                        
*>                                                                 
*>    SECTION HEADING PRINT                                        
*>                                                                 
SECT-START.                                                      
                                                                 
    MOVE SPACE                  TO D-LINE.                       
    MOVE 2                      TO PRT-CTL.                      
    PERFORM PRINT-START THRU PRINT-EXIT.                         
                                                                 
    MOVE SAVE-HEAD-1            TO D-LINE.                       
    MOVE 2                      TO PRT-CTL.                      
    PERFORM PRINT-START THRU PRINT-EXIT.                         
                                                                 
    MOVE 2                      TO PRT-CTL.                      
                                                                 
    IF SAVE-HEAD-2 = SPACE                                       
        GO TO SECT-EXIT.                                         
                                                                 
    MOVE SAVE-HEAD-2            TO D-LINE.                       
    PERFORM PRINT-START THRU PRINT-EXIT.                         
                                                                 
    MOVE 2                      TO PRT-CTL.                      
                                                                 
SECT-EXIT.                                                       
    EXIT.                                                        
*>                                                                 
*>    SPECIAL PRINT FOR SECTIONS 02 AND 12                         
*>                                                                 
SPEC-START.                                                      
                                                                 
    IF LINE-CNT IS GREATER THAN END-PAGE                         
        MOVE D-LINE TO SAVE-LINE                                 
        PERFORM HEAD-START THRU HEAD-EXIT                        
        PERFORM SECT-START THRU SECT-EXIT                        
        MOVE SAVE-LINE TO D-LINE.                                
                                                                 
    PERFORM PRINT-START THRU PRINT-EXIT.                         
                                                                 
SPEC-EXIT.                                                       
    EXIT.                                                        
*>                                                                 
*>    UNIVERSAL PRINT                                              
*>                                                                 
PRINT-START.                                                     
                                                                 
    MOVE D-LINE                 TO P-DATA.                       
    MOVE PRT-CTL                TO P-CTL.                        
    ADD PRT-CTL                 TO LINE-CNT.                     
                                                                 
    IF PRT-CTL = ZERO                                            
        MOVE ZERO               TO LINE-CNT.                     
                                                                 
    ADD 1 TO PRT-CTL.                                            
                                                                 
    MOVE PRINT-CTL (PRT-CTL) TO LCP-ASA                          
    PERFORM LCP-WRITE-POS-PRT                                    
        THRU LCP-WRITE-END-PRT.                                  
                                                                 
    MOVE 1                      TO PRT-CTL.                      
    MOVE SPACE                  TO D-LINE.                       
                                                                 
PRINT-EXIT.                                                      
    EXIT.                                                        
*>                                                                 
*>    END OF JOB                                                   
*>                                                                 
EOJ.                                                             
                                                                 
    CLOSE CARD-FILE                                              
          ELCNTL                                                 
          ELPGMN                                                 
          ELPGMS                                                 
          ELPGMO                                                 
          PRNT-FILE                                              
          DISK-FILE                                              
                                                                 
    IF ELCNTL-FILE-STATUS NOT = ZERO                             
        MOVE 'ERROR OCCURED CLOSE - ELCNTL'  TO  WS-ABEND-MESSAGE
        MOVE ELCNTL-FILE-STATUS  TO  WS-ABEND-FILE-STATUS        
        PERFORM ABEND-PGM.                                       
                                                                 
    IF ELPGMN-FILE-STATUS NOT = ZERO                             
        MOVE 'ERROR OCCURED CLOSE - ELPGMN'  TO  WS-ABEND-MESSAGE
        MOVE ELPGMN-FILE-STATUS  TO  WS-ABEND-FILE-STATUS        
        PERFORM ABEND-PGM.                                       
                                                                 
    IF ELPGMS-FILE-STATUS NOT = ZERO                             
        MOVE 'ERROR OCCURED CLOSE - ELPGMS'  TO  WS-ABEND-MESSAGE
        MOVE ELPGMS-FILE-STATUS  TO  WS-ABEND-FILE-STATUS        
        PERFORM ABEND-PGM.                                       
                                                                 
    IF ELPGMO-FILE-STATUS NOT = ZERO                             
        MOVE 'ERROR OCCURED CLOSE - ELPGMO'  TO  WS-ABEND-MESSAGE
        MOVE ELPGMO-FILE-STATUS  TO  WS-ABEND-FILE-STATUS        
        PERFORM ABEND-PGM.                                       
                                                                 
