L01-CARR-NAMES.                                                  
                                                                 
    MOVE ZERO                   TO X1.                           
                                                                 
    MOVE SPACES                 TO CF-CONTROL-PRIMARY.           
    MOVE ZERO                   TO CF-SEQUENCE-NO.               
    MOVE OT-CLIENT              TO CF-COMPANY-ID.                
    MOVE '6'                    TO CF-RECORD-TYPE.               
                                                                 
    START ELCNTL  KEY IS NOT LESS THAN CF-CONTROL-PRIMARY        
                                                                 
    IF ELCNTL-FILE-STATUS NOT = ZERO                             
        MOVE 'ERROR OCCURED START - ELCNTL'  TO  WS-ABEND-MESSAGE
        MOVE ELCNTL-FILE-STATUS  TO  WS-ABEND-FILE-STATUS        
        PERFORM ABEND-PGM.                                       
                                                                 
                                                                 
    MOVE SPACES                 TO CARRIER-NAME-TBL.             
                                                                 
L01-CARR-LOOP-1.                                                 
                                                                 
    READ ELCNTL NEXT RECORD                                      
        AT END  MOVE X1         TO CLAS-MAXCN                    
                GO TO L01-CARR-END.                              
                                                                 
    IF CF-RECORD-TYPE NOT = '6'                                  
        MOVE X1                 TO CLAS-MAXCN                    
        GO TO L01-CARR-END.                                      
                                                                 
    ADD +1 TO X1.                                                
                                                                 
    IF X1 GREATER +25                                            
        MOVE 1                  TO ERR-LIST (14)                 
        GO TO L01-EXIT.                                          
                                                                 
    MOVE CF-CARRIER-CNTL        TO CN-CODE (X1).                 
    MOVE CF-DOMICILE-STATE      TO CN-DOM-ST (X1).               
    MOVE CF-MAIL-TO-NAME        TO CN-NAME (X1).                 
                                                                 
    MOVE ZEROS                  TO CN-IBNR-UEP-PCT (X1)          
                                   CN-IBNR-R78-PCT (X1)          
                                   CN-IBNR-PRO-PCT (X1).         
                                                                 
    IF CF-IBNR-UEPRM-PERCENT NUMERIC                             
        MOVE CF-IBNR-UEPRM-PERCENT TO CN-IBNR-UEP-PCT (X1).      
    IF CF-IBNR-R78-PERCENT NUMERIC                               
        MOVE CF-IBNR-R78-PERCENT   TO CN-IBNR-R78-PCT (X1).      
    IF CF-IBNR-PRO-PERCENT NUMERIC                               
        MOVE CF-IBNR-PRO-PERCENT   TO CN-IBNR-PRO-PCT (X1).      
    MOVE CF-SECPAY-SWITCH       TO CN-SECPAY-SWITCH (X1)         
                                                                 
                                                                 
    IF  OT-OPT-RESERVE-METHOD-UNAUTH                             
        GO TO L01-CARR-LOOP-1.                                   
                                                                 
    IF  USE-GIVEN-CARRIER                                        
        PERFORM L01-CARR-GET-OPT-DATA THRU L01-CARROPT-EXIT      
        GO TO L01-CARR-LOOP-1                                    
    ELSE                                                         
        IF  CF-CARRIER-CNTL EQUAL W-CARRIER-SWITCH               
            PERFORM L01-CARR-GET-OPT-DATA THRU L01-CARROPT-EXIT  
            MOVE CARR-IBNR-SW (X1)                               
                                TO W-CNTL-IBNR-SW                
            MOVE CARR-IBNR-PERCENT (X1)                          
                                TO W-CNTL-IBNR-PERCENT           
            MOVE CARR-PERCENT-OF-CIDA (X1)                       
                                TO W-CNTL-PERCENT-OF-CIDA        
            PERFORM L01-CARR-REPLACE-PASSED-DATA                 
                THRU L01-CARRREP-EXIT                            
                    VARYING                                      
                RX1 FROM 1 BY 1                                  
                    UNTIL                                        
                RX1 EQUAL X1                                     
            GO TO L01-CARR-LOOP-1                                
        ELSE                                                     
            MOVE W-CNTL-IBNR-SW TO CARR-IBNR-SW (X1)             
            MOVE W-CNTL-IBNR-PERCENT                             
                                TO CARR-IBNR-PERCENT (X1)        
            MOVE W-CNTL-PERCENT-OF-CIDA                          
                                TO CARR-PERCENT-OF-CIDA (X1)     
            GO TO L01-CARR-LOOP-1.                               
                                                                 
L01-CARR-GET-OPT-DATA.                                           
                                                                 
    MOVE CF-IBNR-SW             TO CARR-IBNR-SW (X1).            
                                                                 
    IF  CF-PERCENT-OF-CDT NUMERIC                                
        COMPUTE CARR-PERCENT-OF-CIDA (X1)                        
            = CF-PERCENT-OF-CDT * .01                            
    ELSE                                                         
        DISPLAY 'PERCENT OF CDT NOT NUMERIC - '                  
            X1 ' FOR CARRIER ' CF-CARRIER-CNTL                   
        MOVE +0.0000            TO CARR-PERCENT-OF-CIDA (X1).    
                                                                 
    IF  CF-IBNR-PERCENT NUMERIC                                  
        MOVE CF-IBNR-PERCENT    TO CARR-IBNR-PERCENT (X1)        
    ELSE                                                         
        DISPLAY 'IBNR PERCENT NOT NUMERIC - '                    
            X1 ' FOR CARRIER ' CF-CARRIER-CNTL                   
        MOVE +1.0000            TO CARR-IBNR-PERCENT (X1).       
                                                                 
L01-CARROPT-EXIT.                                                
    EXIT.                                                        
                                                                 
L01-CARR-REPLACE-PASSED-DATA.                                    
                                                                 
    MOVE W-CNTL-IBNR-SW         TO CARR-IBNR-SW (RX1).           
    MOVE W-CNTL-PERCENT-OF-CIDA TO CARR-PERCENT-OF-CIDA (RX1).   
    MOVE W-CNTL-IBNR-PERCENT    TO CARR-IBNR-PERCENT (RX1).      
                                                                 
L01-CARRREP-EXIT.                                                
    EXIT.                                                        
                                                                 
L01-CARR-END.                                                    
    IF CLAS-MAXCN NOT = ZERO                                     
       MOVE +1 TO CLAS-STARTCN                                   
     ELSE                                                        
        MOVE ZERO               TO CLAS-STARTCN.                 
                                                                 
L01-EXIT.                                                        
    EXIT.                                                        
                                                                 
    EJECT                                                        
*>                                                                 
*>  ELPGMN - PROGRAM NAMES TRANSFER                                
*>                                                                 
L02-START.                                                       
                                                                 
    MOVE ZERO                   TO X1.                           
                                                                 
    MOVE LOW-VALUE TO PN-CONTROL-PRIMARY.                        
                                                                 
    START ELPGMN  KEY IS NOT LESS THAN PN-CONTROL-PRIMARY        
                                                                 
    IF ELPGMN-FILE-STATUS NOT = ZERO                             
        MOVE 'ERROR OCCURED START - ELPGMN'  TO  WS-ABEND-MESSAGE
        MOVE ELPGMN-FILE-STATUS  TO  WS-ABEND-FILE-STATUS        
        PERFORM ABEND-PGM.                                       
                                                                 
    MOVE ZEROES TO PROGRAM-NAME-TABLE.                           
                                                                 
L02-PROG-LOOP-1.                                                 
                                                                 
    READ ELPGMN NEXT RECORD                                      
        AT END                                                   
              ADD +1 TO X1                                       
              MOVE HIGH-VALUE TO PROG-NM-TB (X1)                 
              GO TO L02-EXIT.                                    
                                                                 
                                                                 
    IF PN-PROGRAM-SEQUENCE NOT NUMERIC                           
        GO TO L02-PROG-LOOP-1.                                   
                                                                 
    IF (PN-SYSTEM-CODE = 'EC')                                   
        IF ((PN-PROGRAM-SEQUENCE GREATER 000)                    
           AND (PN-PROGRAM-SEQUENCE LESS 301))                   
            GO TO L02-LOOP-ADD.                                  
                                                                 
    IF (PN-SYSTEM-CODE = 'EL')                                   
        IF ((PN-PROGRAM-SEQUENCE GREATER 300)                    
           AND (PN-PROGRAM-SEQUENCE LESS 400))                   
            GO TO L02-LOOP-ADD.                                  
                                                                 
    IF (PN-SYSTEM-CODE = 'EC')                                   
        IF ((PN-PROGRAM-SEQUENCE GREATER 399)                    
           AND (PN-PROGRAM-SEQUENCE LESS 500))                   
            GO TO L02-LOOP-ADD.                                  
                                                                 
    IF (PN-SYSTEM-CODE = 'EL')                                   
        IF ((PN-PROGRAM-SEQUENCE GREATER 499)                    
           AND (PN-PROGRAM-SEQUENCE LESS 601))                   
            GO TO L02-LOOP-ADD.                                  
                                                                 
    IF (PN-SYSTEM-CODE = 'EL')                                   
        IF ((PN-PROGRAM-SEQUENCE GREATER 800)                    
           AND (PN-PROGRAM-SEQUENCE LESS 900))                   
            GO TO L02-LOOP-ADD.                                  
                                                                 
    IF (PN-SYSTEM-CODE = 'EC')                                   
        IF ((PN-PROGRAM-SEQUENCE GREATER 600)                    
           AND (PN-PROGRAM-SEQUENCE LESS 801))                   
            GO TO L02-LOOP-ADD.                                  
                                                                 
    IF (PN-SYSTEM-CODE = 'EC')                                   
        IF ((PN-PROGRAM-SEQUENCE GREATER 399)                    
           AND (PN-PROGRAM-SEQUENCE LESS 801))                   
            GO TO L02-LOOP-ADD.                                  
                                                                 
    IF (PN-SYSTEM-CODE = 'EC')                                   
        IF  PN-PROGRAM-SEQUENCE GREATER 899                      
            GO TO L02-LOOP-ADD.                                  
                                                                 
    IF (PN-SYSTEM-CODE = 'GL')                                   
        IF ((PN-PROGRAM-SEQUENCE GREATER 799)                    
           AND (PN-PROGRAM-SEQUENCE LESS 900))                   
            GO TO L02-LOOP-ADD.                                  
                                                                 
    GO TO L02-PROG-LOOP-1.                                       
                                                                 
L02-LOOP-ADD.                                                    
                                                                 
    ADD +1 TO X1.                                                
                                                                 
    IF X1 GREATER +1500                                          
        MOVE 1                  TO ERR-LIST (16)                 
        GO TO L02-EXIT.                                          
                                                                 
    MOVE PN-PROGRAM-SEQUENCE    TO PNT-PROG (X1).                
    MOVE PN-PROGRAM-DESCRIPTION TO PNT-MESSAGE (X1).             
                                                                 
    IF PN-SYSTEM-CODE = 'EC'                                     
       MOVE 'S'                 TO PNT-SYSTEM (X1)               
     ELSE                                                        
        IF PN-SYSTEM-CODE = 'GL'                                 
           MOVE 'G'             TO PNT-SYSTEM (X1)               
        ELSE                                                     
            MOVE 'C'            TO PNT-SYSTEM (X1).              
                                                                 
    GO TO L02-PROG-LOOP-1.                                       
                                                                 
L02-EXIT.                                                        
    EXIT.                                                        
                                                                 
    EJECT                                                        
                                                                 
*>                                                                 
*>                                                                 
*>    RECORD 01 WRITE - CLAS - DATE CONTROL RECORD                 
*>                                                                 
W01-START.                                                       
                                                                 
    MOVE CONTROL-TBL            TO DATE-DISK.                    
                                                                 
    PERFORM WRITE-START THRU WRITE-EXIT.                         
                                                                 
W01-EXIT.                                                        
    EXIT.                                                        
    EJECT                                                        
