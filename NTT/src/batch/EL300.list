A01-02.                                                          
    IF CTL-PEND-ACT-FILE = SPACE                                 
        GO TO A01-LOOP-2.                                        
    GO TO A01-01.                                                
*>                                                                 
*>        ED=N - EDIT-NUMBER = SPACE                               
A01-03.                                                          
    IF CTL-PEND-ACT-FILE = SPACE                                 
        GO TO A01-01.                                            
    GO TO A01-LOOP-2.                                            
*>                                                                 
*>        UD=Y - UPDATE-SWITCH = 1                                 
A01-04.                                                          
    IF CTL-EP-SW = '1'                                           
        GO TO A01-01.                                            
    GO TO A01-LOOP-2.                                            
*>                                                                 
*>        UD=N - UPDATE-SWITCH = SPACE                             
A01-05.                                                          
    IF CTL-EP-SW = SPACE                                         
        GO TO A01-01.                                            
    GO TO A01-LOOP-2.                                            
*>                                                                 
*>        MO=Y - RUN-MONTH = 12                                    
A01-06.                                                          
    IF CTL-RUN-MO = 12                                           
        GO TO A01-01.                                            
    GO TO A01-LOOP-2.                                            
*>                                                                 
*>        MO/Y - RUN-MONTH NOT = 12                                
A01-07.                                                          
    IF CTL-RUN-MO = 12                                           
        GO TO A01-LOOP-2.                                        
    GO TO A01-01.                                                
*>                                                                 
*>        MO=Q - RUN-MONTH = 3, 6, 9, OR 12                        
A01-08.                                                          
    IF CTL-RUN-MO = 3 OR 6 OR 9 OR 12                            
        GO TO A01-01.                                            
    GO TO A01-LOOP-2.                                            
*>                                                                 
*>        MO/Q - RUN-MONTH NOT = 3, 6, 9, OR 12                    
A01-09.                                                          
    IF CTL-RUN-MO = 3 OR 6 OR 9 OR 12                            
        GO TO A01-LOOP-2.                                        
    GO TO A01-01.                                                
*>                                                                 
*>        MO=E - RUN-MONTH = 2, 4, 6, 8, 10 OR 12                  
A01-10.                                                          
    IF CTL-RUN-MO = 2 OR 4 OR 6 OR 8 OR 10 OR 12                 
        GO TO A01-01.                                            
    GO TO A01-LOOP-2.                                            
*>                                                                 
*>        MO=O - RUN-MONTH = 1, 3, 5, 7, 9 OR 11                   
A01-11.                                                          
    IF CTL-RUN-MO = 1 OR 3 OR 5 OR 7 OR 9 OR 11                  
        GO TO A01-01.                                            
    GO TO A01-LOOP-2.                                            
*>                                                                 
*>        MO=1 - RUN-MONTH = JANUARY                               
A01-12.                                                          
    IF CTL-RUN-MO = 1                                            
        GO TO A01-01.                                            
    GO TO A01-LOOP-2.                                            
*>                                                                 
*>        MO=2 - RUN-MONTH = FEBRUARY                              
A01-13.                                                          
    IF CTL-RUN-MO = 2                                            
        GO TO A01-01.                                            
    GO TO A01-LOOP-2.                                            
*>                                                                 
*>        MO=3 - RUN-MONTH = MARCH                                 
A01-14.                                                          
    IF CTL-RUN-MO = 3                                            
        GO TO A01-01.                                            
    GO TO A01-LOOP-2.                                            
*>                                                                 
*>        MO=4 - RUN-MONTH = APRIL                                 
A01-15.                                                          
    IF CTL-RUN-MO = 4                                            
        GO TO A01-01.                                            
    GO TO A01-LOOP-2.                                            
*>                                                                 
*>        MO=5 - RUN-MONTH = MAY                                   
A01-16.                                                          
    IF CTL-RUN-MO = 5                                            
        GO TO A01-01.                                            
    GO TO A01-LOOP-2.                                            
*>                                                                 
*>        MO=6 - RUN-MONTH = JUNE                                  
A01-17.                                                          
    IF CTL-RUN-MO = 6                                            
        GO TO A01-01.                                            
    GO TO A01-LOOP-2.                                            
*>                                                                 
*>        MO=7 - RUN-MONTH = JULY                                  
A01-18.                                                          
    IF CTL-RUN-MO = 7                                            
        GO TO A01-01.                                            
    GO TO A01-LOOP-2.                                            
*>                                                                 
*>        MO=8 - RUN-MONTH = AUGUST                                
A01-19.                                                          
    IF CTL-RUN-MO = 8                                            
        GO TO A01-01.                                            
    GO TO A01-LOOP-2.                                            
*>                                                                 
*>        MO=9 - RUN-MONTH = SEPTEMBER                             
A01-20.                                                          
    IF CTL-RUN-MO = 9                                            
        GO TO A01-01.                                            
    GO TO A01-LOOP-2.                                            
*>                                                                 
*>        MO=A - RUN-MONTH = OCTOBER                               
A01-21.                                                          
    IF CTL-RUN-MO = 10                                           
        GO TO A01-01.                                            
    GO TO A01-LOOP-2.                                            
*>                                                                 
*>        MO=B - RUN-MONTH = NOVEMBER                              
A01-22.                                                          
    IF CTL-RUN-MO = 11                                           
        GO TO A01-01.                                            
    GO TO A01-LOOP-2.                                            
*>                                                                 
*>        MO=C - RUN-MONTH = DECEMBER                              
A01-23.                                                          
    IF CTL-RUN-MO = 12                                           
