    GO TO A01-LOOP-2.                                            
*>                                                                 
A01-APPLY-DEFAULTS.                                              
    MOVE LOW-VALUE TO PO-CONTROL-PRIMARY.                        
    MOVE 'ELPGMO' TO VSAM-PGM.                                   
    START ELPGMO  KEY IS NOT LESS THAN PO-CONTROL-PRIMARY        
                                                                 
    IF ELPGMO-FILE-STATUS NOT = ZERO                             
        MOVE 'ERROR OCCURED START - ELPGMO'  TO  WS-ABEND-MESSAGE
        MOVE ELPGMO-FILE-STATUS  TO  WS-ABEND-FILE-STATUS        
        PERFORM ABEND-PGM.                                       
                                                                 
                                                                 
A01-DEFAULT-LOOP-1.                                              
    READ ELPGMO NEXT RECORD                                      
         AT END   GO TO A01-END.                                 
                                                                 
    IF (PO-SYSTEM-CODE = 'EL')                                   
        IF ((PO-PROGRAM-SEQUENCE GREATER 300)                    
           AND (PO-PROGRAM-SEQUENCE LESS 400))                   
            GO TO A01-ADD-DEFAULT.                               
    IF (PO-SYSTEM-CODE = 'EL')                                   
        IF ((PO-PROGRAM-SEQUENCE GREATER 800)                    
           AND (PO-PROGRAM-SEQUENCE LESS 900))                   
            GO TO A01-ADD-DEFAULT.                               
    IF (PO-SYSTEM-CODE = 'EC')                                   
        IF ((PO-PROGRAM-SEQUENCE GREATER 000)                    
           AND (PO-PROGRAM-SEQUENCE LESS 301))                   
            GO TO A01-ADD-DEFAULT.                               
    IF (PO-SYSTEM-CODE = 'EC')                                   
        IF ((PO-PROGRAM-SEQUENCE GREATER 399)                    
           AND (PO-PROGRAM-SEQUENCE LESS 801))                   
            GO TO A01-ADD-DEFAULT.                               
    IF (PO-SYSTEM-CODE = 'EC')                                   
        IF  PO-PROGRAM-SEQUENCE GREATER 900                      
            GO TO A01-ADD-DEFAULT.                               
    IF (PO-SYSTEM-CODE = 'GL')                                   
        IF ((PO-PROGRAM-SEQUENCE GREATER 799)                    
           AND (PO-PROGRAM-SEQUENCE LESS 900))                   
            GO TO A01-ADD-DEFAULT.                               
                                                                 
    GO TO A01-DEFAULT-LOOP-1.                                    
                                                                 
A01-ADD-DEFAULT.                                                 
    MOVE PO-PROGRAM-SEQUENCE    TO X1.                           
    MOVE PO-SYSTEM-CODE         TO PROG-SYS-NAME (X1).           
    IF PO-OPTION-TYPE = 'F'                                      
        IF PROG-TBL-FMT (X1) = '9'                               
           IF PO-PGM-OPTION-CD NOT = SPACE                       
              MOVE PO-PGM-OPTION-CD TO PROG-TBL-FMT (X1)         
              MOVE 'D'          TO PROG-OR-FMT (X1).             
    IF PO-OPTION-TYPE = 'P'                                      
        IF PROG-TBL-PRC (X1) = '9'                               
           IF PO-PGM-OPTION-CD NOT = SPACE                       
              MOVE PO-PGM-OPTION-CD TO PROG-TBL-PRC (X1)         
              MOVE 'D'          TO PROG-OR-PRC (X1).             
    IF PO-OPTION-TYPE = 'T'                                      
        IF PROG-TBL-TOT (X1) = '9'                               
           IF PO-PGM-OPTION-CD NOT = SPACE                       
              MOVE PO-PGM-OPTION-CD TO PROG-TBL-TOT (X1)         
              MOVE 'D'          TO PROG-OR-TOT (X1).             
    GO TO A01-DEFAULT-LOOP-1.                                    
                                                                 
A01-END.                                                         
    MOVE SWITCH-SAVE            TO SWITCH-TBL.                   
A01-EXIT.                                                        
    EXIT.                                                        
    EJECT                                                        
*>                                                                 
*>    CARD INPUT 01 EDIT - CLAS DATE CARD                          
*>                                                                 
                                                                 
C01-START.                                                       
    ADD 1 TO CLAS-CNT.                                           
                                                                 
    IF CLAS-CNT IS GREATER THAN 1                                
        MOVE 1                  TO ERR-LIST (79)                 
        GO TO C01-EXIT.                                          
                                                                 
     IF CLAS-RUN-DATE = SPACES OR ZEROS                          
        MOVE WS-CURRENT-DATE    TO WORK-DATE                     
        MOVE WORK-MO            TO CLAS-RUN-MO                   
                                   WS-CLAS-RUN-MO                
                                   DC-YMD-MONTH                  
        MOVE WORK-DA            TO CLAS-RUN-DA                   
                                   WS-CLAS-RUN-DA                
                                   DC-YMD-DAY                    
        MOVE WORK-YR            TO CLAS-RUN-YR                   
                                   WS-CLAS-RUN-YR                
                                   DC-YMD-YEAR                   
        MOVE '3'                TO DC-OPTION-CODE                
        PERFORM 8500-DATE-CONVERT THRU 8500-EXIT                 
        IF NO-CONVERSION-ERROR                                   
           MOVE DC-BIN-DATE-1   TO CLAS-BIN-RUN-DATE             
           MOVE DC-ALPHA-CEN-N  TO WS-CLAS-RUN-CC                
        END-IF                                                   
      ELSE                                                       
         MOVE CLAS-RUN-YR        TO WS-CLAS-RUN-YR               
                                    DC-CYMD-YEAR                 
         MOVE CLAS-RUN-MO        TO WS-CLAS-RUN-MO               
                                    DC-CYMD-MONTH                
         MOVE CLAS-RUN-DA        TO WS-CLAS-RUN-DA               
                                    DC-CYMD-DAY                  
*>*        MOVE CLAS-RUN-CENTURY   TO WS-CLAS-RUN-CC               
         IF CLAS-RUN-YR   >=     50                              
             MOVE 19             TO WS-CLAS-RUN-CC               
                                    DC-CYMD-CEN                  
         ELSE                                                    
             MOVE 20             TO WS-CLAS-RUN-CC               
                                    DC-CYMD-CEN                  
         END-IF                                                  
         MOVE 'L' TO DC-OPTION-CODE                              
         PERFORM 8500-DATE-CONVERT THRU 8500-EXIT                
         IF NO-CONVERSION-ERROR                                  
            MOVE DC-BIN-DATE-1 TO CLAS-BIN-RUN-DATE.             
                                                                 
    IF (CLASIC-RUN-DATE = ZERO) AND (CLAS-RUN-DATE = ZERO)       
