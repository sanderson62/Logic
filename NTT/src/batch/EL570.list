    if elcntl-file-status not = '00'                             
       display 'ERROR-ELCNTL-START ' ELCNTL-FILE-STATUS          
       PERFORM ABEND-PGM                                         
    END-IF                                                       
                                                                 
    read elcntl next record                                      
    move +1                     to i1                            
                                                                 
    perform until                                                
       (elcntl-file-status <> '00')                              
       or (cf-record-type <> '3')                                
       if cf-st-calc-interest not numeric                        
          move +0               to cf-st-calc-interest           
       end-if                                                    
*>       if cf-st-calc-interest = zeros                            
*>          move 0.0800           to cf-st-calc-interest           
*>       end-if                                                    
       if cf-st-calc-interest <> zeros                           
          move cf-state-abbreviation                             
                                to ws-state-abbr (i1)            
          move cf-st-calc-interest                               
                                to ws-state-ref-int-rate (i1)    
          display ' found state with int ' cf-state-code ' '     
             cf-st-calc-interest                                 
          add +1 to i1                                           
       end-if                                                    
       read elcntl next record                                   
    end-perform                                                  
                                                                 
