       from                                                      
          NAPERSOFT_EL513A                                       
       where                                                     
               State         = :tb-state                         
           and Account       = :tb-account                       
           and Cert_No       = :tb-cert-no                       
           and Cert_Eff_Dt   = :tb-cert-eff-dt                   
           and Batch_No      = :tb-batch-no                      
           and Lf_Cancel_Amt = :tb-lf-cancel-amt-a               
           and Ah_Cancel_Amt = :tb-ah-cancel-amt-a               
    end-exec                                                     
                                                                 
    if sqlcode not = 0 and 1                                     
       display "Error: Did not find auto cnc rec "               
