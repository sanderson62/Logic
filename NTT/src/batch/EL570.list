0100-interest-calculation.                                       
                                                                 
    MOVE ' '                    TO ELCERT-FIND-SW                
    MOVE PB-CONTROL-BY-ACCOUNT (1:33)                            
                                TO CM-CONTROL-PRIMARY            
                                                                 
    READ ELCERT                                                  
    IF ELCERT-FILE-STATUS = '10' OR '23'                         
       DISPLAY ' ELCERT  RECORD NOT FOUND ' PB-STATE ' '         
          PB-ACCOUNT ' ' PB-CERT-NO                              
    ELSE                                                         
       IF ELCERT-FILE-STATUS NOT = '00'                          
          DISPLAY ' ERROR - ELCERT - READ ' ELCERT-FILE-STATUS   
          ' ' PB-STATE ' ' PB-ACCOUNT ' ' PB-CERT-NO             
       else                                                      
          set elcert-found to true                               
       END-IF                                                    
    END-IF                                                       
                                                                 
    if pb-state = ws-state-abbr (i1)                             
       continue                                                  
    else                                                         
       perform varying i1 from +1 by +1 until                    
          (i1 > +70)                                             
          or (ws-state-abbr (i1) = pb-state)                     
          or (ws-state-abbr (i1) = spaces)                       
       end-perform                                               
       if (i1 > +70)                                             
          or (ws-state-ref-int-rate (i1) = zeros)                
          or (ws-state-abbr (i1) = spaces)                       
          DISPLAY ' REF BYPASSED, NO INTEREST '                  
             PB-STATE ' ' PB-ACCOUNT ' ' PB-CERT-NO              
          GO TO 0100-ERPNDB-REWRITE                              
       end-if                                                    
    end-if                                                       
                                                                 
    compute ws-int-rate =                                        
       ws-state-ref-int-rate (i1) * +100                         
                                                                 
    move pb-c-lf-cancel-amt     to ws-lf-orig-prm-amt            
    move +0                     to ws-lf-orig-alt-prm-amt        
    move pb-c-ah-cancel-amt     to ws-ah-orig-prm-amt            
                                                                 
    if pb-state not = 'TN'                                       
       go to 0100-continue                                       
    end-if                                                       
                                                                 
    move pb-ci-lf-premium-amt   to ws-lf-orig-prm-amt            
    move pb-ci-lf-alt-premium-amt                                
                                to ws-lf-orig-alt-prm-amt        
    move pb-ci-ah-premium-amt   to ws-ah-orig-prm-amt            
                                                                 
    move pb-control-by-account  to en-control-primary            
    move 'I'                    to en-rec-type                   
    move +0                     to en-seq-no                     
    start ERENDT key >= en-control-primary                       
    if erendt-file-status <> '00'                                
       go to 0100-continue                                       
    end-if                                                       
    move ' '                    to ws-done-sw-endt               
    perform until i-am-done                                      
       read ERENDT next record                                   
       if (erendt-file-status = '00')                            
          and (pb-control-by-account(1:33) =                     
             en-control-primary(1:33))                           
          and (en-rec-type = 'I')                                
          move en-lf-orig-prm-amt  to ws-lf-orig-prm-amt         
          move en-lf-orig-alt-prm-amt                            
                                   to ws-lf-orig-alt-prm-amt     
          move en-ah-orig-prm-amt  to ws-ah-orig-prm-amt         
       else                                                      
          set i-am-done to true                                  
       end-if                                                    
    end-perform                                                  
                                                                 
