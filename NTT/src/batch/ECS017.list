        GO TO 2460-FIND-AGT.                                     
                                                                 
2465-HAVE-AGENT.                                                 
                                                                 
    IF DE-AGT-TYPE (X1) = 'B'                                    
       ADD +1                   TO B-CNT                         
       IF B-CNT = +1                                             
          MOVE DE-AGT (X1)      TO WS-ERCOMP-FIN-RESP            
       ELSE                                                      
          IF B-CNT > +1                                          
*>             MOVE DE-AGT (X1)   TO WS-ERCOMP-FIN-RESP            
             MOVE 'B'           TO WS-ERCOMP-TYPE                
             MOVE LOW-VALUES    TO WS-ERCOMP-ACCOUNT             
             PERFORM 2700-GET-BANK-NAME                          
                                THRU 2700-EXIT                   
             MOVE CO-ACCT-NAME  TO TBL-BK-NAME (NDX)             
             MOVE WS-ERCOMP-FIN-RESP                             
                                TO TBL-ASSIGN-AGT (NDX)          
          END-IF                                                 
       END-IF                                                    
    END-IF                                                       
                                                                 
*> NOTE AGT TYPE 'K' IS ALL OR NOTHING SO ECS010 SHOULD PLUG       
*> ZEROS IN THE COMMISSION PCT IF IT IS PASSED THE NO CHGBACK      
*> AGT TYPES B I AND L ARE EARNED JUST LIKE REGULAR COMMISSIONS SO 
*> WE NEED TO CALC A RATIO HERE                                    
                                                                 
    IF DE-AGT-TYPE (X1) = 'B' OR 'I' OR 'L'                      
                       OR 'J' OR 'K' OR 'N'                      
       IF DE-LF-PRM > ZEROS                                      
          COMPUTE L-OW = DE-L-PC (X1) * +1000                    
       end-if                                                    
       IF DE-AH-PRM > ZEROS                                      
          COMPUTE A-OW = DE-A-PC (X1) * +1000                    
       END-IF                                                    
       IF DE-CANCEL                                              
          IF DE-LF-PRM > ZEROS                                   
             COMPUTE CNC-FACT = DE-LF-RFND / DE-LF-PRM           
             COMPUTE L-OW ROUNDED = (CNC-FACT * L-OW) * -1       
          end-if                                                 
          IF (DE-AH-PRM > ZEROS)                                 
             IF (DE-AGT-TYPE (X1) = 'I' OR 'N')                  
                and (dte-client = 'DCC')                         
                PERFORM 2530-CALC-DDF-FEES THRU 2530-EXIT        
                COMPUTE A-OW = CNC-WK * -1                       
             ELSE                                                
                compute cnc-fact = DE-AH-RFND / DE-AH-PRM        
                IF DE-CANCEL-REASON = 'R'                        
                   MOVE 1    TO CNC-FACT                         
                END-IF                                           
                if de-cancel-fee > de-ah-rfnd                    
                   move +0 to cnc-fact                           
                end-if                                           
                COMPUTE A-OW ROUNDED =                           
                  (CNC-FACT * A-OW) * -1                         
*>                display ' BIJKLN ' DE-ACCOUNT ' '                
*>                   DE-CERT-NO ' ' A-OW                           
             END-IF                                              
          END-IF                                                 
       END-IF                                                    
       IF (DE-RC-ISSUE)                                          
          AND (DE-RECALC-TYPE = 'O')                             
          COMPUTE L-OW = L-OW * -1                               
          COMPUTE A-OW = A-OW * -1                               
       END-IF                                                    
    ELSE                                                         
       IF (DE-AGT-TYPE (X1) = 'M')                               
          AND (CLAS-I-BEN-CATEGORY (CLAS-INDEXA) = 'G' OR 'L')   
          COMPUTE A-OW = DE-A-PC (X1) * DE-LF-PRM-ALT            
          IF DE-CANCEL                                           
             COMPUTE CNC-FACT = DE-AH-RFND / DE-AH-PRM           
             COMPUTE A-OW ROUNDED = (CNC-FACT * A-OW) * -1       
          END-IF                                                 
          IF (DE-RC-ISSUE)                                       
              AND (DE-RECALC-TYPE = 'O')                         
              COMPUTE A-OW = A-OW * -1                           
          END-IF                                                 
       ELSE                                                      
         IF (DE-AGT-TYPE (X1) = 'A')                             
                   OR                                            
            ((DE-AGT-TYPE (X1) = 'O' OR 'P' OR 'M')              
            AND ((CLAS-I-BEN-CATEGORY (CLAS-INDEXL) = 'D')       
               OR (CLAS-I-BEN-CATEGORY (CLAS-INDEXA) = 'D')))    
          COMPUTE L-OW ROUNDED     =                             
                                DE-REI-LFPRM * DE-L-PC (X1)      
          IF (DE-CANCEL)                                         
             AND (DE-LF-PRM NOT = ZEROS)                         
             COMPUTE CNC-FACT = DE-LF-RFND / DE-LF-PRM           
             COMPUTE WS-WORK-REF ROUNDED =                       
                DE-REI-LFPRM * CNC-FACT                          
             COMPUTE L-OW ROUNDED = WS-WORK-REF *                
                DE-L-PC (X1)                                     
             COMPUTE L-OW = L-OW * -1                            
          END-IF                                                 
                                                                 
          COMPUTE A-OW ROUNDED     =                             
                                DE-REI-AHPRM * DE-A-PC (X1)      
          IF (DE-CANCEL)                                         
             AND (DE-AH-PRM NOT = ZEROS)                         
             COMPUTE CNC-FACT = DE-AH-RFND / DE-AH-PRM           
             COMPUTE A-OW ROUNDED = DE-REI-AHPRM * CNC-FACT *    
                DE-A-PC (X1) * -1                                
          END-IF                                                 
         ELSE                                                    
          COMPUTE L-OW ROUNDED     =                             
                                CP-LF-PRM * DE-L-PC (X1)         
          COMPUTE L-OW-ALT ROUNDED =                             
                                CP-LF-PRM-ALT * DE-L-PC (X1)     
          COMPUTE A-OW ROUNDED     =                             
                                CP-AH-PRM * DE-A-PC (X1)         
         END-IF                                                  
       END-IF                                                    
    END-IF                                                       
                                                                 
    IF DE-RC-CANCEL OR DE-RC-ISSUE                               
        IF DTE-CLIENT = 'NCL'                                    
            GO TO 2465-OW-RC-RECORD                              
        ELSE                                                     
            GO TO 2480-ACCUM-RECALC-OW.                          
                                                                 
    IF DTE-CLIENT NOT = 'NCL'                                    
        GO TO 2470-ACCUM-STD-OW.                                 
                                                                 
