2470-ACCUM-STD-OW.                                               
                                                                 
    IF CP-LF-TYPE NOT = '00' AND '  ' AND 'DD' and 'CU'          
       ADD CP-LF-AMT            TO OW-LF-AMT (NDX)               
       IF CLAS-I-BEN-CATEGORY (CLAS-INDEXL) = 'D'                
          IF DE-ISSUE                                            
             IF NOT DUP-AGENT                                    
                ADD DE-REI-LFPRM   TO OW-LF-PRM (NDX)            
             END-IF                                              
          ELSE                                                   
             IF DE-CANCEL                                        
                COMPUTE CNC-FACT = DE-LF-RFND / DE-LF-PRM        
                COMPUTE WS-WORK-REF ROUNDED =                    
                   DE-REI-LFPRM * CNC-FACT * -1                  
                ADD WS-WORK-REF TO OW-LF-PRM (NDX)               
             END-IF                                              
          END-IF                                                 
       ELSE                                                      
          ADD CP-LF-PRM         TO OW-LF-PRM (NDX)               
       END-IF                                                    
       ADD CP-LF-AMT-ALT        TO OW-LF-AMT-ALT (NDX)           
*>       IF AM-DCC-PRODUCT-CODE NOT = 'DDF'                        
          ADD CP-LF-PRM-ALT        TO OW-LF-PRM-ALT (NDX)        
*>       END-IF                                                    
    END-IF                                                       
                                                                 
                                                                 
    IF CP-AH-TYPE NOT = '00' AND '  '                            
       IF CLAS-I-BEN-CATEGORY (CLAS-INDEXA) = 'D'                
          IF DE-ISSUE                                            
             IF NOT DUP-AGENT                                    
                ADD DE-REI-AHPRM   TO OW-AH-PRM (NDX)            
             END-IF                                              
          ELSE                                                   
             IF DE-CANCEL                                        
                COMPUTE CNC-FACT = DE-AH-RFND / DE-AH-PRM        
                COMPUTE WS-WORK-REF ROUNDED =                    
                   DE-REI-AHPRM * CNC-FACT * -1                  
                ADD WS-WORK-REF TO OW-AH-PRM (NDX)               
             END-IF                                              
          END-IF                                                 
       END-IF                                                    
    END-IF                                                       
                                                                 
                                                                 
    ADD CP-AH-AMT            TO OW-AH-AMT (NDX)                  
    ADD L-OW                 TO OW-LF-COM (NDX)                  
    ADD L-OW-ALT             TO OW-LF-COM-ALT (NDX)              
*>    display ' o/w rtn ' cp-account ' ' cp-cert ' ' a-ow          
    ADD A-OW                 TO OW-AH-COM (NDX)                  
    MOVE ' '                 TO WS-DONE-SW                       
    IF DE-AGT-TYPE (X1) = 'B' OR 'L' OR 'K' OR 'M'               
                        OR 'I' OR 'J' OR 'N'                     
       AND (CLAS-I-BEN-CATEGORY (CLAS-INDEXA) = 'G' OR 'L')      
       PERFORM VARYING S1 FROM +1 BY +1 UNTIL                    
          S1 = X1                                                
          IF DE-AGT (X1) = DE-AGT (S1)                           
             SET ALREADY-DONE TO TRUE                            
          END-IF                                                 
       END-PERFORM                                               
       IF NOT ALREADY-DONE                                       
          IF DE-ISSUE                                            
*>             ADD +1.0           TO OW-AH-PRM  (NDX)              
             ADD +1.0           TO OW-AH-CNT  (NDX)              
          ELSE                                                   
             IF DE-CANCEL                                        
                SUBTRACT +1.0   FROM OW-AH-CNT (NDX)             
             END-IF                                              
          END-IF                                                 
       END-IF                                                    
       IF DE-AGT-TYPE (X1) = 'B'                                 
          ADD DE-AH-RFND        TO OW-AH-RFND (NDX)              
       END-IF                                                    
    ELSE                                                         
       IF (CLAS-I-BEN-CATEGORY (CLAS-INDEXA) = 'D')              
          OR (CLAS-I-BEN-CATEGORY (CLAS-INDEXL) = 'D')           
          CONTINUE                                               
       ELSE                                                      
          IF DE-AGT-TYPE (X1) NOT = 'I' AND 'J'                  
             ADD CP-AH-PRM         TO OW-AH-PRM (NDX)            
          END-IF                                                 
       END-IF                                                    
    END-IF                                                       
                                                                 
    EVALUATE TRUE                                                
       WHEN DE-AGT-TYPE (X1) = 'G'                               
          ADD L-OW              TO PRCM-LF-OW-SF                 
          ADD L-OW-ALT          TO PRCM-LF-OW-SF                 
          ADD A-OW              TO PRCM-AH-OW-SF                 
       WHEN DE-AGT-TYPE (X1) = 'I' OR 'J'                        
          ADD L-OW              TO PRCM-DLR-INC                  
          ADD A-OW              TO PRCM-DLR-INC                  
       WHEN DE-AGT-TYPE (X1) = 'L'                               
          ADD A-OW              TO PRCM-AH-LMBA-FEE              
       WHEN DE-AGT-TYPE (X1) = 'A'                               
          ADD L-OW              TO PRCM-LF-LMBA-FEE              
          ADD L-OW-ALT          TO PRCM-LF-LMBA-FEE              
          ADD A-OW              TO PRCM-AH-LMBA-FEE              
       WHEN DE-AGT-TYPE (X1) = 'B'                               
          ADD A-OW              TO PRCM-BANK-FEE                 
       WHEN DE-AGT-TYPE (X1) = 'N' or 'S'                        
          ADD A-OW              TO PRCM-CSO-ADMIN                
       WHEN OTHER                                                
          ADD L-OW              TO PRCM-LF-OW-COMM               
          ADD L-OW-ALT          TO PRCM-LF-OW-COMM               
          ADD A-OW              TO PRCM-AH-OW-COMM               
    END-EVALUATE                                                 
                                                                 
                                                                 
    IF X1 LESS THAN +6                                           
        IF DE-GA-BILL-STATUS (X1) = 'B'                          
            ADD CP-LF-PRM      TO OW-LF-PRM-B (NDX)              
            ADD CP-AH-PRM      TO OW-AH-PRM-B (NDX)              
            ADD CP-LF-PRM-ALT  TO OW-LF-PRM-B-ALT (NDX)          
            ADD L-OW           TO OW-LF-COM-B (NDX)              
            ADD L-OW-ALT       TO OW-LF-COM-B-ALT (NDX)          
            ADD A-OW           TO OW-AH-COM-B (NDX).             
                                                                 
    GO TO 2440-OW-LOOP.                                          
                                                                 
2480-ACCUM-RECALC-OW.                                            
                                                                 
    IF DE-AGT-TYPE (X1) = 'G'                                    
        ADD L-OW          TO RECALC-LF-OW-SF                     
        ADD L-OW-ALT      TO RECALC-LF-OW-SF                     
        ADD A-OW          TO RECALC-AH-OW-SF                     
    ELSE                                                         
        ADD L-OW          TO RECALC-LF-OW-COMM                   
        ADD L-OW-ALT      TO RECALC-LF-OW-COMM                   
        ADD A-OW          TO RECALC-AH-OW-COMM.                  
                                                                 
    IF DE-RECALC-CODE = 'Y' OR '2'                               
        NEXT SENTENCE                                            
    ELSE                                                         
        GO TO 2440-OW-LOOP.                                      
                                                                 
    ADD CP-LF-AMT     TO OW-LF-AMT-RC     (NDX).                 
    ADD CP-LF-PRM     TO OW-LF-PRM-RC     (NDX).                 
    ADD CP-LF-AMT-ALT TO OW-LF-AMT-RC-ALT (NDX).                 
    ADD CP-LF-PRM-ALT TO OW-LF-PRM-RC-ALT (NDX).                 
    ADD CP-AH-AMT     TO OW-AH-AMT-RC     (NDX).                 
    ADD CP-AH-PRM     TO OW-AH-PRM-RC     (NDX).                 
    ADD L-OW          TO OW-LF-COM-RC     (NDX).                 
    ADD L-OW-ALT      TO OW-LF-COM-RC-ALT (NDX).                 
    ADD A-OW          TO OW-AH-COM-RC     (NDX).                 
                                                                 
    GO TO 2440-OW-LOOP.                                          
                                                                 
2490-OW-L-X.                                                     
*> PRODUCER LEVEL RECORDS.                                         
                                                                 
    IF DE-ISSUE OR DE-CANCEL                                     
        NEXT SENTENCE                                            
    ELSE                                                         
        GO TO 2495-RECALC.                                       
                                                                 
    IF DTE-CLIENT = 'NCL'                                        
       MOVE  SAVE-CPTR        TO CP-RECORD.                      
                                                                 
    PERFORM 5200-BUILD-RECALC THRU 5299-EXIT.                    
                                                                 
    PERFORM 3900-WRT-CCM-RTN    THRU 3999-EXIT.                  
                                                                 
    IF NOT DE-REVERSE                                            
        GO TO 2499-EXIT.                                         
                                                                 
    IF DE-CANCEL                                                 
        GO TO 2492-CANCEL.                                       
                                                                 
    PERFORM 2000-FORMAT-RECORD-RTN THRU 2099-EXIT.               
                                                                 
    PERFORM 2200-CALC-REMIT-TO-RTN THRU 2299-EXIT.               
                                                                 
    PERFORM 2500-REV-ACCT-COMM-RTN THRU 2500-XIT.                
                                                                 
    IF DE-BILLED-LFPRM GREATER THAN ZEROS                        
        COMPUTE CP-LF-AMT = CP-LF-AMT * -1                       
        COMPUTE CP-LF-PRM = DE-BILLED-LFPRM * -1                 
        COMPUTE CP-LF-COM ROUNDED = CP-LF-PRM * CP-LPC           
    ELSE                                                         
        MOVE ZEROS              TO CP-LF-PRM  CP-LF-COM          
                                   CP-LF-PRM-ALT  CP-LF-COM-ALT. 
                                                                 
    IF DE-BILLED-AHPRM GREATER THAN ZEROS                        
        COMPUTE CP-AH-PRM         = DE-BILLED-AHPRM * -1         
        COMPUTE CP-AH-COM ROUNDED = CP-AH-PRM * CP-APC           
    ELSE                                                         
        MOVE ZEROS              TO CP-AH-PRM  CP-AH-COM.         
                                                                 
    PERFORM 3900-WRT-CCM-RTN THRU 3999-EXIT.                     
                                                                 
    GO TO 2499-EXIT.                                             
                                                                 
2492-CANCEL.                                                     
    PERFORM 2000-FORMAT-RECORD-RTN THRU 2099-EXIT.               
                                                                 
    PERFORM 2200-CALC-REMIT-TO-RTN THRU 2299-EXIT.               
                                                                 
    PERFORM 2500-REV-ACCT-COMM-RTN THRU 2500-XIT.                
                                                                 
    IF DE-BILLED-LFRFND GREATER THAN ZEROS                       
        COMPUTE CP-LF-AMT = CP-LF-AMT * -1                       
        COMPUTE CP-LF-PRM = DE-BILLED-LFRFND * -1                
        COMPUTE CP-LF-COM ROUNDED = CP-LF-PRM * CP-LPC           
    ELSE                                                         
        MOVE ZEROS              TO CP-LF-PRM  CP-LF-COM          
                                   CP-LF-PRM-ALT  CP-LF-COM-ALT. 
                                                                 
    IF DE-BILLED-AHRFND GREATER THAN ZEROS                       
        COMPUTE CP-AH-PRM         = DE-BILLED-AHRFND * -1        
        COMPUTE CP-AH-COM ROUNDED = CP-AH-PRM * CP-APC           
    ELSE                                                         
        MOVE ZEROS              TO CP-AH-PRM  CP-AH-COM.         
                                                                 
    PERFORM 3900-WRT-CCM-RTN THRU 3999-EXIT.                     
                                                                 
    GO TO 2499-EXIT.                                             
                                                                 
2495-RECALC.                                                     
*> RECALCULATED PRODUCER LEVEL RECORDS.                            
                                                                 
    IF DTE-CLIENT = 'NCL'                                        
       MOVE  SAVE-CPTR        TO CP-RECORD.                      
                                                                 
    PERFORM 5200-BUILD-RECALC THRU 5299-EXIT.                    
                                                                 
    IF DE-RECALC-CODE = 'Y' OR '2'                               
        PERFORM 3900-WRT-CCM-RTN THRU 3999-EXIT.                 
                                                                 
2499-EXIT.                                                       
    EXIT.                                                        
EJECT                                                            
2500-REV-ACCT-COMM-RTN.                                          
    MOVE +0                     TO X1.                           
    MOVE +1                     TO NDX.                          
                                                                 
2500-REV-ACCT-COMM-LOOP.                                         
    ADD +1 TO X1.                                                
                                                                 
    IF X1 GREATER THAN +10                                       
        MOVE 0                  TO X1                            
        GO TO 2500-XIT.                                          
                                                                 
    IF DE-AGT-TYPE (X1) NOT = 'C' AND 'D' AND 'F'                
        GO TO 2500-REV-ACCT-COMM-LOOP.                           
                                                                 
    ADD DE-L-PC (X1) TO CP-LPC.                                  
    ADD DE-A-PC (X1) TO CP-APC.                                  
                                                                 
    GO TO 2500-REV-ACCT-COMM-LOOP.                               
                                                                 
2500-XIT.                                                        
    EXIT.                                                        
                                                                 
2520-REF-CSO-ADMIN-FEE.                                          
                                                                 
    IF DE-CERT-NO = WS-FACT-CERT                                 
       CONTINUE                                                  
    ELSE                                                         
       PERFORM 2550-GET-DDF-FACTORS THRU 2550-EXIT               
    END-IF                                                       
                                                                 
    DISPLAY ' MADE IT TO 2520 ' DE-CERT-NO ' ' DE-AH-TERM ' '    
      DE-DCC-DDF-REM-TRM3 ' ' A-OW ' ' WS-HI-UEF ' ' WS-LO-UEF   
                                                                 
    MOVE ZEROS                  TO CNC-WK                        
                                                                 
    EVALUATE TRUE                                                
       when de-ah-rfnd = de-ah-prm                               
          move a-ow             to cnc-wk                        
       WHEN (DE-DCC-DDF-REM-TRM3 = 0)                            
          OR (DE-CANCEL-REASON = 'R')                            
          or (a-ow = zeros)                                      
          DISPLAY ' REM TERM = 0 OR REPO or a-ow = 0 '           
          MOVE 0                TO CNC-WK                        
                                                                 
       WHEN DE-DCC-DDF-REM-TRM3 = DE-AH-TERM                     
          DISPLAY ' REM TERM  = ORIG TERM '                      
          MOVE A-OW             TO CNC-WK                        
                                                                 
       WHEN (DE-AH-TERM - DE-DCC-DDF-REM-TRM3) < 4               
          DISPLAY ' ORIG TERM - REM TRM < 4 '                    
          COMPUTE W-FACTOR ROUNDED = A-OW -                      
             (DD-1ST-YR-ALLOW (D1)                               
             * WS-MONTH / 3) - (A-OW - DD-1ST-YR-ALLOW (D1))     
             * (1 - WS-HI-UEF) * WS-MONTH / 12                   
          COMPUTE CNC-WK = W-FACTOR * 1                          
                                                                 
*>*** (AdminFee-Yr1 AF)-((AdminFee- Yr1 AF)*(1 - UEF1)*mo/12)      
                                                                 
       WHEN (DE-AH-TERM - DE-DCC-DDF-REM-TRM3) < 13              
          DISPLAY ' ORIG TERM - REM TRM < 13 '                   
          COMPUTE W-FACTOR ROUNDED = (a-ow -                     
             DD-1ST-YR-ALLOW (D1))                               
             - (a-ow - DD-1ST-YR-ALLOW (D1))                     
             * (1 - WS-hi-UEF) * ws-MONTH / 12                   
          COMPUTE CNC-WK = W-FACTOR * 1                          
                                                                 
*>***  (AdminFee-Yr1 AF)*{UEF1 - (UEF1 -UEF2)*mo/12}               
                                                                 
       WHEN OTHER                                                
          DISPLAY ' OTHER '                                      
          COMPUTE W-FACTOR ROUNDED = (a-ow -                     
             DD-1ST-YR-ALLOW (D1))                               
             * (WS-HI-UEF - (WS-HI-UEF - WS-LO-UEF)              
             * ws-MONTH / 12)                                    
          COMPUTE CNC-WK = W-FACTOR * 1                          
    END-EVALUATE                                                 
                                                                 
    DISPLAY ' CSO REF ADM FEE ' DE-CERT-NO ' ' CNC-WK            
                                                                 
    .                                                            
2520-EXIT.                                                       
    EXIT.                                                        
                                                                 
2530-CALC-DDF-FEES.                                              
                                                                 
*>****   THIS PARA IS ONLY FOR COMM TYPES N AND I                  
*>****   DCC DDF REFUNDS ONLY                                      
                                                                 
    IF DE-CERT-NO = WS-FACT-CERT                                 
       CONTINUE                                                  
    ELSE                                                         
       PERFORM 2550-GET-DDF-FACTORS THRU 2550-EXIT               
    END-IF                                                       
                                                                 
    COMPUTE WS-MONTH =                                           
       FUNCTION REM(DE-AH-TERM - DE-DCC-DDF-REM-TRM3, 12)        
    IF WS-MONTH = 0                                              
       MOVE 12 TO WS-MONTH                                       
    END-IF                                                       
    DISPLAY ' WS MONTH ' WS-MONTH                                
                                                                 
    EVALUATE TRUE                                                
       WHEN DE-AGT-TYPE (X1) = 'N'                               
          PERFORM 2520-REF-CSO-ADMIN-FEE                         
                                THRU 2520-EXIT                   
       WHEN (DE-AGT-TYPE (X1) = 'I')                             
          AND (DE-CANCEL-REASON = 'R')                           
             MOVE A-OW TO CNC-WK                                 
       WHEN (DE-AGT-TYPE (X1) = 'I')                             
          EVALUATE TRUE                                          
             WHEN ((DE-AH-TERM - DE-DCC-DDF-REM-TRM3) < 4)       
                OR (NOT DD-IU-PRESENT)                           
                COMPUTE CNC-FACT = DE-REI-AHRFND / DE-REI-AHPRM  
                COMPUTE CNC-WK ROUNDED = CNC-FACT * A-OW         
              WHEN ((DE-AH-TERM - DE-DCC-DDF-REM-TRM3) < 13)     
                 AND (DD-IU-PRESENT)                             
                 COMPUTE WS-CLP-MO3 = DE-REI-AHPRM -             
                    (DE-REI-AHPRM -                              
                    DE-IU-RATE-UP) * (WS-HI-UEF -                
                    WS-LO-UEF) * 3 / 12                          
                 DISPLAY ' UECLPMO3 ' WS-CLP-MO3                 
                 COMPUTE CNC-FACT = WS-CLP-MO3 / DE-REI-AHPRM    
                 COMPUTE WS-COMM-MO3 ROUNDED = CNC-FACT * A-OW   
                 DISPLAY ' UEMGTFO3 ' WS-COMM-MO3                
                 COMPUTE CNC-WK = WS-COMM-MO3 - (WS-COMM-MO3 -   
                   A-OW * WS-HI-UEF) * (WS-MONTH - 3) / 9        
             WHEN (DE-AH-TERM - DE-DCC-DDF-REM-TRM3) > 12        
                COMPUTE CNC-WK = A-OW *                          
                   (WS-HI-UEF - (WS-HI-UEF - WS-LO-UEF) *        
                   WS-MONTH / 12)                                
          END-EVALUATE                                           
          DISPLAY ' CSO REF MGT FEE ' DE-CERT-NO ' ' CNC-WK      
    END-EVALUATE                                                 
                                                                 
    .                                                            
2530-EXIT.                                                       
    EXIT.                                                        
                                                                 
2540-CALC-DDF-COMM.                                              
                                                                 
    IF DE-CERT-NO = WS-FACT-CERT                                 
       DISPLAY ' FACTORS ALREADY BUILT IN 2540 ' DE-CERT-NO      
    ELSE                                                         
       PERFORM 2550-GET-DDF-FACTORS THRU 2550-EXIT               
    END-IF                                                       
                                                                 
    COMPUTE WS-MONTH =                                           
       FUNCTION REM(DE-AH-TERM - DE-DCC-DDF-REM-TRM3, 12)        
    IF WS-MONTH = 0                                              
       MOVE 12 TO WS-MONTH                                       
    END-IF                                                       
    DISPLAY ' WS MONTH ' WS-MONTH                                
                                                                 
    EVALUATE TRUE                                                
       WHEN DE-CANCEL-REASON = 'R'                               
          MOVE A-OW             TO CNC-WK                        
       WHEN ((DE-AH-TERM - DE-DCC-DDF-REM-TRM3) < 4)             
          OR (NOT DD-IU-PRESENT)                                 
             COMPUTE CNC-FACT = DE-REI-AHRFND / DE-REI-AHPRM     
             COMPUTE CNC-WK = CNC-FACT * A-OW                    
       WHEN (DE-AH-TERM - DE-DCC-DDF-REM-TRM3) < 13              
           COMPUTE WS-CLP-MO3 = DE-REI-AHPRM - (DE-REI-AHPRM -   
              DE-IU-RATE-UP) * (WS-HI-UEF -                      
              WS-LO-UEF) * 3 / 12                                
           DISPLAY ' UECLPMO3 ' WS-CLP-MO3                       
           COMPUTE CNC-FACT = WS-CLP-MO3 / DE-REI-AHPRM          
           COMPUTE WS-COMM-MO3 ROUNDED = CNC-FACT * A-OW         
           DISPLAY ' UECOMMO3 ' WS-COMM-MO3                      
           COMPUTE CNC-WK = WS-COMM-MO3 - (WS-COMM-MO3 -         
             A-OW * WS-HI-UEF) * (WS-MONTH - 3) / 9              
       WHEN (DE-AH-TERM - DE-DCC-DDF-REM-TRM3) > 12              
          COMPUTE CNC-WK = a-ow * (WS-HI-UEF - (WS-HI-UEF -      
             WS-LO-UEF) * ws-MONTH / 12)                         
    END-EVALUATE                                                 
                                                                 
    DISPLAY ' CSO REF comm    ' DE-CERT-NO ' ' CNC-WK            
                                                                 
    .                                                            
2540-EXIT.                                                       
    EXIT.                                                        
                                                                 
2550-GET-DDF-FACTORS.                                            
                                                                 
    MOVE ZEROS                  TO CNC-WK                        
                                   WS-LO-UEF WS-HI-UEF           
    move ' '                    to dd-iu-sw                      
                                                                 
    if de-clp-state = spaces                                     
       move de-state            to de-clp-state                  
    end-if                                                       
    PERFORM VARYING D1 FROM +1 BY +1 UNTIL                       
       ((DE-clp-STATE = DD-STATE (D1))                           
       AND (AM-DCC-PRODUCT-CODE = DD-PRODUCT-CD (D1))            
       AND ('A'           = DD-BEN-TYPE   (D1))                  
       AND (DE-AH-TYPE    = DD-BEN-CODE   (D1))                  
       AND (DE-EFF        < DD-PROD-EXP-DT (D1)))                
                       OR                                        
       D1 > max-pdef                                             
    END-PERFORM                                                  
    IF D1 > max-pdef                                             
       DISPLAY ' COULD NOT FIND UEP FACTORS '                    
       GO TO 2550-EXIT                                           
    END-IF                                                       
                                                                 
    PERFORM VARYING P1 FROM +1 BY +1 UNTIL                       
       (P1 > +11)                                                
       OR (DD-PROD-CODE (D1 P1) = 'I')                           
    END-PERFORM                                                  
    IF P1 < +12                                                  
       display ' setting iu present to true '                    
       SET DD-IU-PRESENT        TO TRUE                          
    END-IF                                                       
                                                                 
    MOVE DE-AH-TERM             TO WS-DDF-TERM                   
    IF (DE-LOAN-TERM > WS-DDF-TERM)                              
       AND (DD-TRUNCATED (D1) = 'Y')                             
       MOVE DE-LOAN-TERM        TO WS-DDF-TERM                   
       DISPLAY ' FOUND TRUNCATED ' DE-CERT-NO                    
    END-IF                                                       
                                                                 
    EVALUATE TRUE                                                
       WHEN WS-DDF-TERM > +168                                   
          MOVE 15               TO P1                            
       WHEN WS-DDF-TERM > +156                                   
          MOVE 14               TO P1                            
       WHEN WS-DDF-TERM > +144                                   
          MOVE 13               TO P1                            
       WHEN WS-DDF-TERM > +132                                   
          MOVE 12               TO P1                            
       WHEN WS-DDF-TERM > +120                                   
          MOVE 11               TO P1                            
       WHEN WS-DDF-TERM > +108                                   
          MOVE 10               TO P1                            
       WHEN WS-DDF-TERM > +96                                    
          MOVE 9                TO P1                            
       WHEN WS-DDF-TERM > +84                                    
          MOVE 8                TO P1                            
       WHEN WS-DDF-TERM > +72                                    
          MOVE 7                TO P1                            
       WHEN WS-DDF-TERM > +60                                    
          MOVE 6                TO P1                            
       WHEN WS-DDF-TERM > +48                                    
          MOVE 5                TO P1                            
       WHEN WS-DDF-TERM > +36                                    
          MOVE 4                TO P1                            
       WHEN WS-DDF-TERM > +24                                    
          MOVE 3                TO P1                            
       WHEN WS-DDF-TERM > +12                                    
          MOVE 2                TO P1                            
       WHEN OTHER                                                
          MOVE 1                TO P1                            
