    IF DE-AGT-TYPE (X1) NOT = 'C' AND 'D' AND 'F'                
        GO TO 2420-ACCT-COMM-LOOP.                               
                                                                 
    COMPUTE L-OW ROUNDED     = CP-LF-PRM * DE-L-PC (X1).         
    COMPUTE L-OW-ALT ROUNDED = CP-LF-PRM-ALT * DE-L-PC (X1).     
    IF CLAS-I-BEN-CATEGORY (CLAS-INDEXA) = 'G' OR 'L'            
       IF DE-CANCEL                                              
          COMPUTE CNC-FACT = DE-AH-RFND / DE-AH-PRM              
          IF (AM-DCC-PRODUCT-CODE = 'DDF')                       
             and (dte-client = 'DCC')                            
             COMPUTE A-OW = (DE-AH-PRM - DE-REI-AHPRM -          
                DE-ADDL-CLP)                                     
             PERFORM 2540-CALC-DDF-COMM THRU 2540-EXIT           
             COMPUTE A-OW = CNC-WK * -1                          
          ELSE                                                   
             IF DE-CANCEL-REASON = 'R'                           
                MOVE +1            TO CNC-FACT                   
             END-IF                                              
             COMPUTE A-OW ROUNDED = (CNC-FACT *                  
                (DE-AH-PRM - DE-LF-PRM-ALT                       
                - DE-ADDL-CLP)) * -1                             
          END-IF                                                 
       ELSE                                                      
*>          display ' ah comm ' de-cert ' ' cp-ah-prm ' '          
*>             cp-lf-prm-alt ' ' de-addl-clp ' ' de-lf-prm-alt     
          COMPUTE A-OW = CP-AH-PRM - CP-LF-PRM-ALT               
          - DE-ADDL-CLP                                          
       END-IF                                                    
    ELSE                                                         
       COMPUTE A-OW ROUNDED     = CP-AH-PRM * DE-A-PC (X1)       
    END-IF                                                       
                                                                 
    ADD L-OW         TO CP-LF-COM.                               
    ADD L-OW-ALT     TO CP-LF-COM-ALT.                           
    ADD A-OW         TO CP-AH-COM.                               
