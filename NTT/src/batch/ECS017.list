    IF DE-LF-CANC-DTE > ZERO                                     
        COMPUTE MONTHS-DIFF-LF =                                 
            ((DE-LF-CANC-CCYY * 12)+ DE-LF-CANC-MO) -            
                                                                 
                ((DE-EF-CCYY * 12)+ DE-EF-MO)                    
    ELSE                                                         
        MOVE ZEROS              TO  MONTHS-DIFF-LF.              
                                                                 
    IF DE-AH-CANC-DTE > ZERO                                     
        COMPUTE MONTHS-DIFF-AH =                                 
            (((DE-AH-CANC-CCYY * 12) + DE-AH-CANC-MO) -          
                 ((DE-EF-CCYY  * 12) + DE-EF-MO))                
    ELSE                                                         
        MOVE ZEROS              TO  MONTHS-DIFF-AH.              
                                                                 
*>**************************************************************** 
*>  AS PER CREDIT LIFE DEPT.  WE WILL ASSUME THE LIFE AND A & H  * 
*>  COVERAGE WILL ALWAYS HAVE THE SAME EFFECTIVE DATE. THEREFORE * 
*>  ONLY ONE CHECK WILL BE NECESSARY FOR CLIENT 'UCL'.           * 
*>  F. RHORER  7/2/90                                            * 
*>**************************************************************** 
                                                                 
    IF DTE-CLIENT = 'UCL'                                        
      IF MONTHS-DIFF-LF > 6                                      
           GO TO 2490-OW-L-X                                     
      ELSE                                                       
         GO TO 2420-ACCT-COMM-LOOP.                              
                                                                 
    IF MONTHS-DIFF-LF > 6                                        
        IF MONTHS-DIFF-AH > 6                                    
            GO TO 2490-OW-L-X.                                   
                                                                 
    IF MONTHS-DIFF-LF < 6                                        
        IF MONTHS-DIFF-AH < 6                                    
            GO TO 2420-ACCT-COMM-LOOP.                           
                                                                 
    IF MONTHS-DIFF-LF < 6                                        
        MOVE ZEROS              TO  CP-AH-PRM                    
        GO TO 2420-ACCT-COMM-LOOP.                               
                                                                 
    IF MONTHS-DIFF-AH < 6                                        
        MOVE ZEROS              TO  CP-LF-PRM                    
        GO TO 2420-ACCT-COMM-LOOP.                               
                                                                 
    IF MONTHS-DIFF-LF = 6                                        
        IF MONTHS-DIFF-AH = 6                                    
            IF DE-LF-CANC-DA < DE-EF-DA                          
                IF DE-AH-CANC-DA < DE-EF-DA                      
                    GO TO 2420-ACCT-COMM-LOOP.                   
                                                                 
    IF MONTHS-DIFF-LF = 6                                        
        IF DE-LF-CANC-DA < DE-EF-DA                              
            MOVE ZEROS          TO  CP-AH-PRM                    
            GO TO 2420-ACCT-COMM-LOOP.                           
                                                                 
