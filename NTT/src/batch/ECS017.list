*>                            COPY ELCDTERX SUPPRESS.              
                                                                 
    MOVE RUN-YR              TO WS-RUN-YR                        
    MOVE RUN-MO              TO WS-RUN-MO                        
    MOVE RUN-DA              TO WS-RUN-DA                        
                                                                 
    IF DTE-PGM-OPT LESS '1' OR GREATER '8'                       
        MOVE '1' TO               DTE-PGM-OPT.                   
                                                                 
    MOVE ALPH-DATE              TO P-RUN-DATE.                   
                                                                 
    OPEN INPUT  EP-EXTR  ERACCTT ERCOMP                          
         OUTPUT COMM-TRANS  EXTR-DISK  END-PRINT  RECALC-EXTR.   
                                                                 
    IF DTE-CLIENT = 'DCC' or 'VPP'                               
       OPEN INPUT ERPDEF                                         
    END-IF                                                       
                                                                 
    IF ERACCTT-FILE-STATUS  = '00' OR '97'                       
        NEXT SENTENCE                                            
      ELSE                                                       
        MOVE ERACCTT-FILE-STATUS                                 
                                TO WS-ABEND-FILE-STATUS          
        MOVE ' ERACCTT OPEN ERROR- '                             
                                TO WS-ABEND-MESSAGE              
        GO TO ABEND-PGM.                                         
                                                                 
                                                                 
    IF ERCOMP-FILE-STATUS  = '00' OR '97'                        
       CONTINUE                                                  
    ELSE                                                         
       MOVE ERCOMP-FILE-STATUS  TO WS-ABEND-FILE-STATUS          
       MOVE ' ERCOMP  OPEN ERROR- '                              
                                TO WS-ABEND-MESSAGE              
       PERFORM ABEND-PGM                                         
    END-IF                                                       
                                                                 
    IF ERPDEF-FILE-STATUS  = '00' OR '97'                        
       CONTINUE                                                  
    ELSE                                                         
       MOVE ERPDEF-FILE-STATUS  TO WS-ABEND-FILE-STATUS          
       MOVE ' ERPDEF  OPEN ERROR- '                              
                                TO WS-ABEND-MESSAGE              
       PERFORM ABEND-PGM                                         
    END-IF                                                       
                                                                 
    MOVE '1'                    TO X.                            
    MOVE HDNG-1                 TO P-DATA.                       
    PERFORM 8000-PRT-RTN THRU 8099-PRT-XIT.                      
                                                                 
    MOVE ' '                    TO X.                            
    MOVE COMPANY-NAME           TO HD-CLIENT.                    
    MOVE WS-CURRENT-DATE        TO HD-RUN.                       
    MOVE HDNG-2                 TO P-DATA.                       
    PERFORM 8000-PRT-RTN THRU 8099-PRT-XIT.                      
                                                                 
    MOVE ' '                    TO X.                            
    MOVE ALPH-DATE              TO HD-DATE.                      
    MOVE HDNG-3                 TO P-DATA.                       
    PERFORM 8000-PRT-RTN THRU 8099-PRT-XIT.                      
                                                                 
    PERFORM 1600-GET-ACC-MSTR-RTN THRU 1699-EXIT.                
                                                                 
    PERFORM 2800-CLEAR-TOTALS-RTN THRU 2899-EXIT.                
                                                                 
    IF DTE-CLIENT = 'DCC' or 'VPP'                               
       MOVE 0 TO B-SUB                                           
       MOVE DTE-CLASIC-COMPANY-CD                                
                                TO PD-CONTROL-PRIMARY            
       START ERPDEF KEY >= PD-CONTROL-PRIMARY                    
       IF ERPDEF-FILE-STATUS = '00'                              
          PERFORM UNTIL ERPDEF-FILE-STATUS NOT = '00'            
             READ ERPDEF NEXT RECORD                             
             IF ERPDEF-FILE-STATUS = '00'                        
                ADD 1 TO B-SUB                                   
                IF B-SUB > max-pdef                              
                   DISPLAY ' DDF UEP TABLE BLEW '                
                   GO TO ABEND-PGM                               
                END-IF                                           
                MOVE PD-STATE   TO DD-STATE (B-SUB)              
                MOVE PD-PRODUCT-CD                               
                                TO DD-PRODUCT-CD (B-SUB)         
                MOVE PD-BEN-TYPE                                 
                                TO DD-BEN-TYPE (B-SUB)           
                MOVE PD-BEN-CODE                                 
                                TO DD-BEN-CODE (B-SUB)           
                MOVE PD-PROD-EXP-DT                              
                                TO DD-PROD-EXP-DT (B-SUB)        
                MOVE PD-1ST-YR-ADMIN-ALLOW                       
                                TO DD-1ST-YR-ALLOW (B-SUB)       
                MOVE PD-TRUNCATED TO DD-TRUNCATED (B-SUB)        
                PERFORM VARYING P2 FROM +1 BY +1 UNTIL           
                   P2 > +11                                      
                   MOVE PD-PROD-CODE (P2)                        
                                TO DD-PROD-CODE (B-SUB P2)       
                END-PERFORM                                      
                MOVE PD-EARN-FACTORS                             
                                TO DD-EARN-FACTORS (B-SUB)       
             END-IF                                              
          END-PERFORM                                            
       DISPLAY ' DCC PRODUCT DEF TABLE LOADED SUCCESSFULLY '     
       END-IF                                                    
       CLOSE ERPDEF                                              
    END-IF                                                       
                                                                 
    GO TO 1000-READ-EXTRACT.                                     
                                                                 
EJECT                                                            
1000-READ-EXTRACT.                                               
    READ EP-EXTR AT END                                          
        GO TO 9900-END-JOB.                                      
                                                                 
    IF DE-REIN NOT = SPACE                                       
        GO TO 1000-READ-EXTRACT.                                 
                                                                 
    IF NOT DE-ISSUE     AND                                      
       NOT DE-CANCEL    AND                                      
       NOT DE-RC-ISSUE  AND                                      
       NOT DE-RC-CANCEL AND                                      
       NOT DE-CLAIM                                              
            GO TO 1000-READ-EXTRACT.                             
                                                                 
    IF DE-ENTRY-STATUS = 'D' OR 'V'                              
        GO TO 1000-READ-EXTRACT.                                 
                                                                 
    IF DE-ISSUE                                                  
        IF DE-ENTRY-STATUS =  '5' OR '9'                         
            GO TO 1000-READ-EXTRACT.                             
                                                                 
    IF DE-RC-ISSUE                                               
        IF DE-ENTRY-STATUS =  '5' OR '9'                         
            GO TO 1000-READ-EXTRACT.                             
                                                                 
    IF DE-CANCEL                                                 
        IF DE-ENTRY-STATUS =  '9'                                
            GO TO 1000-READ-EXTRACT.                             
                                                                 
    IF DE-RC-CANCEL                                              
        IF DE-ENTRY-STATUS =  '9'                                
            GO TO 1000-READ-EXTRACT.                             
                                                                 
    IF DE-CLAIM                                                  
        IF DE-PAY-CODE = 'Y' OR 'Z'                              
            GO TO 1000-READ-EXTRACT.                             
                                                                 
