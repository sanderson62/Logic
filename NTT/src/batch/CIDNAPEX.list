       PERFORM 0100-PROCESS-ELNAPS THRU 0100-EXIT                
    END-IF                                                       
                                                                 
    PERFORM 0200-READ-ELNAPS    THRU 0200-EXIT                   
                                                                 
    .                                                            
0050-EXIT.                                                       
    EXIT.                                                        
                                                                 
0100-PROCESS-ELNAPS.                                             
                                                                 
    IF NA-COMPANY-CD NOT = DTE-CLASIC-COMPANY-CD                 
        SET END-OF-ELNAPS TO TRUE                                
        GO TO 0100-EXIT                                          
    END-IF                                                       
                                                                 
    MOVE NA-COMPANY-CD      TO AT-COMPANY-CD.                    
    MOVE NA-CARRIER         TO AT-CARRIER.                       
    MOVE NA-CLAIM-NO        TO AT-CLAIM-NO.                      
    MOVE NA-CERT-NO         TO AT-CERT-NO.                       
    MOVE NA-CORR-TRLR-SEQ   TO AT-SEQUENCE-NO.                   
                                                                 
    READ ELTRLR RECORD INVALID KEY                               
        GO TO 0100-EXIT                                          
    END-READ.                                                    
                                                                 
    IF (AT-LETTER-ANSWERED-DT NOT EQUAL LOW-VALUES AND SPACES)   
      OR (AT-STOP-LETTER-DT NOT EQUAL LOW-VALUES AND SPACES)     
         IF NA-CREATED-IN-NAPERSOFT = 'Y'                        
             MOVE SPACES TO STOP-QWS-REC                         
             IF NA-CERT-SFX > SPACES                             
                 STRING NA-CLAIM-NO ' '                          
                        NA-CERT-NO ' '                           
                        NA-LETTER-ID ' 0'                        
                        NA-ARCHIVE-NO                            
                 INTO STOP-QWS-REC                               
             ELSE                                                
                 STRING NA-CLAIM-NO ' '                          
                        NA-CERT-PRIME ' '                        
                        NA-LETTER-ID ' 0'                        
                        NA-ARCHIVE-NO                            
                 INTO STOP-QWS-REC                               
             END-IF                                              
             WRITE STOP-QWS-RECORD FROM STOP-QWS-REC             
             ADD +1  TO QWS-RECS-STOPPED                         
         END-IF                                                  
    END-IF                                                       
                                                                 
    IF AT-LETTER-ANSWERED-DT IS NOT EQUAL TO LOW-VALUES          
        GO TO 0100-EXIT                                          
    END-IF.                                                      
                                                                 
    IF AT-STOP-LETTER-DT NOT EQUAL LOW-VALUES AND SPACES         
        GO TO 0100-EXIT                                          
    END-IF.                                                      
                                                                 
    IF NA-LETTER-ID = SPACES                                     
       GO TO 0100-EXIT                                           
    END-IF.                                                      
                                                                 
    IF NA-LETTER-ID = 'HOHI' OR 'CI74'                           
       GO TO 0100-EXIT                                           
    END-IF.                                                      
                                                                 
    IF NA-CREATED-IN-NAPERSOFT = 'Y'  AND                        
       (NA-ENCLOSURE-CD (2:1) = 'X' OR                           
        NA-ENCLOSURE-CD (3:1) = 'X')                             
           MOVE SPACES TO QWS-SPEC-HNDL-REC                      
           IF NA-CERT-SFX > SPACES                               
                 STRING NA-CLAIM-NO ' '                          
                        NA-CERT-NO ' '                           
                        NA-LETTER-ID ' 0'                        
                        NA-ARCHIVE-NO                            
                 INTO QWS-SPEC-HNDL-REC                          
           ELSE                                                  
                 STRING NA-CLAIM-NO ' '                          
                        NA-CERT-PRIME ' '                        
                        NA-LETTER-ID ' 0'                        
                        NA-ARCHIVE-NO                            
                 INTO QWS-SPEC-HNDL-REC                          
           END-IF                                                
           WRITE QWS-SPEC-HNDL-RECORD FROM QWS-SPEC-HNDL-REC     
           ADD +1  TO QWS-SPEC-RECS                              
    END-IF                                                       
                                                                 
    MOVE WS-CURRENT-BIN-DT   TO NA-INITIAL-PRINT-DT              
                                AT-INITIAL-PRINT-DATE.           
                                                                 
    REWRITE NAPERSOFT-FILE                                       
         INVALID KEY                                             
             DISPLAY ' CIDNAPEX REWRITE ERROR - ELNAPS'          
             SET END-OF-ELNAPS TO TRUE                           
             GO TO ABEND-PGM.                                    
                                                                 
    REWRITE ACTIVITY-TRAILERS                                    
         INVALID KEY                                             
             DISPLAY ' CIDNAPEX REWRITE ERROR - ELTRLR'          
             SET END-OF-ELNAPS TO TRUE                           
             GO TO ABEND-PGM.                                    
    ADD 1 TO TRL-RECS-IN                                         
                                                                 
    IF NA-CREATED-IN-NAPERSOFT = 'Y'                             
        GO TO 0100-EXIT                                          
    END-IF.                                                      
                                                                 
    PERFORM 0205-READ-ELMSTR THRU 0205-EXIT                      
    PERFORM 0275-READ-ELCERT THRU 0275-EXIT                      
    PERFORM 0210-GET-ELTRLRS THRU 0210-EXIT                      
                                                                 
    MOVE WS-SAVE-NAPER         TO NAPER-DETAIL-RECORD            
                                                                 
    MOVE PARM-CYCLE-DATE        TO EX-CYCLE-DATE                 
    MOVE NA-LETTER-ID           TO EX-LETTER-ID                  
    MOVE NA-CARRIER             TO EX-CARRIER                    
    MOVE NA-CLAIM-NO            TO EX-CLAIM-NO                   
    MOVE FUNCTION REVERSE(NA-CLAIM-NO(3:5)) TO EX-VER-CD         
    MOVE NA-CERT-NO             TO EX-CERT-NO                    
    MOVE NA-ARCHIVE-NO          TO EX-ARCHIVE-NO                 
    MOVE NA-PROCESSOR-ID        TO EX-PROCESSOR-ID               
    MOVE DTE-CLIENT             TO EX-COMPANY-ID                 
    MOVE CL-CERT-ACCOUNT        TO EX-ACCOUNT                    
    MOVE CL-INSURED-LAST-NAME   TO EX-INSURED-LAST-NAME          
    MOVE CL-INSURED-1ST-NAME    TO EX-INSURED-1ST-NAME           
