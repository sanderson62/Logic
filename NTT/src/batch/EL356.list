       OR (CL-COMPANY-CD NOT = DTE-CLASIC-COMPANY-CD)            
       SET END-OF-INPUT         TO TRUE                          
    ELSE                                                         
       IF ELMSTR-FILE-STATUS NOT = '00'                          
          DISPLAY ' ERROR - ELMSTR - READ ' ELMSTR-FILE-STATUS   
          SET END-OF-INPUT      TO TRUE                          
       END-IF                                                    
    END-IF                                                       
                                                                 
