        MOVE CR-AHPRM-CALC         TO RS-AP-CALC                 
    ELSE                                                         
        MOVE CR-AH-NSP-PRM         TO RS-AP-CALC.                
    MOVE RS-AP-CALC                TO REIN-CALCED-AH.            
                                                                 
    IF WT-REMAINING = 'Y' OR 'Z' OR 'L' OR 'R'                   
        MOVE RS-R-LB               TO RS-LIFE-BEN                
        MOVE RS-R-LP               TO RS-LIFE-PREM               
        MOVE RS-R-LPC              TO RS-LP-CALC                 
        MOVE RS-R-AB               TO RS-AH-BEN                  
        MOVE RS-R-AP               TO RS-AH-PREM                 
        MOVE RS-R-APC              TO RS-AP-CALC.                
                                                                 
    IF SUB1 = RE-100-COMP                                        
        GO TO REINSURE-CALC-REMAINDER.                           
                                                                 
    IF CR-DT LESS WT-LO-DATE                                     
        GO TO REINSURE-CALC-X.                                   
                                                                 
    IF CR-DT NOT LESS WT-HI-DATE                                 
        GO TO REINSURE-CALC-X.                                   
                                                                 
    PERFORM REINSURE-CK-LIFE      THRU REINSURE-CK-LIFE-X.       
    PERFORM REINSURE-CK-AH        THRU REINSURE-CK-AH-X.         
                                                                 
    IF WT-INTERACTIVE = 'E'                                      
        PERFORM REIN-CHECK-INTERACT-E THRU REIN-INTERACT-CHECK-X.
                                                                 
    IF WT-INTERACTIVE = 'X'                                      
      IF  (REIN-LF-FLG (SUB1) = 'X' AND REIN-AH-FLG (SUB1) = 'X')
       OR (REIN-LF-FLG (SUB1) = 'X' AND REIN-AH-FLG (SUB1) = ' ')
       OR (REIN-LF-FLG (SUB1) = ' ' AND REIN-AH-FLG (SUB1) = 'X')
          NEXT SENTENCE                                          
      ELSE                                                       
          MOVE 'Y' TO REIN-LF-FLG (SUB1)                         
                      REIN-AH-FLG (SUB1).                        
                                                                 
    IF WT-INTERACTIVE = 'Y'                                      
        IF REIN-LF-FLG (SUB1) = 'X' AND                          
           REIN-AH-FLG (SUB1) = 'Y'                              
            MOVE 'X' TO REIN-AH-FLG (SUB1).                      
                                                                 
    IF WT-INTERACTIVE = 'Y'                                      
        IF REIN-AH-FLG (SUB1) = 'X' AND                          
           REIN-LF-FLG (SUB1) = 'Y'                              
            MOVE 'X' TO REIN-LF-FLG (SUB1).                      
                                                                 
    IF WT-INTERACTIVE = 'Z'                                      
        IF REIN-LF-FLG (SUB1) = 'X'                              
            IF REIN-AH-FLG (SUB1) = 'Y'                          
                MOVE 'X' TO REIN-AH-FLG (SUB1)                   
            ELSE                                                 
                NEXT SENTENCE                                    
        ELSE                                                     
            MOVE 'Y' TO REIN-LF-FLG (SUB1)                       
                        REIN-AH-FLG (SUB1).                      
                                                                 
    IF WT-INTERACTIVE = 'W'                                      
        IF REIN-AH-FLG (SUB1) = 'X'                              
            IF REIN-LF-FLG (SUB1) = 'Y'                          
                MOVE 'X' TO REIN-LF-FLG (SUB1)                   
            ELSE                                                 
                NEXT SENTENCE                                    
        ELSE                                                     
            MOVE 'Y' TO REIN-LF-FLG (SUB1)                       
                        REIN-AH-FLG (SUB1).                      
                                                                 
    IF REIN-LF-FLG (SUB1) = 'X'                                  
        PERFORM REIN-CALC-LIFE THRU REIN-CALC-LIFE-X.            
                                                                 
    IF REIN-LF-FLG (SUB1) = 'E'                                  
        PERFORM REIN-LF-SKIP-LIMIT THRU REIN-CALC-LIFE-X         
        MOVE 'X' TO REIN-LF-FLG (SUB1).                          
                                                                 
    IF REIN-AH-FLG (SUB1) = 'X'                                  
        PERFORM REIN-CALC-AH THRU REIN-CALC-AH-X.                
                                                                 
    IF REIN-AH-FLG (SUB1) = 'E'                                  
        PERFORM REIN-AH-SKIP-LIMIT THRU REIN-CALC-AH-X           
        MOVE 'X'                TO  REIN-AH-FLG (SUB1).          
                                                                 
    IF REIN-LF-FLG (SUB1) NOT = 'X'                              
        MOVE SPACE TO REIN-LF-FLG (SUB1).                        
                                                                 
    IF REIN-AH-FLG (SUB1) NOT = 'X'                              
        MOVE SPACE TO REIN-AH-FLG (SUB1).                        
                                                                 
    GO TO REINSURE-CALC-X.                                       
                                                                 
REINSURE-CK-LIFE.                                                
                                                                 
    IF CR-LFTYP = ZERO                                           
        GO TO REINSURE-CK-LIFE-X.                                
                                                                 
    IF WT-LF-PCT = ZERO                                          
        GO TO REINSURE-CK-LIFE-X.                                
                                                                 
    IF DTE-CLIENT = 'CSO' OR 'CSI'                               
        IF CLAS-I-REIN-YN (CLAS-INDEXL) = 'N'                    
            GO TO REINSURE-CK-LIFE-X.                            
                                                                 
    MOVE WT-LF-BEN-CODE                TO REIN-BEN-CODE.         
                                                                 
    IF CEDING-PROPERTY-ONLY                                      
        IF CLAS-I-CALC-TYPE (CLAS-INDEXL) NOT = '2'              
            GO TO REINSURE-CK-LIFE-X                             
        ELSE                                                     
            NEXT SENTENCE                                        
    ELSE                                                         
        IF CLAS-I-CALC-TYPE (CLAS-INDEXL) = '2'                  
            GO TO REINSURE-CK-LIFE-X.                            
                                                                 
    IF CEDING-AD-D-ONLY                                          
        IF CLAS-I-CALC-TYPE (CLAS-INDEXL) NOT = '4'              
            GO TO REINSURE-CK-LIFE-X                             
        ELSE                                                     
            NEXT SENTENCE                                        
    ELSE                                                         
        IF CLAS-I-CALC-TYPE (CLAS-INDEXL) = '4'                  
            GO TO REINSURE-CK-LIFE-X.                            
                                                                 
    IF CEDING-SINGLE-PREM-ONLY                                   
        IF CLAS-I-CALC-TYPE (CLAS-INDEXL) = 'B'                  
            GO TO REINSURE-CK-LIFE-X.                            
                                                                 
    IF CEDING-OUT-BALANCE-ONLY                                   
        IF CLAS-I-CALC-TYPE (CLAS-INDEXL) NOT = 'B'              
           GO TO REINSURE-CK-LIFE-X.                             
                                                                 
    IF CEDING-REDUCING-ONLY                                      
        IF CLAS-I-RL-AH (CLAS-INDEXL) = 'L' OR 'P'               
            GO TO REINSURE-CK-LIFE-X.                            
                                                                 
    IF CEDING-LEVEL-ONLY                                         
        IF CLAS-I-RL-AH (CLAS-INDEXL) = 'R'                      
           GO TO REINSURE-CK-LIFE-X.                             
                                                                 
    IF CEDING-GROUP-ONLY                                         
        IF CR-IND-GRP NOT = '2'                                  
            GO TO REINSURE-CK-LIFE-X.                            
                                                                 
    IF CEDING-GROUP-RED-LIFE-ONLY                                
        IF (CR-IND-GRP = '1')  OR                                
           (CLAS-I-RL-AH (CLAS-INDEXL) = 'L' OR 'P') OR          
           (CR-AHTYP NOT = ZERO)                                 
            GO TO REINSURE-CK-LIFE-X.                            
                                                                 
    IF CEDING-GROUP-REDUCING-ONLY                                
        IF (CR-IND-GRP = '1')  OR                                
           (CLAS-I-RL-AH (CLAS-INDEXL) = 'L' OR 'P')             
            GO TO REINSURE-CK-LIFE-X.                            
                                                                 
    IF CEDING-INDIVIDUAL-ONLY                                    
        IF CR-IND-GRP NOT = '1'                                  
            GO TO REINSURE-CK-LIFE-X.                            
                                                                 
    IF CEDING-JOINT-ONLY                                         
        IF CLAS-I-JOINT (CLAS-INDEXL) NOT = 'J'                  
            GO TO REINSURE-CK-LIFE-X.                            
                                                                 
    IF CEDING-SGL-PREM-REDUCING                                  
       IF (CLAS-I-RL-AH (CLAS-INDEXL) = 'L' OR 'P')  OR          
          CLAS-I-CALC-TYPE (CLAS-INDEXL) = 'B'                   
           GO TO REINSURE-CK-LIFE-X.                             
                                                                 
    IF CEDING-SGL-PREM-GROUP                                     
        IF CLAS-I-CALC-TYPE (CLAS-INDEXL) = 'B'  OR              
           CR-IND-GRP = '1'                                      
            GO TO REINSURE-CK-LIFE-X.                            
                                                                 
    IF CEDING-GROUP-LEVEL-ONLY                                   
        IF CLAS-I-RL-AH (CLAS-INDEXL) = 'R'  OR                  
           CR-IND-GRP = '1'                                      
            GO TO REINSURE-CK-LIFE-X.                            
                                                                 
    IF CEDING-SGL-PREM-GRP-REDUCING                              
        IF CLAS-I-CALC-TYPE (CLAS-INDEXL) = 'B'  OR              
           (CLAS-I-RL-AH (CLAS-INDEXL) = 'L' OR 'P')  OR         
           CR-IND-GRP = '1'                                      
            GO TO REINSURE-CK-LIFE-X.                            
                                                                 
    IF CEDING-SGL-PREM-GROUP-LEVEL                               
        IF CLAS-I-CALC-TYPE (CLAS-INDEXL) = 'B'  OR              
           CLAS-I-RL-AH (CLAS-INDEXL) = 'R'  OR                  
           CR-IND-GRP = '1'                                      
            GO TO REINSURE-CK-LIFE-X.                            
                                                                 
    IF CEDING-SINGLE-POLICIES-ONLY                               
        IF CLAS-I-JOINT (CLAS-INDEXL) = 'J'                      
           GO TO REINSURE-CK-LIFE-X.                             
                                                                 
    IF DTE-CLIENT EQUAL 'NCL'                                    
       MOVE 'Y'                 TO REIN-LF-FLG (SUB1).           
                                                                 
    IF CR-AGE LESS WT-LFAGE-LO                                   
        GO TO REINSURE-CK-LIFE-X.                                
                                                                 
    IF CR-AGE GREATER WT-LFAGE-HI                                
        GO TO REINSURE-CK-LIFE-X.                                
                                                                 
    IF CR-LF-TERM  NUMERIC                                       
       NEXT SENTENCE                                             
    ELSE                                                         
       MOVE ZEROS               TO CR-LF-TERM                    
    END-IF                                                       
                                                                 
    IF CR-LF-TERM LESS WT-LFTRM-LO                               
        GO TO REINSURE-CK-LIFE-X.                                
                                                                 
    IF CR-LF-TERM GREATER WT-LFTRM-HI                            
        GO TO REINSURE-CK-LIFE-X.                                
                                                                 
    MOVE 'Y' TO REIN-LF-FLG (SUB1).                              
                                                                 
    IF RS-LIFE-BEN NOT GREATER WT-LF-LIM-LO                      
        GO TO REINSURE-CK-LIFE-X.                                
                                                                 
    IF RS-LIFE-BEN GREATER WT-LF-LIM-HI                          
        GO TO REINSURE-CK-LIFE-X.                                
                                                                 
    MOVE 'X' TO REIN-LF-FLG (SUB1).                              
                                                                 
REINSURE-CK-LIFE-X.                                              
    EXIT.                                                        
                                                                 
REINSURE-CK-AH.                                                  
                                                                 
    IF CR-AHTYP = ZERO                                           
        GO TO REINSURE-CK-AH-X.                                  
                                                                 
    IF WT-AH-PCT = ZERO                                          
        GO TO REINSURE-CK-AH-X.                                  
                                                                 
    IF DTE-CLIENT = 'CSO' OR 'CSI'                               
        IF CLAS-I-REIN-YN (CLAS-INDEXA) = 'N'                    
            GO TO REINSURE-CK-AH-X.                              
                                                                 
    MOVE WT-AH-BEN-CODE                TO REIN-BEN-CODE.         
                                                                 
    IF CEDING-UNEMPLOYMENT-ONLY                                  
        IF CLAS-I-CALC-TYPE (CLAS-INDEXA) NOT = '3'              
            GO TO REINSURE-CK-AH-X                               
        ELSE                                                     
            NEXT SENTENCE                                        
    ELSE                                                         
        IF CLAS-I-CALC-TYPE (CLAS-INDEXA) = '3'                  
            GO TO REINSURE-CK-AH-X.                              
                                                                 
    IF CEDING-SINGLE-PREM-ONLY                                   
        IF CLAS-I-CALC-TYPE (CLAS-INDEXA) = 'B'                  
            GO TO REINSURE-CK-AH-X.                              
                                                                 
    IF CEDING-JOINT-ONLY                                         
        IF CLAS-I-JOINT (CLAS-INDEXA) NOT = 'J'                  
            GO TO REINSURE-CK-AH-X.                              
                                                                 
    IF CEDING-OUT-BALANCE-ONLY                                   
        IF CLAS-I-CALC-TYPE (CLAS-INDEXA) NOT = 'B'              
            GO TO REINSURE-CK-AH-X.                              
                                                                 
    IF CEDING-GROUP-ONLY                                         
        IF CR-IND-GRP NOT = '2'                                  
            GO TO REINSURE-CK-AH-X.                              
                                                                 
    IF CEDING-SGL-PREM-GROUP                                     
        IF CLAS-I-CALC-TYPE (CLAS-INDEXA) = 'B'  OR              
           CR-IND-GRP = '1'                                      
            GO TO REINSURE-CK-AH-X.                              
                                                                 
    IF CEDING-INDIVIDUAL-ONLY                                    
        IF CR-IND-GRP NOT = '1'                                  
            GO TO REINSURE-CK-AH-X.                              
                                                                 
    IF CEDING-SINGLE-POLICIES-ONLY                               
        IF CLAS-I-JOINT (CLAS-INDEXA) = 'J'                      
           GO TO REINSURE-CK-AH-X.                               
                                                                 
    IF CEDING-7-DAY-RETRO                                        
        IF CLAS-I-AB3 (CLAS-INDEXA) NOT = (' 7R'  AND  '07R')    
           GO TO REINSURE-CK-AH-X.                               
                                                                 
    IF CEDING-14-DAY-RETRO                                       
        IF CLAS-I-AB3 (CLAS-INDEXA) NOT = '14R'                  
            GO TO REINSURE-CK-AH-X.                              
                                                                 
    IF DTE-CLIENT EQUAL 'NCL'                                    
       MOVE 'Y'                 TO REIN-AH-FLG (SUB1).           
                                                                 
    IF CR-AGE LESS WT-AHAGE-LO                                   
        GO TO REINSURE-CK-AH-X.                                  
                                                                 
    IF CR-AGE GREATER WT-AHAGE-HI                                
        GO TO REINSURE-CK-AH-X.                                  
                                                                 
    IF CR-AH-TERM  NUMERIC                                       
       NEXT SENTENCE                                             
    ELSE                                                         
       MOVE ZEROS               TO CR-AH-TERM                    
    END-IF                                                       
                                                                 
    IF CR-AH-TERM LESS WT-AHTRM-LO                               
        GO TO REINSURE-CK-AH-X.                                  
                                                                 
    IF CR-AH-TERM GREATER WT-AHTRM-HI                            
        GO TO REINSURE-CK-AH-X.                                  
                                                                 
    MOVE 'Y' TO REIN-AH-FLG (SUB1).                              
                                                                 
    IF WT-AHBEN-LIM-LO = ZERO                                    
        AND WT-AHBEN-LIM-HI = ZERO                               
            MOVE +9999999.99 TO WT-AHBEN-LIM-LO                  
                                WT-AHBEN-LIM-HI.                 
                                                                 
    IF WT-AHMOA-LIM-LO = ZERO                                    
        AND WT-AHMOA-LIM-HI = ZERO                               
            MOVE +9999999.99 TO WT-AHMOA-LIM-LO                  
                                WT-AHMOA-LIM-HI.                 
                                                                 
    COMPUTE RW-AHLIM-LO = CR-AH-TERM * WT-AHMOA-LIM-LO.          
    COMPUTE RW-AHLIM-HI = CR-AH-TERM * WT-AHMOA-LIM-HI.          
                                                                 
    IF RW-AHLIM-LO GREATER WT-AHBEN-LIM-LO                       
        MOVE WT-AHBEN-LIM-LO TO RW-AHLIM-LO.                     
                                                                 
    IF RW-AHLIM-HI GREATER WT-AHBEN-LIM-HI                       
        MOVE WT-AHBEN-LIM-HI TO RW-AHLIM-HI.                     
                                                                 
    MOVE RW-AHLIM-LO TO RWF-AH-LIMIT.                            
                                                                 
    COMPUTE RW-AHWRK = RS-AH-BEN * CR-AH-TERM.                   
                                                                 
    IF RW-AHWRK NOT GREATER RW-AHLIM-LO                          
        GO TO REINSURE-CK-AH-X.                                  
                                                                 
    IF RW-AHWRK GREATER RW-AHLIM-HI                              
        GO TO REINSURE-CK-AH-X.                                  
                                                                 
    MOVE 'X' TO REIN-AH-FLG (SUB1).                              
                                                                 
REINSURE-CK-AH-X.                                                
    EXIT.                                                        
                                                                 
REIN-CHECK-INTERACT-E.                                           
                                                                 
    IF RS-LIFE-BEN = ZERO  OR                                    
       RS-AH-BEN = ZERO                                          
        GO TO REIN-INTERACT-CHECK-X.                             
                                                                 
    IF REIN-LF-FLG (SUB1) NOT = 'X'  AND                         
       REIN-AH-FLG (SUB1) NOT = 'X'                              
        GO TO REIN-INTERACT-CHECK-X.                             
                                                                 
    MOVE 'X'                    TO REIN-LF-FLG (SUB1)            
                                   REIN-AH-FLG (SUB1).           
                                                                 
    IF WT-AHBEN-LO = ZERO                                        
        AND WT-AHBEN-HI = ZERO                                   
            MOVE +9999999.99    TO  WT-AHBEN-LO                  
                                    WT-AHBEN-HI.                 
    IF WT-AHMOA-LO = ZERO                                        
        AND WT-AHMOA-HI = ZERO                                   
            MOVE +9999999.99    TO  WT-AHMOA-LO                  
                                    WT-AHMOA-HI.                 
                                                                 
    COMPUTE RW-AHLIM-LO = CR-AH-TERM * WT-AHMOA-LO.              
    COMPUTE RW-AHLIM-HI = CR-AH-TERM * WT-AHMOA-HI.              
                                                                 
    IF RW-AHLIM-LO GREATER WT-AHBEN-LO                           
        MOVE WT-AHBEN-LO        TO  RW-AHLIM-LO.                 
                                                                 
    IF RW-AHLIM-HI GREATER WT-AHBEN-HI                           
        MOVE WT-AHBEN-HI        TO  RW-AHLIM-HI.                 
                                                                 
    COMPUTE RW-AHWRK = RS-AH-BEN * CR-AH-TERM.                   
                                                                 
    IF RW-AHWRK GREATER RW-AHLIM-HI                              
        MOVE RW-AHLIM-HI        TO  RW-AHWRK.                    
                                                                 
    IF RW-AHWRK GREATER RW-AHLIM-LO                              
        COMPUTE RATE-WORK-A ROUNDED = (RW-AHWRK - RW-AHLIM-LO)   
                                      / RW-AHWRK                 
      ELSE                                                       
        MOVE ZERO               TO RATE-WORK-A.                  
                                                                 
    IF RS-LIFE-BEN GREATER WT-LF-LO                              
        COMPUTE RATE-WORK-L ROUNDED = (RS-LIFE-BEN - WT-LF-LO)   
                                      / RS-LIFE-BEN              
      ELSE                                                       
        MOVE ZERO               TO RATE-WORK-L.                  
                                                                 
    IF (RS-LIFE-BEN GREATER WT-LF-LO) AND                        
       (RW-AHWRK NOT GREATER RW-AHLIM-LO)                        
         MOVE 'E'               TO REIN-AH-FLG (SUB1)            
         COMPUTE RWF-AHAMT ROUNDED = (RW-AHWRK * RATE-WORK-L)    
                                     / CR-AH-TERM.               
                                                                 
    IF (RW-AHWRK GREATER RW-AHLIM-LO) AND                        
       (RS-LIFE-BEN NOT GREATER WT-LF-LO)                        
         MOVE 'E'               TO REIN-LF-FLG (SUB1)            
         IF (RW-AHWRK - RW-AHLIM-LO) NOT GREATER RS-LIFE-BEN     
              COMPUTE RWF-LFAMT = RW-AHWRK - RW-AHLIM-LO         
           ELSE                                                  
               MOVE RS-LIFE-BEN TO RWF-LFAMT.                    
                                                                 
    IF (RW-AHWRK GREATER RW-AHLIM-LO) AND                        
       (RS-LIFE-BEN GREATER WT-LF-LO)                            
       IF RATE-WORK-L GREATER RATE-WORK-A                        
         MOVE 'E'               TO REIN-AH-FLG (SUB1)            
         COMPUTE RWF-AHAMT ROUNDED = (RW-AHWRK * RATE-WORK-L)    
                                     / CR-AH-TERM                
        ELSE                                                     
         MOVE 'E'               TO REIN-LF-FLG (SUB1)            
         IF (RW-AHWRK - RW-AHLIM-LO) NOT GREATER RS-LIFE-BEN     
              COMPUTE RWF-LFAMT = RW-AHWRK - RW-AHLIM-LO         
           ELSE                                                  
               MOVE RS-LIFE-BEN TO RWF-LFAMT.                    
                                                                 
REIN-INTERACT-CHECK-X.                                           
    EXIT.                                                        
                                                                 
                                                                 
REIN-CALC-LIFE.                                                  
                                                                 
    IF RS-LIFE-BEN NOT GREATER WT-LF-HI                          
        MOVE RS-LIFE-BEN TO RWF-LFAMT                            
    ELSE                                                         
        MOVE WT-LF-HI TO RWF-LFAMT.                              
                                                                 
    IF RS-LIFE-BEN LESS WT-LF-LO                                 
        MOVE +0 TO RWF-LFAMT                                     
    ELSE                                                         
        IF WT-REMAINING NOT = 'L'                                
          IF RS-LIFE-BEN LESS THAN ZERO                          
              COMPUTE RWF-LFAMT ROUNDED = WT-LF-PCT * RS-LIFE-BEN
          ELSE                                                   
              COMPUTE RWF-LFAMT ROUNDED = WT-LF-PCT *            
                                         (RWF-LFAMT - WT-LF-LO). 
                                                                 
REIN-LF-SKIP-LIMIT.                                              
                                                                 
    MOVE +0 TO RW-LFPRM  RW-LFPRMC.                              
                                                                 
    IF WT-REMAINING = 'I'                                        
        GO TO REIN-CALC-LF-RISK-PREM.                            
                                                                 
    IF WT-REMAINING = 'L'                                        
        COMPUTE RWF-LFPRM ROUNDED = RS-LIFE-PREM * WT-LF-PCT     
        GO TO REIN-CALC-LIFE-X.                                  
                                                                 
    IF RS-LIFE-BEN NOT = ZERO                                    
        COMPUTE RW-LFPRM ROUNDED = (RS-LIFE-PREM *               
                  RWF-LFAMT) / RS-LIFE-BEN                       
        COMPUTE RW-LFPRMC ROUNDED = (RS-LP-CALC *                
                  RWF-LFAMT) / RS-LIFE-BEN                       
    ELSE                                                         
        IF WT-LF-LIM-LO = ZERO                                   
            COMPUTE RW-LFPRM ROUNDED = RS-LIFE-PREM * WT-LF-PCT  
            COMPUTE RW-LFPRMC ROUNDED = RS-LP-CALC * WT-LF-PCT.  
                                                                 
    IF ((WT-LF-QC = '1' OR '3') OR                               
        (WT-LF-QC = '2' AND                                      
         PGM-SUB = +50  AND                                      
         DTE-CLIENT = 'LAP'))                                    
         MOVE RW-LFPRM TO RWF-LFPRM                              
    ELSE                                                         
        MOVE RW-LFPRMC TO RWF-LFPRM.                             
                                                                 
    IF WT-REMAINING = 'Y' OR 'Z' OR 'X' OR 'N'                   
        COMPUTE RS-R-LB = RS-R-LB - RWF-LFAMT                    
        COMPUTE RS-R-LP = RS-R-LP - RW-LFPRM                     
        COMPUTE RS-R-LPC = RS-R-LPC - RW-LFPRMC.                 
                                                                 
    GO TO REIN-CALC-LIFE-X.                                      
                                                                 
REIN-CALC-LF-RISK-PREM.                                          
                                                                 
    IF RWF-LFAMT NOT GREATER THAN +0                             
        MOVE ZEROS                 TO RWF-LFPRM                  
        GO TO REIN-CALC-LIFE-X.                                  
                                                                 
    MOVE RWF-LFAMT                 TO REIN-RISK-AMOUNT.          
    COMPUTE REIN-MONTH-REDUCE = RS-LIFE-BEN / CR-LF-TERM.        
    MOVE +0                        TO REIN-WORKA                 
                                      RW-LFPRM.                  
                                                                 
REIN-LF-RISK-PREM-LOOP.                                          
                                                                 
    COMPUTE RW-LFPRM = RW-LFPRM +                                
               ((REIN-RISK-AMOUNT / +1000) * CR-LF-NSP-PRM-RATE).
                                                                 
    IF CLAS-I-RL-AH (CLAS-INDEXL) NOT = 'L'                      
       SUBTRACT REIN-MONTH-REDUCE FROM REIN-RISK-AMOUNT.         
                                                                 
    IF REIN-WORKA = REIN-EARN-LF-TERM  OR                        
       REIN-RISK-AMOUNT LESS THAN +0                             
        MOVE RW-LFPRM              TO RWF-LFPRM                  
    ELSE                                                         
        ADD +1 TO REIN-WORKA                                     
        GO TO REIN-LF-RISK-PREM-LOOP.                            
                                                                 
                                                                 
REIN-CALC-LIFE-X.                                                
    EXIT.                                                        
                                                                 
REIN-CALC-AH.                                                    
                                                                 
    IF WT-AHBEN-LO = ZERO                                        
        AND WT-AHBEN-HI = ZERO                                   
            MOVE +9999999.99 TO WT-AHBEN-LO                      
                                WT-AHBEN-HI.                     
                                                                 
    IF WT-AHMOA-LO = ZERO                                        
        AND WT-AHMOA-HI = ZERO                                   
            MOVE +9999999.99 TO WT-AHMOA-LO                      
                                WT-AHMOA-HI.                     
                                                                 
    COMPUTE RW-AHLIM-LO = CR-AH-TERM * WT-AHMOA-LO.              
    COMPUTE RW-AHLIM-HI = CR-AH-TERM * WT-AHMOA-HI.              
                                                                 
    IF RW-AHLIM-LO GREATER WT-AHBEN-LO                           
        MOVE WT-AHBEN-LO TO RW-AHLIM-LO.                         
                                                                 
    IF RW-AHLIM-HI GREATER WT-AHBEN-HI                           
        MOVE WT-AHBEN-HI TO RW-AHLIM-HI.                         
                                                                 
    IF RW-AHWRK GREATER RW-AHLIM-HI                              
        MOVE RW-AHLIM-HI TO RW-AHWRK.                            
                                                                 
    IF RW-AHWRK LESS RW-AHLIM-LO                                 
        MOVE +0 TO RWF-AHAMT                                     
    ELSE                                                         
        IF WT-REMAINING = 'L'                                    
            COMPUTE RWF-AHAMT ROUNDED = RW-AHWRK / CR-AH-TERM    
        ELSE                                                     
            COMPUTE RWF-AHAMT ROUNDED =                          
            ((RW-AHWRK - RW-AHLIM-LO) / CR-AH-TERM) * WT-AH-PCT. 
                                                                 
REIN-AH-SKIP-LIMIT.                                              
                                                                 
    IF WT-REMAINING = 'I'                                        
        GO TO REIN-CALC-AH-RISK-PREM.                            
                                                                 
    IF WT-REMAINING = 'L'                                        
        COMPUTE RWF-AHPRM ROUNDED = RS-AH-PREM * WT-AH-PCT       
        GO TO REIN-CALC-AH-X.                                    
                                                                 
    IF RS-AH-BEN NOT = ZERO                                      
        COMPUTE RW-AHPRM ROUNDED = (RS-AH-PREM *                 
                  RWF-AHAMT) / RS-AH-BEN                         
        COMPUTE RW-AHPRMC ROUNDED = (RS-AP-CALC *                
                  RWF-AHAMT) / RS-AH-BEN                         
    ELSE                                                         
        IF RW-AHLIM-LO = ZERO                                    
            COMPUTE RW-AHPRM ROUNDED = RS-AH-PREM * WT-AH-PCT    
            COMPUTE RW-AHPRMC ROUNDED = RS-AP-CALC * WT-AH-PCT.  
                                                                 
    IF ((WT-AH-QC = '1' OR '3') OR                               
        (WT-AH-QC = '2' AND                                      
         PGM-SUB = +50  AND                                      
         DTE-CLIENT = 'LAP'))                                    
        MOVE RW-AHPRM TO RWF-AHPRM                               
    ELSE                                                         
        MOVE RW-AHPRMC TO RWF-AHPRM.                             
                                                                 
    IF RE-NSP-ST-CD-AH = SPACES OR ZERO                          
        IF (WT-REMAINING = 'Y' OR 'Z') AND (WT-AH-QC = '1')      
           AND (RWF-AHAMT = RS-R-AB) AND (RWF-AHAMT NOT = +0)    
               MOVE 'Y' TO RWF-REM-AH-100.                       
                                                                 
    IF WT-REMAINING = 'Y' OR 'Z' OR 'X' OR 'N'                   
        COMPUTE RS-R-AB = RS-R-AB - RWF-AHAMT                    
        COMPUTE RS-R-AP = RS-R-AP - RW-AHPRM                     
        COMPUTE RS-R-APC = RS-R-APC - RW-AHPRMC.                 
                                                                 
    GO TO REIN-CALC-AH-X.                                        
                                                                 
REIN-CALC-AH-RISK-PREM.                                          
                                                                 
    IF RWF-AHAMT NOT GREATER THAN +0                             
        MOVE ZEROS                 TO RWF-AHPRM                  
        GO TO REIN-CALC-AH-X.                                    
                                                                 
    COMPUTE REIN-RISK-AMOUNT = RWF-AHAMT * CR-AH-TERM.           
    MOVE RS-AH-BEN              TO REIN-MONTH-REDUCE.            
    MOVE +0                     TO REIN-WORKA                    
                                   RW-AHPRM.                     
                                                                 
REIN-AH-RISK-PREM-LOOP.                                          
                                                                 
    COMPUTE RW-AHPRM = RW-AHPRM +                                
               ((REIN-RISK-AMOUNT / +1000) * CR-AH-NSP-PRM-RATE).
                                                                 
    SUBTRACT REIN-MONTH-REDUCE FROM REIN-RISK-AMOUNT.            
                                                                 
    IF REIN-WORKA = REIN-EARN-AH-TERM  OR                        
       REIN-RISK-AMOUNT LESS THAN +0                             
        MOVE RW-AHPRM          TO RWF-AHPRM                      
    ELSE                                                         
        ADD +1 TO REIN-WORKA                                     
        GO TO REIN-AH-RISK-PREM-LOOP.                            
                                                                 
                                                                 
REIN-CALC-AH-X.                                                  
    EXIT.                                                        
                                                                 
REINSURE-CALC-REMAINDER.                                         
                                                                 
    COMPUTE RWF-LFAMT = CR-LFAMT + CR-LFAMT-ALT.                 
    COMPUTE RWF-LFPRM = CR-LFPRM + CR-LFPRM-ALT.                 
    MOVE CR-AHAMT TO RWF-AHAMT.                                  
    MOVE CR-AHPRM TO RWF-AHPRM.                                  
                                                                 
    PERFORM REINSURE-SUBTRACT THRU REINSURE-SUBTRACT-X           
        VARYING SUB2 FROM +1 BY +1 UNTIL SUB2 = SUB1.            
                                                                 
    IF RWF-LFAMT NOT = ZEROS OR                                  
       RWF-LFPRM NOT = ZEROS                                     
          MOVE 'X' TO REIN-LF-FLG (SUB1).                        
                                                                 
    IF RWF-AHAMT NOT = ZEROS OR                                  
       RWF-AHPRM NOT = ZEROS                                     
          MOVE 'X' TO REIN-AH-FLG (SUB1).                        
                                                                 
REINSURE-CALC-X.                                                 
                                                                 
    IF REIN-LF-AH-FLGS (SUB1) NOT = SPACES                       
        MOVE 'X' TO REIN-BUS-FLAG.                               
                                                                 
    MOVE RWF-FIELDS TO REIN-WORK-FLDS (SUB1).                    
                                                                 
REINSURE-SUBTRACT.                                               
                                                                 
    SUBTRACT REIN-LFAMT (SUB2) FROM RWF-LFAMT.                   
    SUBTRACT REIN-LFPRM (SUB2) FROM RWF-LFPRM.                   
    SUBTRACT REIN-AHAMT (SUB2) FROM RWF-AHAMT.                   
    SUBTRACT REIN-AHPRM (SUB2) FROM RWF-AHPRM.                   
                                                                 
REINSURE-SUBTRACT-X.                                             
    EXIT.                                                        
                                                                 
REINSURE-CALC-CANCEL.                                            
                                                                 
    MOVE REIN-WORK-FLDS (SUB1) TO RWF-FIELDS.                    
    MOVE RE-COMP-INFO (SUB1) TO WT-COMP-INFO.                    
                                                                 
    IF SUB1 = RE-100-COMP                                        
        GO TO REINSURE-REMAINDER-CANCEL.                         
                                                                 
    IF REIN-LF-FLG (SUB1) NOT = 'X'                              
        GO TO REINSURE-CALC-CANCEL-AH.                           
                                                                 
    IF RWF-LF-RUNOFF-SW = 'N'                                    
        IF CR-LF-CANC-DT NOT LESS THAN RWF-EP-STOP-DATE          
            GO TO REINSURE-CALC-CANCEL-AH.                       
                                                                 
    IF WT-REMAINING = 'I'                                        
        GO TO REINSURE-CALC-LF-RISK-CANCEL.                      
                                                                 
    IF WT-REMAINING = 'L'                                        
        COMPUTE RWF-LFRFND ROUNDED = CR-LFRFND * WT-LF-PCT       
        GO TO REINSURE-CALC-CANCEL-AH.                           
                                                                 
    IF CR-LFPRM = ZERO                                           
       MOVE ZERO                TO REIN-FACTOR                   
    ELSE                                                         
       IF DTE-CLIENT = 'GIC'  AND                                
          CLAS-I-BAL (CLAS-INDEXL) = 'B'                         
          MOVE +1.0             TO REIN-FACTOR                   
       ELSE                                                      
          IF DTE-CLIENT = 'DCC' OR 'VPP'                         
             COMPUTE REIN-FACTOR = CR-LFRFND /                   
                (CR-LFPRM + CR-LFPRM-ALT)                        
          ELSE                                                   
             COMPUTE REIN-FACTOR ROUNDED =  CR-LFRFND-CALC /     
                (CR-LFPRM + CR-LFPRM-ALT)                        
           END-IF                                                
        END-IF                                                   
    END-IF                                                       
                                                                 
    IF CR-LFAMT NOT = ZERO                                       
        IF DTE-CLIENT = 'GTL'  OR  'UAL'  OR  'NCL'  OR  'CVL'   
            IF WT-LF-QC = ('1' OR '4')                           
                COMPUTE RWF-LFRFND ROUNDED =  ((CR-LFRFND *      
                        RWF-LFAMT) / (CR-LFAMT + CR-LFAMT-ALT))  
            ELSE                                                 
                COMPUTE RWF-LFRFND =                             
                           REIN-FACTOR * REIN-CALCED-LIFE        
                COMPUTE RWF-LFRFND ROUNDED =  ((RWF-LFRFND *     
                        RWF-LFAMT) / (CR-LFAMT + CR-LFAMT-ALT))  
        ELSE                                                     
            IF WT-LF-QC = ('1' OR '4')                           
                compute rein-factor rounded =                    
                   RWF-LFAMT / (CR-LFAMT + CR-LFAMT-ALT)         
                COMPUTE RWF-LFRFND ROUNDED =  CR-LFRFND *        
                   rein-factor                                   
            ELSE                                                 
*>                COMPUTE RWF-LFRFND = REIN-FACTOR                 
*>                   * REIN-CALCED-LIFE *                          
*>                   (RWF-LFAMT / (CR-LFAMT + CR-LFAMT-ALT)).      
                COMPUTE RWF-LFRFND =                             
                           REIN-FACTOR * REIN-CALCED-LIFE        
                COMPUTE RWF-LFRFND ROUNDED =  (RWF-LFRFND *      
                       RWF-LFAMT) / (CR-LFAMT + CR-LFAMT-ALT).   
                                                                 
    IF CR-LFAMT = ZERO                                           
        IF WT-LF-LIM-LO = ZERO                                   
            IF WT-LF-QC = ('1' OR '4')                           
                COMPUTE RWF-LFRFND ROUNDED =                     
                      CR-LFRFND * WT-LF-PCT                      
            ELSE                                                 
                COMPUTE RWF-LFRFND =                             
                           REIN-FACTOR * REIN-CALCED-LIFE        
                COMPUTE RWF-LFRFND ROUNDED =                     
                      RWF-LFRFND * WT-LF-PCT.                    
                                                                 
    GO TO REINSURE-CALC-CANCEL-AH.                               
                                                                 
                                                                 
REINSURE-CALC-LF-RISK-CANCEL.                                    
                                                                 
    IF RWF-LFAMT NOT GREATER THAN +0  OR                         
      (CR-LF-CANC-DT = ZEROS)                                    
        MOVE ZERO                    TO RWF-LFRFND               
        GO TO REINSURE-CALC-CANCEL-AH.                           
                                                                 
    IF REIN-RT-SW NOT = 'X'                                      
      IF VOIDING-LF-CANCEL                                       
          COMPUTE RWF-LFRFND = RWF-LFRFND * (-1)                 
          GO TO REINSURE-CALC-CANCEL-AH                          
      ELSE                                                       
          IF ADDITIONAL-LF-CANCEL                                
              GO TO REINSURE-CALC-CANCEL-AH.                     
                                                                 
    IF FLAT-CANCEL-LF                                            
        MOVE RWF-LFPRM               TO RWF-LFRFND               
        GO TO REINSURE-CALC-CANCEL-AH.                           
                                                                 
    IF REIN-EARN-LF-TERM = REIN-LCNC-EARN-TERM                   
        MOVE ZERO                    TO RWF-LFRFND               
        GO TO REINSURE-CALC-CANCEL-AH.                           
                                                                 
    MOVE RWF-LFAMT                   TO REIN-RISK-AMOUNT.        
    COMPUTE REIN-MONTH-REDUCE = RS-LIFE-BEN / CR-LF-TERM.        
    MOVE +0                          TO REIN-WORKA               
                                        RW-LFRFND.               
                                                                 
REINSURE-LF-RISK-CANCEL-LOOP.                                    
                                                                 
    COMPUTE RW-LFRFND = RW-LFRFND +                              
               ((REIN-RISK-AMOUNT / +1000) * CR-LF-NSP-PRM-RATE).
                                                                 
    IF CLAS-I-RL-AH (CLAS-INDEXL) NOT = 'L'                      
       SUBTRACT REIN-MONTH-REDUCE FROM REIN-RISK-AMOUNT.         
                                                                 
    IF REIN-WORKA = REIN-LCNC-EARN-TERM  OR                      
       REIN-RISK-AMOUNT LESS THAN +0                             
        COMPUTE RWF-LFRFND = RWF-LFPRM - RW-LFRFND               
    ELSE                                                         
        ADD +1 TO REIN-WORKA                                     
        GO TO REINSURE-LF-RISK-CANCEL-LOOP.                      
                                                                 
                                                                 
REINSURE-CALC-CANCEL-AH.                                         
                                                                 
    IF REIN-AH-FLG (SUB1) NOT = 'X'                              
        GO TO REINSURE-CALC-CANCEL-SKIP.                         
                                                                 
    IF RWF-AH-RUNOFF-SW = 'N'                                    
        IF CR-AH-CANC-DT NOT LESS THAN RWF-EP-STOP-DATE          
            GO TO REINSURE-CALC-CANCEL-SKIP.                     
                                                                 
    IF RWF-REM-AH-100 = 'Y'                                      
        MOVE CR-AHRFND TO RWF-AHRFND                             
        PERFORM REINSURE-SUB-REM-CANCEL THRU                     
            REINSURE-SUB-REM-CANCEL-X                            
                VARYING SUB2 FROM +1 BY +1 UNTIL SUB2 = SUB1     
        GO TO REINSURE-CALC-CANCEL-SKIP.                         
                                                                 
    IF WT-REMAINING = 'I'                                        
        GO TO REINSURE-CALC-AH-RISK-CANCEL.                      
                                                                 
    IF WT-REMAINING = 'L'                                        
        COMPUTE RWF-AHRFND ROUNDED = CR-AHRFND * WT-AH-PCT       
        GO TO REINSURE-CALC-CANCEL-SKIP.                         
                                                                 
    IF CR-AHPRM = ZERO                                           
       MOVE ZERO                TO REIN-FACTOR                   
    ELSE                                                         
       IF DTE-CLIENT = 'GIC'  AND                                
          CLAS-I-BAL (CLAS-INDEXA) = 'B'                         
           MOVE +1.0            TO REIN-FACTOR                   
       ELSE                                                      
          IF DTE-CLIENT = 'DCC' OR 'VPP'                         
             COMPUTE REIN-FACTOR =                               
                CR-AHRFND / CR-AHPRM                             
          ELSE                                                   
             COMPUTE REIN-FACTOR ROUNDED =                       
                               CR-AHRFND-CALC / CR-AHPRM         
          END-IF                                                 
       END-IF                                                    
    END-IF                                                       
                                                                 
    IF CR-AHAMT NOT = ZERO                                       
       IF (DTE-CLIENT = 'DCC' OR 'VPP')                          
          AND (CLAS-I-REFUND-METHOD (CLAS-INDEXA) = 'D')         
          MOVE CR-AH-RFND-CLP   TO RWF-AHRFND                    
       ELSE                                                      
          IF WT-AH-QC = ('1' OR '4')                             
             compute rein-factor rounded = rwf-ahamt / cr-ahamt  
             compute rwf-ahrfnd rounded =                        
                cr-ahrfnd * rein-factor                          
          ELSE                                                   
             COMPUTE RWF-AHRFND = REIN-FACTOR * REIN-CALCED-AH   
             COMPUTE RWF-AHRFND ROUNDED = (RWF-AHRFND *          
                      RWF-AHAMT) / CR-AHAMT                      
          END-IF                                                 
       END-IF                                                    
    ELSE                                                         
       IF RWF-AH-LIMIT = ZERO                                    
          IF WT-AH-QC = ('1' OR '4')                             
             COMPUTE RWF-AHRFND ROUNDED =                        
                      CR-AHRFND * WT-AH-PCT                      
          ELSE                                                   
             COMPUTE RWF-AHRFND =                                
                           REIN-FACTOR * REIN-CALCED-AH          
             COMPUTE RWF-AHRFND ROUNDED =                        
                      RWF-AHRFND * WT-AH-PCT                     
          END-IF                                                 
       END-IF                                                    
    END-IF                                                       
                                                                 
    GO TO REINSURE-CALC-CANCEL-SKIP.                             
                                                                 
                                                                 
REINSURE-CALC-AH-RISK-CANCEL.                                    
                                                                 
    IF RWF-AHAMT NOT GREATER THAN +0  OR                         
      (CR-AH-CANC-DT = ZEROS)                                    
        MOVE ZERO                    TO RWF-AHRFND               
        GO TO REINSURE-CALC-CANCEL-SKIP.                         
                                                                 
    IF REIN-RT-SW NOT = 'X'                                      
      IF VOIDING-AH-CANCEL                                       
          COMPUTE RWF-AHRFND = RWF-AHRFND * (-1)                 
          GO TO REINSURE-CALC-CANCEL-SKIP                        
      ELSE                                                       
          IF ADDITIONAL-AH-CANCEL                                
              GO TO REINSURE-CALC-CANCEL-SKIP.                   
                                                                 
    IF FLAT-CANCEL-AH                                            
        MOVE RWF-AHPRM               TO RWF-AHRFND               
        GO TO REINSURE-CALC-CANCEL-SKIP.                         
                                                                 
    IF REIN-EARN-AH-TERM = REIN-ACNC-EARN-TERM                   
        MOVE ZERO                    TO RWF-AHRFND               
        GO TO REINSURE-CALC-CANCEL-SKIP.                         
                                                                 
    MOVE RS-AH-BEN              TO REIN-MONTH-REDUCE.            
    COMPUTE REIN-RISK-AMOUNT = RWF-AHAMT * CR-AH-TERM.           
    MOVE +0                     TO REIN-WORKA                    
                                   RW-AHRFND.                    
                                                                 
REINSURE-AH-RISK-CANCEL-LOOP.                                    
                                                                 
    COMPUTE RW-AHRFND = RW-AHRFND +                              
               ((REIN-RISK-AMOUNT / +1000) * CR-AH-NSP-PRM-RATE).
                                                                 
    SUBTRACT REIN-MONTH-REDUCE FROM REIN-RISK-AMOUNT.            
                                                                 
    IF REIN-WORKA = REIN-ACNC-EARN-TERM  OR                      
       REIN-RISK-AMOUNT LESS THAN +0                             
        COMPUTE RWF-AHRFND = RWF-AHPRM - RW-AHRFND               
    ELSE                                                         
        ADD +1 TO REIN-WORKA                                     
        GO TO REINSURE-AH-RISK-CANCEL-LOOP.                      
                                                                 
                                                                 
REINSURE-CALC-CANCEL-SKIP.                                       
                                                                 
    IF RWF-LFRFND NOT = ZEROS                                    
        MOVE 'X' TO REIN-LF-FLG (SUB1).                          
                                                                 
    IF RWF-AHRFND NOT = ZEROS                                    
        MOVE 'X' TO REIN-AH-FLG (SUB1).                          
                                                                 
    GO TO REINSURE-CALC-CANCEL-X.                                
                                                                 
REINSURE-REMAINDER-CANCEL.                                       
                                                                 
    MOVE CR-LFRFND TO RWF-LFRFND.                                
    MOVE CR-AHRFND TO RWF-AHRFND.                                
                                                                 
    PERFORM REINSURE-SUBTRACT-CANCEL THRU                        
        REINSURE-SUBTRACT-CANCEL-X                               
            VARYING SUB2 FROM +1 BY +1 UNTIL SUB2 = SUB1.        
                                                                 
    IF RWF-LFRFND NOT = ZEROS                                    
        MOVE 'X' TO REIN-LF-FLG (SUB1).                          
                                                                 
    IF RWF-AHRFND NOT = ZEROS                                    
        MOVE 'X' TO REIN-AH-FLG (SUB1).                          
                                                                 
REINSURE-CALC-CANCEL-X.                                          
                                                                 
    IF REIN-LF-AH-FLGS (SUB1) NOT = SPACES                       
        MOVE 'X' TO REIN-BUS-FLAG.                               
                                                                 
    MOVE RWF-FIELDS TO REIN-WORK-FLDS (SUB1).                    
                                                                 
REINSURE-SUBTRACT-CANCEL.                                        
                                                                 
    SUBTRACT REIN-LFRFND (SUB2) FROM RWF-LFRFND.                 
    SUBTRACT REIN-AHRFND (SUB2) FROM RWF-AHRFND.                 
                                                                 
REINSURE-SUBTRACT-CANCEL-X.                                      
    EXIT.                                                        
                                                                 
REINSURE-SUB-REM-CANCEL.                                         
                                                                 
    IF REIN-REM-SW (SUB2) = 'Y' OR 'Z' OR 'X' OR 'N'             
        SUBTRACT REIN-AHRFND (SUB2) FROM RWF-AHRFND.             
                                                                 
REINSURE-SUB-REM-CANCEL-X.                                       
    EXIT.                                                        
                                                                 
REINSURE-CALC-CLAIM.                                             
                                                                 
    MOVE REIN-WORK-FLDS (SUB1) TO RWF-FIELDS.                    
    MOVE RE-COMP-INFO (SUB1) TO WT-COMP-INFO.                    
                                                                 
    IF SUB1 = RE-100-COMP                                        
        GO TO REINSURE-REMAINDER-CLAIM.                          
                                                                 
    PERFORM REIN-FIND-CO-LOOP THRU FIND-REI-CO-EXIT              
       VARYING CO-SUB FROM 1 BY 1 UNTIL                          
         (RCT-REIN-CO (CO-SUB) = REIN-COMP (SUB1))  OR           
         (CO-SUB GREATER REIN-CO-TABLE-ENT-CNT)     OR           
         (RCT-REIN-CO (CO-SUB) = SPACES).                        
                                                                 
    IF (CO-SUB GREATER REIN-CO-TABLE-ENT-CNT)  OR                
       (RCT-REIN-CO (CO-SUB) = SPACES)                           
         MOVE ZERO TO CO-SUB.                                    
                                                                 
    MOVE ZEROS                TO RWF-LFCLML                      
                                 RWF-AHCLML.                     
                                                                 
    IF REIN-LF-FLG (SUB1) NOT = 'X'                              
        GO TO REINSURE-CALC-CLAIM-AH.                            
                                                                 
    IF RW-LFCLMWK = ZEROS                                        
        GO TO REINSURE-CALC-CLAIM-AH.                            
                                                                 
    IF CO-SUB NOT = ZERO                                         
        IF RCT-CLM-CUTOFF-DT (CO-SUB) NOT = ZERO                 
            IF CR-DTH-DT LESS RCT-CLM-CUTOFF-DT (CO-SUB)         
                GO TO REINSURE-CALC-CLAIM-AH.                    
                                                                 
    IF RWF-LF-RUNOFF-SW = 'N'                                    
        IF CR-DTH-DT NOT LESS THAN RWF-EP-STOP-DATE              
                GO TO REINSURE-CALC-CLAIM-AH.                    
                                                                 
    IF WT-REMAINING = 'I'                                        
        GO TO REINSURE-CALC-LF-RISK-CLAIM.                       
                                                                 
    IF WT-REMAINING = 'L'                                        
        IF REIN-RT-SW = 'X'                                      
            IF RW-LFCLMWK GREATER WT-LF-HI                       
                MOVE WT-LF-HI TO RWF-LFCLM                       
            ELSE                                                 
                MOVE RW-LFCLMWK TO RWF-LFCLM                     
        ELSE                                                     
            IF (RW-LFCLMWK + RWF-LFCLM) GREATER WT-LF-HI         
                COMPUTE RWF-LFCLML = WT-LF-HI - RWF-LFCLM        
                MOVE WT-LF-HI TO RWF-LFCLM                       
            ELSE                                                 
                MOVE RW-LFCLMWK TO RWF-LFCLML                    
                ADD RWF-LFCLML TO RWF-LFCLM.                     
                                                                 
    IF WT-REMAINING = 'L'                                        
        GO TO REINSURE-CALC-CLAIM-AH.                            
                                                                 
    IF CR-LFAMT NOT = ZERO                                       
        IF DTE-CLIENT = 'GTL'  OR  'UAL'  OR  'NCL'  OR  'CVL'   
           OR 'DCC' OR 'VPP'                                     
            COMPUTE RWF-LFCLML ROUNDED =  ((RW-LFCLMWK *         
                        RWF-LFAMT) / (CR-LFAMT + CR-LFAMT-ALT))  
        ELSE                                                     
            COMPUTE RWF-LFCLML ROUNDED =  RW-LFCLMWK *           
                       (RWF-LFAMT / (CR-LFAMT + CR-LFAMT-ALT)).  
                                                                 
    IF CR-LFAMT = ZERO                                           
        IF WT-LF-LIM-LO = ZERO                                   
            COMPUTE RWF-LFCLML ROUNDED =                         
                      RW-LFCLMWK * WT-LF-PCT.                    
                                                                 
    IF CO-SUB NOT = ZERO                                         
        IF RCT-LF-CLM-PCT (CO-SUB) NOT = ZERO                    
            COMPUTE RWF-LFCLML ROUNDED =                         
                         RW-LFCLMWK * RCT-LF-CLM-PCT (CO-SUB).   
                                                                 
    IF CO-SUB NOT = ZERO                                         
        IF RCT-LF-CLM-MAX (CO-SUB) NOT = ZERO                    
            IF REIN-RT-SW = 'X'                                  
              IF RWF-LFCLML GREATER RCT-LF-CLM-MAX (CO-SUB)      
                    MOVE RCT-LF-CLM-MAX (CO-SUB) TO RWF-LFCLM    
                     GO TO REINSURE-CALC-CLAIM-AH                
                 ELSE                                            
                    MOVE RWF-LFCLML TO RWF-LFCLM                 
                     GO TO REINSURE-CALC-CLAIM-AH                
             ELSE                                                
                IF (RWF-LFCLML + RWF-LFCLM) GREATER              
                                          RCT-LF-CLM-MAX (CO-SUB)
                    COMPUTE RWF-LFCLML =                         
                              RCT-LF-CLM-MAX (CO-SUB) - RWF-LFCLM
                    MOVE RCT-LF-CLM-MAX (CO-SUB) TO RWF-LFCLM    
                     GO TO REINSURE-CALC-CLAIM-AH.               
                                                                 
    ADD RWF-LFCLML TO RWF-LFCLM.                                 
                                                                 
    GO TO REINSURE-CALC-CLAIM-AH.                                
                                                                 
                                                                 
REINSURE-CALC-LF-RISK-CLAIM.                                     
                                                                 
    IF RWF-LFAMT NOT GREATER THAN +0                             
        GO TO REINSURE-CALC-CLAIM-AH.                            
                                                                 
    IF CLAS-I-RL-AH (CLAS-INDEXL) = 'L'                          
        MOVE RWF-LFAMT                   TO REIN-RISK-AMOUNT     
    ELSE                                                         
        COMPUTE REIN-RISK-AMOUNT = RWF-LFAMT -                   
               ((RS-LIFE-BEN / CR-LF-TERM) * REIN-LF-CLM-MONTHS).
                                                                 
    IF REIN-RISK-AMOUNT NOT GREATER THAN +0                      
        GO TO REINSURE-CALC-CLAIM-AH.                            
                                                                 
    IF REIN-RT-SW = 'X'                                          
        IF RW-LFCLMWK GREATER THAN REIN-RISK-AMOUNT              
            MOVE REIN-RISK-AMOUNT        TO RWF-LFCLM            
        ELSE                                                     
            MOVE RW-LFCLMWK              TO RWF-LFCLM            
    ELSE                                                         
        IF (RW-LFCLMWK + RWF-LFCLM) GREATER THAN REIN-RISK-AMOUNT
            COMPUTE RWF-LFCLML = REIN-RISK-AMOUNT - RWF-LFCLM    
            MOVE REIN-RISK-AMOUNT        TO RWF-LFCLM            
        ELSE                                                     
            MOVE RW-LFCLMWK              TO RWF-LFCLML           
            ADD RW-LFCLMWK TO RWF-LFCLM.                         
                                                                 
                                                                 
REINSURE-CALC-CLAIM-AH.                                          
                                                                 
    IF REIN-AH-FLG (SUB1) NOT = 'X'                              
        GO TO REINSURE-CALC-CLAIM-X.                             
                                                                 
    IF RW-AHCLMWK = ZEROS                                        
        GO TO REINSURE-CALC-CLAIM-SKIP.                          
                                                                 
    IF REIN-RT-SW NOT = 'X'                                      
        GO TO REINSURE-CALC-AH-DETAIL.                           
                                                                 
    MOVE CR-DISAB-INCURRED-DETAIL    TO RW-AH-INCURRED-DETAIL.   
    MOVE ZEROS                       TO RW-AHCLMWK.              
                                                                 
    PERFORM CHECK-MULTIPLE-DISABILITIES THRU CHECK-DISABS-EXIT   
            VARYING                                              
        INC-SUB FROM +1 BY +1                                    
            UNTIL                                                
        INC-SUB GREATER THAN +5.                                 
                                                                 
    IF RW-AHCLMWK = ZEROS                                        
        GO TO REINSURE-CALC-CLAIM-SKIP                           
    ELSE                                                         
        GO TO REINSURE-CALC-AH-CONTINUED.                        
                                                                 
CHECK-MULTIPLE-DISABILITIES.                                     
                                                                 
    IF RW-AH-INCUR-DT (INC-SUB) = ZEROS                          
        GO TO CHECK-DISABS-EXIT.                                 
                                                                 
    IF CO-SUB NOT = ZERO                                         
      IF RCT-CLM-CUTOFF-DT (CO-SUB) NOT = ZERO                   
          IF RW-AH-INCUR-DT (INC-SUB) LESS THAN                  
                      RCT-CLM-CUTOFF-DT (CO-SUB)                 
              MOVE ZEROS             TO RW-AH-CLM-AMT (INC-SUB)  
                                        RW-AH-CLM-EXP (INC-SUB)  
              GO TO CHECK-DISABS-EXIT.                           
                                                                 
    IF RWF-AH-RUNOFF-SW = 'N'                                    
      IF RW-AH-INCUR-DT (INC-SUB) NOT LESS THAN RWF-EP-STOP-DATE 
          MOVE ZEROS                 TO RW-AH-CLM-AMT (INC-SUB)  
                                        RW-AH-CLM-EXP (INC-SUB)  
          GO TO CHECK-DISABS-EXIT.                               
                                                                 
    IF WT-REMAINING NOT = 'I'                                    
        COMPUTE RW-AHCLMWK = RW-AHCLMWK                          
                            + RW-AH-CLM-AMT (INC-SUB)            
                            + RW-AH-CLM-EXP (INC-SUB)            
        GO TO CHECK-DISABS-EXIT.                                 
                                                                 
    IF RWF-AHAMT NOT GREATER THAN +0                             
        GO TO CHECK-DISABS-EXIT.                                 
                                                                 
    IF INC-SUB = +1                                              
        MOVE REIN-AH-CLM-MTH-1       TO REIN-AH-CLM-MONTHS.      
    IF INC-SUB = +2                                              
        MOVE REIN-AH-CLM-MTH-2       TO REIN-AH-CLM-MONTHS.      
    IF INC-SUB = +3                                              
        MOVE REIN-AH-CLM-MTH-3       TO REIN-AH-CLM-MONTHS.      
    IF INC-SUB = +4                                              
        MOVE REIN-AH-CLM-MTH-4       TO REIN-AH-CLM-MONTHS.      
    IF INC-SUB = +5                                              
        MOVE REIN-AH-CLM-MTH-5       TO REIN-AH-CLM-MONTHS.      
                                                                 
    COMPUTE REIN-RISK-AMOUNT = (RWF-AHAMT * CR-AH-TERM) -        
                               (RS-AH-BEN * REIN-AH-CLM-MONTHS). 
                                                                 
    IF REIN-RISK-AMOUNT NOT GREATER THAN +0                      
        GO TO CHECK-DISABS-EXIT.                                 
                                                                 
    IF (RW-AH-CLM-AMT (INC-SUB) + RW-AH-CLM-EXP (INC-SUB))       
             GREATER THAN REIN-RISK-AMOUNT                       
        COMPUTE RW-AHCLMWK = RW-AHCLMWK + REIN-RISK-AMOUNT       
    ELSE                                                         
        COMPUTE RW-AHCLMWK = RW-AHCLMWK                          
                            + RW-AH-CLM-AMT (INC-SUB)            
                            + RW-AH-CLM-EXP (INC-SUB).           
                                                                 
CHECK-DISABS-EXIT.                                               
     EXIT.                                                       
                                                                 
REINSURE-CALC-AH-DETAIL.                                         
                                                                 
    IF RW-AHCLMWK = ZEROS                                        
        GO TO REINSURE-CALC-CLAIM-SKIP.                          
                                                                 
    IF CO-SUB NOT = ZERO                                         
        IF RCT-CLM-CUTOFF-DT (CO-SUB) NOT = ZERO                 
            IF CR-DIS-DT LESS RCT-CLM-CUTOFF-DT (CO-SUB)         
                GO TO REINSURE-CALC-CLAIM-SKIP.                  
                                                                 
    IF RWF-AH-RUNOFF-SW = 'N'                                    
        IF CR-DIS-DT NOT LESS THAN RWF-EP-STOP-DATE              
            GO TO REINSURE-CALC-CLAIM-SKIP.                      
                                                                 
REINSURE-CALC-AH-CONTINUED.                                      
                                                                 
    IF RWF-REM-AH-100 = 'Y'                                      
        MOVE RW-AHCLMWK TO RWF-AHCLML                            
        PERFORM REINSURE-SUB-REM-CLAIM THRU                      
            REINSURE-SUB-REM-CLAIM-X                             
                VARYING SUB2 FROM +1 BY +1 UNTIL SUB2 = SUB1     
        ADD RWF-AHCLML TO RWF-AHCLM                              
        GO TO REINSURE-CALC-CLAIM-SKIP.                          
                                                                 
    IF WT-REMAINING = 'I'                                        
        GO TO REINSURE-CALC-AH-RISK-CLAIM.                       
                                                                 
    IF WT-REMAINING = 'L'                                        
        IF REIN-RT-SW = 'X'                                      
            IF RW-AHCLMWK GREATER WT-AHBEN-HI                    
                MOVE WT-AHBEN-HI TO RWF-AHCLM                    
            ELSE                                                 
                MOVE RW-AHCLMWK TO RWF-AHCLM                     
        ELSE                                                     
            IF (RW-AHCLMWK + RWF-AHCLM) GREATER WT-AHBEN-HI      
                COMPUTE RWF-AHCLML = WT-AHBEN-HI - RWF-AHCLM     
                MOVE WT-AHBEN-HI TO RWF-AHCLM                    
            ELSE                                                 
                MOVE RW-AHCLMWK TO RWF-AHCLML                    
                ADD RWF-AHCLML TO RWF-AHCLM.                     
                                                                 
    IF WT-REMAINING = 'L'                                        
        GO TO REINSURE-CALC-CLAIM-SKIP.                          
                                                                 
    IF CR-AHAMT NOT = ZERO                                       
        COMPUTE RWF-AHCLML ROUNDED = (RW-AHCLMWK *               
                           RWF-AHAMT) / CR-AHAMT                 
    ELSE                                                         
        IF RWF-AH-LIMIT = ZERO                                   
            COMPUTE RWF-AHCLML ROUNDED =                         
                      RW-AHCLMWK * WT-AH-PCT.                    
                                                                 
    IF CO-SUB NOT = ZERO                                         
        IF RCT-AH-CLM-PCT (CO-SUB) NOT = ZERO                    
            COMPUTE RWF-AHCLML ROUNDED =                         
                          RW-AHCLMWK * RCT-AH-CLM-PCT (CO-SUB).  
                                                                 
    IF CO-SUB NOT = ZERO                                         
        IF RCT-AH-CLM-MAX (CO-SUB) NOT = ZERO                    
            IF REIN-RT-SW = 'X'                                  
              IF RWF-AHCLML GREATER RCT-AH-CLM-MAX (CO-SUB)      
                    MOVE RCT-AH-CLM-MAX (CO-SUB) TO RWF-AHCLM    
                     GO TO REINSURE-CALC-CLAIM-SKIP              
                 ELSE                                            
                    MOVE RWF-AHCLML TO RWF-AHCLM                 
                     GO TO REINSURE-CALC-CLAIM-SKIP              
             ELSE                                                
                IF (RWF-AHCLML + RWF-AHCLM) GREATER              
                                          RCT-AH-CLM-MAX (CO-SUB)
                    COMPUTE RWF-AHCLML =                         
                              RCT-AH-CLM-MAX (CO-SUB) - RWF-AHCLM
                    MOVE RCT-AH-CLM-MAX (CO-SUB) TO RWF-AHCLM    
                     GO TO REINSURE-CALC-CLAIM-SKIP.             
                                                                 
                                                                 
    IF DTE-CLIENT = 'FLA'                                        
       IF REIN-COMP (SUB1) = ('200000' OR '210000' OR            
                              '220000' OR '230000')              
          PERFORM FLA-SPEC-REIN-CLMS THRU                        
              E-FLA-SPEC-REIN-CLMS.                              
                                                                 
    ADD RWF-AHCLML TO RWF-AHCLM.                                 
                                                                 
    GO TO REINSURE-CALC-CLAIM-SKIP.                              
                                                                 
                                                                 
REINSURE-CALC-AH-RISK-CLAIM.                                     
                                                                 
    IF RWF-AHAMT NOT GREATER THAN +0                             
        GO TO REINSURE-CALC-CLAIM-SKIP.                          
                                                                 
    IF REIN-RT-SW = 'X'                                          
        MOVE RW-AHCLMWK              TO RWF-AHCLM                
        GO TO REINSURE-CALC-CLAIM-SKIP.                          
                                                                 
    COMPUTE REIN-RISK-AMOUNT = (RWF-AHAMT * CR-AH-TERM) -        
                               (RS-AH-BEN * REIN-AH-CLM-MONTHS). 
                                                                 
    IF REIN-RISK-AMOUNT NOT GREATER THAN +0                      
        GO TO REINSURE-CALC-CLAIM-SKIP.                          
                                                                 
    IF REIN-AH-PRIOR-CLMS-PAID GREATER THAN REIN-RISK-AMOUNT     
        GO TO REINSURE-CALC-CLAIM-SKIP.                          
                                                                 
    IF (RW-AHCLMWK + REIN-AH-PRIOR-CLMS-PAID)                    
                               GREATER THAN REIN-RISK-AMOUNT     
        COMPUTE RWF-AHCLML = REIN-RISK-AMOUNT                    
                            - REIN-AH-PRIOR-CLMS-PAID            
        MOVE REIN-RISK-AMOUNT        TO RWF-AHCLM                
    ELSE                                                         
        MOVE RW-AHCLMWK              TO RWF-AHCLML               
        ADD RW-AHCLMWK TO RWF-AHCLM.                             
                                                                 
                                                                 
REINSURE-CALC-CLAIM-SKIP.                                        
                                                                 
    IF RWF-LFCLM NOT = ZEROS                                     
        MOVE 'X' TO REIN-LF-FLG (SUB1).                          
                                                                 
    IF RWF-AHCLM NOT = ZEROS                                     
        MOVE 'X' TO REIN-AH-FLG (SUB1).                          
                                                                 
    GO TO REINSURE-CALC-CLAIM-X.                                 
                                                                 
REINSURE-REMAINDER-CLAIM.                                        
                                                                 
    MOVE RW-LFCLMWK TO RWF-LFCLML.                               
    MOVE RW-AHCLMWK TO RWF-AHCLML.                               
                                                                 
    PERFORM REINSURE-SUBTRACT-CLAIM THRU                         
        REINSURE-SUBTRACT-CLAIM-X                                
            VARYING SUB2 FROM +1 BY +1 UNTIL SUB2 = SUB1.        
                                                                 
    ADD RWF-LFCLML TO RWF-LFCLM.                                 
    ADD RWF-AHCLML TO RWF-AHCLM.                                 
                                                                 
    IF RWF-LFCLM NOT = ZEROS                                     
        MOVE 'X' TO REIN-LF-FLG (SUB1).                          
                                                                 
    IF RWF-AHCLM NOT = ZEROS                                     
        MOVE 'X' TO REIN-AH-FLG (SUB1).                          
                                                                 
REINSURE-CALC-CLAIM-X.                                           
                                                                 
    IF REIN-LF-AH-FLGS (SUB1) NOT = SPACES                       
        MOVE 'X' TO REIN-BUS-FLAG.                               
                                                                 
    MOVE RWF-FIELDS TO REIN-WORK-FLDS (SUB1).                    
                                                                 
REINSURE-SUBTRACT-CLAIM.                                         
                                                                 
    SUBTRACT REIN-LFCLML (SUB2) FROM RWF-LFCLML.                 
    SUBTRACT REIN-AHCLML (SUB2) FROM RWF-AHCLML.                 
                                                                 
REINSURE-SUBTRACT-CLAIM-X.                                       
    EXIT.                                                        
                                                                 
REINSURE-SUB-REM-CLAIM.                                          
                                                                 
    IF REIN-REM-SW (SUB2) = 'Y' OR 'Z' OR 'X' OR 'N'             
        SUBTRACT REIN-AHCLML (SUB2) FROM RWF-AHCLML.             
                                                                 
REINSURE-SUB-REM-CLAIM-X.                                        
    EXIT.                                                        
                                                                 
                                                                 
CLEAR-REIN-HOLD.                                                 
                                                                 
    MOVE SPACES TO REIN-COMP (1).                                
    MOVE SPACES TO REIN-LF-AH-FLGS (1).                          
    MOVE SPACES TO REIN-REM-SW (1).                              
    MOVE SPACES TO REIN-REM-AH-100 (1).                          
                                                                 
    MOVE +0 TO REIN-LFAMT (1)  REIN-LFPRM (1)  REIN-AHAMT (1)    
        REIN-AHPRM (1)  REIN-LFRFND (1)  REIN-AHRFND (1)         
        REIN-LFCLM (1)  REIN-AHCLM (1)  REIN-LFCLML (1)          
        REIN-AHCLML (1)  REIN-DIS-IBNR (1)  REIN-DIS-PAYCUR (1)  
        REIN-DIS-FUTRSV (1)  REIN-DTH-IBNR (1)                   
        REIN-DTH-PAYCUR (1)  REIN-DTH-FUTRSV (1)                 
        REIN-DIS-IBNRL (1)  REIN-DIS-PAYCURL (1)                 
        REIN-DIS-FUTRSVL (1)  REIN-DTH-IBNRL (1)                 
        REIN-DTH-PAYCURL (1)  REIN-DTH-FUTRSVL (1)               
        REIN-AH-LIMIT (1).                                       
                                                                 
    PERFORM CLEAR-REIN-HOLD-ALL VARYING SUB1 FROM 2 BY 1         
        UNTIL REIN-COMP (SUB1) = SPACES.                         
                                                                 
    GO TO CLEAR-REIN-HOLD-X.                                     
                                                                 
CLEAR-REIN-HOLD-ALL.                                             
                                                                 
    MOVE REIN-LEVELS (1) TO REIN-LEVELS (SUB1).                  
                                                                 
CLEAR-REIN-HOLD-X.                                               
    EXIT.                                                        
                                                                 
FLA-SPEC-REIN-CLMS.                                              
                                                                 
    MOVE +250.00 TO FLA-CLM-REIN-BASE  ADJ-FLA-REIN-BASE.        
                                                                 
    IF REIN-COMP (SUB1) = '230000'                               
        MOVE +100.00 TO FLA-CLM-REIN-BASE  ADJ-FLA-REIN-BASE.    
                                                                 
    IF CR-AHAMT NOT = ZERO                                       
        COMPUTE ADJ-FLA-REIN-BASE ROUNDED =                      
            (RW-AHCLMWK / CR-AHAMT) * FLA-CLM-REIN-BASE.         
                                                                 
    IF REIN-RT-SW = 'X'                                          
        MOVE CR-DIS-CC TO CIRD-CC                                
        MOVE CR-DIS-YR TO CIRD-YR                                
        MOVE CR-DIS-MO TO CIRD-MO                                
    ELSE                                                         
        MOVE DTO-CC    TO CIRD-CC                                
        MOVE DTO-YR    TO CIRD-YR                                
        MOVE DTO-MO    TO CIRD-MO.                               
                                                                 
    MOVE ZEROS TO RWF-AHCLML.                                    
                                                                 
    IF CLM-INCUR-REIN-DATE LESS '197809'  AND                    
       CR-AHAMT GREATER FLA-CLM-REIN-BASE                        
        COMPUTE RWF-AHCLML = RW-AHCLMWK - ADJ-FLA-REIN-BASE.     
                                                                 
    IF CLM-INCUR-REIN-DATE GREATER '197806'                      
        COMPUTE RWF-AHCLML = RWF-AHCLML + (RW-AHCLMWK * +.9).    
                                                                 
E-FLA-SPEC-REIN-CLMS.                                            
    EXIT.                                                        
                                                                 
REIN-FIND-CO-LOOP.                                               
                                                                 
    CONTINUE.                                                    
*>  DUMMY PARAGRAPH - NO CODE NEEDED.                              
FIND-REI-CO-EXIT.                                                
    EXIT.                                                        
                                                                 
REIN-CO-TABLE-BUILD.                                             
                                                                 
    PERFORM ZERO-RCT-ENTRIES THRU ZERO-RCT-EXIT                  
        VARYING CO-SUB FROM 1 BY 1                               
                  UNTIL CO-SUB GREATER REIN-CO-TABLE-ENT-CNT.    
                                                                 
    PERFORM ZERO-RC2-ENTRIES THRU ZERO-RC2-EXIT                  
        VARYING CO-SUB FROM 1 BY 1                               
                  UNTIL CO-SUB GREATER REIN-CO-TABLE-ENT-CNT.    
                                                                 
    MOVE LOW-VALUES          TO RE-CONTROL-PRIMARY.              
                                                                 
    START ERRTBL-IN                                              
          KEY IS GREATER THAN RE-CONTROL-PRIMARY                 
            INVALID KEY                                          
              GO TO REIN-BUILD-EXIT.                             
                                                                 
    MOVE 0 TO CO-SUB.                                            
    MOVE 0 TO SUB1.                                              
REIN-CO-BUILD-LOOP.                                              
                                                                 
    READ ERRTBL-IN  NEXT RECORD.                                 
                                                                 
    IF ERRTBL-FILE-STATUS = '10'                                 
       GO TO REIN-BUILD-EXIT.                                    
                                                                 
    IF ERRTBL-FILE-STATUS NOT = ZERO                             
       MOVE ERRTBL-FILE-STATUS TO WS-ABEND-FILE-STATUS           
       MOVE 'READ ERROR ON FILE - ERRTBL'                        
                                 TO WS-ABEND-MESSAGE             
       GO TO ABEND-PGM.                                          
                                                                 
    IF RE-CODE NOT = 'B'                                         
       GO TO REIN-CO-BUILD-LOOP.                                 
                                                                 
    IF RE-CLM-INCURRED-LIM NOT NUMERIC                           
       MOVE ZERO TO RE-CLM-INCURRED-LIM.                         
                                                                 
    IF RE-EARNING-START-DT NOT NUMERIC                           
       MOVE ZERO TO RE-EARNING-START-DT.                         
                                                                 
    IF RE-EARNING-STOP-DT NOT NUMERIC                            
       MOVE ZERO TO RE-EARNING-STOP-DT.                          
                                                                 
    PERFORM VARYING CPSE-NDX FROM 1 BY 1 UNTIL CPSE-NDX > 1500   
       MOVE RCT-CLM-CUTOFF-DT (CPSE-NDX)                         
                       TO  WS-RCT-CLM-CUTOFF-DT-N (CPSE-NDX)     
       MOVE RCT-EARNING-START-DT (CPSE-NDX)                      
                       TO  WS-RCT-EARNING-START-DT-N (CPSE-NDX)  
       MOVE RCT-EARNING-STOP-DT (CPSE-NDX)                       
                       TO  WS-RCT-EARNING-STOP-DT-N (CPSE-NDX)   
    END-PERFORM.                                                 
    MOVE RWF-EP-STOP-DATE       TO  WS-RWF-EP-STOP-DATE-N.       
                                                                 
    IF RE-LF-CLM-PCT NOT NUMERIC                                 
       MOVE ZERO TO RE-LF-CLM-PCT.                               
                                                                 
    IF RE-AH-CLM-PCT NOT NUMERIC                                 
       MOVE ZERO TO RE-AH-CLM-PCT.                               
                                                                 
    IF RE-LF-CLM-MAX NOT NUMERIC                                 
       MOVE ZERO TO RE-LF-CLM-MAX.                               
                                                                 
    IF RE-AH-CLM-MAX NOT NUMERIC                                 
       MOVE ZERO TO RE-AH-CLM-MAX.                               
                                                                 
    IF (RE-AH-CLM-MAX = ZERO) AND                                
       (RE-LF-CLM-MAX = ZERO) AND                                
       (RE-AH-CLM-PCT = ZERO) AND                                
       (RE-LF-CLM-PCT = ZERO) AND                                
       (RE-CLM-INCURRED-LIM = ZERO) AND                          
       (RE-EARNING-START-DT = ZERO) AND                          
       (RE-EARNING-STOP-DT = ZERO)                               
         GO TO REIN-CO-BUILD-EPR.                                
                                                                 
    ADD +1 TO CO-SUB.                                            
                                                                 
    IF CO-SUB GREATER REIN-CO-TABLE-ENT-CNT                      
       MOVE 0202 TO WS-RETURN-CODE                               
       DISPLAY 'REINSURANCE COMPANY SAVE TABLE EXCEEDED'         
       DISPLAY 'ERROR DETECTED IN ECSRIRTN, SEE ECSRITAB'        
       MOVE 'REINSURANCE COMPANY SAVE TABLE EXCEEDED'            
                                 TO WS-ABEND-MESSAGE             
       GO TO ABEND-PGM.                                          
                                                                 
    MOVE RE-COMPANY           TO RCT-REIN-CO (CO-SUB).           
                                                                 
    MOVE RE-CLM-INCURRED-LIM  TO RCT-CLM-CUTOFF-DT (CO-SUB)      
                              WS-RCT-CLM-CUTOFF-DT-N (CO-SUB).   
    MOVE RE-EARNING-START-DT  TO RCT-EARNING-START-DT (CO-SUB)   
                              WS-RCT-EARNING-START-DT-N (CO-SUB).
    MOVE RE-EARNING-STOP-DT   TO RCT-EARNING-STOP-DT (CO-SUB)    
                              WS-RCT-EARNING-STOP-DT-N (CO-SUB). 
                                                                 
    IF RE-EARN-STOP-CODE = 'A'  OR  'L'                          
        MOVE RE-EARN-STOP-CODE  TO RCT-EARN-STOP-CODE (CO-SUB)   
    ELSE                                                         
        MOVE 'B'                TO RCT-EARN-STOP-CODE (CO-SUB).  
                                                                 
    MOVE RE-LF-CLM-PCT  TO RCT-LF-CLM-PCT (CO-SUB).              
    MOVE RE-AH-CLM-PCT  TO RCT-AH-CLM-PCT (CO-SUB).              
    MOVE RE-LF-CLM-MAX  TO RCT-LF-CLM-MAX (CO-SUB).              
    MOVE RE-AH-CLM-MAX  TO RCT-AH-CLM-MAX (CO-SUB).              
                                                                 
REIN-CO-BUILD-EPR.                                               
                                                                 
    IF RE-AH-PR-PCT  NOT NUMERIC                                 
       MOVE ZERO TO RE-AH-PR-PCT.                                
                                                                 
    IF RE-AH-78-PCT  NOT NUMERIC                                 
       MOVE ZERO TO RE-AH-78-PCT.                                
                                                                 
    IF RE-LF-PR-PCT  NOT NUMERIC                                 
       MOVE ZERO TO RE-LF-PR-PCT.                                
                                                                 
    IF RE-LF-78-PCT  NOT NUMERIC                                 
       MOVE ZERO TO RE-LF-78-PCT.                                
                                                                 
    ADD +1 TO SUB1.                                              
                                                                 
    IF SUB1 GREATER REIN-CO-TABLE-ENT-CNT                        
       MOVE 0201 TO WS-RETURN-CODE                               
       MOVE 'REINSURANCE COMPANY SAVE TABLE EXCEEDED'            
                                 TO WS-ABEND-MESSAGE             
       GO TO ABEND-PGM.                                          
                                                                 
    MOVE RE-COMPANY     TO RC2-REIN-CO    (SUB1).                
    MOVE RE-AH-PR-PCT   TO RC2-AH-PRO-PCT (SUB1).                
    MOVE RE-AH-78-PCT   TO RC2-AH-R78-PCT (SUB1).                
    MOVE RE-LF-PR-PCT   TO RC2-LF-PRO-PCT (SUB1).                
    MOVE RE-LF-78-PCT   TO RC2-LF-R78-PCT (SUB1).                
    MOVE RE-NAME        TO RC2-CO-NAME    (SUB1).                
    move re-ah-pe       to rc2-ah-pe-method (sub1).              
    if re-state-exhibit-opt-f = 'Y'                              
       move 'Y'         to rc2-ecs152-rpt (sub1)                 
    else                                                         
       move 'N'         to rc2-ecs152-rpt (sub1)                 
    end-if                                                       
                                                                 
    GO TO REIN-CO-BUILD-LOOP.                                    
                                                                 
REIN-BUILD-EXIT.                                                 
    EXIT.                                                        
                                                                 
ZERO-RCT-ENTRIES.                                                
                                                                 
    MOVE SPACES TO REIN-CO-HOLD-ENTRIES (CO-SUB).                
                                                                 
    MOVE ZERO   TO RCT-CLM-CUTOFF-DT (CO-SUB)                    
                   WS-RCT-CLM-CUTOFF-DT-N (CO-SUB)               
                   RCT-EARNING-START-DT (CO-SUB)                 
                   WS-RCT-EARNING-START-DT-N (CO-SUB)            
                   RCT-EARNING-STOP-DT (CO-SUB)                  
                   WS-RCT-EARNING-STOP-DT-N (CO-SUB)             
                   RCT-LF-CLM-PCT    (CO-SUB)                    
                   RCT-AH-CLM-PCT    (CO-SUB)                    
                   RCT-LF-CLM-MAX    (CO-SUB)                    
                   RCT-AH-CLM-MAX    (CO-SUB).                   
                                                                 
ZERO-RCT-EXIT.                                                   
    EXIT.                                                        
                                                                 
ZERO-RC2-ENTRIES.                                                
                                                                 
    MOVE SPACES TO RC2-CO-NAME (CO-SUB)                          
                   RC2-REIN-CO (CO-SUB).                         
    move spaces      to rc2-ecs152-rpt (co-sub)                  
                                                                 
    MOVE ZERO   TO RC2-LF-R78-PCT    (CO-SUB)                    
                   RC2-LF-PRO-PCT    (CO-SUB)                    
                   RC2-AH-R78-PCT    (CO-SUB)                    
                   RC2-AH-PRO-PCT    (CO-SUB).                   
                                                                 
ZERO-RC2-EXIT.                                                   
    EXIT.                                                        
