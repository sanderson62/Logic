       ELSE                                                      
          COMPUTE GRS-AHCOM = ((GRR-AHCOM + GRP-AHCOM) / 2)      
          MOVE GRS-AHCOM        TO GRD-AHCOM                     
       END-IF                                                    
    END-IF                                                       
                                                                 
    IF (DTE-CLIENT = 'DCC' or 'VPP')                             
       AND (AM-DCC-PRODUCT-CODE = 'DDF')                         
       AND (GR-REIN = 'P')                                       
       IF (WS-CHARGEBACK-SW (1) = 'N') OR                        
          ((AM-COMM-CHARGEBACK (1) NOT = ZEROS) AND              
          (MONTHS-DIFF > AM-COMM-CHARGEBACK (1)))                
          CONTINUE                                               
       ELSE                                                      
          COMPUTE CNC-FACT = GRP-AHPRM / GR-AHPRM                
          COMPUTE GRP-AHCOM ROUNDED = GRP-AHCOM +                
            ((GR-AHPRM - CR-AH-CLP - CR-ADDL-CLP) * CNC-FACT)    
          DISPLAY ' GRP COMM AF C ' GRP-AHCOM                    
          COMPUTE CNC-FACT = GRR-AHPRM / GR-AHPRM                
          COMPUTE GRR-AHCOM ROUNDED = GRR-AHCOM +                
            ((GR-AHPRM - CR-AH-CLP - CR-ADDL-CLP) * CNC-FACT)    
          COMPUTE CNC-FACT = GRD-AHPRM / GR-AHPRM                
          COMPUTE GRD-AHCOM ROUNDED = GRD-AHCOM +                
            ((GR-AHPRM - CR-AH-CLP - CR-ADDL-CLP) * CNC-FACT)    
          PERFORM 1110-CALC-DDF-UECOMM THRU 1110-EXIT            
          COMPUTE GRS-AHCOM = GRS-AHCOM + COMM-WK                
       END-IF                                                    
