       DISPLAY ' FACT ALREADY FOUND IN 1130 ' CR-CERT-NO         
    ELSE                                                         
       PERFORM 1100-GET-DDF-FACTORS                              
                                THRU 1100-EXIT                   
    END-IF                                                       
                                                                 
    COMPUTE WS-MONTH =                                           
       FUNCTION REM(CR-AH-TERM - AH-REM-TRM1, 12)                
    IF WS-MONTH = 0                                              
       MOVE 12 TO WS-MONTH                                       
    END-IF                                                       
                                                                 
    MOVE ZEROS                  TO COMM-WK                       
                                                                 
    EVALUATE TRUE                                                
       WHEN (AH-REM-TRM1 = 0)                                    
          OR (CR-CANCEL-REASON = 'R')                            
          DISPLAY ' REM TERM = 0 OR REPO'                        
          MOVE 0                TO COMM-WK                       
                                                                 
       WHEN AH-REM-TRM1 = CR-AH-TERM                             
          DISPLAY ' REM TERM  = ORIG TERM '                      
          MOVE WS-DDF-CSO-ADMIN-FEE TO COMM-WK                   
                                                                 
       WHEN (CR-AH-TERM - AH-REM-TRM1) < 4                       
          DISPLAY ' ORIG TERM - REM TRM < 4 '                    
          COMPUTE CNC-FACT ROUNDED = WS-DDF-CSO-ADMIN-FEE -      
             (DD-1ST-YR-ALLOW (D1)                               
             * WS-MONTH / 3) - (WS-DDF-CSO-ADMIN-FEE -           
             DD-1ST-YR-ALLOW (D1))                               
             * (1 - WS-HI-UEF) * WS-MONTH / 12                   
          COMPUTE COMM-WK = CNC-FACT * 1                         
                                                                 
*>*** (AdminFee-Yr1 AF)-((AdminFee- Yr1 AF)*(1 - UEF1)*mo/12)      
                                                                 
       WHEN (CR-AH-TERM - AH-REM-TRM1) < 13                      
          DISPLAY ' ORIG TERM - REM TRM < 13 '                   
          COMPUTE CNC-FACT ROUNDED = (WS-DDF-CSO-ADMIN-FEE -     
             DD-1ST-YR-ALLOW (D1))                               
             - (WS-DDF-CSO-ADMIN-FEE - DD-1ST-YR-ALLOW (D1))     
             * (1 - WS-hi-UEF) * WS-MONTH / 12                   
          COMPUTE COMM-WK = CNC-FACT * 1                         
                                                                 
*>***  (AdminFee-Yr1 AF)*{UEF1 - (UEF1 -UEF2)*mo/12}               
                                                                 
       WHEN OTHER                                                
          DISPLAY ' OTHER '                                      
          COMPUTE CNC-FACT ROUNDED = (WS-DDF-CSO-ADMIN-FEE -     
             DD-1ST-YR-ALLOW (D1))                               
             * (WS-HI-UEF - (WS-HI-UEF - WS-LO-UEF)              
             * WS-MONTH / 12)                                    
          COMPUTE COMM-WK = CNC-FACT * 1                         
    END-EVALUATE                                                 
                                                                 
    .                                                            
1130-EXIT.                                                       
    EXIT.                                                        
                                                                 
2000-CALC-REM-AMT.                                               
                                                                 
    IF (DTE-CLIENT = 'ABL') AND (GR-CARRIER NOT EQUAL '2')       
        IF (CR-SEX = 'F') AND                                    
           (GR-MORT-AGE GREATER +22) AND                         
           (CR-MORT = 'E550' OR 'F550')                          
               COMPUTE GR-AGE = GR-AGE - +6.                     
                                                                 
    IF (DTE-CLIENT EQUAL 'NCL') AND                              
       (O-B-CERTIFICATE)                                         
       COMPUTE GR-REM-AMT EQUAL GR-LFBEN * .5                    
       GO TO 2999-CALC-REM-AMT-X.                                
                                                                 
*>***************************************************************  
*>  THIS LOGIC ALLOWS THE CLIENT TO CONTROL THE AGE CALCULATION *  
*>  ROUTINE THROUGH A COMPANY LEVEL CONTROL.  OPTIONS ALLOW THE *  
*>  CLIENT TO 1) USE THE AGE METHOD ASSIGNED TO THE TABLE WHEN  *  
*>  WHEN IT WAS GENERATED, 2) USE AGE-LAST, 3) USE AGE-NEAR.    *  
*>***************************************************************  
                                                                 
    IF  DTE-USE-ALL-AGE-LAST                                     
        COMPUTE GR-MORT-AGE =                                    
            GR-AGE + ((GR-LF-TERM - GR-LF-REMTERM) / +12)        
                                                                 
    ELSE                                                         
        IF  DTE-USE-ALL-AGE-NEAR                                 
            COMPUTE GR-MORT-AGE ROUNDED =                        
                GR-AGE + ((GR-LF-TERM - GR-LF-REMTERM) / +12)    
                                                                 
        ELSE                                                     
            IF  DTE-USE-TABLE-ASSIGNED-METHOD                    
                SET CLAS-MORT-NDX TO +1                          
                SEARCH CLAS-MORT-FLD                             
                    VARYING CLAS-MORT-NDX                        
                                                                 
                    AT END                                       
*>*****THIS IS A DEFAULT TO AGE LAST FOR A TABLE MISSING****       
*>*****ECS080 SHOULD REPORT THE MISSING TABLE           ****       
                        COMPUTE GR-MORT-AGE = GR-AGE +           
                            ((GR-LF-TERM - GR-LF-REMTERM) / +12) 
                                                                 
                    WHEN                                         
                        CLAS-MORT-CODE (CLAS-MORT-NDX)           
                            EQUAL GR-MORT-CODE                   
                        PERFORM 2040-DETERMINE-AGE-METHOD        
                            THRU 2040-EXIT.                      
                                                                 
    ADD +1                      TO W-PASS-CTR.                   
                                                                 
    IF DTE-CLIENT = 'DAC' OR 'BPI'                               
        IF CR-SEX = 'F'                                          
            COMPUTE GR-MORT-AGE = GR-MORT-AGE - +6.              
                                                                 
    IF CLAS-I-RL-AH (CLAS-INDEXL) = 'L' OR 'P'                   
        MOVE GR-LFBEN TO GR-REM-AMT                              
        GO TO 2999-CALC-REM-AMT-X.                               
                                                                 
    if dte-client = 'CID'                                        
       IF GR-LFTYP = 'QD'                                        
*>          MOVE +15.0            TO CP-LOAN-APR  (PAUL CHANGE)    
          MOVE +15.0            TO GR-APR                        
       END-IF                                                    
    END-IF                                                       
                                                                 
    IF (CLAS-I-EP (CLAS-INDEXL) = 'B' OR 'K' OR 'L') AND         
          GR-REIN = 'P'                                          
       COMPUTE INTERMED ROUNDED = CR-LFAMT / GR-LF-TERM          
    ELSE                                                         
       if dte-client = 'AHL' or 'FNL'                            
          COMPUTE INTERMED ROUNDED = GR-LFBEN / GR-LOAN-TERM     
       ELSE                                                      
          COMPUTE INTERMED ROUNDED = GR-LFBEN / GR-LF-TERM       
       END-IF                                                    
    END-IF                                                       
                                                                 
    IF (CLAS-I-EP (CLAS-INDEXL) = 'B' OR 'K' OR 'L')  AND        
       GR-REIN = 'R'                                             
        COMPUTE INTERMED ROUNDED EQUAL                           
            (CR-LFAMT * WS-CEDE-FACT) / GR-LF-TERM.              
                                                                 
    MOVE INTERMED                    TO GR-MO-DEC.               
                                                                 
    IF GR-LF-REMTERM = GR-LF-TERM                                
        IF (CLAS-I-EP (CLAS-INDEXL) = 'B' OR 'K' OR 'L') AND     
           (GR-REIN = 'R')                                       
            NEXT SENTENCE                                        
        ELSE                                                     
            MOVE GR-LFBEN TO GR-REM-AMT                          
            GO TO 2999-CALC-REM-AMT-X.                           
                                                                 
*>    IF GR-SUMMARY-REC  OR                                        
*>       (CLAS-I-EP (CLAS-INDEXL) = 'B' OR 'K' OR 'L')             
*>        GO TO 2010-ORDINARY-REM.                                 
                                                                 
    IF (CLAS-I-EP (CLAS-INDEXL) = 'B' OR 'K' OR 'L')             
        GO TO 2010-ORDINARY-REM.                                 
                                                                 
    IF CLAS-I-EP (CLAS-INDEXL) = 'T'                             
        GO TO 2020-CALC-TEXAS-REM.                               
                                                                 
    IF DTE-CLIENT = 'NCL'                                        
        IF GR-EFF LESS THAN 19910101                             
            GO TO 2010-ORDINARY-REM.                             
                                                                 
    IF GR-LFTYP = 'QD'                                           
*>       MOVE +15.0               TO CP-LOAN-APR  (PAUL CHANGE)    
       MOVE +15.0               TO GR-APR                        
       MOVE 'N'           TO CLAS-I-CALC-TYPE (CLAS-INDEXL)      
       GO TO 2030-CALC-NET-PAY-REM                               
    END-IF                                                       
                                                                 
    IF CLAS-I-EP (CLAS-INDEXL) = 'N'                             
        GO TO 2030-CALC-NET-PAY-REM.                             
                                                                 
*>    IF STATE-ABBR (CLAS-INDEXS) = 'OH'                           
    IF (STATE-ABBR (CLAS-INDEXS) = 'OH') AND                     
       (CR-RATING-CLASS NOT = 'L ')                              
        IF (GR-LF-TERM GREATER THAN +60) AND                     
           (GR-EFF GREATER THAN 19831031) AND                    
           (GR-APR GREATER THAN ZERO)                            
            GO TO 2030-CALC-NET-PAY-REM.                         
                                                                 
    IF STATE-ABBR (CLAS-INDEXS) = 'MT'                           
        IF (GR-LF-TERM GREATER THAN +61) AND                     
           (GR-EFF GREATER THAN 19830318) AND                    
           (GR-APR GREATER THAN ZERO)                            
            GO TO 2030-CALC-NET-PAY-REM.                         
                                                                 
    IF STATE-ABBR (CLAS-INDEXS) = 'UT'                           
        IF (GR-LF-TERM GREATER THAN +62) AND                     
           (GR-EFF GREATER THAN 19810831) AND                    
           (GR-EFF LESS THAN 19830901) AND                       
           (GR-APR GREATER THAN ZERO)                            
            GO TO 2030-CALC-NET-PAY-REM.                         
                                                                 
    IF STATE-ABBR (CLAS-INDEXS) = 'RI'                           
        IF (GR-LF-TERM GREATER THAN +60) AND                     
           (GR-EFF GREATER THAN 19831231) AND                    
           (GR-APR GREATER THAN ZERO)                            
            GO TO 2030-CALC-NET-PAY-REM.                         
                                                                 
2010-ORDINARY-REM.                                               
                                                                 
    if dte-client = 'AHL'                                        
       if gr-loan-term > gr-lf-term                              
          compute gr-rem-amt = intermed *                        
          (gr-lf-remterm +                                       
             (gr-loan-term - gr-lf-term))                        
          go to 2999-calc-rem-amt-x                              
       end-if                                                    
    end-if                                                       
                                                                 
    if dte-client = 'FNL'                                        
       if ws-fnl-rem-term <> 0                                   
          move ws-fnl-rem-term to gr-lf-remterm                  
       end-if                                                    
       if gr-loan-term > gr-lf-term                              
          compute gr-rem-amt rounded = intermed *                
          (gr-lf-remterm +                                       
             (gr-loan-term - gr-lf-term))                        
          go to 2999-calc-rem-amt-x                              
       end-if                                                    
    end-if                                                       
                                                                 
    IF (CLAS-I-EP (CLAS-INDEXL) = 'B' OR 'K' OR 'L')  AND        
       GR-REIN = 'P'                                             
        COMPUTE GR-REM-AMT ROUNDED =                             
                       (INTERMED * GR-LF-REMTERM) + CR-LFAMT-ALT 
    ELSE                                                         
        COMPUTE GR-REM-AMT ROUNDED = INTERMED * GR-LF-REMTERM.   
                                                                 
    IF (CLAS-I-EP (CLAS-INDEXL) = 'B' OR 'K' OR 'L')  AND        
       GR-REIN = 'R'                                             
        COMPUTE GR-REM-AMT ROUNDED EQUAL                         
           (INTERMED * GR-LF-REMTERM) +                          
           (CR-LFAMT-ALT * WS-CEDE-FACT).                        
                                                                 
    GO TO 2999-CALC-REM-AMT-X.                                   
                                                                 
2020-CALC-TEXAS-REM.                                             
    DIVIDE GR-LFBEN BY GR-LF-TERM                                
        GIVING TEX-FACT-1.                                       
    DIVIDE LF-REM-TRM2 BY GR-PMT-FREQ                            
        GIVING TEX-FACT-2                                        
        REMAINDER TEX-FACT-3.                                    
                                                                 
    IF TEX-FACT-3 NOT = ZERO                                     
        ADD +1 TO TEX-FACT-2.                                    
                                                                 
    IF (TEX-FACT-2 * GR-PMT-FREQ) = GR-LF-TERM                   
        MOVE GR-LFBEN TO GR-REM-AMT                              
    ELSE                                                         
        COMPUTE GR-REM-AMT ROUNDED =                             
            (TEX-FACT-1 * (TEX-FACT-2 * GR-PMT-FREQ)).           
                                                                 
    GO TO 2999-CALC-REM-AMT-X.                                   
                                                                 
2030-CALC-NET-PAY-REM.                                           
                                                                 
    MOVE GR-EFF                     TO  DC-GREG-DATE-CYMD        
    MOVE 'L'                        TO  DC-OPTION-CODE.          
    PERFORM 0400-DATE-CONVERSION-ROUTINE THRU 0409-EXIT.         
    MOVE DC-BIN-DATE-1              TO  CP-CERT-EFF-DT.          
                                                                 
    IF CR-LOAN-1ST-PMT-DT IS NOT EQUAL TO ZEROS                  
        MOVE CR-LOAN-1ST-PMT-DT     TO  DC-GREG-DATE-1-YMD       
        MOVE '3'                    TO  DC-OPTION-CODE           
        PERFORM 0400-DATE-CONVERSION-ROUTINE THRU 0409-EXIT      
        MOVE DC-BIN-DATE-1          TO  CP-FIRST-PAY-DATE        
    ELSE                                                         
        MOVE CP-CERT-EFF-DT         TO  DC-BIN-DATE-1            
        MOVE +1                     TO  DC-ELAPSED-MONTHS        
        MOVE +0                     TO  DC-ELAPSED-DAYS          
        MOVE '6'                    TO  DC-OPTION-CODE           
        PERFORM 0400-DATE-CONVERSION-ROUTINE THRU 0409-EXIT      
        MOVE DC-BIN-DATE-2          TO  CP-FIRST-PAY-DATE.       
                                                                 
    MOVE GR-LFBEN                   TO  CP-ORIGINAL-BENEFIT.     
    MOVE GR-APR                     TO  CP-LOAN-APR.             
                                                                 
    if dte-client = 'AHL'                                        
       MOVE +14                 TO CP-LOAN-APR                   
    END-IF                                                       
                                                                 
    MOVE GR-LF-TERM                 TO  CP-ORIGINAL-TERM.        
    MOVE GR-LOAN-TERM               TO  CP-LOAN-TERM.            
    MOVE LF-REM-TRM2                TO  CP-REMAINING-TERM.       
    MOVE CLAS-I-CALC-TYPE (CLAS-INDEXL) TO CP-SPECIAL-CALC-CD.   
    MOVE CLAS-I-RL-AH (CLAS-INDEXL) TO  CP-BENEFIT-TYPE.         
    MOVE CLAS-I-EP (CLAS-INDEXL)    TO  CP-EARNING-METHOD.       
                                                                 
    MOVE ZEROS                  TO CP-R-MAX-TOT-BEN              
    IF (DTE-CLIENT = 'DCC' or 'VPP')                             
       AND (AM-DCC-PRODUCT-CODE <> spaces)                       
       if cr-clp-state = spaces                                  
          move cr-state         to cr-clp-state                  
       end-if                                                    
       if cr-lftyp not = '00' and '  ' and 'DD'                  
          move 'L'              to ws-dd-ben-type                
          move cr-lftyp         to ws-dd-ben-code                
       else                                                      
          move 'A'              to ws-dd-ben-type                
          move cr-ahtyp         to ws-dd-ben-code                
       end-if                                                    
       PERFORM VARYING D1 FROM +1 BY +1 UNTIL                    
          ((CR-clp-STATE = DD-STATE (D1))                        
          AND (AM-DCC-PRODUCT-CODE = DD-PRODUCT-CD (D1))         
          AND (ws-dd-ben-type = DD-BEN-TYPE   (D1))              
          AND (ws-dd-ben-code  = DD-BEN-CODE   (D1))             
          AND (CR-DT         < DD-PROD-EXP-DT (D1)))             
                          OR                                     
          D1 > max-pdef                                          
       END-PERFORM                                               
       IF D1 <= max-pdef                                         
          PERFORM VARYING P1 FROM +1 BY +1 UNTIL                 
             (P1 > +11)                                          
             OR (DD-PROD-CODE (D1 P1) = 'L')                     
          END-PERFORM                                            
          IF P1 < +12                                            
             MOVE DD-MAX-BEN-AMT (D1 P1)                         
                                TO CP-R-MAX-TOT-BEN              
          END-IF                                                 
       END-IF                                                    
    END-IF                                                       
                                                                 
    CALL 'ELRAMTX' USING CALCULATION-PASS-AREA.                  
                                                                 
    MOVE CP-REMAINING-AMT           TO  GR-REM-AMT.              
                                                                 
    if gr-apr > zeros                                            
       if gr-loan-term > gr-lf-term                              
          move gr-loan-term to ws-work-term                      
       else                                                      
          move gr-lf-term to ws-work-term                        
       end-if                                                    
       compute i = gr-apr / 1200                                 
       compute anglen =                                          
          (1 - (1 / (1 + i)) ** ws-work-term) / i                
       compute gr-mo-dec rounded =                               
          gr-lfben / anglen                                      
    end-if                                                       
                                                                 
    GO TO 2999-CALC-REM-AMT-X.                                   
                                                                 
2040-DETERMINE-AGE-METHOD.                                       
                                                                 
    IF DTE-CLIENT EQUAL 'ACM'                                    
            COMPUTE GR-MORT-AGE EQUAL                            
                GR-AGE + ((GR-LF-TERM - GR-LF-REMTERM) / +12)    
    ELSE                                                         
    IF  CLAS-AGE-METHOD (CLAS-MORT-NDX) EQUAL 'AL'               
        COMPUTE GR-MORT-AGE =                                    
            GR-AGE + ((GR-LF-TERM - GR-LF-REMTERM) / +12)        
                                                                 
    ELSE                                                         
        IF  CLAS-AGE-METHOD (CLAS-MORT-NDX) EQUAL 'AN'           
            COMPUTE GR-MORT-AGE ROUNDED =                        
                GR-AGE + ((GR-LF-TERM - GR-LF-REMTERM) / +12).   
                                                                 
                                                                 
2040-EXIT.                                                       
    EXIT.                                                        
                                                                 
2999-CALC-REM-AMT-X.                                             
    EXIT.                                                        
EJECT                                                            
