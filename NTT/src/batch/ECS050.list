EJECT                                                            
0900-CALCULATE-COMMISSIONS.                                      
                                                                 
    MOVE +1                          TO SA.                      
    MOVE ZEROS                       TO GR-LFCOM                 
                                        GR-LFCOM-ALT             
                                        GR-AHCOM                 
                                        WS-GR-LFCOM              
                                        WS-GR-LFCOM-ALT          
                                        WS-GR-AHCOM              
                                                                 
*>    MOVE ZEROS                       TO GR-LFSRV                 
*>                                        GR-LFSRV-ALT             
*>                                        GR-AHSRV.                
                                                                 
    .                                                            
0905-CALC-COMM-LOOP.                                             
                                                                 
    IF DTE-CLIENT = 'POS'                                        
        COMPUTE GR-LFCOM ROUNDED =                               
                      GR-LFCOM + (LF-PRM * CR-LCOM-L (SA))       
        COMPUTE GR-LFCOM-ALT ROUNDED =                           
                  GR-LFCOM-ALT + (LF-PRM-ALT * CR-LCOM-L (SA))   
        COMPUTE GR-AHCOM ROUNDED =                               
                      GR-AHCOM + (GR-AHPRM * CR-LCOM-AH (SA))    
        GO TO 0910-CALC-COMM-SKIP.                               
                                                                 
*>   WS-CB-SW OF SPACES SETS DO-NOT-CHARGE-BACK TO FALSE           
    MOVE ' '                    TO WS-CB-SW                      
                                                                 
    if dte-client = 'AHL'                                        
       continue                                                  
    else                                                         
       IF (WS-CHARGEBACK-SW (SA) = 'N') OR                       
          ((AM-COMM-CHARGEBACK (SA) NOT = ZEROS) AND             
          (MONTHS-DIFF > AM-COMM-CHARGEBACK (SA)))               
          SET DO-NOT-CHARGE-BACK   TO TRUE                       
       END-IF                                                    
    end-if                                                       
                                                                 
*>  I COULD HAVE ADDED THIS IN THE IF STMT BELOW BUT I             
*>  DIDN'T WANT TO SCREW IT UP SO......                            
                                                                 
    IF (CLAS-I-BEN-CATEGORY (CLAS-INDEXL) = 'D')                 
       OR (CLAS-I-BEN-CATEGORY (CLAS-INDEXA) = 'D')              
       IF GR-REIN = 'P'                                          
          IF CR-AGT-TYPE (SA) = 'C' OR 'D'                       
             COMPUTE GR-LFCOM =                                  
                 GR-LFCOM + (LF-PRM * CR-LCOM-L (SA))            
             COMPUTE GR-LFCOM-ALT =                              
                 GR-LFCOM-ALT + (LF-PRM-ALT * CR-LCOM-L (SA))    
             COMPUTE GR-AHCOM =                                  
                 GR-AHCOM + (GR-AHPRM * CR-LCOM-AH (SA))         
             IF DO-NOT-CHARGE-BACK                               
                CONTINUE                                         
             ELSE                                                
                COMPUTE WS-GR-LFCOM =                            
                    WS-GR-LFCOM + (LF-PRM * CR-LCOM-L (SA))      
                COMPUTE WS-GR-LFCOM-ALT =                        
                 WS-GR-LFCOM-ALT + (LF-PRM-ALT * CR-LCOM-L (SA)) 
                COMPUTE WS-GR-AHCOM =                            
                 WS-GR-AHCOM + (GR-AHPRM * CR-LCOM-AH (SA))      
             END-IF                                              
          ELSE                                                   
             IF CR-AGT-TYPE (SA) = 'A' OR 'O' OR 'P'             
                COMPUTE GR-LFCOM =                               
                    GR-LFCOM + (CR-LF-CLP * CR-LCOM-L (SA))      
                COMPUTE GR-AHCOM =                               
                    GR-AHCOM + (CR-AH-CLP * CR-LCOM-AH (SA))     
                IF DO-NOT-CHARGE-BACK                            
                   CONTINUE                                      
                ELSE                                             
                   COMPUTE WS-GR-LFCOM =                         
                     WS-GR-LFCOM + (CR-LF-CLP * CR-LCOM-L (SA))  
                   COMPUTE WS-GR-AHCOM =                         
                     WS-GR-AHCOM + (CR-AH-CLP * CR-LCOM-AH (SA)) 
                END-IF                                           
             END-IF                                              
          END-IF                                                 
       ELSE                                                      
          IF GR-REIN = 'R'                                       
             IF CR-AGT-TYPE (SA) = 'R' OR 'D'                    
                COMPUTE GR-LFCOM =                               
                    GR-LFCOM + (LF-PRM * CR-LCOM-L (SA))         
                COMPUTE GR-LFCOM-ALT =                           
                    GR-LFCOM-ALT + (LF-PRM-ALT * CR-LCOM-L (SA)) 
                COMPUTE GR-AHCOM =                               
                    GR-AHCOM + (GR-AHPRM * CR-LCOM-AH (SA))      
                IF DO-NOT-CHARGE-BACK                            
                   CONTINUE                                      
                ELSE                                             
                   COMPUTE WS-GR-LFCOM =                         
                       WS-GR-LFCOM + (LF-PRM * CR-LCOM-L (SA))   
                   COMPUTE WS-GR-LFCOM-ALT = WS-GR-LFCOM-ALT     
                      + (LF-PRM-ALT * CR-LCOM-L (SA))            
                   COMPUTE WS-GR-AHCOM =                         
                    WS-GR-AHCOM + (GR-AHPRM * CR-LCOM-AH (SA))   
                END-IF                                           
             ELSE                                                
                IF CR-AGT-TYPE (SA) = 'T' OR 'P'                 
                   COMPUTE GR-LFCOM =                            
                       GR-LFCOM + (CR-LF-CLP * CR-LCOM-L (SA))   
                   COMPUTE GR-AHCOM =                            
                       GR-AHCOM + (CR-AH-CLP * CR-LCOM-AH (SA))  
                   IF DO-NOT-CHARGE-BACK                         
                      CONTINUE                                   
                   ELSE                                          
                      COMPUTE WS-GR-LFCOM = WS-GR-LFCOM +        
                         (CR-LF-CLP * CR-LCOM-L (SA))            
                      COMPUTE WS-GR-AHCOM = WS-GR-AHCOM +        
                         (CR-AH-CLP * CR-LCOM-AH (SA))           
                   END-IF                                        
                END-IF                                           
             END-IF                                              
          END-IF                                                 
       END-IF                                                    
       GO TO 0905-CONTINUE                                       
    END-IF                                                       
                                                                 
    IF CLAS-I-BEN-CATEGORY (CLAS-INDEXA) = 'G' OR 'L'            
       IF GR-REIN = 'P'                                          
          IF CR-AGT-TYPE (SA) = 'C' OR 'D'                       
             COMPUTE GR-AHCOM = GR-AHCOM +                       
                (GR-AHPRM - (CR-LFPRM-ALT + CR-ADDL-CLP))        
             IF (NOT DO-NOT-CHARGE-BACK)                         
                AND (AM-DCC-PRODUCT-CODE NOT = 'DDF')            
                COMPUTE WS-GR-AHCOM = WS-GR-AHCOM +              
                   (GR-AHPRM - (CR-LFPRM-ALT + CR-ADDL-CLP))     
             END-IF                                              
          ELSE                                                   
             IF CR-AGT-TYPE (SA) = 'B' OR 'I' OR 'L' OR 'K'      
                                OR 'J' OR 'N'                    
                COMPUTE GR-AHCOM = GR-AHCOM +                    
                (CR-LCOM-AH (SA) * +1000)                        
                IF (NOT DO-NOT-CHARGE-BACK)                      
                   AND (CR-AGT-TYPE (SA) NOT = 'N' AND 'I')      
                   COMPUTE WS-GR-AHCOM = WS-GR-AHCOM +           
                   (CR-LCOM-AH (SA) * +1000)                     
                END-IF                                           
             ELSE                                                
                IF CR-AGT-TYPE (SA) = 'O' OR 'P'                 
                   COMPUTE GR-AHCOM = GR-AHCOM +                 
                   (GR-AHPRM * CR-LCOM-AH (SA))                  
                   IF NOT DO-NOT-CHARGE-BACK                     
                      COMPUTE WS-GR-AHCOM = WS-GR-AHCOM +        
                      (GR-AHPRM * CR-LCOM-AH (SA))               
                   END-IF                                        
                END-IF                                           
             END-IF                                              
          END-IF                                                 
       ELSE                                                      
          IF GR-REIN = 'R'                                       
             IF CR-AGT-TYPE (SA) = 'K'                           
                COMPUTE GR-AHCOM = GR-AHCOM +                    
                (GR-AHPRM / CR-AHPRM) *                          
                (CR-LCOM-AH (SA) * +1000)                        
                IF NOT DO-NOT-CHARGE-BACK                        
                   COMPUTE WS-GR-AHCOM = WS-GR-AHCOM +           
                   (GR-AHPRM / CR-AHPRM) *                       
                   (CR-LCOM-AH (SA) * +1000)                     
                END-IF                                           
             ELSE                                                
                IF CR-AGT-TYPE (SA) = 'P' OR 'T' OR 'R'          
                   COMPUTE GR-AHCOM =                            
                   GR-AHCOM + (GR-AHPRM * CR-LCOM-AH (SA))       
                   IF NOT DO-NOT-CHARGE-BACK                     
                      COMPUTE WS-GR-AHCOM =                      
                      WS-GR-AHCOM + (GR-AHPRM * CR-LCOM-AH (SA)) 
                   END-IF                                        
                END-IF                                           
             END-IF                                              
          END-IF                                                 
       END-IF                                                    
    ELSE                                                         
                                                                 
                                                                 
    IF DTE-CLIENT = 'DCC' or 'VPP'                               
      IF ((GR-REIN = 'P')                                        
        AND (CR-AGT-TYPE (SA) = ('C' OR 'D')))                   
      OR                                                         
        ((GR-REIN = 'R')                                         
        AND (CR-AGT-TYPE (SA) = ('R' OR 'D')))                   
          COMPUTE GR-LFCOM =                                     
              GR-LFCOM + (LF-PRM * CR-LCOM-L (SA))               
          COMPUTE GR-LFCOM-ALT =                                 
              GR-LFCOM-ALT + (LF-PRM-ALT * CR-LCOM-L (SA))       
          COMPUTE GR-AHCOM =                                     
              GR-AHCOM + (GR-AHPRM * CR-LCOM-AH (SA))            
          IF DO-NOT-CHARGE-BACK                                  
             CONTINUE                                            
          ELSE                                                   
             COMPUTE WS-GR-LFCOM =                               
                 WS-GR-LFCOM + (LF-PRM * CR-LCOM-L (SA))         
             COMPUTE WS-GR-LFCOM-ALT =                           
                 WS-GR-LFCOM-ALT + (LF-PRM-ALT * CR-LCOM-L (SA)) 
