    MOVE WS-TIME                TO ME-START-TIME.                
                                                                 
0100-INITIALIZATION-ROUTINE.                                     
                                                                 
                                                                 
    OPEN INPUT  CERT-MASTER                                      
                ERACCTT                                          
         OUTPUT PRINTER.                                         
                                                                 
    IF DTE-CLIENT = 'DCC' or 'VPP'                               
       OPEN INPUT ERPDEF                                         
    END-IF                                                       
                                                                 
    IF ERACCTT-FILE-STATUS  = '00' OR '97'                       
        NEXT SENTENCE                                            
      ELSE                                                       
        MOVE '11'                   TO W-ABEND-CODE-1            
        MOVE ERACCTT-FILE-STATUS    TO W-ABEND-CODE-2            
        MOVE WS-ABEND-CODE          TO WS-RETURN-CODE            
        MOVE 'OPEN ERROR - ERACCTT' TO WS-ABEND-MESSAGE          
        DISPLAY ' ERROR - ERACCTT - OPEN ' ERACCTT-FILE-STATUS   
        GO TO ABEND-PGM.                                         
                                                                 
    IF ERPDEF-FILE-STATUS  = '00' OR '97'                        
       CONTINUE                                                  
    ELSE                                                         
       MOVE '61'                TO W-ABEND-CODE-1                
       MOVE ERPDEF-FILE-STATUS  TO W-ABEND-CODE-2                
       MOVE 'OPEN ERROR - ERPDEF ' TO WS-ABEND-MESSAGE           
       DISPLAY ' ERROR - ERPDEF  - OPEN ' ERPDEF-FILE-STATUS     
       MOVE WS-ABEND-CODE       TO WS-RETURN-CODE                
       GO TO ABEND-PGM                                           
    END-IF                                                       
                                                                 
    IF DTE-PGM-OPT = '1' OR '3'                                  
        OPEN OUTPUT ALPHA-EXTRACT.                               
                                                                 
    IF DTE-PGM-OPT = '2' OR '3'                                  
        OPEN OUTPUT GAAP-EXTRACT.                                
                                                                 
    MOVE 'R'                    TO CP-RATE-FILE.                 
    MOVE 'O'                    TO CP-IO-FUNCTION.               
    PERFORM 3090-CALL-RATING-ROUTINE THRU 3099-EXIT.             
    IF IO-ERROR                                                  
        MOVE 0302               TO WS-RETURN-CODE                
        MOVE 'ERROR OCCURED OPENING - ELRATE'                    
                                TO WS-ABEND-MESSAGE              
        GO TO ABEND-PGM.                                         
                                                                 
    MOVE WS-CURRENT-DATE        TO H2-DATE                       
                                   ME-START-DATE.                
    MOVE ME-START-MO            TO ME-CNDS-MO.                   
    MOVE ME-START-DA            TO ME-CNDS-DA.                   
    MOVE ME-START-YR            TO ME-CNDS-YR.                   
    MOVE COMPANY-NAME           TO H2-COMP.                      
    MOVE ALPH-DATE              TO H3-DATE.                      
    MOVE LIFE-OVERRIDE-L6       TO HD4A-DESC-1.                  
    MOVE AH-OVERRIDE-L6         TO HD4A-DESC-2.                  
                                                                 
    COMPUTE CUR-CALC = (RUN-CCYY * +12) + RUN-MO.                
    MOVE CUR-CALC               TO MONTH-END-MOYR.               
                                                                 
    MOVE HIGH-VALUES            TO REIN-HOLD-AREAS.              
    MOVE SPACES                 TO REIN-ONLY-HOLDERS             
                                   REIN-LEVELS-END.              
                                                                 
    MOVE RUN-DATE               TO VALUATION-DATE.               
    MOVE BIN-RUN-DATE           TO WS-BIN-VAL-DT.                
                                                                 
    IF DTE-CLIENT NOT EQUAL 'NCL'                                
       GO TO 0109-CONTINUE-INIT.                                 
                                                                 
    MOVE RUN-DATE               TO WS-NCL-OB-DATES(1)            
                                                                 
    MOVE +0                     TO DC-ELAPSED-MONTHS             
                                   DC-ELAPSED-DAYS.              
    MOVE BIN-RUN-DATE           TO DC-BIN-DATE-1.                
    MOVE +0                     TO DC-ELAPSED-DAYS.              
    MOVE -1                     TO DC-ELAPSED-MONTHS.            
    MOVE '6'                    TO DC-OPTION-CODE.               
    PERFORM 0400-DATE-CONVERSION-ROUTINE THRU 0409-EXIT.         
    MOVE DC-GREG-DATE-CYMD      TO WS-NCL-OB-DATES (2).          
    MOVE +0                     TO DC-ELAPSED-DAYS.              
    MOVE -2                     TO DC-ELAPSED-MONTHS.            
    MOVE '6'                    TO DC-OPTION-CODE.               
    PERFORM 0400-DATE-CONVERSION-ROUTINE THRU 0409-EXIT.         
    MOVE DC-GREG-DATE-CYMD      TO WS-NCL-OB-DATES (3).          
    MOVE +0                     TO DC-ELAPSED-MONTHS             
                                   DC-ELAPSED-DAYS.              
                                                                 
    DISPLAY '     '.                                             
    DISPLAY ' CURRENT MONTH DATE ' WS-NCL-OB-DATES (1).          
    DISPLAY ' CURRENT DATE - 1   ' WS-NCL-OB-DATES (2).          
    DISPLAY ' CURRENT DATE - 2   ' WS-NCL-OB-DATES (3).          
    DISPLAY '     '.                                             
                                                                 
    PERFORM 0102-CLEAR-OB THRU 0102-OB-EXIT                      
        VARYING SUB FROM +1 BY +1 UNTIL SUB GREATER THAN +20.    
                                                                 
    MOVE +1                     TO SUB.                          
                                                                 
    PERFORM 0106-LOAD-LIFE THRU 0106-EXIT                        
        VARYING CLAS-INDEXL FROM CLAS-STARTL BY +1               
          UNTIL CLAS-INDEXL GREATER THAN CLAS-MAXL.              
                                                                 
    MOVE +1                     TO SUB.                          
                                                                 
    PERFORM 0107-LOAD-AH THRU 0107-EXIT                          
        VARYING CLAS-INDEXA FROM CLAS-STARTA BY +1               
          UNTIL CLAS-INDEXA GREATER THAN CLAS-MAXA.              
                                                                 
    GO TO 0109-CONTINUE-INIT.                                    
                                                                 
0102-CLEAR-OB.                                                   
                                                                 
     MOVE ZEROS                 TO WS-LIFE-OB-CODE (1 SUB)       
                                   WS-LIFE-OB-CODE (2 SUB)       
                                   WS-LIFE-OB-CODE (3 SUB)       
                                   WS-AH-OB-CODE (1 SUB)         
                                   WS-AH-OB-CODE (2 SUB)         
                                   WS-AH-OB-CODE (3 SUB).        
     MOVE +0                    TO WS-LIFE-PREM (1 SUB)          
                                   WS-LIFE-PREM (2 SUB)          
                                   WS-LIFE-PREM (3 SUB)          
                                   WS-AH-PREM (1 SUB)            
                                   WS-AH-PREM (2 SUB)            
                                   WS-AH-PREM (3 SUB)            
                                   WS-LIFE-BEN (1 SUB)           
                                   WS-LIFE-BEN (2 SUB)           
                                   WS-LIFE-BEN (3 SUB)           
                                   WS-AH-BEN (1 SUB)             
                                   WS-AH-BEN (2 SUB)             
                                   WS-AH-BEN (3 SUB).            
                                                                 
0102-OB-EXIT.                                                    
    EXIT.                                                        
                                                                 
0103-CLEAR-OB.                                                   
                                                                 
     MOVE +0                    TO WS-LIFE-PREM (1 SUB)          
                                   WS-LIFE-PREM (2 SUB)          
                                   WS-LIFE-PREM (3 SUB)          
                                   WS-AH-PREM (1 SUB)            
                                   WS-AH-PREM (2 SUB)            
                                   WS-AH-PREM (3 SUB)            
                                   WS-LIFE-BEN (1 SUB)           
                                   WS-LIFE-BEN (2 SUB)           
                                   WS-LIFE-BEN (3 SUB)           
                                   WS-AH-BEN (1 SUB)             
                                   WS-AH-BEN (2 SUB)             
                                   WS-AH-BEN (3 SUB).            
                                                                 
0103-OB-EXIT.                                                    
    EXIT.                                                        
                                                                 
0106-LOAD-LIFE.                                                  
                                                                 
    IF CLAS-I-CALC-TYPE (CLAS-INDEXL) EQUAL 'B'                  
       MOVE CLAS-I-BEN (CLAS-INDEXL)                             
                                TO WS-LIFE-OB-CODE (1 SUB)       
                                   WS-LIFE-OB-CODE (2 SUB)       
                                   WS-LIFE-OB-CODE (3 SUB)       
       ADD +1 TO SUB.                                            
                                                                 
0106-EXIT.                                                       
    EXIT.                                                        
                                                                 
0107-LOAD-AH.                                                    
                                                                 
    IF CLAS-I-CALC-TYPE (CLAS-INDEXA) EQUAL 'B'                  
       MOVE CLAS-I-BEN (CLAS-INDEXA)                             
                                TO WS-AH-OB-CODE (1 SUB)         
                                   WS-AH-OB-CODE (2 SUB)         
                                   WS-AH-OB-CODE (3 SUB)         
       ADD +1 TO SUB.                                            
                                                                 
0107-EXIT.                                                       
    EXIT.                                                        
                                                                 
0109-CONTINUE-INIT.                                              
                                                                 
    IF DTE-CLIENT = 'DCC' or 'VPP'                               
       MOVE 0 TO P1                                              
       MOVE DTE-CLASIC-COMPANY-CD                                
                                TO PD-CONTROL-PRIMARY            
       START ERPDEF KEY >= PD-CONTROL-PRIMARY                    
       IF ERPDEF-FILE-STATUS = '00'                              
          PERFORM UNTIL ERPDEF-FILE-STATUS NOT = '00'            
             READ ERPDEF NEXT RECORD                             
             IF ERPDEF-FILE-STATUS = '00'                        
                ADD 1 TO P1                                      
                IF P1 > max-pdef                                 
                   DISPLAY ' DDF UEP TABLE BLEW '                
                   GO TO ABEND-PGM                               
                END-IF                                           
                MOVE PD-STATE   TO DD-STATE (P1)                 
                MOVE PD-PRODUCT-CD                               
                                TO DD-PRODUCT-CD (P1)            
                MOVE PD-BEN-TYPE                                 
                                TO DD-BEN-TYPE (P1)              
                MOVE PD-BEN-CODE                                 
                                TO DD-BEN-CODE (P1)              
                MOVE PD-PROD-EXP-DT                              
                                TO DD-PROD-EXP-DT (P1)           
                MOVE PD-1ST-YR-ADMIN-ALLOW                       
                                TO DD-1ST-YR-ALLOW (P1)          
                MOVE PD-TRUNCATED TO DD-TRUNCATED (P1)           
                PERFORM VARYING P2 FROM +1 BY +1 UNTIL           
                   P2 > +11                                      
                   MOVE PD-PROD-CODE (P2)                        
                                TO DD-PROD-CODE (P1 P2)          
                   move pd-max-att-age(p2)                       
                                to dd-max-att-age(p1 p2)         
                   move pd-ben-pct(p2)                           
                                to dd-ben-pct(p1 p2)             
                   MOVE PD-MAX-AMT (P2)                          
                                TO DD-MAX-BEN-AMT (P1 P2)        
                END-PERFORM                                      
                MOVE PD-EARN-FACTORS                             
                                TO DD-EARN-FACTORS (P1)          
             ELSE                                                
                IF ERPDEF-FILE-STATUS NOT = '10'                 
                   DISPLAY ' BAD RD NXT ' ERPDEF-FILE-STATUS     
                END-IF                                           
             END-IF                                              
          END-PERFORM                                            
       ELSE                                                      
          DISPLAY ' BAD START ERPDEF ' ERPDEF-FILE-STATUS        
       END-IF                                                    
       CLOSE ERPDEF                                              
    END-IF                                                       
                                                                 
    PERFORM CLEAR-REIN-HOLD THRU CLEAR-REIN-HOLD-X.              
    MOVE TOT-ACCUMS             TO ZERO-ACCUMS.                  
                                                                 
    MOVE SPACES                 TO ALPHA-RECORD.                 
    MOVE 'AX'                   TO AX-RECORD-ID.                 
    MOVE ZEROS                  TO AX-AGE   AX-APR  AX-PMT-FREQ  
                                   AX-LF-TYP        AX-AH-TYP    
                                   AX-LF-TERM       AX-AH-TERM   
                                   AX-LF-REMTERM    AX-AH-REMTERM
                                   AX-LF-AMT        AX-AH-AMT    
                                   AX-LF-REMAMT     AX-AH-REMAMT 
                                   AX-LF-PRM        AX-AH-PRM    
                                   AX-LF-REFUND     AX-AH-REFUND 
                                   AX-LF-CLAIM-PMTS AX-DT        
                                   AX-AH-CLAIM-PMTS AX-LF-CNCL   
                                   AX-CAN-MICROFILM-NO AX-DEATH  
                                   AX-ISS-MICROFILM-NO AX-LF-EXIT
                                   AX-LF-AMT-ALT    AX-LF-EXPIRES
                                   AX-LF-REMAMT-ALT AX-AH-CNCL   
                                   AX-LF-PRM-ALT    AX-LUMP-SUM  
                                   AX-AH-EXIT       AX-AH-EXPIRES
                                   AX-ENTRY.                     
                                                                 
    MOVE ALPHA-RECORD           TO INITIALIZED-ALPHA-RECORD.     
                                                                 
    MOVE SPACES            TO GAAP-RECORD.                       
    MOVE 'GR'              TO GR-RECORD-ID.                      
    MOVE ZEROS             TO GR-LFTYP          GR-AHTYP         
                              GR-APR            GR-PMT-FREQ      
                              GR-LOAN-TERM      GR-AGE           
                              GR-ACC-EXPIRES                     
                              GR-LF-TERM        GR-AH-TERM       
                              GR-LF-REMTERM     GR-AH-REMTERM    
                              GR-LF-UP-REMTERM  GR-AH-UP-REMTERM 
                              GR-LFBEN          GR-AHBEN         
                              GR-LFPRM          GR-AHPRM         
                              GR-LFCOM          GR-AHCOM         
                              GR-LFEXP          GR-AHEXP         
                              GR-LFTAX          GR-AHTAX         
                              GRP-LFPRM         GRP-AHPRM        
                              GRP-LFCOM         GRP-AHCOM        
                              GRP-LFEXP         GRP-AHEXP        
                              GRP-LFTAX         GRP-AHTAX        
                              GRR-LFPRM         GRR-AHPRM        
                              GRR-LFCOM         GRR-AHCOM        
                              GRR-LFEXP         GRR-AHEXP        
                              GRR-LFTAX         GRR-AHTAX        
                              GRD-LFPRM         GRD-AHPRM        
                              GRD-LFCOM         GRD-AHCOM        
                              GRS-LFPRM         GRS-AHPRM        
                              GRS-LFCOM         GRS-AHCOM        
                              GR-UNLD-STAT-MORT-RESV             
*>                              GR-LFSRV          GR-AHSRV         
                              GR-MORT-AGE       GR-REM-AMT       
                              GR-MO-DEC         GR-MORT-FACT     
                              GR-RESV           GR-ALT-RESV      
                              GR-CNT            GR-CNT-LF        
                              GR-CNT-AH         GR-AH-REM-BEN    
                              GR-DIR-CERT-LFBEN GR-EFF           
                              GR-ENT-DT         GR-LF-EXPIRE-DATE
                            GR-AH-EXPIRE-DATE gr-loaded-stat-uep.
                                                                 
    MOVE GAAP-RECORD       TO INITIALIZED-GAAP-RECORD.           
                                                                 
    PERFORM 0590-READ-AM-MSTR THRU 0598-EXIT.                    
                                                                 
0110-READ-CERT-MASTER.                                           
                                                                 
    READ CERT-MASTER                                             
             AT END  GO TO 9000-END-THE-JOB.                     
                                                                 
    IF CR-LFTYP = SPACES                                         
       MOVE ZEROS TO CR-LFTYP                                    
    END-IF                                                       
    IF CR-AHTYP = SPACES                                         
       MOVE ZEROS TO CR-AHTYP                                    
    END-IF                                                       
    IF DTE-CLIENT EQUAL 'HAN'                                    
       IF (CR-GROUPING EQUAL '097713' OR '010184')               
                          OR                                     
          ((CR-CARRIER EQUAL '2') AND                            
           (CR-DT GREATER THAN  19930630 ))                      
          GO TO 0110-READ-CERT-MASTER.                           
                                                                 
    IF DTE-CLIENT = 'NCL'                                        
       IF CR-ACCT-CONTROL NOT = WS-HOLD-AM-CONTROL  AND          
          WS-HOLD-AM-CONTROL NOT = LOW-VALUES                    
           MOVE CERTIFICATE-RECORD  TO WS-HOLD-CERT              
           PERFORM 0395-CHECK-OUT-OB THRU 0395-EXIT              
           MOVE WS-HOLD-CERT        TO CERTIFICATE-RECORD        
           PERFORM 0103-CLEAR-OB THRU 0103-OB-EXIT               
           VARYING SUB FROM +1 BY +1 UNTIL SUB GREATER THAN +20. 
                                                                 
    if dte-client = 'AHL'                                        
       if (cr-carrier = '8')                                     
          or (cr-account = '00DMREOH02'                          
                        or '00CSRG5104'                          
                        or '00CSRG5107'                          
                        or '00CSRG5108'                          
                        or '00FSS07116'                          
                        or '00CRS07121'                          
                        or '00CWA07101'                          
                        or '00AR537422'                          
                        or '00AR537430'                          
                        or '00AR537432'                          
                        or '00AR537434'                          
                        or '00AR537435'                          
                        or '00AR537437')                         
          continue                                               
       else                                                      
          perform varying c1 from +2 by +1 until                 
             (c1 > +10)                                          
             or (cr-com-agt (c1) = zeros or spaces)              
             inspect cr-agt-type (c1) converting                 
             'P' TO 'O'                                          
          end-perform                                            
       end-if                                                    
    end-if                                                       
                                                                 
*>    COPY ELCCRTM1.                                               
*>vvv-> file: /apps/prod/cid1p/copy/ELCCRTM1
*>*****************************************************************
*>                                                                *
*>                                                                *
*>                            ELCCRTM1                            *
*>           COPYBOOK REVIEWED FOR YEAR 2000 COMPLIANCE            
*>                            VMOD=2.001                          *
*>                                                                *
*>   COPY FILE VARIABLES TO WORKING STORAGE                       *
*>   FOR THE YEAR 2000 DATE MODIFICATION                          *
*>                                                                *
*>*****************************************************************
    MOVE CR-DT                   TO  WS-CR-DT-N.                 
    MOVE CR-LF-EXPIRE-DATE       TO  WS-CR-LF-EXPIRE-DATE-N.     
    MOVE CR-AH-EXPIRE-DATE       TO  WS-CR-AH-EXPIRE-DATE-N.     
    MOVE CR-ENTRY-DATE           TO  WS-CR-ENTRY-DATE-N.         
    MOVE CR-LF-CANC-DT           TO  WS-CR-LF-CANC-DT-N.         
    MOVE CR-LF-CANCEL-EXIT-DATE  TO  WS-CR-LF-CANCEL-EXIT-DATE-N.
    MOVE CR-LF-CLAIM-EXIT-DATE   TO  WS-CR-LF-CLAIM-EXIT-DATE-N. 
    MOVE CR-AH-CANC-DT           TO  WS-CR-AH-CANC-DT-N.         
    MOVE CR-AH-CANCEL-EXIT-DATE  TO                              
                                 WS-CR-AH-CANCEL-EXIT-DATE-N.    
    MOVE CR-AH-SETTLEMENT-EXIT-DATE TO                           
                                WS-CR-AH-SETTLEMENT-EXIT-DATEN.  
    MOVE CR-DTH-DT               TO  WS-CR-DTH-DT-N.             
    MOVE CR-DTH-PAY-DT           TO  WS-CR-DTH-PAY-DT-N.         
    MOVE CR-DIS-DT               TO  WS-CR-DIS-DT-N.             
    MOVE CR-DIS-PAY-DT           TO  WS-CR-DIS-PAY-DT-N.         
    MOVE CR-DIS-PTO-DT           TO  WS-CR-DIS-PTO-DT-N.         
    PERFORM  VARYING COPY-NDX FROM 1 BY 1 UNTIL COPY-NDX > 5     
          MOVE CR-DIS-INCUR-DT (COPY-NDX)   TO                   
                               WS-CR-DIS-INCUR-DT-N (COPY-NDX)   
    END-PERFORM.                                                 
                                                                 
*>^^^-> file: /apps/prod/cid1p/copy/ELCCRTM1
                                                                 
    MOVE CR-ACCT-CONTROL     TO WS-HOLD-AM-CONTROL.              
                                                                 
    IF CR-CARRIER NOT = PRIOR-CARRIER                            
        PERFORM 8000-CARRIER-BREAK-ROUTINE THRU 8999-EXIT.       
                                                                 
    IF DTE-CLIENT EQUAL 'FIM'                                    
       IF CR-REIN-TABLE EQUAL '38K'                              
          MOVE SPACES TO CR-REIN-TABLE.                          
                                                                 
    MOVE SPACES                 TO O-B-SWITCH                    
                                   SUMMARY-SWITCH                
                                   GR-REIN.                      
                                                                 
    ADD +1                      TO TOT-READ.                     
                                                                 
    IF CR-ENTRY-STATUS = '5' OR 'M'                              
        ADD +1                  TO TOT-REIS.                     
                                                                 
    IF CR-ENTRY-STATUS = 'D'                                     
        ADD +1                  TO TOT-DECLINED.                 
                                                                 
    IF CR-ENTRY-STATUS = 'V'                                     
        ADD +1                  TO TOT-VOIDED.                   
                                                                 
    IF CR-ENTRY-STATUS = '9'                                     
        ADD +1                  TO TOT-REIO.                     
                                                                 
    PERFORM 7000-REIN-ONLY-STATUS THRU 7599-EXIT.                
                                                                 
                                                                 
    PERFORM 0500-MATCH-TO-ACC     THRU 0599-EXIT.                
                                                                 
                                                                 
    MOVE CLAS-STARTL TO CLAS-INDEXL.                             
    MOVE CLAS-STARTA TO CLAS-INDEXA.                             
    MOVE CLAS-STARTS TO CLAS-INDEXS.                             
                                                                 
    MOVE SPACES TO LF-JOINT-SWITCH, AH-JOINT-SWITCH.             
                                                                 
0120-STATE-LOOKUP.                                               
    IF CR-STATE NOT = STATE-SUB (CLAS-INDEXS)                    
        IF CLAS-INDEXS NOT = CLAS-MAXS                           
            ADD +1 TO CLAS-INDEXS                                
            GO TO 0120-STATE-LOOKUP.                             
                                                                 
    IF CR-LFTYP = ZERO                                           
        MOVE ZERO TO CLAS-INDEXL                                 
        GO TO 0140-FIND-AH.                                      
                                                                 
0130-FIND-LIFE-LOOP.                                             
    IF CLAS-INDEXL GREATER CLAS-MAXL OR CLAS-STARTL = ZERO       
        DISPLAY 'LIFE BENEFIT ' CR-LFTYP ' NOT IN TABLE'         
        DISPLAY 'RETURN CODE - 0401'                             
        MOVE 0401 TO WS-RETURN-CODE                              
        GO TO 0130-LIFE-LOOP-EXIT.                               
*>**      GO TO ABEND-PGM.                                         
                                                                 
    IF CR-LFTYP NOT = CLAS-I-BEN (CLAS-INDEXL)                   
        ADD +1 TO CLAS-INDEXL                                    
        GO TO 0130-FIND-LIFE-LOOP.                               
                                                                 
    MOVE CLAS-I-JOINT (CLAS-INDEXL) TO LF-JOINT-SWITCH.          
                                                                 
0130-LIFE-LOOP-EXIT.                                             
    EXIT.                                                        
                                                                 
0140-FIND-AH.                                                    
    IF CR-AHTYP = ZERO                                           
        MOVE ZERO TO CLAS-INDEXA                                 
        GO TO 0160-CALC-REM-TERM.                                
                                                                 
0150-FIND-AH-LOOP.                                               
    IF CLAS-INDEXA GREATER CLAS-MAXA OR CLAS-STARTA = ZEROS      
        DISPLAY 'A&H BENEFIT ' CR-AHTYP ' NOT IN TABLE'          
        MOVE 0402 TO WS-RETURN-CODE                              
        GO TO ABEND-PGM.                                         
                                                                 
    IF CR-AHTYP NOT = CLAS-I-BEN (CLAS-INDEXA)                   
        ADD +1 TO CLAS-INDEXA                                    
        GO TO 0150-FIND-AH-LOOP.                                 
                                                                 
    MOVE CLAS-I-JOINT (CLAS-INDEXA) TO AH-JOINT-SWITCH.          
                                                                 
    IF AH-EARN-METHOD = SPACE                                    
        MOVE CLAS-I-EP (CLAS-INDEXA) TO AH-EARN-METHOD.          
                                                                 
0160-CALC-REM-TERM.                                              
                                                                 
    MOVE CR-DT                      TO DC-GREG-DATE-CYMD.        
    MOVE 'L'                        TO DC-OPTION-CODE.           
    PERFORM 0400-DATE-CONVERSION-ROUTINE THRU 0409-EXIT          
    MOVE DC-BIN-DATE-1              TO CP-CERT-EFF-DT            
                                       WS-BIN-CR-DT.             
                                                                 
    move low-values to ws-bin-lf-exp-dt                          
    move zeros      to ws-fnl-rem-term                           
    if dte-client = 'FNL'                                        
       if cr-pmt-freq = 1 or 2                                   
          MOVE CR-lf-expire-date TO DC-GREG-DATE-CYMD            
          MOVE 'L'               TO DC-OPTION-CODE               
          PERFORM 0400-DATE-CONVERSION-ROUTINE                   
                                 THRU 0409-EXIT                  
          if no-conversion-error                                 
             MOVE DC-BIN-DATE-1  TO ws-bin-lf-exp-dt             
                                    dc-bin-date-2                
             move ws-bin-val-dt  to dc-bin-date-1                
             move '1'            to dc-option-code               
             PERFORM 0400-DATE-CONVERSION-ROUTINE                
                                 THRU 0409-EXIT                  
             if no-conversion-error                              
                move dc-elapsed-months                           
                                 to ws-fnl-rem-term              
                if dc-odd-days-over > +14                        
                   add +1 to ws-fnl-rem-term                     
                end-if                                           
                display ' found annual ' cr-cert-no ' '          
                   ws-fnl-rem-term                               
             end-if                                              
          end-if                                                 
       end-if                                                    
    end-if                                                       
                                                                 
    IF CR-LOAN-1ST-PMT-DT NOT NUMERIC                            
        MOVE ZEROS                  TO CR-LOAN-1ST-PMT-DT.       
                                                                 
    MOVE LOW-VALUES             TO     CP-FIRST-PAY-DATE         
                                       ws-bin-1st-pmt-dt         
                                                                 
    IF CR-LOAN-1ST-PMT-DT NOT = ZEROS                            
        MOVE CR-LOAN-1ST-PMT-DT     TO DC-GREG-DATE-1-YMD        
        MOVE ZEROS                  TO DC-ELAPSED-MONTHS         
                                       DC-ELAPSED-DAYS           
        MOVE '3'                    TO DC-OPTION-CODE            
        PERFORM 0400-DATE-CONVERSION-ROUTINE THRU 0409-EXIT      
        IF NO-CONVERSION-ERROR                                   
            MOVE DC-BIN-DATE-1      TO CP-FIRST-PAY-DATE         
                                       ws-bin-1st-pmt-dt         
        ELSE                                                     
            MOVE ZEROS              TO CR-LOAN-1ST-PMT-DT.       
                                                                 
    IF CP-FIRST-PAY-DATE  LESS THAN  CP-CERT-EFF-DT              
        MOVE ZEROS  TO  CR-LOAN-1ST-PMT-DT.                      
                                                                 
    IF CR-LOAN-1ST-PMT-DT = ZEROS                                
        MOVE CP-CERT-EFF-DT         TO DC-BIN-DATE-1             
        MOVE +1                     TO DC-ELAPSED-MONTHS         
        MOVE ZEROS                  TO DC-ELAPSED-DAYS           
        MOVE '6'                    TO DC-OPTION-CODE            
