    MOVE CP-R78-U-PRM               TO GRR-AHCOM.                
    MOVE CP-PRORATA-U-PRM           TO GRP-AHCOM.                
    MOVE CP-STATE-U-PRM             TO GRS-AHCOM.                
    MOVE CP-DOMICILE-U-PRM          TO GRD-AHCOM.                
                                                                 
    IF DTE-CLIENT = 'NCL'                                        
        MOVE GRP-AHCOM              TO GRD-AHCOM                 
        IF GR-STATE = 'NM'  OR  'WI'                             
            COMPUTE GRS-AHCOM = ((GRR-AHCOM + GRP-AHCOM) / 2)    
        ELSE                                                     
            MOVE GRR-AHCOM          TO GRS-AHCOM.                
                                                                 
    IF DTE-CLIENT = 'NCL'                                        
       IF GR-EFF GREATER THAN 19941231                           
          COMPUTE GRD-AHCOM = ((GRR-AHCOM + GRP-AHCOM) / 2).     
                                                                 
    IF (DTE-CLIENT = 'DCC' or 'VPP')                             
       AND (AM-DCC-PRODUCT-CODE NOT = 'DDF')                     
       IF CP-SPECIAL-CALC-CD = 'C'                               
          MOVE GRP-AHCOM        TO GRS-AHCOM                     
