          Count(*)                                               
       INTO                                                      
          :TB-MONTH-END-DT,                                      
          :WS-REC-CNTR                                           
       FROM                                                      
          DCC_CU                                                 
       GROUP BY MONTH_END_DT                                     
       HAVING convert(varchar(10),MONTH_END_DT,102)              
             = :ws-test-date                                     
