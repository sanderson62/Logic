*>-----------------------------------------------------------------
1021-ANTICIPATION-REF.                                           
    MOVE '6'                  TO CP-REFUND-TYPE-USED.            
                                                                 
    MOVE CP-LOAN-TERM         TO WS-SAVE-LOAN-TERM.              
    IF CP-LOAN-TERM = ZEROS                                      
       MOVE CP-ORIGINAL-TERM TO CP-LOAN-TERM                     
    END-IF                                                       
                                                                 
    COMPUTE CP-LOAN-TERM = CP-LOAN-TERM - CP-ORIGINAL-TERM +     
                           CP-REMAINING-TERM.                    
                                                                 
    MOVE CP-ORIGINAL-BENEFIT  TO WS-SAVE-BENEFIT.                
    MOVE CP-ORIGINAL-TERM     TO WS-SAVE-TERM.                   
    MOVE CP-EARNING-METHOD    TO WS-SAVE-EARN-METHOD.            
    move cp-term-or-ext-days  to ws-save-ext-days                
                                                                 
    MOVE CP-REMAINING-BENEFIT TO CP-ORIGINAL-BENEFIT.            
    MOVE CP-REMAINING-TERM    TO CP-ORIGINAL-TERM.               
    MOVE CP-RATING-METHOD     TO CP-EARNING-METHOD.              
                                                                 
    IF CP-REMAINING-TERM = WS-SAVE-TERM                          
        MOVE 'N'              TO CP-ROA-REFUND                   
    ELSE                                                         
        MOVE 'Y'              TO CP-ROA-REFUND.                  
                                                                 
*>*****    TO COMPUTE PREMIUM AT REFUND DATE                       
                                                                 
    PERFORM 2500-GET-RATE.                                       
                                                                 
    MOVE 'N'                  TO CP-ROA-REFUND.                  
    MOVE WS-SAVE-BENEFIT      TO CP-ORIGINAL-BENEFIT.            
    MOVE WS-SAVE-TERM         TO CP-ORIGINAL-TERM.               
    MOVE WS-SAVE-LOAN-TERM    TO CP-LOAN-TERM.                   
    move ws-save-ext-days     to cp-term-or-ext-days             
                                                                 
    IF CP-PREMIUM-RATE = ZEROS                                   
       MOVE '7'                 TO CP-RETURN-CODE                
       MOVE WS-SAVE-EARN-METHOD TO CP-EARNING-METHOD             
       GO TO 1999-CALC-REF-AMT-X.                                
                                                                 
    MOVE CP-CALC-PREMIUM         TO WS-REMAIN-PREM.              
                                                                 
*>*****    TO COMPUTE PREMIUM AT ORIGINAL ISSUE DATE               
                                                                 
    PERFORM 2500-GET-RATE.                                       
                                                                 
    MOVE WS-SAVE-EARN-METHOD     TO CP-EARNING-METHOD.           
                                                                 
    IF CP-PREMIUM-RATE = ZEROS                                   
       MOVE '7'            TO CP-RETURN-CODE                     
       GO TO 1999-CALC-REF-AMT-X.                                
                                                                 
    MOVE CP-CALC-PREMIUM         TO WS-ORIG-PREM.                
                                                                 
    IF  WS-ORIG-PREM = ZEROS                                     
        MOVE ZEROS TO WS-NP-REF-FACTOR                           
       ELSE                                                      
        COMPUTE WS-NP-REF-FACTOR ROUNDED = WS-REMAIN-PREM /      
                                           WS-ORIG-PREM.         
                                                                 
    COMPUTE CP-CALC-REFUND ROUNDED = WS-NP-REF-FACTOR *          
                                       CP-ORIGINAL-PREMIUM.      
                                                                 
    GO TO 1999-CALC-REF-AMT-X.                                   
                                                                 
*>---------------------------------------------------------------  
1022-REF-CRITICAL-PERIOD-LF.                                     
    GO TO 1023-REF-PR.                                           
                                                                 
*>---------------------------------------------------------------  
1023-REF-PR.                                                     
    MOVE '2'               TO CP-REFUND-TYPE-USED.               
    COMPUTE CP-CALC-REFUND ROUNDED =                             
           CP-ORIGINAL-PREMIUM * PR-FACTOR.                      
                                                                 
    GO TO 1999-CALC-REF-AMT-X.                                   
                                                                 
*>---------------------------------------------------------------  
1024-REF-PR-DAYS.                                                
    MOVE CP-CERT-EFF-DT     TO DC-BIN-DATE-1.                    
    MOVE CP-VALUATION-DT    TO DC-BIN-DATE-2.                    
    MOVE '1'                TO DC-OPTION-CODE.                   
    PERFORM 9100-CONVERT-DATE THRU 9100-EXIT.                    
                                                                 
    IF DATE-CONVERSION-ERROR                                     
        MOVE '2' TO CP-RETURN-CODE                               
        GO TO 1999-CALC-REF-AMT-X.                               
                                                                 
    COMPUTE CP-CALC-REFUND ROUNDED =                             
        (CP-ORIGINAL-PREMIUM / CP-TERM-OR-EXT-DAYS) *            
                (CP-TERM-OR-EXT-DAYS - DC-ELAPSED-DAYS).         
                                                                 
    GO TO 1999-CALC-REF-AMT-X.                                   
                                                                 
*>---------------------------------------------------------------  
1025-REF-MEAN.                                                   
    MOVE '8'               TO CP-REFUND-TYPE-USED.               
    COMPUTE CP-CALC-REFUND ROUNDED =                             
         ((CP-ORIGINAL-PREMIUM * PR-FACTOR) +                    
          (CP-ORIGINAL-PREMIUM * R78-FACTOR)) * +.5.             
                                                                 
    GO TO 1999-CALC-REF-AMT-X.                                   
                                                                 
*>---------------------------------------------------------------  
1026-REF-R78.                                                    
    MOVE '1'               TO CP-REFUND-TYPE-USED.               
*>    IF CP-TERM-TIMES-TERM                                        
*>        COMPUTE R78-FACTOR =                                     
*>            (CP-REMAINING-TERM * CP-REMAINING-TERM) /            
*>               (CP-ORIGINAL-TERM * (CP-ORIGINAL-TERM + 1)).      
*>                                                                 
    COMPUTE CP-CALC-REFUND ROUNDED =                             
       CP-ORIGINAL-PREMIUM * R78-FACTOR.                         
                                                                 
    GO TO 1999-CALC-REF-AMT-X.                                   
                                                                 
*>---------------------------------------------------------------  
1027-TEXAS-REFUND.                                               
    MOVE '4'               TO CP-REFUND-TYPE-USED.               
    IF CP-PAY-FREQUENCY = ZERO                                   
       MOVE ZERO TO CP-CALC-REFUND                               
       MOVE '5'  TO CP-RETURN-CODE                               
       GO TO 1999-CALC-REF-AMT-X.                                
                                                                 
    COMPUTE TEX-FACT-4 = (CP-ORIGINAL-TERM * CP-ORIGINAL-TERM) + 
        (CP-PAY-FREQUENCY * CP-ORIGINAL-TERM).                   
                                                                 
    DIVIDE CP-REMAINING-TERM BY CP-PAY-FREQUENCY                 
        GIVING TEX-FACT-5                                        
            REMAINDER TEX-FACT-6.                                
                                                                 
    COMPUTE TEX-FACT-5 = TEX-FACT-5 * CP-PAY-FREQUENCY.          
                                                                 
    COMPUTE TEX-FACT-7 = (TEX-FACT-5 * TEX-FACT-5) +             
        (TEX-FACT-5 * CP-PAY-FREQUENCY) +                        
        (2 * (TEX-FACT-6 * (TEX-FACT-5 + CP-PAY-FREQUENCY))).    
                                                                 
    COMPUTE TEX-FACT-8 ROUNDED = TEX-FACT-7 / TEX-FACT-4.        
                                                                 
    COMPUTE CP-CALC-REFUND ROUNDED = CP-ORIGINAL-PREMIUM         
                                       * TEX-FACT-8.             
                                                                 
    GO TO 1999-CALC-REF-AMT-X.                                   
                                                                 
*>---------------------------------------------------------------  
1028-NET-PAY-REFUND.                                             
                                                                 
    MOVE '5'               TO CP-REFUND-TYPE-USED.               
    IF CP-LOAN-APR = ZERO                                        
       move '1'                 to cp-refund-type-used           
       compute cp-calc-refund rounded =                          
          cp-original-premium * r78-factor                       
       go to 1999-calc-ref-amt-x                                 
