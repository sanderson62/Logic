       AND (CP-STATE-STD-ABBRV = 'IN')                           
       AND (DC-GREG-DATE-CYMD < 20030401)                        
       GO TO 1026-REF-R78                                        
    END-IF                                                       
                                                                 
    IF (CP-COMPANY-ID = 'CID')                                   
       AND (CP-STATE-STD-ABBRV = 'FL')                           
       AND (DC-GREG-DATE-CYMD > 20030630)                        
