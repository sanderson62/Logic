       ELSE                                                      
          COMPUTE CP-CALC-REFUND = CP-ORIGINAL-PREMIUM -         
             CP-DDF-ADMIN-FEES                                   
       END-IF                                                    
       MOVE 'R'                 TO CP-REFUND-TYPE-USED           
       GO TO 1999-CALC-REF-AMT-X                                 
    END-IF                                                       
                                                                 
    IF CP-REMAINING-TERM = ZERO   OR                             
       CP-REMAINING-TERM NEGATIVE                                
         MOVE ZERO TO CP-CALC-REFUND                             
         GO TO 1999-CALC-REF-AMT-X.                              
                                                                 
    MOVE CP-CERT-EFF-DT     TO DC-BIN-DATE-1.                    
    MOVE SPACE              TO DC-OPTION-CODE.                   
    PERFORM 9100-CONVERT-DATE THRU 9100-EXIT.                    
                                                                 
    IF DATE-CONVERSION-ERROR                                     
        MOVE '2' TO CP-RETURN-CODE                               
        GO TO 1999-CALC-REF-AMT-X.                               
                                                                 
    IF CP-AH                                                     
       GO TO 1030-GET-AH-REFUND.                                 
                                                                 
*>*******************    LIFE REFUNDS    **************************
                                                                 
    IF CP-TERM-OR-EXT-DAYS NOT NUMERIC                           
        MOVE +0                TO CP-TERM-OR-EXT-DAYS.           
                                                                 
    IF CP-LOAN-TERM NOT NUMERIC                                  
        MOVE +0                TO CP-LOAN-TERM.                  
                                                                 
    COMPUTE PR-FACTOR = CP-REMAINING-TERM / CP-ORIGINAL-TERM.    
                                                                 
    COMPUTE R78-FACTOR =                                         
           (CP-REMAINING-TERM * (CP-REMAINING-TERM + 1)) /       
             (CP-ORIGINAL-TERM * (CP-ORIGINAL-TERM + 1)).        
