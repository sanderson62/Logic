    MOVE '3'               TO CP-REFUND-TYPE-USED.               
                                                                 
    IF (CP-ORIGINAL-TERM - CP-REMAINING-TERM)                    
       GREATER ZERO AND LESS 13                                  
        COMPUTE CP-CALC-REFUND = CP-CALC-REFUND -                
                (CP-CALC-REFUND * .05).                          
                                                                 
    IF (CP-ORIGINAL-TERM - CP-REMAINING-TERM)                    
       GREATER 12 AND LESS 25                                    
