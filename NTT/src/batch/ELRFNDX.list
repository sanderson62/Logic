         ** ((M - WS-COUNTER + 1) * -1))) / J                    
       COMPUTE WK2 = (1 / (1 + j)) ** (M - WS-COUNTER + 1)       
       COMPUTE WK5-CSL = RA * (ANGLEM + (ANGLEN-M * WK2))        
       COMPUTE WK3 = WK3 + (WK5-CSL * CP-DCC-LF-RATE / +1000)    
          + (WK5-CSL * CP-DCC-AH-RATE / +1000)                   
    END-PERFORM                                                  
                                                                 
    MOVE WK3                    TO CP-CALC-PREMIUM               
                                                                 
    GO TO 99999-RETURN                                           
                                                                 
    .                                                            
12001-CHECK-DCC-MNET-TOT-S.                                      
                                                                 
    IF (CP-COMPANY-ID EQUAL 'DCC' OR 'VPP')                      
       AND (CP-EARN-AS-NET-PAY)                                  
       AND (CP-BENEFIT-CD (1:1) = 'S')                           
       AND (CP-BEN-CATEGORY = 'N')                               
       CONTINUE                                                  
    ELSE                                                         
       GO TO 12001-CONTINUE-NET                                  
    END-IF                                                       
                                                                 
*>    I = APR/1200                                                 
*>    J = APR/1200 THEN ADJUSTED FOR THE LIFE RATE (K)             
*>    RA = PAYMENT AMOUNT                                          
*>    WK5-CSL = THE BALANCE (PAY OFF )                             
*>    WK3 = THE ACCUMULATED PREMIUM                                
*>    WK2 = (1 + I) ^ (m * -1)                                     
*>    OD  = GAMMA                                                  
*>                                                                 
    COMPUTE J = I + (CP-DCC-LF-RATE / +1000)                     
                                                                 
    COMPUTE OD = ((1 + (CP-DAYS-TO-1ST-PMT * J / 30)))           
       / (1 + J)                                                 
                                                                 
    COMPUTE ANGLEM = ((1 - ((1 + J)                              
         ** (M * -1))) / J) / OD                                 
                                                                 
    COMPUTE ANGLEN-M = ((1 - ((1 + I)                            
         ** ((N - M) * -1))) / I)                                
         * (1 + J) ** (-1 * M)                                   
                                                                 
    COMPUTE ANGLEN-M = ANGLEN-M / OD                             
                                                                 
    MOVE CP-MONTHLY-PAYMENT     TO RA                            
                                                                 
    COMPUTE WK1 = (M + 1 / OD * (CP-DAYS-TO-1ST-PMT - +30)       
       / +30)                                                    
    COMPUTE WK4 = (((M - ANGLEM) / J) + (ANGLEN-M * ((1 + J)     
       ** (WK1) - 1)) / J) * RA                                  
                                                                 
    COMPUTE WK5 = (((((M * (M + 1) / 2)                          
       + (CP-DAYS-TO-1ST-PMT - +30)                              
       / +30 * M - (M - ANGLEM) / J) / J)) * RA *                
       CP-DCC-AH-RATE / +1000 * 12 / 12)                         
                                                                 
    COMPUTE WK3 = (WK4 - WK5) * (CP-DCC-LF-RATE / +1000) *       
       (12 / 12)                                                 
                                                                 
    COMPUTE WK5-CSL = RA * (M * (M + 1) / 2 + M *                
       (CP-DAYS-TO-1ST-PMT - +30) / +30) *                       
       (CP-DCC-AH-RATE / +1000) * 12 / 12                        
                                                                 
    COMPUTE WK3 = WK3 + WK5-CSL                                  
    MOVE WK3                    TO CP-CALC-PREMIUM               
                                                                 
    COMPUTE K = I + (CP-DCC-LF-RATE / +1000)                     
    COMPUTE Y = (1 + (CP-DAYS-TO-1ST-PMT * K / 30))              
       / (1 + K)                                                 
                                                                 
    COMPUTE ANGLEN-M = ((1 - ((1 + I)                            
         ** ((N - M) * -1))) / I)                                
    COMPUTE WK2 = (1 + K) ** (M * -1)                            
    COMPUTE ANGLEN-M = ANGLEN-M * WK2                            
    COMPUTE ANGLEN-M = ANGLEN-M / Y                              
                                                                 
                                                                 
    COMPUTE ANGLEM = ((1 - ((1 + K)                              
         ** (M * -1))) / K) / Y                                  
                                                                 
*>    COMPUTE RA = CP-ORIGINAL-BENEFIT / (ANGLEM + ANGLEN-M)       
                                                                 
    COMPUTE WK6 = (((M - ANGLEM) / K) + (ANGLEN-M *              
       ((1 + K) ** (M + 1 / Y                                    
       * (CP-DAYS-TO-1ST-PMT - +30) / 30)                        
       - 1)) / K) * RA                                           
    COMPUTE WK7 = (((((M * (M + 1) / 2)                          
       + (CP-DAYS-TO-1ST-PMT - +30)                              
       / 30 * M - (M - ANGLEM) / K) / K)) * RA *                 
       CP-DCC-AH-RATE / 1000 * 12 / 12)                          
    COMPUTE WK3 = (WK6 - WK7) * (CP-DCC-LF-RATE / 1000)          
       * (12 / 12)                                               
                                                                 
                                                                 
    MOVE WK3                    TO CP-LF-PREM                    
                                                                 
                                                                 
                                                                 
    GO TO 99999-RETURN                                           
                                                                 
    .                                                            
12001-CONTINUE-NET.                                              
                                                                 
    IF NET-STD                                                   
       IF CP-COMPANY-ID EQUAL 'CDC' OR 'GTL' OR 'CID' OR 'MON'   
*>          OR 'DCC'                                               
        COMPUTE WK3 ROUNDED = ((I * M) + VX - SX) * 2 * N        
        COMPUTE WK3 ROUNDED = WK3 / ((1 - VX) * M * I)           
        COMPUTE WK3 ROUNDED = WK3 / ((2 * N) - M + 1)            
        COMPUTE WK3 ROUNDED = (WK3 * RA * K-I)                   
                              * ((1 + N) / (1 + M))              
        if (i = .0000000001)                                     
           and (wk3 = zeros)                                     
           move 1               to wk3                           
        end-if                                                   
       ELSE                                                      
        COMPUTE WK3 ROUNDED = ((I * M) + VX - SX) * 2 * N        
        COMPUTE WK3 ROUNDED = WK3 / ((1 - VX) * M * I)           
        COMPUTE WK3 ROUNDED = WK3 / ((2 * N) - M + 1)            
        COMPUTE WK3 ROUNDED = WK3 * RA * K-I.                    
                                                                 
    IF NET-SMP                                                   
        COMPUTE N2 = N * N                                       
        COMPUTE N3 = N2 * N                                      
        COMPUTE WK3 ROUNDED = 2 * N2 * WK1                       
        COMPUTE WK3 ROUNDED = WK3 + (N3 * I) - (N2 * I)          
        COMPUTE WK3 ROUNDED = WK3 + (4 * N * WK1)                
        COMPUTE WK3 ROUNDED = WK3 * (1 + I) * 10                 
        COMPUTE WK3 ROUNDED = WK3 / (36 * (N + 1) * WK1).        
                                                                 
    IF NPO-PRUDENTIAL                                            
      PERFORM 12100-PRU-CALC THRU 12199-EXIT                     
          VARYING WS-COUNTER FROM 1 BY 1                         
              UNTIL WS-COUNTER = N                               
      COMPUTE WK3 ROUNDED = (N * (I * PWK / (PWK - 1))) - 1      
      COMPUTE WK3 ROUNDED = WK3 / (I * 12).                      
                                                                 
    MOVE WK3 TO FACTOR.                                          
                                                                 
                                                                 
    GO TO 99999-RETURN.                                          
                                                                 
12100-PRU-CALC.                                                  
    IF WS-COUNTER = 1                                            
        COMPUTE PWK = (I + 1)                                    
    ELSE                                                         
        COMPUTE PWK = PWK * (I + 1).                             
                                                                 
12199-EXIT.                                                      
     EXIT.                                                       
                                                                 
99000-ERROR.                                                     
    MOVE +9999.999999999 TO N-P-FACTOR.                          
    MOVE '4'             TO CP-RETURN-CODE.                      
                                                                 
    GO TO 99999-EXIT.                                            
                                                                 
99999-RETURN.                                                    
    MOVE FACTOR          TO N-P-FACTOR.                          
                                                                 
99999-EXIT.                                                      
     EXIT.                                                       
