              MOVE CR-LF-CURRENT-STATUS TO CR-LF-STATUS-AT-CANCEL
              MOVE '6' TO CR-LF-CURRENT-STATUS                   
              MOVE INCURRED-DATE TO CR-LF-CANC-DT                
                                    WS-CR-LF-CANC-DT-N           
              MOVE EOM-RUN-DATE TO CR-LF-CANCEL-EXIT-DATE        
                                   WS-CR-LF-CANCEL-EXIT-DATE-N   
           ELSE                                                  
              MOVE CR-LF-CURRENT-STATUS TO CR-LF-STATUS-AT-DEATH 
              MOVE '7'            TO CR-LF-CURRENT-STATUS        
              MOVE EOM-RUN-DATE   TO CR-LF-CLAIM-EXIT-DATE       
                                   WS-CR-LF-CLAIM-EXIT-DATE-N.   
                                                                 
    MOVE CLAS-I-AB3 (CLAS-INDEXL)     TO TRD-LFTYP.              
    MOVE CR-LF-TERM                   TO TRD-LFTRM.              
    MOVE PC-CLAIM-PAYMENT        TO  TRD-LFCOM-CLM.              
    IF CLAS-I-RL-AH (CLAS-INDEXL) = 'P'                          
        MOVE 'PROPERTY  '       TO TRD-LF-ACTION                 
    ELSE                                                         
        MOVE 'CLAIM PYMT'       TO TRD-LF-ACTION.                
    MOVE CR-LF-CURRENT-STATUS   TO TRD-LF-STATUS.                
                                                                 
    IF NOT CR-POLICY-IS-REIN-ONLY                                
        ADD PC-CLAIM-PAYMENT    TO TPL-CLAIMS (1).               
                                                                 
0720-CANCEL-AH-COVERAGE.                                         
                                                                 
    IF CR-AHTYP = ZEROS  OR                                      
       NOT CR-AH-POLICY-IS-ACTIVE                                
        GO TO 0740-DEATH-CLAIM-VOID.                             
                                                                 
    MOVE CR-AH-CURRENT-STATUS          TO CR-AH-STATUS-AT-CANCEL.
    MOVE '7'                           TO CR-AH-CURRENT-STATUS.  
    MOVE CR-DTH-DT                     TO CR-AH-CANC-DT          
                                          WS-CR-AH-CANC-DT-N.    
    MOVE EOM-RUN-DATE               TO CR-AH-CANCEL-EXIT-DATE    
                                    WS-CR-AH-CANCEL-EXIT-DATE-N. 
                                                                 
    MOVE CLAS-I-AB3 (CLAS-INDEXA)      TO TRD-AHTYP.             
    MOVE CR-AH-TERM                    TO TRD-AHTRM.             
    MOVE CR-DTH-MO                     TO TRD-AHACTMO.           
    MOVE CR-DTH-DA                     TO TRD-AHACTDA.           
    MOVE CR-DTH-YR                     TO TRD-AHACTYR.           
    MOVE '/'                           TO TRD-AH-S1  TRD-AH-S2.  
    MOVE 'CLAIM CANC'                  TO TRD-AH-ACTION.         
    MOVE CR-AH-CURRENT-STATUS          TO TRD-AH-STATUS.         
                                                                 
0740-DEATH-CLAIM-VOID.                                           
                                                                 
    IF CR-DTHAMT = ZERO  AND                                     
       PC-CLAIM-PAYMENT NEGATIVE                                 
        NEXT SENTENCE                                            
    ELSE                                                         
        GO TO 0780-END-CLAIMS-PROCESSING.                        
                                                                 
    MOVE '*'                           TO DEATH-VOID-FLAG.       
                                                                 
    MOVE ZEROS                         TO CR-LF-CLAIM-EXIT-DATE  
                                       WS-CR-LF-CLAIM-EXIT-DATE-N
                                          CR-NUM-DTH-CLM         
                                          CR-DTH-RPT-DT          
                                          CR-DTH-PAY-DT          
                                          WS-CR-DTH-PAY-DT-N     
*>                                          CR-DTHAMT-YTD          
*>                                          CR-DTHAMT-LAST         
                                          CR-DTH-AGE.            
    MOVE ' '                           TO CR-DTH-PAY-CD.         
                                                                 
    IF CR-LF-DEATH-CLAIM-APPLIED                                 
        MOVE CR-LF-STATUS-AT-DEATH     TO CR-LF-CURRENT-STATUS   
        MOVE ' '                       TO CR-LF-STATUS-AT-DEATH  
    ELSE                                                         
        MOVE CR-LF-STATUS-AT-DEATH     TO CR-LF-STATUS-AT-CANCEL 
        MOVE ' '                       TO CR-LF-STATUS-AT-DEATH. 
                                                                 
    MOVE 'VOID CLAIM'                  TO TRD-LF-ACTION.         
    MOVE CR-LF-CURRENT-STATUS          TO TRD-LF-STATUS.         
                                                                 
    IF CR-AH-DEATH-CLAIM-APPLIED                                 
        MOVE CR-AH-STATUS-AT-CANCEL    TO CR-AH-CURRENT-STATUS   
        MOVE ' '                       TO CR-AH-STATUS-AT-CANCEL 
        MOVE ZEROS                     TO CR-AH-CANC-DT          
                                          WS-CR-AH-CANC-DT-N     
                                          CR-AH-CANCEL-EXIT-DATE 
                                      WS-CR-AH-CANCEL-EXIT-DATE-N
        MOVE CLAS-I-AB3 (CLAS-INDEXA)  TO TRD-AHTYP              
        MOVE CR-AH-TERM                TO TRD-AHTRM              
        MOVE 'VOID CANCL'              TO TRD-AH-ACTION          
        MOVE CR-AH-CURRENT-STATUS      TO TRD-AH-STATUS.         
                                                                 
    GO TO 0780-END-CLAIMS-PROCESSING.                            
                                                                 
*>*****************************************************************
*>**       P R O C E S S   D I S A B I L I T Y   C L A I M      ***
*>*****************************************************************
                                                                 
0750-PROCESS-DISABILITY-CLAIM.                                   
                                                                 
    ADD +1                       TO W-AH-CLAIMS.                 
                                                                 
    MOVE INCURRED-DATE           TO CR-DIS-DT                    
                                    WS-CR-DIS-DT-N.              
                                                                 
    PERFORM 0790-TABLE-DISAB-PERIODS THRU 0791-TABLE-AH-EXIT.    
                                                                 
    IF REIN-AH-CLM-MONTHS = +1                                   
        MOVE REIN-AH-CLM-MTH-1        TO REIN-AH-CLM-MONTHS      
    ELSE                                                         
      IF REIN-AH-CLM-MONTHS = +2                                 
          MOVE REIN-AH-CLM-MTH-2      TO REIN-AH-CLM-MONTHS      
      ELSE                                                       
        IF REIN-AH-CLM-MONTHS = +3                               
            MOVE REIN-AH-CLM-MTH-3    TO REIN-AH-CLM-MONTHS      
        ELSE                                                     
          IF REIN-AH-CLM-MONTHS = +4                             
              MOVE REIN-AH-CLM-MTH-4  TO REIN-AH-CLM-MONTHS      
          ELSE                                                   
              MOVE REIN-AH-CLM-MTH-5  TO REIN-AH-CLM-MONTHS.     
                                                                 
    IF PC-CHARGEBLE-EXPENSE  OR                                  
       PC-NON-CHARGEBLE-EXPENSE                                  
        IF CR-AHTYP = '00'                                       
            MOVE '99'             TO CR-AHTYP  CLAS-LOOK         
            PERFORM 8400-A-H-TYPE-LOOKUP THRU                    
                    8499-A-H-LOOKUP-X.                           
                                                                 
    IF PC-CHARGEBLE-EXPENSE  OR                                  
       PC-NON-CHARGEBLE-EXPENSE                                  
        ADD PC-CLAIM-PAYMENT      TO CR-DISEXP                   
        ADD PC-CLAIM-PAYMENT      TO CR-DISEXP-YTD               
        ADD PC-CLAIM-PAYMENT      TO TPA-CLAIMS (1)              
        MOVE PC-CLAIM-PAYMENT     TO TRD-AHCOM-CLM               
        MOVE 'CLAIM EXP '         TO TRD-AH-ACTION               
        GO TO 0780-END-CLAIMS-PROCESSING.                        
                                                                 
    ADD +1                       TO CR-NUM-DIS-CLM.              
                                                                 
    MOVE REPORTED-DATE           TO  CR-DIS-RPT-DT.              
    MOVE PAYMENT-DATE            TO  CR-DIS-PAY-DT               
                                     WS-CR-DIS-PAY-DT-N.         
    MOVE PAY-TO-DATE             TO  CR-DIS-PTO-DT               
                                     WS-CR-DIS-PTO-DT-N.         
                                                                 
    ADD  PC-CLAIM-PAYMENT         TO CR-DISAMT.                  
    ADD  PC-CLAIM-PAYMENT         TO CR-DISAMT-YTD.              
    MOVE PC-CLAIM-PAYMENT         TO CR-DISAMT-LAST.             
                                                                 
    MOVE PC-NO-OF-DAYS-PAID       TO CR-DAYS-DISAB.              
    IF CR-DIS-PAY-CD NOT = 'S'                                   
        MOVE PC-PAYMENT-TYPE      TO CR-DIS-PAY-CD.              
    INSPECT CR-DIS-PAY-CD                                        
        CONVERTING '12349' TO 'PFSXV'.                           
    MOVE PC-CAUSE-CODE            TO CR-DISAB-CAUSE.             
                                                                 
    IF PC-LUMP-SUM-PAYMENT                                       
        IF CR-AH-SETTLEMENT-EXIT-DATE = ZEROS                    
          MOVE CR-AH-CURRENT-STATUS TO CR-AH-STATUS-AT-SETTLEMENT
          MOVE '6'                  TO CR-AH-CURRENT-STATUS      
          MOVE EOM-RUN-DATE    TO CR-AH-SETTLEMENT-EXIT-DATE     
                                  WS-CR-AH-SETTLEMENT-EXIT-DATEN.
                                                                 
    MOVE CLAS-I-AB3 (CLAS-INDEXA) TO TRD-AHTYP.                  
    MOVE CR-AH-TERM               TO TRD-AHTRM.                  
    MOVE PC-CLAIM-PAYMENT         TO TRD-AHCOM-CLM.              
    MOVE 'CLAIM PYMT'             TO TRD-AH-ACTION.              
    MOVE CR-AH-CURRENT-STATUS     TO TRD-AH-STATUS.              
                                                                 
    IF NOT CR-POLICY-IS-REIN-ONLY                                
        ADD PC-CLAIM-PAYMENT      TO TPA-CLAIMS (1).             
                                                                 
    IF NOT PC-LUMP-SUM-PAYMENT                                   
        GO TO 0780-END-CLAIMS-PROCESSING.                        
                                                                 
0770-CANCEL-LF-COVERAGE.                                         
                                                                 
    IF CR-LFTYP = ZEROS  OR                                      
       NOT CR-LF-POLICY-IS-ACTIVE                                
        GO TO 0780-END-CLAIMS-PROCESSING.                        
                                                                 
    MOVE CR-LF-CURRENT-STATUS          TO CR-LF-STATUS-AT-CANCEL.
    MOVE '6'                           TO CR-LF-CURRENT-STATUS.  
    MOVE CR-DIS-PAY-DT                 TO CR-LF-CANC-DT          
                                          WS-CR-LF-CANC-DT-N.    
    MOVE EOM-RUN-DATE               TO CR-LF-CANCEL-EXIT-DATE    
                                    WS-CR-LF-CANCEL-EXIT-DATE-N. 
                                                                 
    MOVE CLAS-I-AB3 (CLAS-INDEXL)      TO TRD-LFTYP.             
    MOVE CR-LF-TERM                    TO TRD-LFTRM.             
    MOVE CR-DIS-PAY-MO                 TO TRD-LFACTMO.           
    MOVE CR-DIS-PAY-DA                 TO TRD-LFACTDA.           
    MOVE CR-DIS-PAY-YR                 TO TRD-LFACTYR.           
    MOVE '/'                           TO TRD-LF-S1  TRD-LF-S2.  
    MOVE 'CLAIM CANC'                  TO TRD-LF-ACTION.         
    MOVE CR-LF-CURRENT-STATUS          TO TRD-LF-STATUS.         
                                                                 
0780-END-CLAIMS-PROCESSING.                                      
                                                                 
    IF PC-LF-CLAIM  OR  PC-OB-LF-CLAIM                           
        MOVE DTO-MO               TO TRD-LFACTMO                 
        MOVE DTO-DA               TO TRD-LFACTDA                 
        MOVE DTO-YR               TO TRD-LFACTYR                 
        MOVE '/'                  TO TRD-LF-S1  TRD-LF-S2        
    ELSE                                                         
        MOVE PAY-MO               TO TRD-AHACTMO                 
        MOVE PAY-DA               TO TRD-AHACTDA                 
        MOVE PAY-YR               TO TRD-AHACTYR                 
        MOVE '/'                  TO TRD-AH-S1  TRD-AH-S2.       
                                                                 
    PERFORM 8690-PRINT-DETAIL-LINE THRU 8699-PRT-DETAIL-X.       
                                                                 
    IF NOT CR-POLICY-IS-REIN-ONLY                                
        PERFORM 2300-BUILD-CLAIM-EXTRACT THRU 2399-BLD-CLAIM-X   
        ADD +1                    TO BC-G-CLM.                   
                                                                 
    PERFORM 3300-REINSURE-CLAIM THRU 3399-REINSURE-CLAIM-X.      
                                                                 
    IF DEATH-CLAIM-VOIDED                                        
        MOVE ZEROS                TO CR-DTH-DT                   
                                     WS-CR-DTH-DT-N.             
    MOVE SPACE                    TO DEATH-VOID-FLAG.            
                                                                 
    MOVE '*'                      TO TRANSACTION-FLAG.           
    GO TO 0300-READ-PENDING-TRANSACTIONS.                        
                                                                 
EJECT                                                            
0790-TABLE-DISAB-PERIODS.                                        
                                                                 
    MOVE +1                       TO SUB14.                      
    PERFORM 0794-REVIEW-TABLE     THRU 0794-REVIEW-TABLE-EXIT.   
                                                                 
    MOVE +1                       TO SUB14.                      
                                                                 
*>*************************************************************    
*>*                                                          **    
*>*   THIS ROUTINE SEARCHES THROUGH THE DISAB-DETAIL-DATA    **    
*>*  AND MATCHES INCURRED DATE.  IT THEN ADDS THE AMOUNT OF  **    
*>*  THE CLAIM TO THAT OCCURRENCE.  IF THE INCURRED DATE     **    
*>*  DOES NOT MATCH ANY PREVIOUS INCURRED DATE,  IT WILL     **    
*>*  INITIALIZE THE NEXT OCCURRENCE.  (OLDEST TO MOST RECENT)**    
*>*                                                          **    
*>*    IF ALL 5 OCCURRENCES ARE FILLED,  THE TWO OLDEST      **    
*>*  AMOUNTS ARE COMBINED LEAVING THE LAST OCCURRENCE OPEN   **    
*>*  FOR THE MOST RECENT CLAIM INFORMATION.                  **    
*>*                                                          **    
*>*************************************************************    
                                                                 
0791-TABLE-AH-AMOUNT.                                            
                                                                 
    IF SUB14 GREATER THAN DISABILITY-TBL-MAX                     
        PERFORM 0792-MOVE-TABLE  THRU 0792-MOVE-TABLE-EXIT.      
                                                                 
    IF CR-DIS-INCUR-DT (SUB14)  EQUAL ZEROS                      
        MOVE INCURRED-DATE        TO CR-DIS-INCUR-DT (SUB14)     
                                     WS-CR-DIS-INCUR-DT-N (SUB14)
        IF REIN-IS-RISK-PREMIUM                                  
            MOVE INCURRED-DATE     TO DC-GREG-DATE-CYMD          
            PERFORM 3025-CALC-ELAPSED-MONTHS THRU 3026-CALC-EXIT 
            IF SUB14 = +1                                        
              MOVE DC-ELAPSED-MONTHS TO REIN-AH-CLM-MTH-1        
            ELSE                                                 
            IF SUB14 = +2                                        
              MOVE DC-ELAPSED-MONTHS TO REIN-AH-CLM-MTH-2        
            ELSE                                                 
            IF SUB14 = +3                                        
              MOVE DC-ELAPSED-MONTHS TO REIN-AH-CLM-MTH-3        
            ELSE                                                 
            IF SUB14 = +4                                        
              MOVE DC-ELAPSED-MONTHS TO REIN-AH-CLM-MTH-4        
            ELSE                                                 
              MOVE DC-ELAPSED-MONTHS TO REIN-AH-CLM-MTH-5.       
                                                                 
                                                                 
    IF INCURRED-DATE EQUAL CR-DIS-INCUR-DT (SUB14)               
        MOVE SUB14                TO REIN-AH-CLM-MONTHS          
        COMPUTE REIN-AH-PRIOR-CLMS-PAID = CR-INCUR-DISAMT (SUB14)
                                       +  CR-INCUR-DISEXP (SUB14)
        IF PC-CHARGEBLE-EXPENSE  OR                              
           PC-NON-CHARGEBLE-EXPENSE                              
            ADD PC-CLAIM-PAYMENT  TO CR-INCUR-DISEXP (SUB14)     
            GO TO 0791-TABLE-AH-EXIT                             
        ELSE                                                     
            ADD  PC-CLAIM-PAYMENT TO CR-INCUR-DISAMT (SUB14)     
            GO TO 0791-TABLE-AH-EXIT.                            
                                                                 
    ADD +1                       TO SUB14.                       
                                                                 
    GO TO 0791-TABLE-AH-AMOUNT.                                  
                                                                 
0791-TABLE-AH-EXIT.                                              
    EXIT.                                                        
                                                                 
EJECT                                                            
*>*************************************************************    
*>*                                                          **    
*>* THIS ROUTINE IS PERFORMED WHEN THERE HAVE BEEN MORE THAN **    
*>* 5 DISABILITY PERIODS ATTACHED TO THE SAME CERTIFICATE.   **    
*>*                                                          **    
*>* THE SECOND OCCURRENCE IS ADDED TO THE 1ST OCCURRENCE     **    
*>* COMBINING THE TWO OLDEST OCCURRENCES INTO ONE BUCKET     **    
*>* THE NEWEST CLAIM INFORMATION IS MOVED INTO THE LAST      **    
*>* OCCURRENCE.                                              **    
*>*************************************************************    
0792-MOVE-TABLE.                                                 
                                                                 
    MOVE CR-DIS-INCUR-DT (2)  TO CR-DIS-INCUR-DT (1)             
                                 WS-CR-DIS-INCUR-DT-N (1).       
    ADD  CR-INCUR-DISAMT (2)  TO CR-INCUR-DISAMT (1).            
    ADD  CR-INCUR-DISEXP (2)  TO CR-INCUR-DISEXP (1).            
                                                                 
    MOVE +2                   TO SUB14.                          
    MOVE +3                   TO SUB15.                          
    PERFORM 0794A-BUMP-TBL    THRU 0794A-BUMP-TBL-EXIT.          
                                                                 
    MOVE DISABILITY-TBL-MAX   TO SUB14.                          
                                                                 
    MOVE INCURRED-DATE        TO CR-DIS-INCUR-DT (SUB14)         
                                 WS-CR-DIS-INCUR-DT-N (SUB14).   
                                                                 
0792-MOVE-TABLE-EXIT.                                            
    EXIT.                                                        
                                                                 
EJECT                                                            
*>*************************************************************    
*>*                                                          **    
*>*  A REVIEW OF ALL OCCURRENCES IS PERFORMED TO ELIMINATE   **    
*>* ANY WITH NO PAYMENTS OR EXPENSES ATTACHED. THESE CAN     **    
*>* OCCUR WHEN DISABILITY PAYMENTS HAVE BEEN VOIDED, BECAUSE **    
*>* CLAIM ATTACHED TO WRONG CERTIFICATE, OR WRONG CLAIM TYPE **    
*>* PAID, OR A CHANGE IN THE INCURRED DATE OF A CLAIM.       **    
*>*************************************************************    
                                                                 
0794-REVIEW-TABLE.                                               
                                                                 
    IF SUB14 GREATER THAN DISABILITY-TBL-MAX                     
        GO TO 0794-REVIEW-TABLE-EXIT.                            
                                                                 
    IF CR-DIS-INCUR-DT (SUB14) EQUAL ZEROS                       
        GO TO 0794-REVIEW-TABLE-EXIT.                            
                                                                 
    IF CR-INCUR-DISAMT (SUB14) EQUAL ZEROS  AND                  
       CR-INCUR-DISEXP (SUB14) EQUAL ZEROS                       
        DISPLAY CR-ACCOUNT ' ' CR-CERT-NO ' ' SUB14              
        MOVE ZEROS            TO CR-DIS-INCUR-DT (SUB14)         
                                 WS-CR-DIS-INCUR-DT-N (SUB14)    
        MOVE SUB14            TO SUB15                           
        ADD +1                TO SUB15                           
        PERFORM 0794A-BUMP-TBL   THRU 0794A-BUMP-TBL-EXIT        
        MOVE +1               TO SUB14                           
        GO TO 0794-REVIEW-TABLE.                                 
                                                                 
    ADD +1                    TO SUB14.                          
                                                                 
    GO TO 0794-REVIEW-TABLE.                                     
                                                                 
0794-REVIEW-TABLE-EXIT.                                          
    EXIT.                                                        
                                                                 
0794A-BUMP-TBL.                                                  
                                                                 
    IF SUB15 GREATER THAN DISABILITY-TBL-MAX                     
        GO TO 0794A-BUMP-TBL-EXIT.                               
                                                                 
    MOVE CR-DISAB-DETAIL-DATA (SUB15)  TO                        
                            CR-DISAB-DETAIL-DATA (SUB14).        
    MOVE WS-CR-DIS-INCUR-DT-N (SUB15)                            
                                TO WS-CR-DIS-INCUR-DT-N (SUB14)  
                                                                 
    MOVE ZEROS                TO CR-DIS-INCUR-DT (SUB15)         
                                 WS-CR-DIS-INCUR-DT-N (SUB15)    
                                 CR-INCUR-DISAMT (SUB15)         
                                 CR-INCUR-DISEXP (SUB15).        
                                                                 
    ADD +1                    TO SUB14                           
                                 SUB15.                          
    GO TO 0794A-BUMP-TBL.                                        
                                                                 
0794A-BUMP-TBL-EXIT.                                             
    EXIT.                                                        
                                                                 
0795-DCC-NSP-LF.                                                 
                                                                 
    MOVE CR-DT                      TO DC-GREG-DATE-CYMD.        
    MOVE 'L'                        TO DC-OPTION-CODE.           
    PERFORM 8000-DATE-CONVERT-ROUTINE THRU 8099-DATE-CONVERT-X.  
    MOVE DC-BIN-DATE-1              TO CP-CERT-EFF-DT.           
    MOVE SPACES                     TO CP-ACCT-FLD-5.            
    MOVE DTE-CLIENT                 TO CP-COMPANY-ID.            
    MOVE CR-AGE                     TO CP-ISSUE-AGE.             
    MOVE CR-APR                     TO CP-LOAN-APR.              
    MOVE CR-PMT-FREQ                TO CP-PAY-FREQUENCY.         
    MOVE CR-LOAN-TERM               TO CP-LOAN-TERM.             
    MOVE ZEROS                      TO CP-DEVIATION-CODE.        
    MOVE CR-RATING-CLASS            TO CP-CLASS-CODE.            
                                                                 
    MOVE '3'                        TO CP-PROCESS-TYPE.          
    MOVE DTE-CLASIC-COMPANY-CD      TO CP-COMPANY-CD.            
    MOVE CR-PMT-EXTENSION-DAYS      TO CP-TERM-OR-EXT-DAYS.      
                                                                 
    IF CR-LFTYP = ZERO                                           
        GO TO 0795-EXIT                                          
    END-IF                                                       
                                                                 
    IF (CLAS-I-BAL (CLAS-INDEXL) = 'B')                          
        GO TO 0795-EXIT                                          
    END-IF                                                       
    MOVE CR-LFPRM                   TO CP-ORIGINAL-PREMIUM       
    MOVE CLAS-I-RL-AH (CLAS-INDEXL) TO CP-BENEFIT-TYPE.          
    MOVE CR-LF-TERM                 TO CP-ORIGINAL-TERM.         
    MOVE CR-LFAMT                   TO CP-ORIGINAL-BENEFIT       
                                       CP-RATING-BENEFIT-AMT.    
    MOVE CLAS-I-EP (CLAS-INDEXL)    TO CP-EARNING-METHOD.        
    MOVE CLAS-I-BAL (CLAS-INDEXL)   TO CP-SPECIAL-CALC-CD.       
    MOVE CR-LFTYP                   TO CP-BENEFIT-CD.            
    MOVE LIFE-OVERRIDE-L1           TO CP-LIFE-OVERRIDE-CODE.    
    MOVE ZEROS                      TO CP-RATE-DEV-PCT.          
    MOVE CR-STATE                   TO CP-STATE.                 
    MOVE STATE-ABBR (CLAS-INDEXS)   TO CP-STATE-STD-ABBRV.       
                                                                 
    IF CP-STATE-STD-ABBRV = 'OR'                                 
        COMPUTE CP-RATING-BENEFIT-AMT = CR-LFAMT + CR-LFAMT-ALT. 
                                                                 
    PERFORM 8100-GET-RATE THRU 8199-GET-RATE-X.                  
                                                                 
    IF CP-ERROR-RATE-NOT-FOUND  OR                               
       CP-ERROR-RATE-IS-ZERO                                     
        MOVE 'LIFE NET SINGLE PREMIUM RATE NOT FOUND'            
                                    TO WS-ABEND-MESSAGE          
        MOVE 0302                   TO WS-ABEND-CODE             
        DISPLAY '******************************'                 
        DISPLAY '***** ERROR LOCATION 018 *****'                 
        DISPLAY '******************************'                 
        MOVE WS-ABEND-CODE        TO WS-RETURN-CODE              
        GO TO ABEND-PGM.                                         
                                                                 
    MOVE CP-CALC-PREMIUM          TO CR-LF-NSP-PRM.              
                                                                 
*>    COMPUTE CR-LF-NSP-PRM ROUNDED =                              
*>                      (CR-LFAMT / 100) * CP-PREMIUM-RATE.        
                                                                 
    COMPUTE CR-LF-NSP-PRM ROUNDED =  CR-LF-NSP-PRM +             
                  ((CR-LFAMT-ALT / 100) * CP-PREMIUM-RATE).      
                                                                 
    MOVE CP-PREMIUM-RATE            TO CR-LF-NSP-PRM-RATE.       
                                                                 
0795-exit.                                                       
    exit.                                                        
                                                                 
EJECT                                                            
*>*****************************************************************
*>**   P R O C E S S   R E S E R V E   T R A N S A C T I O N S  ***
*>*****************************************************************
                                                                 
0800-PROCESS-RESERVES.                                           
                                                                 
    IF PC-AH-CLAIM  OR  PC-OB-AH-CLAIM                           
      IF CARRIER-UEP-PCT (CLAS-INDEXCN) NOT = ZEROS              
          MOVE ZEROS             TO  PC-IBNR-RESERVE-AMT.        
                                                                 
    if (dte-client = 'AHL')                                      
       and (PC-AH-CLAIM OR PC-OB-AH-CLAIM)                       
       and (cr-ahtyp (1:1) = 'M')                                
       and ((pc-future-reserve-amt > zeros)                      
           or (pc-ptc-reserve-amt > zeros))                      
       compute pc-ibnr-reserve-amt =                             
          (pc-future-reserve-amt + pc-ptc-reserve-amt) * .205    
       display ' using 205 of ptc and future ' cr-cert-no        
    end-if                                                       
                                                                 
    if (dte-client = 'AHL')                                      
       and (PC-LF-CLAIM OR PC-OB-LF-CLAIM)                       
       and (cr-lftyp (1:1) = 'M')                                
       and (pc-ptc-reserve-amt > zeros)                          
       compute pc-ibnr-reserve-amt = pc-ptc-reserve-amt * .217   
       display ' using 217 of ptc ' cr-cert-no                   
    end-if                                                       
                                                                 
    PERFORM 3000-REINSURE-ROUTINE THRU 3099-REINSURE-ROUTINE-X.  
                                                                 
    MOVE PC-INCURRED-DT          TO  DC-BIN-DATE-1.              
    MOVE ' '                     TO  DC-OPTION-CODE.             
    PERFORM 8000-DATE-CONVERT-ROUTINE THRU 8099-DATE-CONVERT-X.  
    MOVE DC-GREG-DATE-CYMD       TO  INCURRED-DATE.              
                                                                 
    MOVE PC-REPORTED-DT          TO  DC-BIN-DATE-1.              
    MOVE ' '                     TO  DC-OPTION-CODE.             
    PERFORM 8000-DATE-CONVERT-ROUTINE THRU 8099-DATE-CONVERT-X.  
    MOVE DC-GREG-DATE-1-YMD      TO  REPORTED-DATE.              
                                                                 
    MOVE PC-PAID-THRU-DT         TO  DC-BIN-DATE-1.              
    MOVE ' '                     TO  DC-OPTION-CODE.             
    PERFORM 8000-DATE-CONVERT-ROUTINE THRU 8099-DATE-CONVERT-X.  
    MOVE DC-GREG-DATE-CYMD       TO  PAY-TO-DATE.                
                                                                 
    PERFORM 8600-SETUP-DETAIL-LINE THRU 8689-SETUP-DETAIL-X.     
                                                                 
