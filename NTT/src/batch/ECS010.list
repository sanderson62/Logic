    IF FINAL-FLG = SPACE                                         
        GO TO 5599-FINAL-BREAK-X.                                
                                                                 
    IF DTE-FICH = '1'                                            
        MOVE '2'  TO  DTE-FICH.                                  
    MOVE +6 TO SUB6.                                             
    PERFORM 8700-PRINT-REPORT-HEADINGS.                          
    MOVE SPACES   TO TPH1-TYPE.                                  
    MOVE SPACES   TO TPH1-DESC.                                  
    MOVE 'FINAL'  TO TPH2-DESC.                                  
    MOVE 'TOTALS' TO TPH3-DESC.                                  
    PERFORM 6000-TOTAL-PAGE-HEADINGS THRU 6199-PAGE-HEADINGS-X.  
    PERFORM 6500-PRINT-TOTAL-PAGE THRU 6599-TOTAL-PAGE-PRINT-X.  
                                                                 
5599-FINAL-BREAK-X.                                              
    EXIT.                                                        
                                                                 
