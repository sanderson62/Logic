          IF ELCRTT-FILE-STATUS = '00'                           
             MOVE PB-I-VIN      TO CS-VIN-NUMBER                 
             REWRITE CERTIFICATE-TRAILERS                        
             IF ELCRTT-FILE-STATUS NOT = '00'                    
                DISPLAY ' BAD REWRITE ELCRTT '                   
                             ELCRTT-FILE-STATUS                  
                PERFORM ABEND-PGM                                
             END-IF                                              
          ELSE                                                   
             DISPLAY ' BAD READ    ELCRTT '                      
                              ELCRTT-FILE-STATUS                 
             PERFORM ABEND-PGM                                   
          END-IF                                                 
       ELSE                                                      
          DISPLAY ' BAD WRITE ELCRTT '                           
                             ELCRTT-FILE-STATUS                  
          PERFORM ABEND-PGM                                      
       END-IF                                                    
    END-IF.                                                      
                                                                 
    .                                                            
0950-EXIT.                                                       
    EXIT.                                                        
                                                                 
                                                                 
*>*****************************************************************
*>**      S E T   C O M M I S S I O N   S T R U C T U R E       ***
*>*****************************************************************
                                                                 
1000-SET-COMMISSION-STRUCTURE.                                   
                                                                 
    MOVE +1                     TO IA                            
                                   IB.                           
                                                                 
1100-GET-COMMISSION-LEVEL.                                       
                                                                 
    MOVE AM-AGT-COMMS (IA)      TO WK-LEVEL.                     
                                                                 
    IF WK-AGT = '0000000000' OR SPACES                           
        MOVE +0                 TO WK-L-RT                       
                                   WK-J-RT                       
                                   WK-A-RT                       
        GO TO 1200-SET-COMMISSION-LEVEL.                         
                                                                 
1110-LIFE-COMMISSION.                                            
                                                                 
    IF CR-LFTYP = ZERO  OR                                       
      (CR-ENTRY-STATUS = 'P' OR 'D' OR 'V')                      
        MOVE +0 TO WK-L-RT                                       
                   WK-J-RT                                       
        GO TO 1130-A-H-COMMISSION.                               
                                                                 
    IF CLAS-I-JOINT (CLAS-INDEXL) = 'J'                          
        GO TO 1120-JOINT-LIFE-COMMISSION.                        
                                                                 
    IF WK-L-RT NUMERIC                                           
       AND (WK-L-RTX (3:1) NOT = 'L' AND 'M')                    
        MOVE WK-L-RT                TO WS-COMMISSION             
        PERFORM 6650-GET-RES-STATE-COMM  THRU 6700-COMM-EXIT     
        MOVE WS-COMMISSION          TO WK-L-RT                   
        GO TO 1130-A-H-COMMISSION.                               
                                                                 
    MOVE LIFE-OVERRIDE-L1           TO CTBL-BEN-TYPE.            
    MOVE CR-LFTYP                   TO CTBL-BEN-CODE.            
    MOVE ZEROS                      TO WS-SUB1.                  
    PERFORM 6650-GET-RES-STATE-COMM  THRU 6700-COMM-EXIT         
                                                                 
    IF WS-SUB1 = ZEROS                                           
        MOVE WK-L-RTX               TO CTBL-TABLE-CD             
    ELSE                                                         
        MOVE WK-RATE                TO WK-L-RT                   
        GO TO 1130-A-H-COMMISSION.                               
                                                                 
    PERFORM 1300-READ-ERCTBL-FILE THRU 1399-READ-ERCTBL-X.       
                                                                 
    MOVE CR-LFAMT                   TO COMM-CK-AMT.              
    MOVE CR-LF-TERM                 TO WS-COMM-TERM.             
    PERFORM 1400-GET-COMM-PERCENT THRU 1489-GET-PERCENT-X.       
    MOVE WK-RATE                    TO WK-L-RT.                  
                                                                 
    GO TO 1130-A-H-COMMISSION.                                   
                                                                 
1120-JOINT-LIFE-COMMISSION.                                      
                                                                 
    IF WK-J-RT NUMERIC                                           
       AND (WK-J-RTX (3:1) NOT = 'L' AND 'M')                    
        MOVE WK-J-RT                TO WS-COMMISSION             
        PERFORM 6650-GET-RES-STATE-COMM  THRU 6700-COMM-EXIT     
        MOVE WS-COMMISSION          TO WK-J-RT                   
        GO TO 1130-A-H-COMMISSION.                               
                                                                 
    MOVE LIFE-OVERRIDE-L1           TO CTBL-BEN-TYPE.            
    MOVE CR-LFTYP                   TO CTBL-BEN-CODE.            
    MOVE ZEROS                      TO WS-SUB1.                  
    PERFORM 6650-GET-RES-STATE-COMM  THRU 6700-COMM-EXIT         
                                                                 
    IF WS-SUB1 = ZEROS                                           
        MOVE WK-J-RTX               TO CTBL-TABLE-CD             
    ELSE                                                         
        MOVE WK-RATE                TO WK-J-RT                   
        GO TO 1130-A-H-COMMISSION.                               
                                                                 
    PERFORM 1300-READ-ERCTBL-FILE THRU 1399-READ-ERCTBL-X.       
                                                                 
    MOVE CR-LFAMT                   TO COMM-CK-AMT.              
    MOVE CR-LF-TERM                 TO WS-COMM-TERM.             
    PERFORM 1400-GET-COMM-PERCENT THRU 1489-GET-PERCENT-X.       
    MOVE WK-RATE                    TO WK-J-RT.                  
                                                                 
1130-A-H-COMMISSION.                                             
                                                                 
    IF CR-AHTYP = ZERO  OR                                       
      (CR-ENTRY-STATUS = 'P' OR 'D' OR 'V')                      
        MOVE +0 TO WK-A-RT                                       
        GO TO 1200-SET-COMMISSION-LEVEL.                         
                                                                 
    IF WK-A-RT NUMERIC                                           
       AND (WK-A-RTX (3:1) NOT = 'L' AND 'M')                    
        MOVE WK-A-RT                TO WS-COMMISSION             
        PERFORM 6650-GET-RES-STATE-COMM  THRU 6700-COMM-EXIT     
        MOVE WS-COMMISSION          TO WK-A-RT                   
        GO TO 1200-SET-COMMISSION-LEVEL.                         
                                                                 
    MOVE AH-OVERRIDE-L1             TO CTBL-BEN-TYPE.            
    MOVE CR-AHTYP                   TO CTBL-BEN-CODE.            
    MOVE ZEROS                      TO WS-SUB1.                  
    PERFORM 6650-GET-RES-STATE-COMM  THRU 6700-COMM-EXIT         
                                                                 
    IF WS-SUB1 = ZEROS                                           
        MOVE WK-A-RTX               TO CTBL-TABLE-CD             
    ELSE                                                         
        MOVE WK-RATE                TO WK-A-RT                   
        GO TO 1200-SET-COMMISSION-LEVEL.                         
                                                                 
    PERFORM 1300-READ-ERCTBL-FILE THRU 1399-READ-ERCTBL-X.       
                                                                 
    COMPUTE COMM-CK-AMT = CR-AHAMT * CR-AH-TERM.                 
    MOVE CR-AH-TERM                 TO WS-COMM-TERM.             
    PERFORM 1400-GET-COMM-PERCENT THRU 1489-GET-PERCENT-X.       
    MOVE WK-RATE                    TO WK-A-RT.                  
                                                                 
1200-SET-COMMISSION-LEVEL.                                       
                                                                 
    MOVE WK-AGT                      TO WK-N-AGT  (IA).          
    IF WK-AGT = '0000000000' OR SPACES                           
        MOVE SPACE                   TO WK-N-TYPE (IA)           
    ELSE                                                         
        MOVE WK-A-TYPE               TO WK-N-TYPE (IA).          
                                                                 
    IF CR-LFTYP = ZERO                                           
        MOVE +0                      TO WK-L-RT1  (IA)           
    ELSE                                                         
        IF CLAS-I-JOINT (CLAS-INDEXL) = 'J'                      
            MOVE WK-J-RT             TO WK-L-RT1 (IA)            
        ELSE                                                     
            MOVE WK-L-RT             TO WK-L-RT1 (IA).           
                                                                 
    MOVE WK-A-RT                     TO WK-A-RT1 (IA).           
                                                                 
    IF RC-COMM-FLG = 'X'                                         
       IF (AM-RECALC-LV-INDIC (IA) = 'N')                        
*>          OR ((CLAS-I-BEN-CATEGORY (CLAS-INDEXA) = 'G' OR 'L')   
*>             AND (IA = +5))                                      
           MOVE CR-COM-AGT  (IA)         TO WK-N-AGT  (IA)       
           MOVE CR-AGT-TYPE (IA)         TO WK-N-TYPE (IA)       
           MOVE CR-LCOM-L   (IA)         TO WK-L-RT1  (IA)       
           MOVE CR-LCOM-AH  (IA)         TO WK-A-RT1  (IA).      
                                                                 
    IF IA LESS THAN +10                                          
        ADD +1 TO IA                                             
        GO TO 1100-GET-COMMISSION-LEVEL.                         
                                                                 
    GO TO 1499-SET-COMM-X.                                       
                                                                 
1300-READ-ERCTBL-FILE.                                           
                                                                 
    IF CTBL-SEARCH = LAST-CTBL                                   
        GO TO 1399-READ-ERCTBL-X.                                
                                                                 
    MOVE SPACE                          TO CTBL-ERROR-SWITCH.    
                                                                 
    IF COMM-OPEN-SW = ' '                                        
        MOVE 'X'                        TO COMM-OPEN-SW          
        MOVE DTE-CLASIC-COMPANY-CD      TO CTBL-COMP-CD          
        OPEN INPUT ERCTBL                                        
        IF ERCTBL-FILE-STATUS = '00'  OR  '97'                   
            NEXT SENTENCE                                        
           ELSE                                                  
            MOVE '41'                   TO ABEND-CODE-1          
            MOVE ERCTBL-FILE-STATUS     TO ABEND-CODE-2          
            DISPLAY '******************************'             
            DISPLAY '***** ERROR LOCATION 012 *****'             
            DISPLAY '******************************'             
            DISPLAY '******************************'             
            MOVE WS-ABEND-CODE          TO WS-RETURN-CODE        
            GO TO ABEND-PGM.                                     
                                                                 
    MOVE CTBL-SEARCH                    TO CT-CONTROL-PRIMARY    
                                           LAST-CTBL.            
    READ ERCTBL.                                                 
                                                                 
    IF ERCTBL-FILE-STATUS = '23'                                 
        DISPLAY 'INVALID COMM TABLE CODE FOR: ' CR-FULL-CONTROL  
                ' ENTERED: ' CR-ENTRY-STATUS '-' CR-ENTRY-DATE   
                ' CTBL: ' CTBL-SEARCH                            
        MOVE '*'                TO CTBL-ERROR-SWITCH             
        MOVE '44'               TO ABEND-CODE-1                  
        MOVE ERCTBL-FILE-STATUS TO ABEND-CODE-2                  
        DISPLAY 'CT-CONTROL-PRIMARY = ' CT-CONTROL-PRIMARY       
        DISPLAY '******************************'                 
        DISPLAY '***** ERROR LOCATION 013 *****'                 
        DISPLAY '******************************'                 
        MOVE WS-ABEND-CODE      TO WS-RETURN-CODE                
*>        IF DTE-CLIENT NOT = 'NCL'                                
        IF DTE-CLIENT NOT = 'NCL' AND 'CID' AND 'CSO'            
          AND 'AHL' AND 'FNL'                                    
            GO TO ABEND-PGM                                      
        ELSE                                                     
            NEXT SENTENCE                                        
    ELSE                                                         
        IF ERCTBL-FILE-STATUS NOT = '00'                         
            MOVE 'READ ERROR ON ERCTBL' TO WS-ABEND-MESSAGE      
            MOVE '44'               TO ABEND-CODE-1              
            MOVE ERCTBL-FILE-STATUS TO ABEND-CODE-2              
            DISPLAY '******************************'             
            DISPLAY '***** ERROR LOCATION 014 *****'             
            DISPLAY '******************************'             
            MOVE WS-ABEND-CODE      TO WS-RETURN-CODE            
            GO TO ABEND-PGM.                                     
                                                                 
    ADD +1                            TO ERCTBL-READS.           
                                                                 
1399-READ-ERCTBL-X.                                              
    EXIT.                                                        
                                                                 
1400-GET-COMM-PERCENT.                                           
                                                                 
    IF CTBL-ERROR                                                
        MOVE ZEROS                      TO WK-RATE               
        GO TO 1489-GET-PERCENT-X.                                
                                                                 
    MOVE +1 TO IB.                                               
    IF (COMM-CK-AMT GREATER CT-TBF (1)) AND                      
       (CT-TBF (1) NOT EQUAL +9999999.99)                        
        ADD +9 TO IB                                             
        IF (COMM-CK-AMT GREATER CT-TBF (2)) AND                  
           (CT-TBF (2) NOT EQUAL +9999999.99)                    
            ADD +9 TO IB.                                        
                                                                 
    IF CR-AGE GREATER CT-AGE (1)                                 
        ADD +3 TO IB                                             
        IF CR-AGE GREATER CT-AGE (2)                             
            ADD +3 TO IB.                                        
                                                                 
    IF WS-COMM-TERM GREATER CT-TRM (1)                           
        ADD +1 TO IB                                             
        IF WS-COMM-TERM GREATER CT-TRM (2)                       
            ADD +1 TO IB.                                        
                                                                 
    IF CT-RT (IB) NOT NUMERIC                                    
        MOVE LAST-CTBL     TO CT-CONTROL-PRIMARY                 
        MOVE CT-RT-R (IB)  TO CTBL-TABLE-CD                      
        GO TO 1300-READ-ERCTBL-FILE.                             
                                                                 
    MOVE CT-RT (IB) TO WK-RATE.                                  
                                                                 
1489-GET-PERCENT-X.                                              
    EXIT.                                                        
                                                                 
1499-SET-COMM-X.                                                 
    EXIT.                                                        
EJECT                                                            
*>*****************************************************************
*>**  C O M P U T E   A C C T   L E V E L   C O M M I S S I O N ***
*>*****************************************************************
                                                                 
1500-GET-ACCT-LEVEL-LF-COMM.                                     
                                                                 
    MOVE ZEROS                      TO ACCT-COMM-LF              
                                       ACCT-COMM-AH.             
                                                                 
    MOVE +1                         TO IA.                       
                                                                 
1510-ACCT-LEVEL-LOOP.                                            
                                                                 
    IF CR-AGT-TYPE (IA) = 'C'  OR  'D' OR 'F'                    
        IF WS-LEVELS-CHARGEBACK-SWITCHES = SPACES                
            ADD CR-LCOM-L (IA)  TO ACCT-COMM-LF                  
            ADD CR-LCOM-AH (IA) TO ACCT-COMM-AH                  
        ELSE                                                     
            IF WS-LF-CHARGEBACK-SW (IA)  =  'Y'                  
                ADD CR-LCOM-L (IA)  TO ACCT-COMM-LF.             
                                                                 
    IF IA LESS THAN +10                                          
        ADD +1 TO IA                                             
        GO TO 1510-ACCT-LEVEL-LOOP.                              
                                                                 
1519-ACCT-LF-COMM-X.                                             
    EXIT.                                                        
                                                                 
                                                                 
1550-GET-ACCT-LEVEL-AH-COMM.                                     
                                                                 
    MOVE ZEROS                      TO ACCT-COMM-AH.             
                                                                 
    MOVE +1                         TO IA.                       
                                                                 
1555-ACCT-LEVEL-LOOP.                                            
                                                                 
    IF CR-AGT-TYPE (IA) = 'C'  OR  'D' OR 'F'                    
        IF WS-AH-CHARGEBACK-SW (IA)  =  'Y'                      
            ADD CR-LCOM-AH (IA)  TO ACCT-COMM-AH.                
                                                                 
    IF IA LESS THAN +10                                          
        ADD +1 TO IA                                             
        GO TO 1555-ACCT-LEVEL-LOOP.                              
                                                                 
1599-ACCT-AH-COMM-X.                                             
    EXIT.                                                        
EJECT                                                            
*>*****************************************************************
*>**      C O M M I S S I O N   R E C A L C   R O U T I N E     ***
*>*****************************************************************
                                                                 
1600-RECALC-COMM-EXTR.                                           
                                                                 
    IF RC-COMM-FLG = 'X'  OR                                     
       (DTE-PGM-OPT = '2'  OR  '3')  OR                          
       CR-POLICY-IS-DECLINED  OR                                 
       CR-POLICY-IS-VOID                                         
        GO TO 1899-RECALC-COMM-EXTR-X.                           
                                                                 
    MOVE 'X'                           TO RC-COMM-FLG.           
                                                                 
    IF CR-ENTRY-DATE = EOM-RUN-DATE                              
        GO TO 1899-RECALC-COMM-EXTR-X.                           
                                                                 
    PERFORM 1000-SET-COMMISSION-STRUCTURE THRU 1499-SET-COMM-X.  
                                                                 
    IF CR-COMPENSATION-LEVELS = WK-NEW-RATES                     
        GO TO 1899-RECALC-COMM-EXTR-X.                           
                                                                 
    MOVE WK-NEW-RATES                  TO SAVE-WK-NEW-RATES.     
                                                                 
    PERFORM 1610-CK-DUPLICATE-ENTRIES                            
        THRU 1619-CK-DUPLICATE-ENTRIES-XIT                       
            VARYING SUB11 FROM 1 BY 1 UNTIL SUB11 GREATER 10.    
                                                                 
    IF CR-COMPENSATION-LEVELS = WK-NEW-RATES                     
        MOVE SAVE-WK-NEW-RATES         TO CR-COMPENSATION-LEVELS 
                                          WK-NEW-RATES           
        GO TO 1899-RECALC-COMM-EXTR-X.                           
                                                                 
    MOVE +1                            TO SUB8.                  
    GO TO 1700-REVERSE-OLD-COMM-LOOP.                            
                                                                 
1610-CK-DUPLICATE-ENTRIES.                                       
    PERFORM 1620-CK-DUPL-ENTR-NEW                                
        THRU 1629-CK-DUPL-ENTR-NEW-XIT                           
            VARYING SUB12 FROM 1 BY 1 UNTIL SUB12 GREATER 10.    
                                                                 
1619-CK-DUPLICATE-ENTRIES-XIT.                                   
    EXIT.                                                        
                                                                 
1620-CK-DUPL-ENTR-NEW.                                           
    IF CR-AGT-LEVELS (SUB11) = WK-N-R (SUB12)                    
        MOVE SPACES                    TO CR-COM-AGT (SUB11)     
        MOVE SPACE                     TO CR-AGT-TYPE (SUB11)    
        MOVE +0                        TO CR-LCOM-L (SUB11)      
                                          CR-LCOM-AH (SUB11)     
        MOVE CR-AGT-LEVELS (SUB11)     TO WK-N-R (SUB12)         
        MOVE 11                        TO SUB12.                 
                                                                 
1629-CK-DUPL-ENTR-NEW-XIT.                                       
    EXIT.                                                        
                                                                 
1700-REVERSE-OLD-COMM-LOOP.                                      
    COMPUTE CR-LCOM-L (SUB8) = CR-LCOM-L (SUB8) * -1.            
    COMPUTE CR-LCOM-AH (SUB8) = CR-LCOM-AH (SUB8) * -1.          
                                                                 
    IF SUB8 LESS THAN +10                                        
        ADD +1 TO SUB8                                           
        GO TO 1700-REVERSE-OLD-COMM-LOOP.                        
                                                                 
    MOVE 'O' TO RECALC-TYPE.                                     
                                                                 
    PERFORM 1710-DO-R-C-EXTRACTS THRU 1719-DO-R-C-EXTRACTS-X.    
                                                                 
    GO TO 1800-MOVE-NEW-COMM-RATES.                              
                                                                 
1710-DO-R-C-EXTRACTS.                                            
                                                                 
    PERFORM 2000-INITIALIZE-DETAIL-EXTRACT THRU 2099-INIT-DTL-X. 
                                                                 
    PERFORM 1720-CHECK-COMM-LEVELS                               
        THRU 1729-CHECK-COMM-LEVELS-EXIT                         
            VARYING SUB13 FROM +1 BY +1                          
                UNTIL SUB13 GREATER THAN 10.                     
                                                                 
    MOVE '8'                           TO DE-TRANS.              
    MOVE RECALC-TYPE                   TO DE-RECALC-TYPE.        
*>****PERFORM EXTRACT-DATE-RELOAD.                                 
    MOVE DETAIL-EXTRACT                TO SAVE-EXTRACT.          
    PERFORM 2500-WRITE-DETAIL-EXTRACTS THRU 2599-WRT-DETAIL-X.   
    MOVE SAVE-EXTRACT                  TO DETAIL-EXTRACT.        
    PERFORM EXTRACT-DATE-LOAD.                                   
    MOVE 'R'                           TO DE-REIN.               
    MOVE DETAIL-EXTRACT                TO SAVE-EXTRACT.          
    MOVE +1                            TO SUB1.                  
    PERFORM 3110-REINSURE-ISSUE-LOOP                             
        THRU 3119-REINSURE-ISSUE-LOOP-END.                       
                                                                 
    IF CR-LF-CANCEL-APPLIED  OR  CR-AH-CANCEL-APPLIED            
        PERFORM 2200-BUILD-CANCEL-EXTRACT THRU                   
                                      2259-END-CANCEL-BUILD      
        PERFORM 1720-CHECK-COMM-LEVELS                           
            THRU 1729-CHECK-COMM-LEVELS-EXIT                     
                VARYING SUB13 FROM +1 BY +1                      
                    UNTIL SUB13 GREATER THAN 10                  
        MOVE '7'                       TO DE-TRANS               
        MOVE RECALC-TYPE               TO DE-RECALC-TYPE         
*>********PERFORM EXTRACT-DATE-RELOAD                              
        MOVE DETAIL-EXTRACT            TO SAVE-EXTRACT           
        PERFORM 2500-WRITE-DETAIL-EXTRACTS THRU 2599-WRT-DETAIL-X
        MOVE SAVE-EXTRACT              TO DETAIL-EXTRACT         
        PERFORM EXTRACT-DATE-LOAD                                
        MOVE 'R'                       TO DE-REIN                
        MOVE DETAIL-EXTRACT            TO SAVE-EXTRACT           
        MOVE +1                        TO SUB1                   
        PERFORM 3210-REINSURE-CANCEL-LOOP                        
            THRU 3219-REINSURE-CANCEL-LOOP-END.                  
                                                                 
1719-DO-R-C-EXTRACTS-X.                                          
    EXIT.                                                        
                                                                 
1720-CHECK-COMM-LEVELS.                                          
                                                                 
    IF  DE-AGT (SUB13)       =  SPACES  AND                      
        DE-AGT-TYPE (SUB13)  =  SPACES  AND                      
        DE-L-PC (SUB13)      =  ZEROS   AND                      
        DE-A-PC (SUB13)      =  ZEROS                            
        MOVE '0000000000'        TO  DE-AGT (SUB13).             
                                                                 
1729-CHECK-COMM-LEVELS-EXIT.                                     
    EXIT.                                                        
                                                                 
1800-MOVE-NEW-COMM-RATES.                                        
    MOVE WK-NEW-RATES                  TO CR-COMPENSATION-LEVELS.
    MOVE 'N'                           TO RECALC-TYPE.           
    PERFORM 1710-DO-R-C-EXTRACTS THRU 1719-DO-R-C-EXTRACTS-X.    
    MOVE SAVE-WK-NEW-RATES             TO WK-NEW-RATES           
                                          CR-COMPENSATION-LEVELS.
    IF DTE-CLIENT = 'DCC'                                        
       IF CR-LF-NSP-PRM NOT = ZEROS                              
          COMPUTE CR-LF-NSP-PRM ROUNDED =                        
             (CR-LFPRM + CR-LFPRM-ALT) -                         
             ((CR-LFPRM + CR-LFPRM-ALT) * CR-LCOM-L (1))         
       END-IF                                                    
       IF (CR-AH-NSP-PRM NOT = ZEROS)                            
          AND (CLAS-I-BEN-CATEGORY (CLAS-INDEXA)                 
                                       NOT = 'G' AND 'L')        
          COMPUTE CR-AH-NSP-PRM ROUNDED =                        
          CR-AHPRM -                                             
          (CR-AHPRM * CR-LCOM-AH (1))                            
       END-IF                                                    
    END-IF                                                       
    .                                                            
                                                                 
1899-RECALC-COMM-EXTR-X.                                         
    EXIT.                                                        
EJECT                                                            
*>*****************************************************************
*>**      I N I T I A L I Z E   D E T A I L   E X T R A C T     ***
*>*****************************************************************
                                                                 
2000-INITIALIZE-DETAIL-EXTRACT.                                  
                                                                 
    MOVE SPACES                   TO DETAIL-EXTRACT.             
                                                                 
    MOVE 'DE'                     TO DE-RECORD-ID.               
    MOVE CR-COMPANY-CD            TO DE-COMPANY-CD.              
    MOVE CR-FULL-CONTROL          TO DE-CONTROL.                 
                                                                 
    MOVE AM-REPORT-CODE-1         TO DE-REPORT-CODE-1.           
    MOVE AM-REPORT-CODE-2         TO DE-REPORT-CODE-2.           
                                                                 
    MOVE CR-NAME                  TO DE-NAME.                    
    MOVE CR-AGE                   TO DE-AGE.                     
    MOVE CR-SEX                   TO DE-SEX.                     
    MOVE CR-SOC-SEC               TO DE-SOC-SEC-NO.              
                                                                 
    MOVE CR-LFTYP                 TO DE-LF-TYPE.                 
    MOVE CR-LF-TERM               TO DE-LF-TERM                  
    MOVE CR-LOAN-TERM             TO DE-LOAN-TERM.               
    MOVE CR-LFAMT                 TO DE-LF-BEN.                  
    MOVE ZEROS                    TO DE-LF-CNBEN.                
    MOVE CR-LFPRM                 TO DE-LF-PRM.                  
    MOVE CR-LFAMT-ALT             TO DE-LF-BEN-ALT.              
    MOVE ZEROS                    TO DE-LF-CNBEN-ALT.            
    MOVE CR-LFPRM-ALT             TO DE-LF-PRM-ALT.              
    MOVE ZEROS                    TO DE-LF-RFND.                 
                                                                 
    MOVE CR-LF-CURRENT-STATUS     TO DE-LF-STAT-CDE.             
    MOVE ZEROS                    TO DE-LF-CANC-DTE              
                                     DE-LF-CANC-EXIT-DT.         
                                                                 
    MOVE CR-AHTYP                 TO DE-AH-TYPE.                 
    MOVE CR-AH-TERM               TO DE-AH-TERM.                 
    MOVE CR-AHAMT                 TO DE-AH-BEN.                  
    MOVE CR-AHPRM                 TO DE-AH-PRM.                  
    MOVE ZEROS                    TO DE-AH-RFND.                 
                                                                 
    MOVE CR-AH-CURRENT-STATUS     TO DE-AH-STAT-CDE.             
    MOVE ZEROS                    TO DE-AH-CANC-DTE              
                                     DE-AH-CANC-EXIT-DT.         
    IF DTE-CLIENT = 'DCC'                                        
       IF CR-ENTRY-STATUS = 'M'                                  
          MOVE CR-MOB-NET-TOT-FEES TO DE-MOB-NET-TOT-FEES        
       ELSE                                                      
          IF AM-DCC-PRODUCT-CODE = 'DDF'                         
             MOVE CR-DDF-IU-RATE-UP TO DE-IU-RATE-UP             
          END-IF                                                 
       END-IF                                                    
    END-IF                                                       
                                                                 
    IF DTE-CLIENT = 'DCC'                                        
       MOVE CR-ADDL-CLP         TO DE-ADDL-CLP                   
       MOVE CR-CLP-STATE        TO DE-CLP-STATE                  
    ELSE                                                         
       IF (CR-LFTYP NOT = ZERO   AND                             
          CLAS-I-CALC-TYPE (CLAS-INDEXL) = 'Z')  OR              
          (CR-AHTYP NOT = ZERO  AND                              
          CLAS-I-CALC-TYPE (CLAS-INDEXA) = 'Z')                  
          MOVE CR-LIVES           TO DE-LIVES                    
          MOVE ZERO               TO DE-CANCEL-CNT-ITD           
                                     DE-CANCEL-CNT-YTD           
       END-IF                                                    
    END-IF                                                       
                                                                 
    MOVE CR-MEMBER-NO             TO DE-MEMBER-NO.               
    IF AM-ZIP-PRIME NUMERIC                                      
       MOVE AM-ZIP-PRIME          TO DE-ZIP                      
     ELSE                                                        
       MOVE ZEROS                 TO DE-ZIP.                     
    MOVE CR-LOAN-OFFICER          TO DE-LN-OFFICER.              
                                                                 
    MOVE CR-APR                   TO DE-APR.                     
    MOVE CR-PMT-FREQ              TO DE-PMT-FREQ.                
    MOVE CR-GRPTYP                TO DE-ACC-GPCD.                
    MOVE CR-IND-GRP               TO DE-IG.                      
    MOVE AM-REMIT-TO              TO DE-REMIT-TO.                
    MOVE CR-MEMBER-NO             TO DE-MEMBER-NO.               
                                                                 
    MOVE CR-ENTRY-STATUS          TO DE-ENTRY-STATUS.            
    MOVE CR-ENTRY-DATE            TO DE-ENTRY-DTE                
    MOVE CR-CANCEL-REASON         TO DE-CANCEL-REASON            
                                                                 
    MOVE CR-UNDERWRITING-CODE     TO DE-UNDERWRITING-CODE.       
*>    MOVE AM-POOL-PRIME            TO DE-NCL-POOL-CODE.           
                                                                 
    MOVE ZEROS                    TO DE-REI-LFAMT  DE-REI-LFPRM  
                                     DE-REI-AHAMT  DE-REI-AHPRM  
                                     DE-REI-LFRFND DE-REI-AHRFND 
                                     DE-REI-CNAMT.               
                                                                 
    MOVE +1                       TO SUB1.                       
                                                                 
*> THIS WAS ADDED SO I COULD PASS THE CLP TO ECS017                
*> FOR DEALER DIRECT OTHERWISE THERE WAS NO WAY TO                 
*> CALC IT IN ECS017.                                              
    IF DTE-CLIENT = 'DCC'                                        
       IF CLAS-I-BEN-CATEGORY (CLAS-INDEXL) = 'D'                
          MOVE CR-LF-CLP          TO DE-REI-LFPRM                
          IF CR-ACCOUNT = '05500500DD'                           
             DISPLAY ' LF CLP ' DE-CERT-NO ' ' DE-REI-LFPRM      
               ' ' DE-REI-LFRFND                                 
          END-IF                                                 
       END-IF                                                    
       IF (CLAS-I-BEN-CATEGORY (CLAS-INDEXA) = 'D')              
                     OR                                          
          ((CLAS-I-BEN-CATEGORY (CLAS-INDEXA) = 'G')             
           AND (AM-DCC-PRODUCT-CODE = 'DDF'))                    
          MOVE CR-AH-CLP          TO DE-REI-AHPRM                
          MOVE CR-AH-RFND-CLP     TO DE-REI-AHRFND               
       END-IF                                                    
    END-IF                                                       
                                                                 
                                                                 
    PERFORM EXTRACT-DATE-LOAD.                                   
                                                                 
    MOVE ZEROS                    TO WS-DE-PROC-DT-N             
                                     WS-DE-INCUR-N               
                                     WS-DE-PAY-N                 
                                     WS-DE-PAID-TO-N             
                                     WS-DE-ACC-EXP-DTE-N         
                                     WS-DE-ACC-EFF-DTE-N         
                                     WS-DE-RSV-INCUR-N           
                                     WS-DE-RSV-PAYTO-N           
                                     WS-DE-ACC-EXP-DTE-RSV-N     
                                     WS-DE-ACC-EFF-DTE-RSV-N.    
                                                                 
2090-GL-CODE-LOOP.                                               
                                                                 
    MOVE AM-GL-CODES (SUB1)       TO DE-GL-CODES (SUB1).         
                                                                 
    IF SUB1 LESS THAN +10                                        
        ADD +1 TO SUB1                                           
        GO TO 2090-GL-CODE-LOOP.                                 
                                                                 
    MOVE AM-RECALC-COMM           TO DE-RECALC-CODE.             
                                                                 
    MOVE CR-COMPENSATION-LEVELS   TO DE-COMM-LEVELS.             
                                                                 
    IF (DTE-CLIENT = 'DCC')                                      
       AND (CR-AHTYP NOT = SPACES AND ZEROS)                     
       AND (CLAS-I-BEN-CATEGORY (CLAS-INDEXA) = 'G' OR 'L')      
       AND (AM-DCC-PRODUCT-CODE NOT = 'DDF')                     
       AND (RECALC-TYPE = SPACES)                                
       MOVE +1                  TO IA                            
*>       IF CR-ACCOUNT = '05500500DD'                              
*>*      IF CR-CERT-NO = '0008773536 '                             
*>          DISPLAY ' MADE IT TO 2000 '                            
*>          DISPLAY ' CERT AREA  REIN ' DE-REIN ' ' CR-CERT-NO     
*>          PERFORM VARYING IB FROM +1 BY +1 UNTIL                 
*>             (IB > +10)                                          
*>             IF CR-COM-AGT (IB) NOT = SPACES AND ZEROS           
*>                MOVE IB TO DIL-BEN-CODE                          
*>                MOVE CR-LCOM-AH (IB) TO DIL-AMOUNT               
*>                MOVE CR-COM-AGT (IB) TO DIL-COMMENT              
*>                DISPLAY DISPLAY-IBNR-LINE                        
*>             END-IF                                              
*>          END-PERFORM                                            
*>          DISPLAY ' TRLR AREA  '                                 
*>          DISPLAY ' CS CERT NO ' CS-CERT-NO                      
*>          PERFORM VARYING IB FROM +1 BY +1 UNTIL                 
*>             (IB > +10)                                          
*>             IF WS-BNK-AGT (IB) NOT = SPACES AND ZEROS           
*>                MOVE IB TO DIL-BEN-CODE                          
*>                MOVE WS-BNK-SPP-FEES (IB) TO DIL-AMOUNT          
*>                MOVE WS-BNK-AGT (IB) TO DIL-COMMENT              
*>                DISPLAY DISPLAY-IBNR-LINE                        
*>             END-IF                                              
*>          END-PERFORM                                            
*>       END-IF                                                    
       PERFORM VARYING IB FROM +1 BY +1 UNTIL                    
          IB > +10                                               
          IF DE-AGT (IB) NOT = SPACES AND ZEROS                  
             CONTINUE                                            
          ELSE                                                   
             IF WS-BNK-AGT (IA) NOT = SPACES AND ZEROS           
                MOVE WS-BNK-AGT (IA)  TO DE-AGT (IB)             
                MOVE WS-BNK-TYPE (IA) TO DE-AGT-TYPE (IB)        
                COMPUTE DE-A-PC (IB) =                           
                       WS-BNK-SPP-FEES (IA) / +1000              
                MOVE +0              TO DE-L-PC (IB)             
                ADD +1               TO IA                       
             END-IF                                              
*>             IF CS-AGT (IA) NOT = SPACES AND ZEROS               
*>                MOVE CS-AGT (IA)     TO DE-AGT (IB)              
*>                MOVE CS-COM-TYP (IA) TO DE-AGT-TYPE (IB)         
*>                COMPUTE DE-A-PC (IB) = CS-SPP-FEES (IA) / +1000  
*>                MOVE +0              TO DE-L-PC (IB)             
*>                ADD +1               TO IA                       
*>             END-IF                                              
          END-IF                                                 
       END-PERFORM                                               
    END-IF                                                       
                                                                 
                                                                 
                                                                 
    .                                                            
2099-INIT-DTL-X.                                                 
    EXIT.                                                        
                                                                 
