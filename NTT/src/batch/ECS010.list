3000-REINSURE-ROUTINE.                                           
                                                                 
    IF (CR-POLICY-IS-DECLINED) OR                                
       (CR-POLICY-IS-VOID)                                       
       GO TO 3099-REINSURE-ROUTINE-X.                            
                                                                 
    IF REIN-FLG = 'X'                                            
        GO TO 3099-REINSURE-ROUTINE-X.                           
                                                                 
    MOVE 'X' TO REIN-FLG.                                        
                                                                 
    PERFORM CLEAR-REIN-HOLD THRU CLEAR-REIN-HOLD-X.              
                                                                 
    IF CR-REIN-TABLE = SPACES OR ZEROS                           
        GO TO 3030-RE-CALCULATE-REINSURANCE.                     
                                                                 
*>*****************************************************************
*>**    R E A D   R E I N S U R A N C E   T A B L E   F I L E   ***
*>*****************************************************************
                                                                 
3010-RR-READ-REIN.                                               
                                                                 
    IF REIN-OPEN-SW = ' '                                        
        MOVE 'X'                       TO REIN-OPEN-SW           
        MOVE DTE-CLASIC-COMPANY-CD     TO REIN-SRCH-COMP-CD      
        MOVE 'A'                       TO REIN-SRCH-CODE         
        OPEN INPUT ERRTBL-IN                                     
        IF ERRTBL-FILE-STATUS = '00'  OR  '97'                   
            PERFORM REIN-CO-TABLE-BUILD THRU REIN-BUILD-EXIT     
         ELSE                                                    
            MOVE '21'                  TO ABEND-CODE-1           
            MOVE ERRTBL-FILE-STATUS    TO ABEND-CODE-2           
            DISPLAY '******************************'             
            DISPLAY '***** ERROR LOCATION 015 *****'             
            DISPLAY '******************************'             
            MOVE WS-ABEND-CODE        TO WS-RETURN-CODE          
            GO TO ABEND-PGM.                                     
                                                                 
    IF CR-REIN-TABLE = REIN-SRCH                                 
        GO TO 3019-RR-READ-REIN-X.                               
                                                                 
    MOVE +0                            TO REIN-SUB-1.            
    MOVE 999999                        TO REIN-LOW-TIME.         
    MOVE CR-REIN-TABLE                 TO REIN-SRCH.             
                                                                 
3011-RR-CHECK-REIN-SAVE-TABLE.                                   
                                                                 
    ADD +1                             TO REIN-SUB-1.            
    IF REIN-SUB-1 GREATER THAN REIN-MAX  OR                      
       REIN-SUB-1 GREATER THAN REIN-SUB-2                        
        GO TO 3015-RR-READ-ERRTBL.                               
                                                                 
    IF REIN-SEARCH = SR-CONTROL (REIN-SUB-1)                     
        MOVE SR-ENTRY (REIN-SUB-1)     TO REINSURANCE-RECORD     
        ACCEPT WS-TIME-OF-DAY   FROM TIME                        
        MOVE WS-TIME                   TO SR-TIME (REIN-SUB-1)   
        GO TO 3019-RR-READ-REIN-X.                               
                                                                 
    IF SR-TIME (REIN-SUB-1) LESS THAN REIN-LOW-TIME              
        MOVE SR-TIME (REIN-SUB-1)      TO REIN-LOW-TIME          
        MOVE REIN-SUB-1                TO REIN-LOW-SUB.          
                                                                 
    GO TO 3011-RR-CHECK-REIN-SAVE-TABLE.                         
                                                                 
3015-RR-READ-ERRTBL.                                             
                                                                 
    MOVE REIN-SEARCH                   TO RE-CONTROL-PRIMARY.    
                                                                 
    READ ERRTBL-IN.                                              
                                                                 
    IF ERRTBL-FILE-STATUS  = '23'                                
        DISPLAY 'INVALID REINSURANCE TABLE CODE - ' REIN-SRCH    
                                          ' ' CR-FULL-CONTROL    
        MOVE '24'                      TO ABEND-CODE-1           
        MOVE ERRTBL-FILE-STATUS        TO ABEND-CODE-2           
        DISPLAY '******************************'                 
        DISPLAY '***** ERROR LOCATION 016 *****'                 
        DISPLAY '******************************'                 
        MOVE WS-ABEND-CODE        TO WS-RETURN-CODE              
        GO TO ABEND-PGM.                                         
                                                                 
    IF ERRTBL-FILE-STATUS  = '00'  OR  '97'                      
        NEXT SENTENCE                                            
      ELSE                                                       
        MOVE '24'                      TO ABEND-CODE-1           
        MOVE ERRTBL-FILE-STATUS        TO ABEND-CODE-2           
        DISPLAY '******************************'                 
        DISPLAY '***** ERROR LOCATION 017 *****'                 
        DISPLAY '******************************'                 
        MOVE WS-ABEND-CODE        TO WS-RETURN-CODE              
        GO TO ABEND-PGM.                                         
                                                                 
    ADD +1                            TO ERRTBL-READS.           
                                                                 
    IF REIN-SUB-1 GREATER THAN REIN-MAX                          
        MOVE REINSURANCE-RECORD       TO SR-ENTRY (REIN-LOW-SUB) 
        ACCEPT WS-TIME-OF-DAY   FROM TIME                        
        MOVE WS-TIME                  TO SR-TIME (REIN-LOW-SUB)  
    ELSE                                                         
        MOVE REINSURANCE-RECORD       TO SR-ENTRY (REIN-SUB-1)   
        ACCEPT WS-TIME-OF-DAY   FROM TIME                        
        MOVE WS-TIME                  TO SR-TIME (REIN-SUB-1)    
        MOVE REIN-SUB-1               TO REIN-SUB-2.             
                                                                 
3019-RR-READ-REIN-X.                                             
    EXIT.                                                        
                                                                 
3020-CALCULATE-REINSURANCE.                                      
                                                                 
*>    IF DTE-CLIENT = '???'  AND                                   
*>       (CR-REIN-TABLE = 'SE1')                                   
*>        PERFORM 3640-NSP-REIN-RATING-ROUTINE THRU 3669-NSP-REIN-X
*>        IF CR-STATE NOT = STATE-L                                
*>            MOVE CR-STATE             TO STATE-L                 
*>            PERFORM 8200-STATE-CODE-LOOKUP                       
*>                         THRU 8299-STATE-LOOKUP-X.               
                                                                 
                                                                 
*>    IF DTE-CLIENT = 'DCC'                                        
*>       IF CR-LF-NSP-PRM NOT = ZEROS                              
*>          COMPUTE CR-LF-NSP-PRM ROUNDED =                        
*>             (CR-LFPRM + CR-LFPRM-ALT) -                         
*>             ((CR-LFPRM + CR-LFPRM-ALT) * CR-LCOM-L (1))         
*>       END-IF                                                    
*>       IF CR-AH-NSP-PRM NOT = ZEROS                              
*>          COMPUTE CR-AH-NSP-PRM ROUNDED =                        
*>          CR-AHPRM -                                             
*>          (CR-AHPRM * CR-LCOM-AH (1))                            
*>       END-IF                                                    
*>    END-IF                                                       
                                                                 
                                                                 
    IF RE-REMAINING (1) = 'I'                                    
        MOVE '*'                      TO RISK-REIN-FLAG          
    ELSE                                                         
        GO TO 3029-CONTINUE-REIN-CALC.                           
                                                                 
    MOVE SPACE                        TO REIN-CANCEL-LF-SW       
                                         REIN-CANCEL-AH-SW.      
    MOVE ZEROS                        TO REIN-EARN-LF-TERM       
                                         REIN-LCNC-EARN-TERM     
                                         REIN-LF-CLM-MONTHS      
                                         REIN-EARN-AH-TERM       
                                         REIN-ACNC-EARN-TERM     
                                         REIN-AH-PRIOR-CLMS-PAID 
                                         REIN-AH-CLM-MONTHS      
                                         REIN-AH-CLM-MTH-1       
                                         REIN-AH-CLM-MTH-2       
                                         REIN-AH-CLM-MTH-3       
                                         REIN-AH-CLM-MTH-4       
                                         REIN-AH-CLM-MTH-5.      
                                                                 
*>    MOVE CR-DT                        TO DC-GREG-DATE-CYMD.      
*>    MOVE 'L'                          TO DC-OPTION-CODE.         
*>    PERFORM 8000-DATE-CONVERT-ROUTINE THRU 8099-DATE-CONVERT-X.  
*>    MOVE DC-BIN-DATE-1                TO WS-CR-BIN-DATE.         
                                                                 
    IF CR-LFTYP = ZEROS  OR  SPACES                              
        GO TO 3023-CONTINUE.                                     
                                                                 
    MOVE EOM-RUN-DATE                 TO VALUATION-DT.           
                                                                 
    IF CR-LF-CANCEL-EXIT-DATE NOT = ZEROS                        
      IF CR-LF-CANCEL-EXIT-DATE LESS THAN VALUATION-DT           
          MOVE CR-LF-CANCEL-EXIT-DATE TO VALUATION-DT.           
                                                                 
    IF CR-LF-CLAIM-EXIT-DATE NOT = ZEROS                         
      IF CR-LF-CLAIM-EXIT-DATE LESS THAN VALUATION-DT            
          MOVE CR-LF-CLAIM-EXIT-DATE  TO VALUATION-DT.           
                                                                 
    MOVE VALUATION-DT                 TO DC-GREG-DATE-CYMD.      
    PERFORM 3025-CALC-ELAPSED-MONTHS THRU 3026-CALC-EXIT.        
                                                                 
    IF DC-ELAPSED-MONTHS GREATER THAN CR-LF-TERM                 
        MOVE CR-LF-TERM               TO MONTHS-DIFF-LF          
    ELSE                                                         
        MOVE DC-ELAPSED-MONTHS        TO MONTHS-DIFF-LF.         
                                                                 
    IF CR-LF-CANC-DT NOT = ZEROS                                 
        MOVE CR-LF-CANC-DT            TO DC-GREG-DATE-CYMD       
        PERFORM 3025-CALC-ELAPSED-MONTHS THRU 3026-CALC-EXIT     
        MOVE DC-ELAPSED-MONTHS        TO REIN-LCNC-EARN-TERM     
        IF CR-DT = CR-LF-CANC-DT                                 
            MOVE '*'                  TO REIN-CANCEL-LF-SW.      
                                                                 
    IF CR-DTH-DT NOT = ZEROS                                     
        MOVE CR-DTH-DT                TO DC-GREG-DATE-CYMD       
        PERFORM 3025-CALC-ELAPSED-MONTHS THRU 3026-CALC-EXIT     
        MOVE DC-ELAPSED-MONTHS        TO REIN-LF-CLM-MONTHS.     
                                                                 
    MOVE MONTHS-DIFF-LF               TO REIN-EARN-LF-TERM.      
                                                                 
                                                                 
3023-CONTINUE.                                                   
                                                                 
    IF CR-AHTYP = ZEROS  OR  SPACES                              
       GO TO 3029-CONTINUE-REIN-CALC.                            
                                                                 
    MOVE EOM-RUN-DATE                 TO VALUATION-DT.           
                                                                 
    IF CR-AH-CANCEL-EXIT-DATE NOT = ZEROS                        
      IF CR-AH-CANCEL-EXIT-DATE LESS THAN VALUATION-DT           
          MOVE CR-AH-CANCEL-EXIT-DATE TO VALUATION-DT.           
                                                                 
    IF CR-AH-SETTLEMENT-EXIT-DATE NOT = ZEROS                    
      IF CR-AH-SETTLEMENT-EXIT-DATE LESS THAN VALUATION-DT       
          MOVE CR-AH-SETTLEMENT-EXIT-DATE TO VALUATION-DT.       
                                                                 
    MOVE VALUATION-DT                 TO DC-GREG-DATE-CYMD.      
    PERFORM 3025-CALC-ELAPSED-MONTHS THRU 3026-CALC-EXIT.        
                                                                 
    IF DC-ELAPSED-MONTHS GREATER THAN CR-AH-TERM                 
        MOVE CR-AH-TERM               TO MONTHS-DIFF-AH          
    ELSE                                                         
        MOVE DC-ELAPSED-MONTHS        TO MONTHS-DIFF-AH.         
                                                                 
    IF CR-AH-CANC-DT NOT = ZEROS                                 
        MOVE CR-AH-CANC-DT           TO DC-GREG-DATE-CYMD        
        PERFORM 3025-CALC-ELAPSED-MONTHS THRU 3026-CALC-EXIT     
        MOVE DC-ELAPSED-MONTHS        TO REIN-ACNC-EARN-TERM     
        IF CR-DT = CR-AH-CANC-DT                                 
            MOVE '*'                  TO REIN-CANCEL-AH-SW.      
                                                                 
    IF CR-DIS-INCUR-DT (1) NOT = ZEROS                           
        MOVE CR-DIS-INCUR-DT (1)    TO DC-GREG-DATE-CYMD         
        PERFORM 3025-CALC-ELAPSED-MONTHS THRU 3026-CALC-EXIT     
        MOVE DC-ELAPSED-MONTHS        TO REIN-AH-CLM-MTH-1.      
                                                                 
    IF CR-DIS-INCUR-DT (2) NOT = ZEROS                           
        MOVE CR-DIS-INCUR-DT (2)    TO DC-GREG-DATE-CYMD         
        PERFORM 3025-CALC-ELAPSED-MONTHS THRU 3026-CALC-EXIT     
        MOVE DC-ELAPSED-MONTHS        TO REIN-AH-CLM-MTH-2.      
                                                                 
    IF CR-DIS-INCUR-DT (3) NOT = ZEROS                           
        MOVE CR-DIS-INCUR-DT (3)      TO DC-GREG-DATE-CYMD       
        PERFORM 3025-CALC-ELAPSED-MONTHS THRU 3026-CALC-EXIT     
        MOVE DC-ELAPSED-MONTHS        TO REIN-AH-CLM-MTH-3.      
                                                                 
    IF CR-DIS-INCUR-DT (4) NOT = ZEROS                           
        MOVE CR-DIS-INCUR-DT (4)    TO DC-GREG-DATE-CYMD         
        PERFORM 3025-CALC-ELAPSED-MONTHS THRU 3026-CALC-EXIT     
        MOVE DC-ELAPSED-MONTHS        TO REIN-AH-CLM-MTH-4.      
                                                                 
    IF CR-DIS-INCUR-DT (5) NOT = ZEROS                           
        MOVE CR-DIS-INCUR-DT (5)    TO DC-GREG-DATE-CYMD         
        PERFORM 3025-CALC-ELAPSED-MONTHS THRU 3026-CALC-EXIT     
        MOVE DC-ELAPSED-MONTHS        TO REIN-AH-CLM-MTH-5.      
                                                                 
    MOVE MONTHS-DIFF-AH               TO REIN-EARN-AH-TERM.      
                                                                 
                                                                 
    GO TO 3029-CONTINUE-REIN-CALC.                               
                                                                 
                                                                 
3025-CALC-ELAPSED-MONTHS.                                        
                                                                 
    MOVE 'L'                          TO DC-OPTION-CODE.         
    PERFORM 8000-DATE-CONVERT-ROUTINE THRU 8099-DATE-CONVERT-X.  
    MOVE DC-BIN-DATE-1                TO DC-BIN-DATE-2.          
    MOVE WS-CR-BIN-DATE               TO DC-BIN-DATE-1.          
    MOVE '1'                          TO DC-OPTION-CODE.         
    MOVE ' '                          TO DC-CENTURY-ADJUSTMENT.  
    MOVE ZEROS                        TO DC-ELAPSED-MONTHS       
                                         DC-ODD-DAYS-OVER        
                                         DC-ELAPSED-DAYS.        
    PERFORM 8000-DATE-CONVERT-ROUTINE THRU 8099-DATE-CONVERT-X.  
                                                                 
3026-CALC-EXIT.                                                  
    EXIT.                                                        
                                                                 
EJECT                                                            
