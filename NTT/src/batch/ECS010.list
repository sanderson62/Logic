*>********************************************************         
*>  THIS ROUTINE IS USED TO CALCULATE THE A&H IBNR       *         
*>  RESERVES USING A PERCENTAGE OF THE UNEARNED PREMIUM. *         
*>  THESE PERCENTAGES ARE ENTERED THROUGH SCREEN EL105A  *         
*>  (CARRIER CONTROLS) AND PASSED THROUGHOUT THE SYSTEM  *         
*>  IN THE DATE CARD.                                    *         
*>  'CARRIER-UEP-PCT' IS THE ACTUAL PERCENT PER CARRIER. *         
*>********************************************************         
                                                                 
4599A-CALC-IBNR.                                                 
                                                                 
    COMPUTE EW-CLM-IBNR =  ((EP-ISS-PRM - EP-CNC-PRM) -          
       (((EP-PRM-78 + EP-PRM-78-ADJ)                             
                             * CARRIER-R78-PCT (CLAS-INDEXCN)) + 
        ((EP-PRM-PR + EP-PRM-PR-ADJ)                             
                             * CARRIER-PRO-PCT (CLAS-INDEXCN)))) 
                        * CARRIER-UEP-PCT (CLAS-INDEXCN).        
                                                                 
    IF EW-CLM-IBNR NOT GREATER THAN ZEROS                        
        MOVE ZEROS                 TO EW-CLM-IBNR                
        GO TO 4599A-CALC-EXIT.                                   
                                                                 
    IF EP-REIN = 'R'                                             
        MOVE 'Y'                   TO W-REIN-PROCESS-IND.        
                                                                 
    PERFORM 4740-CREATE-DETAIL-EXTR THRU 4740-EXIT.              
                                                                 
4599A-CALC-EXIT.                                                 
     EXIT.                                                       
4599B-SEARCH.                                                    
     EXIT.                                                       
                                EJECT                            
*>*****************************************************************
*>**         CALCULATE OPTIONAL METHOD I B N R                  ***
*>*****************************************************************
                                                                 
4600-GET-OPT-IBNR.                                               
                                                                 
    IF  CR-POLICY-IS-ACTIVE                                      
        IF  CR-POLICY-IS-REISSUE                                 
            OR CR-POLICY-IS-MONTHLY                              
            IF  W-LIFE                                           
                GO TO 4600-EXIT                                  
            ELSE                                                 
                NEXT SENTENCE                                    
        ELSE                                                     
            NEXT SENTENCE                                        
    ELSE                                                         
        IF  W-LIFE                                               
            GO TO 4600-EXIT.                                     
                                                                 
    IF  W-LIFE                                                   
        PERFORM 4620-DETERMINE-LIFE-IBNR THRU 4620-EXIT          
    ELSE                                                         
        PERFORM 4640-GET-A-H-EARNED-PREMIUM THRU 4640-EXIT.      
                                                                 
4600-EXIT.                                                       
    EXIT.                                                        
                                                                 
4620-DETERMINE-LIFE-IBNR.                                        
                                                                 
    IF  CR-LF-POLICY-IS-ACTIVE                                   
        ADD +1                  TO W-LF-ACTIVE                   
        IF  CR-LF-POLICY-IS-REISSUE                              
            OR CR-LF-POLICY-IS-MONTHLY                           
            MOVE ZEROS          TO W-WK-IBNR                     
            ADD +1              TO W-LF-REISSUE                  
            GO TO 4620-EXIT                                      
        ELSE                                                     
            NEXT SENTENCE                                        
    ELSE                                                         
        ADD +1                  TO W-LF-INACTIVE                 
        MOVE ZEROS              TO W-WK-IBNR                     
        GO TO 4620-EXIT.                                         
                                                                 
    IF  CLAS-I-CALC-TYPE (CLAS-INDEXL) = 'B'                     
        MOVE ZEROS              TO W-WK-IBNR                     
        ADD +1                  TO W-LF-CALC-B                   
        GO TO 4620-EXIT.                                         
                                                                 
    IF  (CLAS-I-EP (CLAS-INDEXL) = 'B'                           
                AND                                              
            CLAS-I-CALC-TYPE (CLAS-INDEXL) NOT = 'L'             
                AND                                              
            W-BAL-REMTRM NOT GREATER THAN ZERO)                  
            OR                                                   
        (CLAS-I-EP (CLAS-INDEXL) = 'B'                           
                AND                                              
            CLAS-I-CALC-TYPE (CLAS-INDEXL) = 'L'                 
                AND                                              
            REM-TRM2 NOT GREATER THAN ZERO)                      
        MOVE ZEROS              TO W-WK-IBNR                     
        ADD +1                  TO W-LF-BALLOON                  
        GO TO 4620-EXIT                                          
    ELSE                                                         
        IF  (CLAS-I-EP (CLAS-INDEXL) NOT = 'B'                   
                AND                                              
            REM-TRM2 NOT GREATER THAN ZERO)                      
            ADD +1              TO W-LF-REMTERM-0                
            MOVE ZEROS          TO W-WK-IBNR                     
            GO TO 4620-EXIT.                                     
                                                                 
    PERFORM 4680-CALC-REMAINING-AMT-LF THRU 4680-EXIT.           
                                                                 
    IF  CP-REMAINING-AMT NOT GREATER THAN ZEROS                  
        ADD +1                  TO W-LF-REMAMT-0                 
        MOVE ZEROS              TO W-WK-IBNR                     
        GO TO 4620-EXIT.                                         
                                                                 
    IF  AM-LIFE-IBNR-PCT NUMERIC                                 
            AND                                                  
        AM-LIFE-IBNR-PCT GREATER THAN ZEROS                      
        COMPUTE W-WK-IBNR ROUNDED                                
            = CP-REMAINING-AMT                                   
            / 1000                                               
            * AM-LIFE-IBNR-PCT                                   
    ELSE                                                         
        COMPUTE W-WK-IBNR ROUNDED                                
            = CP-REMAINING-AMT                                   
            / 1000                                               
            * COMPANY-IBNR-LIFE-FACTOR.                          
                                                                 
    IF  CR-LF-IS-REIN-ONLY                                       
        ADD +1                  TO W-LF-REIN                     
    ELSE                                                         
        ADD +1                  TO W-LF-DATA-USED                
        ADD W-WK-IBNR           TO W-LIFE-IBNR-IN                
        ADD CP-REMAINING-AMT    TO W-LIFE-REMAINING-AMT          
        IF  CR-LFTYP = '01'                                      
            ADD CP-REMAINING-AMT                                 
                                TO W-01-REMAMT                   
            ADD +1              TO W-01-DATA-USED                
        ELSE                                                     
        IF  CR-LFTYP = '02'                                      
            ADD CP-REMAINING-AMT                                 
                                TO W-02-REMAMT                   
            ADD +1              TO W-02-DATA-USED                
        ELSE                                                     
        IF  CR-LFTYP = '03'                                      
            ADD CP-REMAINING-AMT                                 
                                TO W-03-REMAMT                   
            ADD +1              TO W-03-DATA-USED                
        ELSE                                                     
        IF  CR-LFTYP = '04'                                      
            ADD CP-REMAINING-AMT                                 
                                TO W-04-REMAMT                   
            ADD +1              TO W-04-DATA-USED                
        ELSE                                                     
        IF  CR-LFTYP = '05'                                      
            ADD CP-REMAINING-AMT                                 
                                TO W-05-REMAMT                   
            ADD +1              TO W-05-DATA-USED                
        ELSE                                                     
        IF  CR-LFTYP = '06'                                      
            ADD CP-REMAINING-AMT                                 
                                TO W-06-REMAMT                   
            ADD +1              TO W-06-DATA-USED                
        ELSE                                                     
        IF  CR-LFTYP = '30'                                      
            ADD CP-REMAINING-AMT                                 
                                TO W-30-REMAMT                   
            ADD +1              TO W-30-DATA-USED                
        ELSE                                                     
        IF  CR-LFTYP = '31'                                      
            ADD CP-REMAINING-AMT                                 
                                TO W-31-REMAMT                   
            ADD +1              TO W-31-DATA-USED                
        ELSE                                                     
            ADD CP-REMAINING-AMT                                 
                                TO W-XX-REMAMT                   
            ADD +1              TO W-XX-DATA-USED.               
                                                                 
    IF  CR-POLICY-IS-REIN-ONLY                                   
            OR                                                   
        CR-LF-IS-REIN-ONLY                                       
        ADD ISS-BEN             TO EPEC-REIN-ISS-BEN (SUB3)      
        MOVE 'R'                TO EPEC-REIN-ONLY-IND (SUB3)     
        ADD W-WK-IBNR           TO EPEC-LIFE-IBNR (SUB3)         
    ELSE                                                         
        ADD W-WK-IBNR           TO EPEC-LIFE-IBNR (SUB3).        
                                                                 
4620-EXIT.                                                       
    EXIT.                                                        
                                EJECT                            
4640-GET-A-H-EARNED-PREMIUM.                                     
                                                                 
    IF  CR-AH-POLICY-IS-ACTIVE                                   
        ADD +1                  TO W-AH-ACTIVE                   
        IF  CR-AH-POLICY-IS-REISSUE                              
            OR CR-AH-POLICY-IS-MONTHLY                           
            ADD +1              TO W-AH-REISSUE                  
        ELSE                                                     
            NEXT SENTENCE                                        
    ELSE                                                         
        ADD +1                  TO W-AH-INACTIVE.                
                                                                 
    IF  CLAS-I-CALC-TYPE (CLAS-INDEXA) = 'B'                     
        ADD +1                  TO W-AH-CALC-B                   
        GO TO 4640-EXIT.                                         
                                                                 
    IF  REM-TRM2 NOT GREATER THAN ZEROS                          
        MOVE 'Y'                TO W-RTRM-0-SW                   
    ELSE                                                         
        MOVE SPACES             TO W-RTRM-0-SW.                  
                                                                 
    PERFORM 4660-DETERMINE-EARNED-PREMIUM THRU 4660-EXIT.        
                                                                 
*>***** NOTE:  THERE IS AN ASSUMPTION THAT STATISTICALLY ONLY ONE  
*>*****        ACTUALLY CLAIM PER 90 DAY PERIOD AND THAT THE CLAIM 
*>*****        ITD FIELD (CR-DISAMT) CONTAINS ONLY THE AMOUNT OF   
*>*****        THAT CLAIM.                                         
                                                                 
    PERFORM 4650-DETERMINE-CLM-AMT-AH THRU 4650-EXIT.            
                                                                 
    IF  CR-POLICY-IS-REIN-ONLY                                   
             OR                                                  
        CR-AH-IS-REIN-ONLY                                       
        ADD +1                  TO W-AH-REIN                     
        ADD ISS-BEN             TO EPEC-REIN-ISS-BEN (SUB3)      
        MOVE 'R'                TO EPEC-REIN-ONLY-IND (SUB3)     
        ADD W-CLMAMT            TO EPEC-CLAIMS-MADE-PAID (SUB3)  
        ADD W-EARNED-PREMIUM    TO EPEC-EARNED-PREMIUM-AH (SUB3) 
        GO TO 4640-EXIT.                                         
                                                                 
    ADD W-EARNED-PREMIUM        TO EPEC-EARNED-PREMIUM-AH (SUB3) 
                                   W-EPEC-EP-IN.                 
    ADD W-CLMAMT                TO EPEC-CLAIMS-MADE-PAID (SUB3)  
                                   W-CLAIMS-MADE-PAID-IN.        
    ADD +1                      TO W-AH-DATA-USED.               
    ADD W-EARNED-PREMIUM-1      TO W-EPEC-EP-1.                  
    ADD W-EARNED-PREMIUM-2      TO W-EPEC-EP-2.                  
                                                                 
    IF  W-CLMAMT GREATER THAN ZEROS                              
        ADD +1                  TO W-AH-CLAIM-CTR                
        ADD W-EARNED-PREMIUM    TO W-AH-CLAIM-EP.                
                                                                 
    IF  W-RTRM-0                                                 
            AND                                                  
        NOT CR-POLICY-IS-REIN-ONLY                               
            AND                                                  
        NOT CR-AH-IS-REIN-ONLY                                   
        ADD W-CLMAMT            TO W-RTRM-0-CLMAMT               
        ADD W-EARNED-PREMIUM-1  TO W-RTRM-EP-1                   
        ADD W-EARNED-PREMIUM-2  TO W-RTRM-EP-2                   
        ADD W-EARNED-PREMIUM    TO W-RTRM-EP                     
        ADD +1                  TO W-AH-REMTERM-0.               
                                                                 
4640-EXIT.                                                       
    EXIT.                                                        
                                EJECT                            
4650-DETERMINE-CLM-AMT-AH.                                       
                                                                 
    MOVE ZEROS                  TO W-CLMAMT.                     
                                                                 
    IF  CR-DIS-DT NOT GREATER THAN ZEROS                         
            OR                                                   
        CR-DIS-DT NOT GREATER THAN W-CUTOFF-DATE                 
        GO TO 4650-EXIT.                                         
                                                                 
    IF  CR-DIS-PTO-DT GREATER THAN ZEROS                         
            AND                                                  
        CR-DIS-PTO-DT GREATER THAN CR-DIS-DT                     
        MOVE CR-DIS-DT              TO DC-GREG-DATE-CYMD         
        MOVE 'L'                    TO DC-OPTION-CODE            
        PERFORM 8000-DATE-CONVERT-ROUTINE                        
            THRU 8099-DATE-CONVERT-X                             
        MOVE DC-BIN-DATE-1          TO W-DIS-DT                  
        MOVE CR-DIS-PTO-DT          TO DC-GREG-DATE-CYMD         
        MOVE 'L'                    TO DC-OPTION-CODE            
        PERFORM 8000-DATE-CONVERT-ROUTINE                        
            THRU 8099-DATE-CONVERT-X                             
        MOVE DC-BIN-DATE-1          TO W-DIS-PTO-DT              
        IF  DATE-CONVERSION-ERROR                                
            DISPLAY 'CERT DATES ERROR' CR-FULL-CONTROL           
            MOVE 'CERT DATES ERROR' TO WS-ABEND-MESSAGE          
            MOVE DC-ERROR-CODE      TO WS-ABEND-FILE-STATUS      
            MOVE 0303               TO WS-ABEND-CODE             
            MOVE WS-ABEND-CODE      TO WS-RETURN-CODE            
            GO TO ABEND-PGM                                      
        ELSE                                                     
            MOVE W-DIS-DT           TO DC-BIN-DATE-1             
            MOVE W-DIS-PTO-DT       TO DC-BIN-DATE-2             
            MOVE '1'                TO DC-OPTION-CODE            
            MOVE ZEROS              TO DC-ELAPSED-MONTHS         
                                       DC-ELAPSED-DAYS           
            PERFORM 8000-DATE-CONVERT-ROUTINE                    
                THRU 8099-DATE-CONVERT-X                         
            IF  DATE-CONVERSION-ERROR                            
                DISPLAY 'DIS TO PTO DATE ERROR '                 
                    CR-FULL-CONTROL                              
                MOVE 'DIS TO PTO DATE ERROR '                    
                                    TO WS-ABEND-MESSAGE          
                MOVE DC-ERROR-CODE  TO WS-ABEND-FILE-STATUS      
                MOVE 0303           TO WS-ABEND-CODE             
                MOVE WS-ABEND-CODE  TO WS-RETURN-CODE            
                GO TO ABEND-PGM                                  
            ELSE                                                 
                COMPUTE W-CLMAMT ROUNDED                         
                    = CR-AHAMT                                   
                    * (DC-ELAPSED-DAYS / 30)                     
                GO TO 4650-EXIT.                                 
                                                                 
    IF  CR-DIS-PAY-DT NOT GREATER THAN ZEROS                     
            OR                                                   
        CR-DIS-PAY-DT LESS THAN CR-DIS-DT                        
        GO TO 4650-EXIT.                                         
                                                                 
    COMPUTE W-90-DAY-MAX ROUNDED                                 
        = CR-AHAMT * 3.                                          
                                                                 
    IF  CR-DISAMT NOT GREATER THAN W-90-DAY-MAX                  
        MOVE CR-DISAMT          TO W-CLMAMT                      
    ELSE                                                         
        MOVE W-90-DAY-MAX       TO W-CLMAMT.                     
                                                                 
    IF  NOT CR-POLICY-IS-REIN-ONLY                               
             AND                                                 
        NOT CR-AH-IS-REIN-ONLY                                   
        ADD +1          TO W-CLM-EXCEPT-CNT                      
        ADD W-CLMAMT    TO W-CLM-EXCEPT-AMT.                     
                                                                 
4650-EXIT.                                                       
    EXIT.                                                        
                                EJECT                            
4660-DETERMINE-EARNED-PREMIUM.                                   
                                                                 
    MOVE ZEROS                  TO W-EARNED-PREMIUM.             
                                                                 
    PERFORM 4670-CALC-EP THRU 4670-EXIT.                         
                                                                 
    MOVE W-EARNED-PREMIUM       TO W-EARNED-PREMIUM-1.           
                                                                 
    IF  CR-ENTRY-DATE NOT GREATER THAN W-CUTOFF-DATE             
        MOVE ZEROS              TO W-EARNED-PREMIUM              
        MOVE W-CUTOFF-DATE      TO VALUATION-DT                  
        MOVE '7'                TO W-VAL-SOURCE                  
        PERFORM 8500-REMAINING-TERM-ROUTINE                      
            THRU 8599-REM-TERM-X                                 
        PERFORM 4666-SAVE-AHCR-DATA THRU 4666-EXIT               
        PERFORM 4800-CALC-EARNED-PREMIUM-AH THRU 4899-EXIT       
        PERFORM 4668-RESTORE-AHCR-DATA THRU 4668-EXIT            
        MOVE EPR-R78            TO W-OPT-R78                     
        MOVE EPR-PRO            TO W-OPT-PRO                     
        MOVE EPR-ST             TO W-OPT-ST                      
        PERFORM 4670-CALC-EP THRU 4670-EXIT                      
        MOVE W-EARNED-PREMIUM   TO W-EARNED-PREMIUM-2            
    ELSE                                                         
        MOVE ZEROS              TO W-EARNED-PREMIUM-2.           
                                                                 
    COMPUTE W-EARNED-PREMIUM ROUNDED                             
        = W-EARNED-PREMIUM-1 - W-EARNED-PREMIUM-2.               
                                                                 
    IF  W-EARNED-PREMIUM LESS THAN ZERO                          
        ADD +1                  TO W-EP-LESS-THAN-0              
        MOVE ZERO               TO W-EARNED-PREMIUM.             
                                                                 
4660-EXIT.                                                       
    EXIT.                                                        
                                EJECT                            
4666-SAVE-AHCR-DATA.                                             
                                                                 
    MOVE SPACES                 TO WX-STATUS-AT-CANCEL           
                                   WX-STATUS-AT-SETTLEMENT.      
                                                                 
    MOVE ZEROS                  TO WX-CANC-DT                    
                                   WX-EOM-RUN-DATE               
                                   WX-SETTLEMENT-EXIT-DATE.      
                                                                 
    IF  CR-AH-CANC-DT GREATER THAN W-CUTOFF-DATE                 
        MOVE CR-AH-CANC-DT      TO WX-CANC-DT                    
        MOVE CR-AH-STATUS-AT-CANCEL                              
                                TO WX-STATUS-AT-CANCEL           
        MOVE ZEROS              TO CR-AH-CANC-DT                 
                                   WS-CR-AH-CANC-DT-N            
        MOVE SPACES             TO CR-AH-STATUS-AT-CANCEL.       
                                                                 
