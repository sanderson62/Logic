*>*****************************************************************
*>**           B U I L D   C A N C E L   E X T R A C T          ***
*>*****************************************************************
                                                                 
2200-BUILD-CANCEL-EXTRACT.                                       
                                                                 
    PERFORM 2000-INITIALIZE-DETAIL-EXTRACT THRU 2099-INIT-DTL-X. 
                                                                 
    MOVE 'C'                           TO DE-TRANS.              
                                                                 
    IF CR-LFTYP = ZERO                                           
        GO TO 2250-CONTINUE-CANCEL-BUILD.                        
                                                                 
    IF CLAS-I-CALC-TYPE (CLAS-INDEXL) NOT = 'Z'                  
        IF (CR-LF-STATUS-AT-CANCEL = ' ' OR '7')  AND            
            CR-LFRFND = +0                                       
             MOVE +0                   TO TEMP-5                 
             GO TO 2250-CONTINUE-CANCEL-BUILD.                   
                                                                 
    IF CLAS-I-CALC-TYPE (CLAS-INDEXL) = 'Z'                      
         IF CR-LFRFND = +0                                       
             GO TO 2250-CONTINUE-CANCEL-BUILD.                   
                                                                 
    MOVE +1 TO TEMP-5.                                           
    MOVE CR-LF-TERM             TO ORIG-TERM.                    
    MOVE CR-LF-CANC-DT          TO VALUATION-DT.                 
    MOVE LIFE-OVERRIDE-L1       TO WE-LF-AH.                     
    MOVE '1'                    TO W-VAL-SOURCE.                 
    PERFORM 8500-REMAINING-TERM-ROUTINE THRU 8599-REM-TERM-X.    
                                                                 
    IF CLAS-I-RL-AH (CLAS-INDEXL) = 'L' OR 'P'                   
        MOVE CR-LFAMT TO DE-LF-CNBEN                             
        GO TO 2250-CONTINUE-CANCEL-BUILD.                        
                                                                 
    IF CLAS-I-CALC-TYPE (CLAS-INDEXL) = 'Z'                      
        GO TO 2220-BLD-DE-CANCEL-SUM.                            
                                                                 
    IF REM-TRM3 = CR-LF-TERM                                     
        MOVE CR-LFAMT TO DE-LF-CNBEN                             
        GO TO 2250-CONTINUE-CANCEL-BUILD.                        
                                                                 
    IF CLAS-I-EP (CLAS-INDEXL) = 'T'                             
        GO TO 2210-BLD-DE-CANCEL-TEX.                            
                                                                 
    IF DTE-CLIENT = 'NCL'                                        
        IF CR-DT LESS THAN 19910101                              
            GO TO 2205-BLD-DE-CANCEL-BEN.                        
                                                                 
    IF CLAS-I-EP (CLAS-INDEXL) = 'N'                             
        GO TO 2230-BLD-DE-CANCEL-NP.                             
                                                                 
    IF DTE-CLIENT = 'FUL'                                        
        GO TO 2240-BLD-DE-CANCEL-FUL.                            
                                                                 
*>    IF STATE-ABBR (CLAS-INDEXS) = 'OH'                           
    IF (STATE-ABBR (CLAS-INDEXS) = 'OH') AND                     
       (CR-RATING-CLASS NOT = 'L ')                              
        IF (CR-APR GREATER THAN ZERO)  AND                       
           (CR-LF-TERM GREATER THAN 060)  AND                    
           (CR-DT GREATER THAN 19831031)                         
             GO TO 2230-BLD-DE-CANCEL-NP.                        
                                                                 
    IF STATE-ABBR (CLAS-INDEXS) = 'MT'                           
        IF (CR-APR GREATER THAN ZERO)  AND                       
           (CR-LF-TERM GREATER THAN 061)  AND                    
           (CR-DT GREATER THAN 19830318)                         
             GO TO 2230-BLD-DE-CANCEL-NP.                        
                                                                 
    IF STATE-ABBR (CLAS-INDEXS) = 'UT'                           
        IF (CR-APR GREATER THAN ZERO)  AND                       
           (CR-LF-TERM GREATER THAN 062)  AND                    
           (CR-DT GREATER THAN 19810831)  AND                    
           (CR-DT LESS THAN 19830901)                            
             GO TO 2230-BLD-DE-CANCEL-NP.                        
                                                                 
    IF STATE-ABBR (CLAS-INDEXS) = 'RI'                           
        IF (CR-APR GREATER THAN ZERO)  AND                       
           (CR-LF-TERM GREATER THAN 060)  AND                    
           (CR-DT GREATER THAN 19831231)                         
             GO TO 2230-BLD-DE-CANCEL-NP.                        
                                                                 
2205-BLD-DE-CANCEL-BEN.                                          
                                                                 
    IF CR-LF-TERM EQUAL +0                                       
        MOVE +0          TO TEMP-5                               
    ELSE                                                         
        COMPUTE TEMP-5 = REM-TRM3 / CR-LF-TERM.                  
                                                                 
    COMPUTE DE-LF-CNBEN = CR-LFAMT * TEMP-5.                     
                                                                 
    GO TO 2250-CONTINUE-CANCEL-BUILD.                            
                                                                 
2210-BLD-DE-CANCEL-TEX.                                          
                                                                 
    DIVIDE CR-LFAMT BY CR-LF-TERM                                
        GIVING TEX-FACT-1.                                       
    DIVIDE REM-TRM3 BY CR-PMT-FREQ                               
        GIVING TEX-FACT-2                                        
            REMAINDER TEX-FACT-3.                                
    IF TEX-FACT-3 NOT = ZERO                                     
        ADD +1 TO TEX-FACT-2.                                    
    COMPUTE DE-LF-CNBEN =                                        
        (TEX-FACT-1 * (TEX-FACT-2 * CR-PMT-FREQ)).               
                                                                 
    IF CR-LFAMT EQUAL +0                                         
        MOVE +0                  TO TEMP-5                       
    ELSE                                                         
        COMPUTE TEMP-5 = DE-LF-CNBEN / CR-LFAMT.                 
                                                                 
    GO TO 2250-CONTINUE-CANCEL-BUILD.                            
                                                                 
2220-BLD-DE-CANCEL-SUM.                                          
                                                                 
    MOVE CR-SUM-CAN-CNT-YTD      TO DE-CANCEL-CNT-YTD.           
    MOVE CR-SUM-CAN-CNT-ITD      TO DE-CANCEL-CNT-ITD.           
                                                                 
    IF CR-LFRFND NOT LESS CR-LFPRM                               
        MOVE CR-LFAMT           TO DE-LF-CNBEN                   
         GO TO 2250-CONTINUE-CANCEL-BUILD.                       
                                                                 
    COMPUTE TEMP-6 ROUNDED = CR-LFRFND / CR-LFPRM.               
    COMPUTE TEMP-7 ROUNDED = TEMP-6 * CR-LFAMT.                  
    IF CLAS-I-RL-AH (CLAS-INDEXL) = 'L' OR 'P'                   
        MOVE TEMP-7             TO DE-LF-CNBEN                   
      ELSE                                                       
        COMPUTE TEMP-5 = REM-TRM3 / CR-LF-TERM                   
        COMPUTE DE-LF-CNBEN = TEMP-7 * TEMP-5.                   
                                                                 
    IF CR-LFAMT EQUAL +0                                         
        MOVE +0                  TO TEMP-5                       
    ELSE                                                         
        COMPUTE TEMP-5 ROUNDED =  DE-LF-CNBEN / CR-LFAMT.        
                                                                 
    IF CLAS-I-EP (CLAS-INDEXL) NOT = 'N'                         
        GO TO 2250-CONTINUE-CANCEL-BUILD.                        
                                                                 
2230-BLD-DE-CANCEL-NP.                                           
                                                                 
    MOVE CR-DT                      TO  DC-GREG-DATE-CYMD.       
    MOVE 'L'                        TO  DC-OPTION-CODE.          
    PERFORM 8000-DATE-CONVERT-ROUTINE THRU 8099-DATE-CONVERT-X.  
    MOVE DC-BIN-DATE-1              TO  CP-CERT-EFF-DT.          
                                                                 
    IF CR-LOAN-1ST-PMT-DT NOT = ZEROS                            
      MOVE CR-LOAN-1ST-PMT-DT       TO  DC-GREG-DATE-1-YMD       
      MOVE '3'                      TO  DC-OPTION-CODE           
      PERFORM 8000-DATE-CONVERT-ROUTINE THRU 8099-DATE-CONVERT-X 
      MOVE DC-BIN-DATE-1            TO  CP-FIRST-PAY-DATE        
    ELSE                                                         
      MOVE CP-CERT-EFF-DT           TO  DC-BIN-DATE-1            
      MOVE '6'                      TO  DC-OPTION-CODE           
      MOVE +1                       TO  DC-ELAPSED-MONTHS        
      MOVE +0                       TO  DC-ELAPSED-DAYS          
      PERFORM 8000-DATE-CONVERT-ROUTINE THRU 8099-DATE-CONVERT-X 
      MOVE DC-BIN-DATE-2            TO  CP-FIRST-PAY-DATE.       
                                                                 
    MOVE CR-RATING-CLASS            TO  CP-CLASS-CODE            
    MOVE CR-LFAMT                   TO  CP-ORIGINAL-BENEFIT.     
    MOVE CR-APR                     TO  CP-LOAN-APR.             
    MOVE CR-LF-TERM                 TO  CP-ORIGINAL-TERM.        
    MOVE CR-LOAN-TERM               TO  CP-LOAN-TERM.            
    MOVE REM-TRM3                   TO  CP-REMAINING-TERM.       
    MOVE CLAS-I-CALC-TYPE (CLAS-INDEXL) TO  CP-SPECIAL-CALC-CD.  
    MOVE CLAS-I-RL-AH     (CLAS-INDEXL) TO  CP-BENEFIT-TYPE.     
    MOVE CLAS-I-EP        (CLAS-INDEXL) TO  CP-EARNING-METHOD.   
                                                                 
    MOVE ZEROS                  TO CP-R-MAX-TOT-BEN              
    IF (DTE-CLIENT = 'DCC')                                      
       AND (AM-DCC-PRODUCT-CODE <> spaces)                       
       if cr-clp-state = spaces                                  
          move cr-state         to cr-clp-state                  
       end-if                                                    
       if cr-lftyp not = '00' and '  ' and 'DD'                  
          move 'L'              to ws-dd-ben-type                
          move cr-lftyp         to ws-dd-ben-code                
       else                                                      
          move 'A'              to ws-dd-ben-type                
          move cr-ahtyp         to ws-dd-ben-code                
       end-if                                                    
       PERFORM VARYING D1 FROM +1 BY +1 UNTIL                    
          ((CR-clp-STATE = DD-STATE (D1))                        
          AND (AM-DCC-PRODUCT-CODE = DD-PRODUCT-CD (D1))         
          AND (ws-dd-ben-type = DD-BEN-TYPE   (D1))              
          AND (ws-dd-ben-code = DD-BEN-CODE   (D1))              
          AND (CR-DT         < DD-PROD-EXP-DT (D1)))             
                          OR                                     
          D1 > max-pdef                                          
       END-PERFORM                                               
       IF D1 <= max-pdef                                         
          PERFORM VARYING P1 FROM +1 BY +1 UNTIL                 
             (P1 > +11)                                          
             OR (DD-PROD-CODE (D1 P1) = 'L')                     
          END-PERFORM                                            
          IF P1 < +12                                            
             MOVE DD-MAX-BEN-AMT (D1 P1)                         
                                TO CP-R-MAX-TOT-BEN              
          END-IF                                                 
       END-IF                                                    
    END-IF                                                       
                                                                 
    CALL 'ELRAMTX' USING CALCULATION-PASS-AREA.                  
                                                                 
    IF CLAS-I-CALC-TYPE (CLAS-INDEXL) = 'Z'                      
        COMPUTE DE-LF-CNBEN =                                    
                CP-REMAMT-FACTOR * (TEMP-7 / +1000)              
     ELSE                                                        
         COMPUTE DE-LF-CNBEN ROUNDED =                           
                 CP-REMAMT-FACTOR * (CR-LFAMT / +1000).          
                                                                 
    IF CR-LFAMT EQUAL +0                                         
        MOVE +0                  TO TEMP-5                       
    ELSE                                                         
        COMPUTE TEMP-5 = DE-LF-CNBEN / CR-LFAMT.                 
                                                                 
    GO TO 2250-CONTINUE-CANCEL-BUILD.                            
                                                                 
2240-BLD-DE-CANCEL-FUL.                                          
                                                                 
    MOVE CR-APR                  TO NP-APR.                      
    MOVE CR-LF-TERM              TO NP-ORIG.                     
    MOVE REM-TRM3                TO NP-REM.                      
    MOVE '2'                     TO NP-OPT.                      
    MOVE CR-AHPRM                TO NP-AHPRM.                    
    MOVE CR-ACCOUNT              TO NP-ACCOUNT.                  
    MOVE CR-LFAMT                TO NP-BENEFIT.                  
                                                                 
*>    CALL 'FULNETRM'                                              
*>        USING NP-APR NP-ORIG NP-REM NP-OPT NP-BENEFIT NP-AHPRM   
*>              NP-ACCOUNT NP-REMAINING.                           
                                                                 
    MOVE NP-REMAINING            TO DE-LF-CNBEN.                 
                                                                 
    IF CR-LFAMT EQUAL +0                                         
        MOVE +0                  TO TEMP-5                       
    ELSE                                                         
        COMPUTE TEMP-5 = DE-LF-CNBEN / CR-LFAMT.                 
                                                                 
2250-CONTINUE-CANCEL-BUILD.                                      
                                                                 
    MOVE CR-LFRFND                   TO DE-LF-RFND.              
    MOVE CR-LF-STATUS-AT-CANCEL      TO DE-LF-PREV-STAT.         
    MOVE CR-LF-CANC-DT               TO DE-LF-CANC-DTE           
                                        WS-DE-LF-CANC-DTE-N.     
    MOVE CR-LF-CANCEL-EXIT-DATE      TO DE-LF-CANC-EXIT-DT       
                                        WS-DE-LF-CANC-EXIT-DT-N. 
    MOVE CR-LF-COMM-CHARGEBACK       TO DE-LF-COMM-CHARGEBACK.   
                                                                 
                                                                 
    MOVE ZEROS                  TO DCC-DDF-REM-TRM3              
    IF (DTE-CLIENT = 'DCC')                                      
       AND (AM-DCC-PRODUCT-CODE = 'DDF')                         
       AND (CR-AHTYP NOT = '00' AND '  ')                        
       MOVE AH-OVERRIDE-L1      TO WE-LF-AH                      
       MOVE CR-AH-TERM          TO ORIG-TERM                     
       MOVE CR-AH-CANC-DT       TO VALUATION-DT                  
       MOVE '1'                 TO W-VAL-SOURCE                  
       DISPLAY ' 2250 B4 RTRM ' REM-TRM1 ' ' cr-cert-no          
       PERFORM 8500-REMAINING-TERM-ROUTINE                       
                                THRU 8599-REM-TERM-X             
       DISPLAY ' 2250 AFTER RTRM ' REM-TRM1                      
       MOVE REM-TRM1            TO DE-DCC-DDF-REM-TRM3           
    END-IF                                                       
                                                                 
    MOVE CR-AHRFND                   TO DE-AH-RFND.              
                                                                 
    IF DTE-CLIENT = 'DCC'                                        
       IF DE-AH-RFND = ZEROS                                     
          MOVE ZEROS                 TO DE-ADDL-CLP              
       END-IF                                                    
    END-IF                                                       
                                                                 
    MOVE CR-AH-STATUS-AT-CANCEL      TO DE-AH-PREV-STAT.         
    MOVE CR-AH-CANC-DT               TO DE-AH-CANC-DTE           
                                        WS-DE-AH-CANC-DTE-N.     
    MOVE CR-AH-CANCEL-EXIT-DATE      TO DE-AH-CANC-EXIT-DT       
                                        WS-DE-AH-CANC-EXIT-DT-N. 
    MOVE CR-AH-COMM-CHARGEBACK       TO DE-AH-COMM-CHARGEBACK.   
                                                                 
    IF PB-LAST-MAINT-BY = 'E570'                                 
        MOVE 'R'                     TO DE-AUTO-GEND-IND.        
                                                                 
    IF CHECK-BILLED                                              
        MOVE 'N' TO BILLED-SWITCH                                
    ELSE                                                         
        GO TO 2259-END-CANCEL-BUILD.                             
                                                                 
    MOVE +1                        TO SUB1.                      
2255-GA-BILL-LOOP.                                               
                                                                 
    IF PB-GA-BILL-DT (SUB1) NOT = LOW-VALUES  AND  SPACES        
        MOVE 'B'                   TO DE-GA-BILL-STATUS (SUB1).  
                                                                 
    IF SUB1 LESS THAN +5                                         
        ADD +1 TO SUB1                                           
        GO TO 2255-GA-BILL-LOOP.                                 
                                                                 
    MOVE PB-RECORD-BILL            TO DE-BILL-SW.                
    MOVE PB-C-REFUND-SW            TO DE-REFUND-SW.              
    MOVE ZEROS                     TO DE-BILLED-LFPRM            
                                      DE-BILLED-LFRFND           
                                      DE-BILLED-AHPRM            
                                      DE-BILLED-AHRFND.          
                                                                 
    IF PB-BILLED-DT = LOW-VALUES  OR  SPACES                     
        IF PB-LF-BILLED-AMTS = ZERO  AND                         
           PB-AH-BILLED-AMTS = ZERO                              
            MOVE ' '                    TO DE-BILL-STATUS        
        ELSE                                                     
            IF PB-LF-BILLED-AMTS = PB-C-LF-CANCEL-AMT  AND       
               PB-AH-BILLED-AMTS = PB-C-AH-CANCEL-AMT            
                MOVE 'B'                TO DE-BILL-STATUS        
            ELSE                                                 
                MOVE 'R'                TO DE-BILL-STATUS        
                MOVE PB-LF-BILLED-AMTS  TO DE-BILLED-LFRFND      
                MOVE PB-AH-BILLED-AMTS  TO DE-BILLED-AHRFND      
    ELSE                                                         
        MOVE 'B'                        TO DE-BILL-STATUS.       
                                                                 
2259-END-CANCEL-BUILD.                                           
    EXIT.                                                        
                                                                 
2290-WRITE-CANCEL-EXTRACT.                                       
                                                                 
    PERFORM 2500-WRITE-DETAIL-EXTRACTS THRU 2599-WRT-DETAIL-X.   
                                                                 
2299-BLD-CANCEL-X.                                               
    EXIT.                                                        
EJECT                                                            
