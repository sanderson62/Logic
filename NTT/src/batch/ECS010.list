    IF  MONTHS-DIFF-LF GREATER THAN AM-COMM-CHARGEBACK (SUB)     
        MOVE  'N'        TO  WS-LF-CHARGEBACK-SW (SUB).          
                                                                 
    GO TO  4120-LIFE-CHARGEBACK-LOOP.                            
                                                                 
                                                                 
4120-CONTINUE-LIFE-CANCEL.                                       
                                                                 
    COMPUTE EPR-R78 = (WE-LFPRM + WE-LFPRM-ALT) - WE-LFRFND.     
    MOVE EPR-R78 TO EPR-PRO  EPR-ST.                             
                                                                 
    IF WE-LFRFND = +0 AND WE-DTHAMT NOT = +0                     
        GO TO 4190-EP-DO-LF-COMM.                                
                                                                 
    IF CLAS-I-RL-AH (CLAS-INDEXL) = 'L' OR 'P'                   
        COMPUTE CNC-BEN = WE-LFAMT + WE-LFAMT-ALT                
        GO TO 4190-EP-DO-LF-COMM.                                
                                                                 
    MOVE CR-LF-CANC-DT TO VALUATION-DT.                          
    MOVE '3'           TO W-VAL-SOURCE.                          
    PERFORM 8500-REMAINING-TERM-ROUTINE THRU 8599-REM-TERM-X.    
                                                                 
    IF REM-TRM3 = CR-LF-TERM                                     
        COMPUTE CNC-BEN = WE-LFAMT + WE-LFAMT-ALT                
        GO TO 4190-EP-DO-LF-COMM.                                
                                                                 
    IF CLAS-I-EP (CLAS-INDEXL) = 'T'                             
        GO TO 4122-EP-DO-LF-CNCL-TEX.                            
                                                                 
    IF DTE-CLIENT = 'NCL'                                        
        IF CR-DT LESS THAN 19910101                              
            GO TO 4121-EP-DO-LF-CNC-BEN.                         
                                                                 
    IF CLAS-I-EP (CLAS-INDEXL) = 'N'                             
        GO TO 4126-EP-DO-LF-CNCL-NP.                             
                                                                 
    IF DTE-CLIENT = 'FUL'                                        
        GO TO 4128-EP-DO-LF-CNCL-FUL.                            
                                                                 
*>    IF STATE-ABBR (CLAS-INDEXS) = 'OH'                           
    IF (STATE-ABBR (CLAS-INDEXS) = 'OH') AND                     
       (CR-RATING-CLASS NOT = 'L ')                              
        IF (CR-APR GREATER THAN ZERO)  AND                       
           (CR-LF-TERM GREATER THAN 060)  AND                    
           (CR-DT GREATER THAN 19831031)                         
             GO TO 4126-EP-DO-LF-CNCL-NP.                        
                                                                 
    IF STATE-ABBR (CLAS-INDEXS) = 'MT'                           
        IF (CR-APR GREATER THAN ZERO)  AND                       
           (CR-LF-TERM GREATER THAN 061)  AND                    
           (CR-DT GREATER THAN 19830318)                         
             GO TO 4126-EP-DO-LF-CNCL-NP.                        
                                                                 
    IF STATE-ABBR (CLAS-INDEXS) = 'UT'                           
        IF (CR-APR GREATER THAN ZERO)  AND                       
           (CR-LF-TERM GREATER THAN 062)  AND                    
           (CR-DT GREATER THAN 19810831)  AND                    
           (CR-DT LESS THAN 19830901)                            
             GO TO 4126-EP-DO-LF-CNCL-NP.                        
                                                                 
    IF STATE-ABBR (CLAS-INDEXS) = 'RI'                           
        IF (CR-APR GREATER THAN ZERO)  AND                       
           (CR-LF-TERM GREATER THAN 060)  AND                    
           (CR-DT GREATER THAN 19831231)                         
             GO TO 4126-EP-DO-LF-CNCL-NP.                        
                                                                 
4121-EP-DO-LF-CNC-BEN.                                           
                                                                 
    COMPUTE TEMP-4 ROUNDED EQUAL REM-TRM3 / CR-LF-TERM.          
                                                                 
    COMPUTE TEMP-3 ROUNDED EQUAL                                 
        (WE-LFAMT + WE-LFAMT-ALT) * TEMP-4.                      
                                                                 
    MOVE TEMP-3                 TO CNC-BEN.                      
                                                                 
    GO TO 4190-EP-DO-LF-COMM.                                    
                                                                 
4122-EP-DO-LF-CNCL-TEX.                                          
                                                                 
    DIVIDE WE-LFAMT BY CR-LF-TERM                                
        GIVING TEX-FACT-1.                                       
    DIVIDE REM-TRM3 BY CR-PMT-FREQ                               
        GIVING TEX-FACT-2                                        
            REMAINDER TEX-FACT-3.                                
    IF TEX-FACT-3 NOT = ZERO                                     
        ADD +1 TO TEX-FACT-2.                                    
                                                                 
    COMPUTE CNC-BEN = (TEX-FACT-1 * (TEX-FACT-2 * CR-PMT-FREQ)). 
                                                                 
    GO TO 4190-EP-DO-LF-COMM.                                    
                                                                 
4124-EP-DO-LF-CNCL-SUM.                                          
                                                                 
    IF WE-LFRFND = ZERO                                          
        GO TO 4130-EP-DO-LF-EP.                                  
                                                                 
    MOVE WE-LFRFND TO CNC-PRM.                                   
                                                                 
    MOVE  'YYYYYYYYYY'           TO  WS-LF-CHARGEBACK-LEVELS.    
                                                                 
    IF WE-LFRFND NOT LESS WE-LFPRM                               
        MOVE WE-LFAMT TO CNC-BEN                                 
         GO TO 4130-EP-DO-LF-EP.                                 
                                                                 
                                                                 
    COMPUTE TEMP-7 ROUNDED = WE-LFAMT * (WE-LFRFND / WE-LFPRM).  
                                                                 
    IF CLAS-I-RL-AH (CLAS-INDEXL) = 'L' OR 'P'                   
        MOVE TEMP-7 TO CNC-BEN                                   
         GO TO 4130-EP-DO-LF-EP.                                 
                                                                 
    MOVE CR-LF-CANC-DT TO VALUATION-DT.                          
    MOVE '4'           TO W-VAL-SOURCE.                          
    PERFORM 8500-REMAINING-TERM-ROUTINE THRU 8599-REM-TERM-X.    
                                                                 
    IF CLAS-I-EP (CLAS-INDEXL) NOT = 'N'                         
        COMPUTE CNC-BEN = TEMP-7 * (REM-TRM3 / CR-LF-TERM)       
        GO TO 4130-EP-DO-LF-EP.                                  
                                                                 
4126-EP-DO-LF-CNCL-NP.                                           
                                                                 
    IF WE-LFAMT = ZERO                                           
       GO TO 4190-EP-DO-LF-COMM.                                 
                                                                 
    MOVE CR-DT                      TO  DC-GREG-DATE-CYMD.       
    MOVE 'L'                        TO  DC-OPTION-CODE.          
    PERFORM 8000-DATE-CONVERT-ROUTINE THRU 8099-DATE-CONVERT-X.  
    MOVE DC-BIN-DATE-1              TO  CP-CERT-EFF-DT.          
                                                                 
    IF CR-LOAN-1ST-PMT-DT NOT = ZEROS                            
      MOVE CR-LOAN-1ST-PMT-DT       TO  DC-GREG-DATE-1-YMD       
      MOVE '3'                      TO  DC-OPTION-CODE           
      PERFORM 8000-DATE-CONVERT-ROUTINE THRU 8099-DATE-CONVERT-X 
      MOVE DC-BIN-DATE-1            TO  CP-FIRST-PAY-DATE        
    ELSE                                                         
      MOVE CP-CERT-EFF-DT           TO  DC-BIN-DATE-1            
      MOVE '6'                      TO  DC-OPTION-CODE           
      MOVE +1                       TO  DC-ELAPSED-MONTHS        
      MOVE +0                       TO  DC-ELAPSED-DAYS          
      PERFORM 8000-DATE-CONVERT-ROUTINE THRU 8099-DATE-CONVERT-X 
      MOVE DC-BIN-DATE-2            TO  CP-FIRST-PAY-DATE.       
                                                                 
    MOVE CR-LFAMT                   TO  CP-ORIGINAL-BENEFIT.     
    MOVE CR-APR                     TO  CP-LOAN-APR.             
    MOVE CR-LF-TERM                 TO  CP-ORIGINAL-TERM.        
    MOVE CR-LOAN-TERM               TO  CP-LOAN-TERM.            
    MOVE REM-TRM3                   TO  CP-REMAINING-TERM.       
    MOVE CLAS-I-CALC-TYPE (CLAS-INDEXL) TO  CP-SPECIAL-CALC-CD.  
    MOVE CLAS-I-RL-AH     (CLAS-INDEXL) TO  CP-BENEFIT-TYPE.     
    MOVE CLAS-I-EP        (CLAS-INDEXL) TO  CP-EARNING-METHOD.   
                                                                 
                                                                 
    MOVE ZEROS                  TO CP-R-MAX-TOT-BEN              
    IF (DTE-CLIENT = 'DCC')                                      
       AND (AM-DCC-PRODUCT-CODE <> spaces)                       
       if cr-clp-state = spaces                                  
          move cr-state         to cr-clp-state                  
       end-if                                                    
       if cr-lftyp not = '00' and '  ' and 'DD'                  
          move 'L'              to ws-dd-ben-type                
          move cr-lftyp         to ws-dd-ben-code                
       else                                                      
          move 'A'              to ws-dd-ben-type                
          move cr-ahtyp         to ws-dd-ben-code                
       end-if                                                    
       PERFORM VARYING D1 FROM +1 BY +1 UNTIL                    
          ((CR-clp-STATE = DD-STATE (D1))                        
          AND (AM-DCC-PRODUCT-CODE = DD-PRODUCT-CD (D1))         
          AND (ws-dd-ben-type = DD-BEN-TYPE   (D1))              
          AND (ws-dd-ben-code = DD-BEN-CODE   (D1))              
          AND (CR-DT         < DD-PROD-EXP-DT (D1)))             
                          OR                                     
          D1 > max-pdef                                          
       END-PERFORM                                               
       IF D1 <= max-pdef                                         
          PERFORM VARYING P1 FROM +1 BY +1 UNTIL                 
             (P1 > +11)                                          
             OR (DD-PROD-CODE (D1 P1) = 'L')                     
          END-PERFORM                                            
          IF P1 < +12                                            
             MOVE DD-MAX-BEN-AMT (D1 P1)                         
                                TO CP-R-MAX-TOT-BEN              
          END-IF                                                 
       END-IF                                                    
    END-IF                                                       
                                                                 
    CALL 'ELRAMTX' USING CALCULATION-PASS-AREA.                  
                                                                 
    IF CLAS-I-CALC-TYPE (CLAS-INDEXL) = 'Z'                      
      COMPUTE CNC-BEN = (CP-REMAMT-FACTOR * (TEMP-7 / +1000))    
      GO TO 4130-EP-DO-LF-EP                                     
    ELSE                                                         
      COMPUTE CNC-BEN = (CP-REMAMT-FACTOR * (WE-LFAMT / +1000)). 
                                                                 
    GO TO 4190-EP-DO-LF-COMM.                                    
                                                                 
4128-EP-DO-LF-CNCL-FUL.                                          
                                                                 
    IF WE-LFAMT = ZERO                                           
       GO TO 4190-EP-DO-LF-COMM.                                 
                                                                 
    MOVE CR-APR              TO NP-APR.                          
    MOVE CR-LF-TERM          TO NP-ORIG.                         
    MOVE REM-TRM3            TO NP-REM.                          
    MOVE '2'                 TO NP-OPT.                          
    MOVE CR-AHPRM            TO NP-AHPRM.                        
    MOVE CR-ACCOUNT          TO NP-ACCOUNT.                      
    MOVE CR-LFAMT            TO NP-BENEFIT.                      
                                                                 
*>    CALL 'FULNETRM'                                              
*>        USING NP-APR NP-ORIG NP-REM NP-OPT NP-BENEFIT NP-AHPRM   
*>              NP-ACCOUNT NP-REMAINING.                           
                                                                 
    MOVE NP-REMAINING TO CNC-BEN.                                
                                                                 
    GO TO 4190-EP-DO-LF-COMM.                                    
                                                                 
4130-EP-DO-LF-EP.                                                
                                                                 
    MOVE SPACE                  TO STATUTORY-SWITCH.             
                                                                 
    MOVE EOM-RUN-DATE TO VALUATION-DT.                           
    MOVE '5'           TO W-VAL-SOURCE.                          
                                                                 
    IF REIN-EP-FLG = 'R'                                         
      IF REIN-REM-SW (SUB5) = 'I'                                
          COMPUTE EPR-R78 = WE-LFPRM + WE-LFPRM-ALT              
          MOVE EPR-R78             TO EPR-PRO  EPR-ST            
          GO TO 4190-EP-DO-LF-COMM.                              
                                                                 
    IF REIN-EP-FLG = 'R'                                         
        IF REIN-LF-RUNOFF-SW (SUB5) = 'N'  AND                   
           EARNING-STOP-DATE LESS THAN VALUATION-DT              
            MOVE '*'               TO LIFE-EARNED-SWITCH         
            MOVE EARNING-STOP-DATE TO VALUATION-DT               
            MOVE '10'              TO W-VAL-SOURCE.              
                                                                 
    PERFORM 8500-REMAINING-TERM-ROUTINE THRU 8599-REM-TERM-X.    
                                                                 
    IF REM-TRM2 GREATER CR-LF-TERM                               
        MOVE +0 TO EPR-R78  EPR-PRO  EPR-ST                      
        GO TO 4190-EP-DO-LF-COMM.                                
                                                                 
    IF CLAS-I-CALC-TYPE (CLAS-INDEXL) = 'Z'                      
        GO TO 4180-EP-DO-LF-EP-SUM.                              
                                                                 
    IF CLAS-I-EP (CLAS-INDEXL) = 'B'                             
        IF CLAS-I-CALC-TYPE (CLAS-INDEXL) = 'L'                  
            IF REM-TRM2 NOT GREATER THAN +0                      
                COMPUTE EPR-R78 = WE-LFPRM + WE-LFPRM-ALT        
                MOVE EPR-R78 TO EPR-PRO  EPR-ST                  
                GO TO 4190-EP-DO-LF-COMM                         
            ELSE                                                 
                NEXT SENTENCE                                    
        ELSE                                                     
            IF W-BAL-REMTRM NOT GREATER +0                       
                COMPUTE EPR-R78 = WE-LFPRM + WE-LFPRM-ALT        
                MOVE EPR-R78 TO EPR-PRO  EPR-ST                  
                GO TO 4190-EP-DO-LF-COMM                         
            ELSE                                                 
                NEXT SENTENCE                                    
    ELSE                                                         
       IF DTE-CLIENT = 'NCL'  AND                                
          CLAS-I-CALC-TYPE (CLAS-INDEXL) = 'B'                   
           MOVE WE-LFPRM TO EPR-R78  EPR-PRO  EPR-ST             
       ELSE                                                      
        IF REM-TRM2 NOT GREATER +0  OR                           
           CLAS-I-CALC-TYPE (CLAS-INDEXL) = 'B'                  
            MOVE WE-LFPRM TO EPR-R78  EPR-PRO  EPR-ST            
            GO TO 4190-EP-DO-LF-COMM.                            
                                                                 
    MOVE +1 TO ACTIVE-COUNT.                                     
                                                                 
    IF CLAS-I-EP (CLAS-INDEXL) = 'T'                             
        GO TO 4140-EP-DO-LF-EP-TEX.                              
                                                                 
    IF CLAS-I-EP (CLAS-INDEXL) = 'N'                             
        GO TO 4150-EP-DO-LF-EP-NP.                               
                                                                 
*>    IF STATE-ABBR (CLAS-INDEXS) = 'OH'                           
    IF (STATE-ABBR (CLAS-INDEXS) = 'OH') AND                     
       (CR-RATING-CLASS NOT = 'L ')                              
        IF (CR-APR GREATER THAN ZERO)  AND                       
           (CR-LF-TERM GREATER THAN 060)  AND                    
           (CR-DT GREATER THAN 19831031)                         
            MOVE '*'             TO STATUTORY-SWITCH             
             GO TO 4150-EP-DO-LF-EP-NP.                          
                                                                 
    IF STATE-ABBR (CLAS-INDEXS) = 'MT'                           
        IF (CR-APR GREATER THAN ZERO)  AND                       
           (CR-LF-TERM GREATER THAN 061)  AND                    
           (CR-DT GREATER THAN 19830318)                         
            MOVE '*'             TO STATUTORY-SWITCH             
             GO TO 4150-EP-DO-LF-EP-NP.                          
                                                                 
    IF STATE-ABBR (CLAS-INDEXS) = 'UT'                           
        IF (CR-APR GREATER THAN ZERO)  AND                       
           (CR-LF-TERM GREATER THAN 062)  AND                    
           (CR-DT GREATER THAN 19810831)  AND                    
           (CR-DT LESS THAN 19830901)                            
            MOVE '*'             TO STATUTORY-SWITCH             
             GO TO 4150-EP-DO-LF-EP-NP.                          
                                                                 
    IF STATE-ABBR (CLAS-INDEXS) = 'RI'                           
        IF (CR-APR GREATER THAN ZERO)  AND                       
           (CR-LF-TERM GREATER THAN 060)  AND                    
           (CR-DT GREATER THAN 19831231)                         
            MOVE '*'             TO STATUTORY-SWITCH             
             GO TO 4150-EP-DO-LF-EP-NP.                          
                                                                 
    GO TO 4160-EP-DO-LF-EP-R78.                                  
                                                                 
4140-EP-DO-LF-EP-TEX.                                            
                                                                 
    IF CR-PMT-FREQ = 0                                           
      DISPLAY '*************************************'            
      DISPLAY '***** CR-PMT-FREQ EQUAL TO ZERO *****'            
      DISPLAY '*************************************'            
      DISPLAY '***** CR-FULL-CONTROL -->' CR-FULL-CONTROL        
      DISPLAY '*************************************'            
      COMPUTE TEX-FACT-4 =                                       
         (CR-LF-TERM * CR-LF-TERM) +                             
                              (WS-CR-PMT-FREQ * CR-LF-TERM)      
    ELSE                                                         
       COMPUTE TEX-FACT-4 =                                      
        (CR-LF-TERM * CR-LF-TERM) + (CR-PMT-FREQ * CR-LF-TERM).  
                                                                 
    IF CR-PMT-FREQ = 0                                           
      DIVIDE REM-TRM1 BY WS-CR-PMT-FREQ                          
          GIVING TEX-FACT-5                                      
              REMAINDER TEX-FACT-6                               
    ELSE                                                         
       DIVIDE REM-TRM1 BY CR-PMT-FREQ                            
           GIVING TEX-FACT-5                                     
               REMAINDER TEX-FACT-6.                             
                                                                 
    IF CR-PMT-FREQ > 0                                           
       MOVE CR-PMT-FREQ TO WS-CR-PMT-FREQ.                       
                                                                 
    COMPUTE TEX-FACT-5 = TEX-FACT-5 * CR-PMT-FREQ.               
    COMPUTE TEX-FACT-7 = (TEX-FACT-5 * TEX-FACT-5) +             
        (TEX-FACT-5 * CR-PMT-FREQ) +                             
        (2 * (TEX-FACT-6 * (TEX-FACT-5 + CR-PMT-FREQ))).         
    COMPUTE TEX-FACT-8 ROUNDED = TEX-FACT-7 / TEX-FACT-4.        
                                                                 
    COMPUTE EPR-R78 ROUNDED = WE-LFPRM - (WE-LFPRM * TEX-FACT-8).
                                                                 
    GO TO 4170-EP-DO-LF-EP-PRO.                                  
                                                                 
4150-EP-DO-LF-EP-NP.                                             
                                                                 
    IF WE-LFPRM = ZERO                                           
        GO TO 4160-EP-DO-LF-EP-R78.                              
                                                                 
    if cr-apr = zeros                                            
       display ' found 0 apr cert ' cr-cert-no                   
       go to 4160-ep-do-lf-ep-r78                                
    end-if                                                       
    IF DTE-CLIENT = 'CRI'                                        
      IF STATE-ABBR (CLAS-INDEXS) = ('AB' OR 'BC' OR 'MB' OR     
            'NS' OR 'ON' OR 'NB' OR 'NF' OR 'QB' OR 'SK' OR 'PE')
          GO TO 4160-EP-DO-LF-EP-R78.                            
                                                                 
    IF DTE-CLIENT = 'AHL'                                        
       GO TO 4160-EP-DO-LF-EP-R78                                
    END-IF                                                       
                                                                 
    IF DTE-CLIENT = 'NCL'                                        
      IF CR-DT LESS THAN 19910101                                
          GO TO 4160-EP-DO-LF-EP-R78.                            
                                                                 
    IF REM-TRM1 = CR-LF-TERM                                     
        MOVE +0 TO EPR-R78  EPR-PRO  EPR-ST                      
        GO TO 4190-EP-DO-LF-COMM.                                
                                                                 
    MOVE CR-APR TO NP-APR.                                       
    MOVE CR-LF-TERM TO NP-ORIG  NP-CAP.                          
    MOVE REM-TRM1 TO NP-REM.                                     
    MOVE CLAS-I-CALC-TYPE (CLAS-INDEXL) TO NP-OPT.               
    IF NP-OPT = 'T' OR 'U' OR 'V' OR 'W' OR 'X'                  
        MOVE CR-LOAN-TERM TO NP-ORIG.                            
                                                                 
    MOVE 'R' TO NP-OPT.                                          
    CALL 'ECSNETRM'                                              
        USING NP-APR NP-ORIG NP-REM NP-OPT NP-CAP NP-FACTOR.     
                                                                 
    COMPUTE EPR-R78 ROUNDED = WE-LFPRM - (NP-FACTOR * WE-LFPRM). 
                                                                 
    IF STATUTORY-REQUIREMENT                                     
        MOVE EPR-R78                TO EPR-ST.                   
                                                                 
4160-EP-DO-LF-EP-R78.                                            
                                                                 
    IF (CLAS-I-CALC-TYPE (CLAS-INDEXL) EQUAL 'B') AND            
       (DTE-CLIENT EQUAL 'NCL')                                  
       IF (CR-CCYY EQUAL EOM-CCYY) AND                           
          (CR-MO EQUAL EOM-MO)                                   
          MOVE '*'            TO OB-DATE-1-SWITCH                
          MOVE EW-REIN-CO     TO WS-OB-REIN-CO (SUB3)            
          MOVE LIFE-OVERRIDE-L1 TO WS-OB-LF-AH (SUB3)            
          MOVE CR-LFTYP       TO WS-OB-BENEFIT-CODE (SUB3)       
          COMPUTE WS-EPR-R78 (SUB3 1) =                          
                  WS-EPR-R78 (SUB3 1) + (WE-LFPRM * .5)          
          GO TO 4190-EP-DO-LF-COMM                               
       ELSE                                                      
       IF (CR-CCYY EQUAL OB-CCYY (1)) AND                        
          (CR-MO EQUAL OB-MO (1))                                
          MOVE '*'            TO OB-DATE-2-SWITCH                
          MOVE EW-REIN-CO     TO WS-OB-REIN-CO (SUB3)            
          MOVE LIFE-OVERRIDE-L1 TO WS-OB-LF-AH (SUB3)            
          MOVE CR-LFTYP       TO WS-OB-BENEFIT-CODE (SUB3)       
          COMPUTE WS-EPR-R78 (SUB3 2) =                          
                  WS-EPR-R78 (SUB3 2) + (WE-LFPRM * .5)          
          GO TO 4190-EP-DO-LF-COMM                               
       ELSE                                                      
       IF (CR-CCYY EQUAL OB-CCYY (2)) AND                        
          (CR-MO EQUAL OB-MO (2))                                
          MOVE '*'            TO OB-DATE-3-SWITCH                
          MOVE EW-REIN-CO     TO WS-OB-REIN-CO (SUB3)            
          MOVE LIFE-OVERRIDE-L1 TO WS-OB-LF-AH (SUB3)            
          MOVE CR-LFTYP       TO WS-OB-BENEFIT-CODE (SUB3)       
          COMPUTE WS-EPR-R78 (SUB3 3) =                          
                  WS-EPR-R78 (SUB3 3) + (WE-LFPRM * .5)          
          GO TO 4190-EP-DO-LF-COMM                               
       ELSE                                                      
          GO TO 4190-EP-DO-LF-COMM.                              
                                                                 
    COMPUTE TEMP-1 = CR-LF-TERM * (CR-LF-TERM + 1).              
                                                                 
    IF DTE-R78 = '1'                                             
        COMPUTE TEMP-2 = REM-TRM1 * REM-TRM1                     
    ELSE                                                         
        COMPUTE TEMP-2 = REM-TRM1 * (REM-TRM1 + 1).              
                                                                 
    COMPUTE TEMP-4 ROUNDED EQUAL TEMP-2 / TEMP-1.                
                                                                 
    IF REM-TRM1 GREATER THAN +0                                  
        COMPUTE TEMP-7 ROUNDED EQUAL                             
            WE-LFPRM - (WE-LFPRM * TEMP-4)                       
    ELSE                                                         
        MOVE WE-LFPRM                TO TEMP-7.                  
                                                                 
    IF STATE-ABBR (CLAS-INDEXS) = 'AL'                           
        MOVE TEMP-7                 TO EPR-ST.                   
                                                                 
    IF CLAS-I-EP (CLAS-INDEXL) NOT = 'B'                         
        MOVE ZEROS                  TO TEMP-8                    
        GO TO 4165-END-LF-EP-R78.                                
                                                                 
    IF EPR-ST = ZEROS                                            
        MOVE TEMP-7                 TO EPR-ST.                   
                                                                 
    IF CLAS-I-CALC-TYPE (CLAS-INDEXL) NOT = 'L'                  
        COMPUTE TEMP-1 = (CR-LF-TERM + 1) * (CR-LF-TERM + 2)     
        COMPUTE TEMP-2 = (REM-TRM1 + 1) * (REM-TRM1 + 2)         
        IF DTE-R78 = '1'                                         
            COMPUTE TEMP-2 = (REM-TRM1 + 1) * (REM-TRM1 + 1).    
                                                                 
    COMPUTE TEMP-4 ROUNDED EQUAL TEMP-2 / TEMP-1.                
                                                                 
    COMPUTE TEMP-8 ROUNDED =                                     
              WE-LFPRM-ALT - (WE-LFPRM-ALT * TEMP-4).            
                                                                 
4165-END-LF-EP-R78.                                              
                                                                 
    IF EPR-R78 = ZEROS                                           
        COMPUTE EPR-R78 = TEMP-7 + TEMP-8                        
    ELSE                                                         
        COMPUTE EPR-R78 = EPR-R78 + TEMP-8.                      
                                                                 
4170-EP-DO-LF-EP-PRO.                                            
                                                                 
    IF DTE-CLIENT = 'MIC' OR 'MCC'                               
        COMPUTE TEMP-5 ROUNDED = (REM-TRM1 - .5) / CR-LF-TERM    
    ELSE                                                         
        COMPUTE TEMP-5 ROUNDED = REM-TRM1 / CR-LF-TERM.          
                                                                 
    IF REM-TRM1 GREATER THAN +0                                  
        COMPUTE TEMP-7 ROUNDED = WE-LFPRM - (WE-LFPRM * TEMP-5)  
    ELSE                                                         
        MOVE WE-LFPRM                  TO TEMP-7.                
                                                                 
    IF CLAS-I-EP (CLAS-INDEXL) NOT = 'B'                         
        MOVE ZEROS                     TO TEMP-8                 
        GO TO 4175-END-LF-EP-PRO.                                
                                                                 
    IF CLAS-I-CALC-TYPE (CLAS-INDEXL) NOT = 'L'                  
        IF DTE-CLIENT = 'MIC' OR 'MCC'                           
            COMPUTE TEMP-5 ROUNDED =                             
                          (REM-TRM1 + .5) / (CR-LF-TERM + 1)     
        ELSE                                                     
            COMPUTE TEMP-5 ROUNDED =                             
                          (REM-TRM1 + 1) / (CR-LF-TERM + 1).     
                                                                 
    COMPUTE TEMP-8 ROUNDED =                                     
                  WE-LFPRM-ALT - (WE-LFPRM-ALT * TEMP-5).        
                                                                 
    COMPUTE EPR-ST = EPR-ST + TEMP-8.                            
                                                                 
4175-END-LF-EP-PRO.                                              
                                                                 
    COMPUTE EPR-PRO = TEMP-7 + TEMP-8.                           
                                                                 
    GO TO 4185-EP-DO-LF-STATUTORY.                               
                                                                 
4180-EP-DO-LF-EP-SUM.                                            
                                                                 
    IF REM-TRM1 NOT GREATER +0                                   
        COMPUTE EPR-R78 = (WE-LFPRM + WE-LFPRM-ALT) - WE-LFRFND  
        MOVE EPR-R78 TO EPR-PRO  EPR-ST                          
        GO TO 4190-EP-DO-LF-COMM.                                
                                                                 
    COMPUTE ACTIVE-COUNT = CR-LIVES - CR-SUM-CAN-CNT-ITD.        
                                                                 
    IF WE-LFPRM = ZERO  AND  WE-LFPRM-ALT = ZERO                 
        MOVE ZEROS                 TO TEMP-7                     
    ELSE                                                         
        COMPUTE TEMP-7 ROUNDED = WE-LFPRM - (WE-LFRFND *         
                      (WE-LFPRM / (WE-LFPRM + WE-LFPRM-ALT))).   
                                                                 
    IF TEMP-7 NOT GREATER ZERO                                   
        IF CLAS-I-EP (CLAS-INDEXL) NOT = 'B'                     
            MOVE ZEROS             TO EPR-R78  EPR-PRO  EPR-ST   
            GO TO 4190-EP-DO-LF-COMM                             
        ELSE                                                     
            MOVE ZEROS             TO TEMP-7.                    
                                                                 
    COMPUTE TEMP-1 = CR-LF-TERM * (CR-LF-TERM + 1).              
    COMPUTE TEMP-2 = REM-TRM1 * (REM-TRM1 + 1).                  
    IF DTE-R78 = '1'                                             
        COMPUTE TEMP-2 = REM-TRM1 * REM-TRM1.                    
                                                                 
    COMPUTE TEMP-4 ROUNDED = TEMP-2 / TEMP-1.                    
    COMPUTE TEMP-5 ROUNDED = REM-TRM1 / CR-LF-TERM.              
                                                                 
    COMPUTE EPR-R78 ROUNDED = TEMP-7 - (TEMP-7 * TEMP-4).        
                                                                 
    COMPUTE EPR-PRO ROUNDED = TEMP-7 - (TEMP-7 * TEMP-5).        
                                                                 
    IF CLAS-I-EP (CLAS-INDEXL) NOT = 'B'                         
        GO TO 4185-EP-DO-LF-STATUTORY.                           
                                                                 
    MOVE EPR-R78                  TO EPR-ST.                     
                                                                 
    IF WE-LFPRM = ZERO  AND  WE-LFPRM-ALT = ZERO                 
        MOVE ZEROS                 TO TEMP-8                     
    ELSE                                                         
        COMPUTE TEMP-8 ROUNDED = WE-LFPRM-ALT - (WE-LFRFND *     
                    (WE-LFPRM-ALT / (WE-LFPRM + WE-LFPRM-ALT))). 
                                                                 
    IF TEMP-8 NOT GREATER ZERO                                   
        GO TO 4185-EP-DO-LF-STATUTORY.                           
                                                                 
    COMPUTE EPR-R78 ROUNDED = EPR-R78 +                          
                             (TEMP-8 - (TEMP-8 * TEMP-4)).       
                                                                 
    COMPUTE EPR-PRO ROUNDED = EPR-PRO +                          
                             (TEMP-8 - (TEMP-8 * TEMP-5)).       
                                                                 
    COMPUTE EPR-ST ROUNDED = EPR-ST +                            
                            (TEMP-8 - (TEMP-8 * TEMP-5)).        
                                                                 
4185-EP-DO-LF-STATUTORY.                                         
                                                                 
*>07635      IF STATE-ABBR (CLAS-INDEXS) = 'AL'                    
*>07636          MOVE EPR-R78                TO EPR-ST.            
    IF STATE-ABBR (CLAS-INDEXS) = 'WY'                           
        MOVE EPR-PRO                TO EPR-ST.                   
                                                                 
    IF DTE-CLIENT = 'AHL'                                        
       IF CLAS-I-RL-AH (CLAS-INDEXL) = 'R'                       
          MOVE EPR-R78          TO EPR-ST                        
       ELSE                                                      
          MOVE EPR-PRO          TO EPR-ST                        
       END-IF                                                    
    END-IF                                                       
                                                                 
    IF (DTE-CLIENT = 'CID')                                      
       AND ((STATE-ABBR (CLAS-INDEXS) = 'VA' OR 'TX')            
                  OR                                             
           ((STATE-ABBR (CLAS-INDEXS) = 'MN')                    
           AND (CR-DT > 20091231)))                              
       AND (CLAS-I-RL-AH (CLAS-INDEXL) NOT = 'L')                
       AND (CLAS-I-EP (CLAS-INDEXL) NOT = 'B')                   
          GO TO 4187-EP-DO-LF-STAT-ROA                           
    END-IF.                                                      
                                                                 
    IF EPR-ST NOT = ZEROS                                        
        GO TO 4190-EP-DO-LF-COMM.                                
                                                                 
    IF CLAS-I-RL-AH (CLAS-INDEXL) = 'R'                          
        MOVE EPR-R78                TO EPR-ST                    
    ELSE                                                         
        MOVE EPR-PRO                TO EPR-ST.                   
                                                                 
    GO TO 4190-EP-DO-LF-COMM.                                    
                                                                 
4187-EP-DO-LF-STAT-ROA.                                          
    PERFORM 4196-CALC-REM-AMT THRU 4199-EXIT.                    
    MOVE CR-DT                      TO DC-GREG-DATE-CYMD.        
    MOVE 'L'                        TO DC-OPTION-CODE.           
    PERFORM 8000-DATE-CONVERT-ROUTINE THRU 8099-DATE-CONVERT-X.  
    MOVE DC-BIN-DATE-1              TO CP-CERT-EFF-DT.           
    IF CR-LOAN-1ST-PMT-DT IS NOT = ZEROS                         
       MOVE CR-LOAN-1ST-PMT-DT      TO  DC-GREG-DATE-1-YMD       
       MOVE '3'                     TO  DC-OPTION-CODE           
       PERFORM 8000-DATE-CONVERT-ROUTINE THRU                    
                             8099-DATE-CONVERT-X                 
       MOVE DC-BIN-DATE-1           TO  CP-FIRST-PAY-DATE        
    ELSE                                                         
       MOVE CP-CERT-EFF-DT          TO  DC-BIN-DATE-1            
       MOVE +1                      TO  DC-ELAPSED-MONTHS        
       MOVE +0                      TO  DC-ELAPSED-DAYS          
       MOVE '6'                     TO  DC-OPTION-CODE           
       PERFORM 8000-DATE-CONVERT-ROUTINE THRU                    
                             8099-DATE-CONVERT-X                 
       MOVE DC-BIN-DATE-2           TO  CP-FIRST-PAY-DATE        
    END-IF.                                                      
    MOVE REM-TRM1                   TO CP-REMAINING-TERM.        
    MOVE CLAS-I-RL-AH (CLAS-INDEXL) TO CP-BENEFIT-TYPE.          
    MOVE SPACES                     TO CP-ACCT-FLD-5.            
    MOVE DTE-CLIENT                 TO CP-COMPANY-ID.            
    MOVE STATE-SUB (CLAS-INDEXS)    TO CP-STATE.                 
    MOVE STATE-ABBR (CLAS-INDEXS)   TO CP-STATE-STD-ABBRV.       
    MOVE CR-AGE                     TO CP-ISSUE-AGE.             
    MOVE CR-LFPRM                   TO CP-ORIGINAL-PREMIUM       
    MOVE CR-LF-CRIT-PERIOD          TO CP-CRITICAL-MONTHS        
    MOVE CR-LF-TERM                 TO CP-ORIGINAL-TERM          
                                       CP-LOAN-TERM.             
    MOVE CR-LFAMT                   TO CP-ORIGINAL-BENEFIT       
                                       CP-RATING-BENEFIT-AMT.    
    MOVE WS-REM-AMT                 TO CP-REMAINING-BENEFIT      
    MOVE CR-APR                     TO CP-LOAN-APR.              
    MOVE CR-PMT-FREQ                TO CP-PAY-FREQUENCY.         
    MOVE CLAS-I-EP (CLAS-INDEXL)    TO CP-RATING-METHOD.         
    MOVE '6'                        TO CP-EARNING-METHOD.        
    MOVE '3'                        TO CP-PROCESS-TYPE.          
    MOVE ' '                        TO CP-SPECIAL-CALC-CD.       
    IF CR-RATING-CLASS NOT = SPACE AND ZERO                      
        MOVE CR-RATING-CLASS        TO CP-CLASS-CODE             
    ELSE                                                         
        MOVE AM-CAL-TABLE           TO CP-CLASS-CODE.            
    MOVE CR-LF-DEV-CODE             TO CP-DEVIATION-CODE.        
    MOVE CR-LF-DEV-PCT              TO CP-RATE-DEV-PCT.          
    MOVE CR-LFTYP                   TO CP-BENEFIT-CD.            
    MOVE DTE-CLASIC-COMPANY-CD      TO CP-COMPANY-CD.            
    MOVE CR-PMT-EXTENSION-DAYS      TO CP-TERM-OR-EXT-DAYS.      
    MOVE LIFE-OVERRIDE-L1           TO CP-LIFE-OVERRIDE-CODE.    
                                                                 
    IF CP-STATE-STD-ABBRV = 'OR'                                 
       COMPUTE CP-RATING-BENEFIT-AMT = CR-LFAMT + CR-LFAMT-ALT   
    END-IF                                                       
                                                                 
    IF (CP-RATE-AS-STANDARD) AND (CP-EARN-AS-NET-PAY)            
       COMPUTE CP-ORIGINAL-BENEFIT =                             
        (CR-LFAMT - CR-LFPRM)                                    
    END-IF                                                       
                                                                 
    IF CP-TRUNCATED-LIFE                                         
       MOVE CR-LOAN-TERM            TO CP-LOAN-TERM              
    END-IF                                                       
                                                                 
    IF CP-TERM-IS-DAYS                                           
       MOVE CR-LF-TERM-IN-DAYS    TO CP-TERM-OR-EXT-DAYS         
    ELSE                                                         
       MOVE ZEROS                 TO CP-TERM-OR-EXT-DAYS         
    END-IF                                                       
                                                                 
    CALL 'ELRFNDX' USING CALCULATION-PASS-AREA.                  
                                                                 
    COMPUTE EPR-ST = WE-LFPRM - CP-CALC-REFUND.                  
                                                                 
4190-EP-DO-LF-COMM.                                              
                                                                 
    MOVE +0 TO SUB1.                                             
    PERFORM 4300-EP-COMM-LOOP THRU 4350-EP-COMM-LOOP-X           
        10 TIMES.                                                
                                                                 
    IF REIN-EP-FLG NOT = 'R'  OR                                 
       CR-POLICY-IS-REIN-ONLY                                    
        MOVE ISS-BEN TO GROSS-ISS-BEN-LF                         
        MOVE CNC-BEN TO GROSS-CNC-BEN-LF                         
        MOVE ISS-PRM TO GROSS-ISS-PRM-LF                         
        MOVE CNC-PRM TO GROSS-CNC-PRM-LF.                        
                                                                 
    ADD ISS-BEN TO EW-ISS-BEN.                                   
    ADD GROSS-ISS-BEN-LF TO EW-ISS-BEN-GROSS.                    
    ADD CNC-BEN TO EW-CNC-BEN.                                   
    ADD GROSS-CNC-BEN-LF TO EW-CNC-BEN-GROSS.                    
    ADD ISS-PRM TO EW-ISS-PRM.                                   
    ADD GROSS-ISS-PRM-LF TO EW-ISS-PRM-GROSS.                    
    ADD CNC-PRM TO EW-CNC-PRM.                                   
    ADD GROSS-CNC-PRM-LF TO EW-CNC-PRM-GROSS.                    
                                                                 
    COMPUTE EW-EPR-R78 = EW-EPR-R78 + EPR-R78.                   
    COMPUTE EW-EPR-PRO = EW-EPR-PRO + EPR-PRO.                   
    COMPUTE EW-EPR-ST  = EW-EPR-ST  + EPR-ST.                    
                                                                 
    IF CR-LF-ISS-PREM-TAX NOT NUMERIC                            
       MOVE +0                  TO CR-LF-ISS-PREM-TAX            
    END-IF                                                       
    IF CR-LF-CNC-PREM-TAX NOT NUMERIC                            
       MOVE +0                  TO CR-LF-CNC-PREM-TAX            
    END-IF                                                       
                                                                 
    if dte-client = 'AHL'                                        
       COMPUTE ISS-PRMTAX =                                      
          (ISS-PRM * CR-LF-ISS-PREM-TAX) -                       
          (CNC-PRM * CR-LF-CNC-PREM-TAX)                         
    else                                                         
       COMPUTE ISS-PRMTAX rounded =                              
          (ISS-PRM * CR-LF-ISS-PREM-TAX) -                       
          (CNC-PRM * CR-LF-CNC-PREM-TAX)                         
    end-if                                                       
                                                                 
    COMPUTE EW-ISS-PRM-TAX = EW-ISS-PRM-TAX + ISS-PRMTAX         
                                                                 
    IF LIFE-FULLY-EARNED                                         
        COMPUTE EW-EPR-R78-ADJ = EW-EPR-R78-ADJ +                
                                           (ISS-PRM - EPR-R78)   
        COMPUTE EW-EPR-PRO-ADJ = EW-EPR-PRO-ADJ +                
                                           (ISS-PRM - EPR-PRO)   
        COMPUTE EW-EPR-ST-ADJ  = EW-EPR-ST-ADJ +                 
                                           (ISS-PRM - EPR-ST).   
                                                                 
    IF  DTE-OPT-RESERVE-METHOD-AUTH                              
        IF  REIN-EP-FLG NOT = 'R'                                
                OR                                               
            CR-POLICY-IS-REIN-ONLY                               
            MOVE 'L'      TO W-CLAIM-TYPE                        
            MOVE EPR-R78  TO W-OPT-R78                           
            MOVE EPR-PRO  TO W-OPT-PRO                           
            MOVE EPR-ST   TO W-OPT-ST                            
            PERFORM 4600-GET-OPT-IBNR THRU 4600-EXIT             
            MOVE ZEROS    TO EW-CLM-IBNR                         
        ELSE                                                     
            IF  STOP-LF-DATE LESS THAN EOM-RUN-DATE              
                MOVE 'N'        TO EPEC-LINE-NOT-USED-IND (SUB3) 
                MOVE ZEROS      TO EW-CLM-IBNR                   
            ELSE                                                 
                MOVE ZEROS      TO EW-CLM-IBNR                   
    ELSE                                                         
        IF  DTE-CLIENT NOT EQUAL 'ITG'                           
            ADD WE-DTH-IBNR                                      
                      TO EW-CLM-IBNR                             
        ELSE                                                     
            IF  CLAS-I-BAL (CLAS-INDEXL) = 'B'                   
                    AND                                          
                CR-DT GREATER THAN LAST-YRS-DATE                 
                COMPUTE  EW-CLM-IBNR                             
                    = EW-CLM-IBNR + (WE-LFAMT * .0000500).       
                                                                 
    ADD WE-DTH-PAYCUR TO EW-CLM-PAYCUR.                          
    ADD WE-DTH-FUTRSV TO EW-CLM-FUTRSV.                          
                                                                 
4191-CONTINUE.                                                   
                                                                 
    MOVE 'Y'          TO EW-LEVEL-USED.                          
                                                                 
    IF CR-POLICY-UNDERWRITTEN                                    
        MOVE EPEC-WORK       TO EPEC-LEVELS-II (SUB3)            
        MOVE EPEC-OPT-WORK-TABLE                                 
                             TO EPEC-UNDW-OPT-DATA-TABLE         
    ELSE                                                         
        MOVE EPEC-WORK       TO EPEC-LEVELS (SUB3)               
        MOVE EPEC-OPT-WORK-TABLE                                 
                             TO EPEC-OPT-DATA-TABLE.             
                                                                 
    GO TO 4200-EP-DO-AH.                                         
EJECT                                                            
4194-SLOT-INTO-EFFECTED-MONTHS.                                  
                                                                 
    IF  CR-DT NOT GREATER THAN EOM-DATE (SUBM)                   
            AND                                                  
        CR-LF-EXPIRE-DATE GREATER THAN EOM-DATE (SUBM)           
            AND                                                  
        (CR-LF-CANC-DT GREATER THAN EOM-DATE (SUBM)              
                OR                                               
         CR-LF-CANC-DT EQUAL ZEROS)                              
            AND                                                  
        (CR-LF-CLAIM-EXIT-DATE GREATER THAN EOM-DATE (SUBM)      
                OR                                               
         CR-LF-CLAIM-EXIT-DATE EQUAL ZEROS)                      
        MOVE 'Y'                 TO W-TABLE-USED-SW              
        IF  CLAS-I-CALC-TYPE (CLAS-INDEXL) = 'Z'                 
            COMPUTE EPEC-LIFE-CTR (SUB3 SUBM) ROUNDED            
                = EPEC-LIFE-CTR (SUB3 SUBM)                      
                + CR-LIVES                                       
        ELSE                                                     
            COMPUTE EPEC-LIFE-CTR (SUB3 SUBM)                    
                = EPEC-LIFE-CTR (SUB3 SUBM) + 1.                 
                                                                 
4194-EXIT.                                                       
    EXIT.                                                        
                                                                 
4196-CALC-REM-AMT.                                               
                                                                 
    IF CR-LFTYP = 'QD'                                           
       MOVE +15.0               TO CR-APR                        
    END-IF                                                       
                                                                 
    MOVE +0                     TO WS-REM-AMT                    
    IF CLAS-I-RL-AH (CLAS-INDEXL) = 'L' OR 'P'                   
       IF CLAS-I-EP (CLAS-INDEXL) = 'B' OR 'K' OR 'L'            
          COMPUTE WS-REM-AMT =                                   
            CR-LFAMT + CR-LFAMT-ALT                              
       ELSE                                                      
          MOVE CR-LFAMT         TO WS-REM-AMT                    
       END-IF                                                    
       GO TO 4199-EXIT                                           
    END-IF                                                       
                                                                 
    COMPUTE INTERMED ROUNDED = CR-LFAMT / CR-LF-TERM             
                                                                 
    IF REM-TRM1 = CR-LF-TERM                                     
       IF CLAS-I-EP (CLAS-INDEXL) = 'B' OR 'K' OR 'L'            
          COMPUTE WS-REM-AMT =                                   
            CR-LFAMT + CR-LFAMT-ALT                              
       ELSE                                                      
          MOVE CR-LFAMT         TO WS-REM-AMT                    
       END-IF                                                    
       GO TO 4199-EXIT                                           
    END-IF                                                       
                                                                 
    IF CLAS-I-EP (CLAS-INDEXL) = 'B' OR 'K' OR 'L'               
       GO TO 4198-ORDINARY-REM                                   
    END-IF                                                       
                                                                 
    IF CLAS-I-EP (CLAS-INDEXL) = 'T'                             
       GO TO 4198-CALC-TEXAS-REM                                 
    END-IF                                                       
                                                                 
    IF CR-LFTYP = 'QD'                                           
       MOVE +15.0               TO CR-APR                        
       MOVE 'N'                 TO                               
                      CLAS-I-CALC-TYPE (CLAS-INDEXL)             
       GO TO 4198-CALC-NET-PAY-REM                               
    END-IF                                                       
                                                                 
    IF CLAS-I-EP (CLAS-INDEXL) = 'N'                             
       GO TO 4198-CALC-NET-PAY-REM                               
    END-IF                                                       
                                                                 
    IF (STATE-ABBR (CLAS-INDEXS) = 'OH') AND                     
       (CR-RATING-CLASS NOT = 'L ')                              
        IF (CR-LF-TERM GREATER THAN +60) AND                     
           (CR-DT GREATER THAN 19831031) AND                     
           (CR-APR GREATER THAN ZERO)                            
            GO TO 4198-CALC-NET-PAY-REM                          
       END-IF                                                    
    END-IF                                                       
                                                                 
    IF STATE-ABBR (CLAS-INDEXS) = 'MT'                           
        IF (CR-LF-TERM GREATER THAN +61) AND                     
           (CR-DT  GREATER THAN 19830318) AND                    
           (CR-APR GREATER THAN ZERO)                            
            GO TO 4198-CALC-NET-PAY-REM                          
       END-IF                                                    
    END-IF                                                       
                                                                 
    IF STATE-ABBR (CLAS-INDEXS) = 'UT'                           
        IF (CR-LF-TERM GREATER THAN +62) AND                     
           (CR-DT  GREATER THAN 19810831) AND                    
           (CR-DT  LESS THAN 19830901) AND                       
           (CR-APR GREATER THAN ZERO)                            
            GO TO 4198-CALC-NET-PAY-REM                          
       END-IF                                                    
    END-IF                                                       
                                                                 
    IF STATE-ABBR (CLAS-INDEXS) = 'RI'                           
        IF (CR-LF-TERM GREATER THAN +60) AND                     
           (CR-DT  GREATER THAN 19831231) AND                    
           (CR-APR GREATER THAN ZERO)                            
            GO TO 4198-CALC-NET-PAY-REM                          
       END-IF                                                    
    END-IF                                                       
                                                                 
    .                                                            
4198-ORDINARY-REM.                                               
                                                                 
    IF CLAS-I-EP (CLAS-INDEXL) = 'B' OR 'K' OR 'L'               
        COMPUTE WS-REM-AMT ROUNDED =                             
                 (INTERMED * REM-TRM2) + CR-LFAMT-ALT            
    ELSE                                                         
        COMPUTE WS-REM-AMT ROUNDED = INTERMED * REM-TRM2         
    END-IF                                                       
                                                                 
    GO TO 4199-EXIT                                              
                                                                 
    .                                                            
4198-CALC-TEXAS-REM.                                             
                                                                 
    DIVIDE CR-LFAMT BY CR-LF-TERM                                
        GIVING TEX-FACT-1.                                       
    DIVIDE REM-TRM2 BY CR-PMT-FREQ                               
        GIVING TEX-FACT-2                                        
        REMAINDER TEX-FACT-3.                                    
                                                                 
    IF TEX-FACT-3 NOT = ZERO                                     
        ADD +1                  TO TEX-FACT-2                    
    END-IF                                                       
                                                                 
    IF (TEX-FACT-2 * CR-PMT-FREQ) = CR-LF-TERM                   
        MOVE CR-LFAMT           TO WS-REM-AMT                    
    ELSE                                                         
        COMPUTE WS-REM-AMT ROUNDED =                             
            (TEX-FACT-1 * (TEX-FACT-2 * CR-PMT-FREQ))            
    END-IF                                                       
                                                                 
    GO TO 4199-EXIT                                              
                                                                 
    .                                                            
4198-CALC-NET-PAY-REM.                                           
                                                                 
    MOVE CR-DT                      TO DC-GREG-DATE-CYMD.        
    MOVE 'L'                        TO DC-OPTION-CODE.           
    PERFORM 8000-DATE-CONVERT-ROUTINE THRU 8099-DATE-CONVERT-X.  
    MOVE DC-BIN-DATE-1              TO CP-CERT-EFF-DT.           
                                                                 
    IF CR-LOAN-1ST-PMT-DT IS NOT = ZEROS                         
       MOVE CR-LOAN-1ST-PMT-DT  TO  DC-GREG-DATE-1-YMD           
       MOVE '3'                 TO  DC-OPTION-CODE               
       PERFORM 8000-DATE-CONVERT-ROUTINE THRU                    
                                 8099-DATE-CONVERT-X             
       MOVE DC-BIN-DATE-1       TO  CP-FIRST-PAY-DATE            
    ELSE                                                         
       MOVE CP-CERT-EFF-DT      TO  DC-BIN-DATE-1                
       MOVE +1                  TO  DC-ELAPSED-MONTHS            
       MOVE +0                  TO  DC-ELAPSED-DAYS              
       MOVE '6'                 TO  DC-OPTION-CODE               
       PERFORM 8000-DATE-CONVERT-ROUTINE THRU                    
                                 8099-DATE-CONVERT-X             
       MOVE DC-BIN-DATE-2       TO  CP-FIRST-PAY-DATE            
    END-IF                                                       
                                                                 
    IF CLAS-I-EP (CLAS-INDEXL) = 'B' OR 'K' OR 'L'               
       COMPUTE CP-ORIGINAL-BENEFIT = CR-LFAMT + CR-LFAMT-ALT     
    ELSE                                                         
       MOVE CR-LFAMT            TO  CP-ORIGINAL-BENEFIT          
    END-IF                                                       
    MOVE CR-APR                 TO  CP-LOAN-APR                  
    MOVE CR-LF-TERM             TO  CP-ORIGINAL-TERM             
    MOVE CR-LOAN-TERM           TO  CP-LOAN-TERM                 
    MOVE REM-TRM2               TO  CP-REMAINING-TERM            
    MOVE CLAS-I-CALC-TYPE (CLAS-INDEXL)                          
                                TO CP-SPECIAL-CALC-CD            
    MOVE CLAS-I-RL-AH (CLAS-INDEXL)                              
                                TO  CP-BENEFIT-TYPE              
    MOVE CLAS-I-EP (CLAS-INDEXL)                                 
                                TO  CP-EARNING-METHOD            
                                                                 
    MOVE ZEROS                  TO CP-R-MAX-TOT-BEN              
    IF (DTE-CLIENT = 'DCC')                                      
       AND (AM-DCC-PRODUCT-CODE <> spaces)                       
       if cr-clp-state = spaces                                  
          move cr-state         to cr-clp-state                  
       end-if                                                    
       if cr-lftyp not = '00' and '  ' and 'DD'                  
          move 'L'              to ws-dd-ben-type                
          move cr-lftyp         to ws-dd-ben-code                
       else                                                      
          move 'A'              to ws-dd-ben-type                
          move cr-ahtyp         to ws-dd-ben-code                
       end-if                                                    
       PERFORM VARYING D1 FROM +1 BY +1 UNTIL                    
          ((CR-clp-STATE = DD-STATE (D1))                        
          AND (AM-DCC-PRODUCT-CODE = DD-PRODUCT-CD (D1))         
          AND (ws-dd-ben-type = DD-BEN-TYPE   (D1))              
          AND (ws-dd-ben-code = DD-BEN-CODE   (D1))              
          AND (CR-DT         < DD-PROD-EXP-DT (D1)))             
                          OR                                     
          D1 > max-pdef                                          
       END-PERFORM                                               
       IF D1 <= max-pdef                                         
          PERFORM VARYING P1 FROM +1 BY +1 UNTIL                 
             (P1 > +11)                                          
             OR (DD-PROD-CODE (D1 P1) = 'L')                     
          END-PERFORM                                            
          IF P1 < +12                                            
             MOVE DD-MAX-BEN-AMT (D1 P1)                         
                                TO CP-R-MAX-TOT-BEN              
          END-IF                                                 
       END-IF                                                    
    END-IF                                                       
                                                                 
    CALL 'ELRAMTX' USING CALCULATION-PASS-AREA                   
                                                                 
    MOVE CP-REMAINING-AMT       TO  WS-REM-AMT                   
                                                                 
    .                                                            
4199-EXIT.                                                       
    EXIT.                                                        
                                                                 
EJECT                                                            
