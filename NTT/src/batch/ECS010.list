                                       CNC-PRM  EPR-ST           
                                       WX-CLM-AMT                
                                       WX-CLM-EXP                
                                       WX-CLM-CNT                
                                       WX-CLM-CRT                
                                       WX-CNC-CNT                
                                       wx-cnc-crt-cnt            
                                       ISS-PRMTAX                
                                       WX-ACTIVE-COUNT.          
    MOVE SPACE                      TO A-H-EARNED-SWITCH.        
                                                                 
    MOVE WE-AHPRM TO ISS-PRM.                                    
                                                                 
    IF DTE-CLIENT = 'GIC'                                        
        IF REIN-EP-FLG NOT = 'R'  AND                            
           CLAS-I-CALC-TYPE (CLAS-INDEXA) = 'B'  AND             
           (CR-ENTRY-STATUS NOT = '5' AND '3' AND 'M')           
           MOVE CR-AHPRM-CALC TO ISS-PRM.                        
                                                                 
    IF REIN-EP-FLG = 'R'  AND                                    
       REIN-AH-RUNOFF-SW (SUB5) = 'N'                            
        NEXT SENTENCE                                            
    ELSE                                                         
        GO TO 4810-EP-DO-AH-CLM.                                 
                                                                 
    IF CR-DIS-DT NOT GREATER THAN EARNING-STOP-DATE              
        IF WE-DSPY-AMT NOT = +0                                  
            ADD WE-DSPY-AMT    TO WX-CLM-AMT                     
            ADD WE-DSPY-EXP    TO WX-CLM-EXP                     
            ADD CR-NUM-DIS-CLM TO WX-CLM-CNT                     
            ADD +1             TO WX-CLM-CNT.                    
                                                                 
    IF CR-AH-STATUS-AT-SETTLEMENT NOT = ' '                      
        IF CR-DIS-DT GREATER THAN EARNING-STOP-DATE              
            GO TO 4830-EP-DO-AH-EP.                              
                                                                 
    IF CR-AH-STATUS-AT-CANCEL NOT = ' '                          
        IF CR-AH-CANC-DT GREATER THAN EARNING-STOP-DATE          
            GO TO 4830-EP-DO-AH-EP.                              
                                                                 
    GO TO 4815-EP-DO-AH-CLM-EPR.                                 
                                                                 
4810-EP-DO-AH-CLM.                                               
                                                                 
    IF WE-DSPY-AMT NOT = +0                                      
        ADD WE-DSPY-AMT    TO WX-CLM-AMT                         
        ADD WE-DSPY-EXP    TO WX-CLM-EXP                         
        ADD CR-NUM-DIS-CLM TO WX-CLM-CNT                         
        ADD +1             TO WX-CLM-CNT.                        
                                                                 
4815-EP-DO-AH-CLM-EPR.                                           
                                                                 
    IF CR-AH-STATUS-AT-SETTLEMENT NOT = ' '                      
        MOVE WE-AHPRM TO EPR-R78  EPR-PRO  EPR-ST.               
                                                                 
4820-EP-DO-AH-CNCL.                                              
                                                                 
    IF CR-AH-STATUS-AT-CANCEL = ' '                              
        IF CR-AH-STATUS-AT-SETTLEMENT NOT = ' '  AND             
           CLAS-I-CALC-TYPE (CLAS-INDEXA) NOT = 'Z'              
            GO TO 4899-EXIT                                      
        ELSE                                                     
            GO TO 4830-EP-DO-AH-EP.                              
                                                                 
    IF (DTE-CLIENT = 'DCC')                                      
       AND (CLAS-I-BEN-CATEGORY (CLAS-INDEXA) = 'G' OR 'L')      
       IF CR-AHRFND NOT = ZEROS                                  
          ADD +1                TO WX-CNC-CNT                    
       END-IF                                                    
    ELSE                                                         
       ADD +1                   TO WX-CNC-CNT                    
       if not cancel-already-counted                             
          add +1                to wx-cnc-crt-cnt                
       end-if                                                    
    END-IF                                                       
                                                                 
    MOVE WE-AHRFND TO CNC-PRM.                                   
                                                                 
*>    MOVE CR-DT                 TO  DC-GREG-DATE-CYMD.            
*>    MOVE 'L'                   TO  DC-OPTION-CODE.               
*>    PERFORM 8000-DATE-CONVERT-ROUTINE THRU 8099-DATE-CONVERT-X.  
*>    MOVE DC-BIN-DATE-1         TO  WS-CR-BIN-DATE.               
                                                                 
    MOVE CR-AH-CANC-DT         TO DC-GREG-DATE-CYMD.             
    MOVE 'L'                   TO  DC-OPTION-CODE.               
    PERFORM 8000-DATE-CONVERT-ROUTINE THRU 8099-DATE-CONVERT-X.  
    MOVE DC-BIN-DATE-1         TO  WS-BIN-CANCEL-DATE.           
                                                                 
    MOVE WS-CR-BIN-DATE          TO  DC-BIN-DATE-1.              
    MOVE WS-BIN-CANCEL-DATE      TO  DC-BIN-DATE-2.              
    MOVE '1'                     TO  DC-OPTION-CODE.             
    MOVE ZEROS                   TO  MONTHS-DIFF-AH.             
    MOVE ZEROS                   TO  DC-ELAPSED-MONTHS           
                                     DC-ODD-DAYS-OVER            
                                     DC-ELAPSED-DAYS.            
    PERFORM 8000-DATE-CONVERT-ROUTINE THRU 8099-DATE-CONVERT-X.  
    MOVE DC-ELAPSED-MONTHS       TO  MONTHS-DIFF-AH.             
                                                                 
    IF DC-ODD-DAYS-OVER   GREATER THAN  ZERO                     
        ADD  +1                  TO  MONTHS-DIFF-AH.             
                                                                 
    MOVE  'YYYYYYYYYY'           TO  WS-AH-CHARGEBACK-LEVELS     
                                     WS-DCC-DDF-CB-LEVELS        
                                                                 
    if (dte-client = 'AHL')                                      
       and (cr-ah-cancel-exit-date < 20120401)                   
       go to 4820-continue-ah-cancel                             
    end-if                                                       
                                                                 
    IF DTE-CLIENT = 'ACE'                                        
        GO TO 4820-CONTINUE-AH-CANCEL.                           
                                                                 
    MOVE  ZEROS                  TO  SUB.                        
                                                                 
4820-AH-CHARGEBACK-LOOP.                                         
                                                                 
    ADD    +1                    TO  SUB.                        
                                                                 
    IF  SUB  GREATER THAN  +10                                   
        GO TO  4820-CONTINUE-AH-CANCEL.                          
                                                                 
    IF CR-CANCEL-REASON = 'R'                                    
       IF (AM-DCC-PRODUCT-CODE = 'DDF')                          
          IF AM-COM-TYP (SUB) = 'L' OR 'N'                       
             MOVE 'N'              TO WS-AH-CHARGEBACK-SW (SUB)  
             GO TO  4820-AH-CHARGEBACK-LOOP                      
          END-IF                                                 
          IF AM-COM-TYP (SUB) = 'J'                              
             MOVE 'Y'              TO WS-AH-CHARGEBACK-SW (SUB)  
             GO TO  4820-AH-CHARGEBACK-LOOP                      
          END-IF                                                 
       END-IF                                                    
    END-IF                                                       
                                                                 
    IF  AM-COMM-CHARGEBACK (SUB)  NOT  NUMERIC  OR               
        AM-COMM-CHARGEBACK (SUB)  =  ZEROS                       
        GO TO  4820-AH-CHARGEBACK-LOOP.                          
                                                                 
    IF  AM-COMM-CHARGEBACK (SUB)  EQUAL    '99'                  
        MOVE  'N'    TO  WS-AH-CHARGEBACK-SW (SUB).              
                                                                 
    IF  (MONTHS-DIFF-AH GREATER THAN AM-COMM-CHARGEBACK (SUB))   
        OR (CR-AHRFND = ZEROS)                                   
        MOVE  'N'        TO  WS-AH-CHARGEBACK-SW (SUB)           
    END-IF                                                       
                                                                 
    GO TO  4820-AH-CHARGEBACK-LOOP.                              
                                                                 
                                                                 
4820-CONTINUE-AH-CANCEL.                                         
                                                                 
    IF DTE-CLIENT = 'GIC'                                        
        IF REIN-EP-FLG NOT = 'R'  AND                            
           CLAS-I-CALC-TYPE (CLAS-INDEXA) = 'B'                  
           MOVE CR-AHRFND-CALC TO CNC-PRM.                       
                                                                 
    COMPUTE EPR-R78 = WE-AHPRM - WE-AHRFND.                      
    MOVE EPR-R78 TO EPR-PRO  EPR-ST.                             
                                                                 
    IF CR-AH-LUMP-SUM-DISAB                                      
        GO TO 4899-EXIT.                                         
                                                                 
    MOVE WE-AHAMT TO CNC-BEN.                                    
                                                                 
    GO TO 4899-EXIT.                                             
                                                                 
4825-EP-DO-AH-CNCL-SUM.                                          
                                                                 
    IF WE-DSPY-AMT NOT = +0                                      
        ADD WE-DSPY-AMT    TO WX-CLM-AMT                         
        ADD WE-DSPY-EXP    TO WX-CLM-EXP                         
        ADD CR-NUM-DIS-CLM TO WX-CLM-CNT                         
        ADD +1             TO WX-CLM-CNT.                        
                                                                 
    IF WE-AHRFND = ZERO                                          
       GO TO 4830-EP-DO-AH-EP.                                   
                                                                 
    MOVE WE-AHRFND TO CNC-PRM.                                   
                                                                 
    ADD CR-SUM-CAN-CNT-ITD TO WX-CNC-CNT.                        
                                                                 
    MOVE  'YYYYYYYYYY'           TO  WS-AH-CHARGEBACK-LEVELS     
                                     WS-DCC-DDF-CB-LEVELS        
                                                                 
    IF WE-AHRFND NOT LESS WE-AHPRM                               
        MOVE WE-AHAMT TO CNC-BEN                                 
         GO TO 4830-EP-DO-AH-EP.                                 
                                                                 
    COMPUTE TEMP-6 ROUNDED = WE-AHRFND / WE-AHPRM.               
    COMPUTE TEMP-7 ROUNDED = TEMP-6 * WE-AHAMT.                  
    MOVE TEMP-7 TO CNC-BEN.                                      
                                                                 
4830-EP-DO-AH-EP.                                                
                                                                 
    MOVE EOM-RUN-DATE TO VALUATION-DT.                           
    MOVE '8'           TO W-VAL-SOURCE.                          
                                                                 
    IF REIN-EP-FLG = 'R'                                         
      IF REIN-REM-SW (SUB5) = 'I'                                
          MOVE WE-AHPRM            TO EPR-R78  EPR-PRO  EPR-ST   
          GO TO 4899-EXIT.                                       
                                                                 
    IF REIN-EP-FLG = 'R'                                         
        IF REIN-AH-RUNOFF-SW (SUB5) = 'N'  AND                   
           EARNING-STOP-DATE LESS THAN VALUATION-DT              
            MOVE '*'               TO A-H-EARNED-SWITCH          
            MOVE EARNING-STOP-DATE TO VALUATION-DT               
            MOVE '10'              TO W-VAL-SOURCE.              
                                                                 
    PERFORM 8500-REMAINING-TERM-ROUTINE THRU 8599-REM-TERM-X.    
                                                                 
    IF REM-TRM2 GREATER CR-AH-TERM                               
        MOVE +0 TO EPR-R78  EPR-PRO  EPR-ST                      
        GO TO 4899-EXIT.                                         
                                                                 
    IF CLAS-I-CALC-TYPE (CLAS-INDEXA) = 'Z'                      
        COMPUTE WE-AHPRM = WE-AHPRM - WE-AHRFND.                 
*>                                                                 
4835-CHECK-OB-AH-NCL.                                            
                                                                 
    IF (DTE-CLIENT EQUAL 'NCL') AND                              
       (CLAS-I-CALC-TYPE (CLAS-INDEXA) EQUAL 'B')                
          MOVE WE-AHPRM         TO EPR-R78                       
                                   EPR-PRO                       
                                   EPR-ST                        
       IF (CR-CCYY EQUAL EOM-CCYY) AND                           
          (CR-MO EQUAL EOM-MO)                                   
          MOVE '*'            TO OB-DATE-1-SWITCH                
          MOVE EW-REIN-CO     TO WS-OB-REIN-CO (SUB3)            
          MOVE AH-OVERRIDE-L1 TO WS-OB-LF-AH (SUB3)              
          MOVE CR-AHTYP       TO WS-OB-BENEFIT-CODE (SUB3)       
          COMPUTE WS-EPR-R78 (SUB3 1) =                          
                  WS-EPR-R78 (SUB3 1) + (WE-AHPRM * .5)          
          GO TO 4899-EXIT                                        
       ELSE                                                      
       IF (CR-CCYY EQUAL OB-CCYY (1)) AND                        
          (CR-MO EQUAL OB-MO (1))                                
          MOVE '*'            TO OB-DATE-2-SWITCH                
          MOVE EW-REIN-CO     TO WS-OB-REIN-CO (SUB3)            
          MOVE AH-OVERRIDE-L1 TO WS-OB-LF-AH (SUB3)              
          MOVE CR-AHTYP       TO WS-OB-BENEFIT-CODE (SUB3)       
          COMPUTE WS-EPR-R78 (SUB3 2) =                          
                  WS-EPR-R78 (SUB3 2) + (WE-AHPRM * .5)          
          GO TO 4899-EXIT                                        
       ELSE                                                      
       IF (CR-CCYY EQUAL OB-CCYY (2)) AND                        
          (CR-MO EQUAL OB-MO (2))                                
          MOVE '*'            TO OB-DATE-3-SWITCH                
          MOVE EW-REIN-CO     TO WS-OB-REIN-CO (SUB3)            
          MOVE AH-OVERRIDE-L1 TO WS-OB-LF-AH (SUB3)              
          MOVE CR-AHTYP       TO WS-OB-BENEFIT-CODE (SUB3)       
          COMPUTE WS-EPR-R78 (SUB3 3) =                          
                  WS-EPR-R78 (SUB3 3) + (WE-AHPRM * .5)          
          GO TO 4899-EXIT                                        
       ELSE                                                      
          GO TO 4899-EXIT.                                       
                                                                 
    IF REM-TRM2 NOT GREATER +0                                   
        MOVE WE-AHPRM TO EPR-R78  EPR-PRO  EPR-ST                
        GO TO 4899-EXIT.                                         
                                                                 
    IF CLAS-I-CALC-TYPE (CLAS-INDEXA) = 'B'                      
        MOVE WE-AHPRM TO EPR-R78  EPR-PRO  EPR-ST                
        GO TO 4899-EXIT.                                         
                                                                 
    IF CLAS-I-CALC-TYPE (CLAS-INDEXA) NOT = 'Z'                  
        MOVE +1 TO WX-ACTIVE-COUNT                               
    ELSE                                                         
        COMPUTE WX-ACTIVE-COUNT = CR-LIVES - CR-SUM-CAN-CNT-ITD. 
                                                                 
    COMPUTE TEMP-1 = CR-AH-TERM * (CR-AH-TERM + 1).              
    COMPUTE TEMP-2 = REM-TRM1 * (REM-TRM1 + 1).                  
    IF DTE-R78 = '1'                                             
        COMPUTE TEMP-2 = REM-TRM1 * REM-TRM1.                    
    COMPUTE TEMP-4 ROUNDED = TEMP-2 / TEMP-1.                    
                                                                 
    IF DTE-CLIENT = 'MIC' OR 'MCC'                               
        COMPUTE TEMP-5 ROUNDED = (REM-TRM1 - .5) / CR-AH-TERM    
    ELSE                                                         
        COMPUTE TEMP-5 ROUNDED = REM-TRM1 / CR-AH-TERM.          
                                                                 
    COMPUTE EPR-R78 ROUNDED = WE-AHPRM * TEMP-4.                 
    COMPUTE EPR-R78 = WE-AHPRM - EPR-R78.                        
                                                                 
    COMPUTE EPR-PRO ROUNDED = WE-AHPRM * TEMP-5.                 
    COMPUTE EPR-PRO = WE-AHPRM - EPR-PRO.                        
                                                                 
    MOVE CLAS-I-EP (CLAS-INDEXA) TO AH-EARN-METHOD.              
                                                                 
*>    IF AM-EARN-METHOD-A NOT EQUAL SPACES AND ZEROS               
*>        MOVE AM-EARN-METHOD-A TO AH-EARN-METHOD.                 
                                                                 
    MOVE SPACES TO STATUTORY-SWITCH-AH.                          
                                                                 
    IF (STATE-ABBR (CLAS-INDEXS) = 'VA') AND                     
       (CR-DT GREATER THAN  19921231)                            
       AND (DTE-CLIENT NOT = 'DCC' AND 'AHL')                    
        IF CR-AH-TERM GREATER THAN  +61                          
            MOVE 'A' TO AH-EARN-METHOD                           
            MOVE '*' TO STATUTORY-SWITCH-AH                      
            GO TO 4845-EP-DO-AH-EP-AN                            
        ELSE                                                     
            GO TO 4860-EP-DO-AH-STATUTORY.                       
                                                                 
    IF (AH-EARN-METHOD EQUAL 'N')                                
                 OR                                              
       ((DTE-CLIENT = 'DCC')                                     
        AND (CLAS-I-BEN-CATEGORY (CLAS-INDEXA) = 'G' OR 'L')     
        AND (CLAS-I-REFUND-METHOD (CLAS-INDEXA)                  
                                   NOT = 'G' AND 'D'))           
        GO TO 4840-EP-DO-AH-EP-NP                                
    END-IF                                                       
                                                                 
    IF AH-EARN-METHOD EQUAL 'A' OR 'C'                           
        MOVE 'A' TO AH-EARN-METHOD                               
        GO TO 4845-EP-DO-AH-EP-AN.                               
                                                                 
    GO TO 4860-EP-DO-AH-STATUTORY.                               
                                                                 
4840-EP-DO-AH-EP-NP.                                             
                                                                 
    IF WE-AHPRM = ZERO                                           
        GO TO 4860-EP-DO-AH-STATUTORY.                           
                                                                 
    IF CR-APR GREATER THAN +0                                    
        NEXT SENTENCE                                            
    ELSE                                                         
        GO TO 4860-EP-DO-AH-STATUTORY.                           
                                                                 
    IF DTE-CLIENT = 'NCL'                                        
      IF CR-DT LESS THAN 19910101                                
          GO TO 4860-EP-DO-AH-STATUTORY.                         
                                                                 
    IF REM-TRM1 = CR-AH-TERM                                     
        MOVE +0 TO EPR-R78  EPR-PRO  EPR-ST                      
        GO TO 4899-EXIT.                                         
                                                                 
    MOVE CR-APR TO NP-APR.                                       
    MOVE CR-AH-TERM TO NP-ORIG  NP-CAP.                          
    MOVE REM-TRM1 TO NP-REM.                                     
    MOVE CLAS-I-CALC-TYPE (CLAS-INDEXA) TO NP-OPT.               
    IF NP-OPT = 'T' OR 'U' OR 'V' OR 'W' OR 'X'                  
        MOVE CR-LOAN-TERM TO NP-ORIG.                            
                                                                 
    IF (DTE-CLIENT = 'DCC')                                      
       AND (CLAS-I-BEN-CATEGORY (CLAS-INDEXA) = 'G' OR 'L')      
       MOVE 'S'                 TO NP-OPT                        
    ELSE                                                         
       MOVE 'R'                 TO NP-OPT                        
    END-IF                                                       
                                                                 
                                                                 
*>    MOVE 'R' TO NP-OPT.                                          
    CALL 'ECSNETRM'                                              
        USING NP-APR NP-ORIG NP-REM NP-OPT NP-CAP NP-FACTOR.     
                                                                 
    IF (DTE-CLIENT = 'DCC')                                      
       AND (CLAS-I-BEN-CATEGORY (CLAS-INDEXA) = 'G' OR 'L')      
       COMPUTE EPR-ST ROUNDED =                                  
          WE-AHPRM - (NP-FACTOR * WE-AHPRM)                      
       GO TO 4899-EXIT                                           
    ELSE                                                         
       COMPUTE EPR-R78 ROUNDED =                                 
          WE-AHPRM - (NP-FACTOR * WE-AHPRM)                      
    END-IF                                                       
                                                                 
    GO TO 4860-EP-DO-AH-STATUTORY.                               
                                                                 
4845-EP-DO-AH-EP-AN.                                             
    MOVE CR-DT                      TO DC-GREG-DATE-CYMD.        
    MOVE 'L'                        TO DC-OPTION-CODE.           
    PERFORM 8000-DATE-CONVERT-ROUTINE THRU 8099-DATE-CONVERT-X.  
    MOVE DC-BIN-DATE-1              TO CP-CERT-EFF-DT.           
    MOVE CLAS-I-RL-AH (CLAS-INDEXA) TO CP-BENEFIT-TYPE.          
    MOVE SPACES                     TO CP-ACCT-FLD-5.            
    MOVE DTE-CLIENT                 TO CP-COMPANY-ID.            
    MOVE STATE-SUB (CLAS-INDEXS)    TO CP-STATE.                 
    MOVE STATE-ABBR (CLAS-INDEXS)   TO CP-STATE-STD-ABBRV.       
    MOVE CR-AGE                     TO CP-ISSUE-AGE.             
    MOVE REM-TRM1                   TO CP-ORIGINAL-TERM.         
    MOVE CR-AHAMT                   TO CP-ORIGINAL-BENEFIT       
                                       CP-RATING-BENEFIT-AMT.    
    IF CP-STATE-STD-ABBRV = 'OR'                                 
        COMPUTE CP-RATING-BENEFIT-AMT =                          
                                 CR-AHAMT * REM-TRM1.            
    MOVE CR-APR                     TO CP-LOAN-APR.              
    MOVE CR-PMT-FREQ                TO CP-PAY-FREQUENCY.         
    MOVE AH-EARN-METHOD             TO CP-EARNING-METHOD.        
    MOVE '3'                        TO CP-PROCESS-TYPE.          
    MOVE CLAS-I-BAL (CLAS-INDEXA)   TO CP-SPECIAL-CALC-CD.       
    MOVE CR-LOAN-TERM               TO CP-LOAN-TERM.             
    IF CR-RATING-CLASS NOT = SPACE AND ZERO                      
        MOVE CR-RATING-CLASS        TO CP-CLASS-CODE             
    ELSE                                                         
        MOVE AM-CAL-TABLE           TO CP-CLASS-CODE.            
    MOVE AM-AH-DEVIATION            TO CP-DEVIATION-CODE.        
    MOVE CR-AHTYP                   TO CP-BENEFIT-CD.            
    MOVE DTE-CLASIC-COMPANY-CD      TO CP-COMPANY-CD.            
    MOVE CR-PMT-EXTENSION-DAYS      TO CP-TERM-OR-EXT-DAYS.      
    MOVE AH-OVERRIDE-L1             TO CP-AH-OVERRIDE-CODE.      
    MOVE AM-AH-DEVIATION-PCT        TO CP-RATE-DEV-PCT.          
                                                                 
    PERFORM 8100-GET-RATE THRU 8199-GET-RATE-X.                  
                                                                 
    IF CP-ERROR-RATE-NOT-FOUND  OR                               
       CP-ERROR-RATE-IS-ZERO  OR                                 
       CP-ERROR-IN-DATES                                         
        GO TO 4860-EP-DO-AH-STATUTORY.                           
                                                                 
    COMPUTE EPR-R78 ROUNDED =                                    
            (REM-TRM1 * WE-AHAMT / +100 *                        
              CP-PREMIUM-RATE)                                   
             / CR-AHPRM-CALC * CR-AHPRM                          
                                                                 
    COMPUTE EPR-R78 = WE-AHPRM - EPR-R78.                        
                                                                 
    IF STATUTORY-REQUIREMENT-AH                                  
        MOVE EPR-R78                TO EPR-ST                    
        GO TO 4899-EXIT.                                         
                                                                 
4860-EP-DO-AH-STATUTORY.                                         
    IF (DTE-CLIENT EQUAL 'CID') AND                              
       (STATE-ABBR (CLAS-INDEXS) = 'VA')                         
        GO TO 4867-EP-DO-AH-STAT-ROA                             
    END-IF.                                                      
                                                                 
4860-EP-DO-AH-STATUTORY-VA.                                      
                                                                 
    IF (DTE-CLIENT = 'DCC')                                      
       AND (CLAS-I-BEN-CATEGORY (CLAS-INDEXA) = 'G' OR 'L')      
       AND (CLAS-I-REFUND-METHOD (CLAS-INDEXA) = 'G')            
       AND (CLAS-I-BEN (CLAS-INDEXA) NOT = '55' AND '56')        
       MOVE EPR-R78             TO EPR-ST                        
       GO TO 4899-EXIT                                           
    END-IF                                                       
                                                                 
    IF (DTE-CLIENT = 'DCC')                                      
       AND (CLAS-I-BEN-CATEGORY (CLAS-INDEXA) = 'G' OR 'L')      
       AND (CLAS-I-REFUND-METHOD (CLAS-INDEXA) = 'G')            
       AND (CLAS-I-BEN (CLAS-INDEXA) = '55' OR '56')             
       MOVE +1.0000                TO TEMP-6                     
       IF REM-TRM1 < +9                                          
          COMPUTE TEMP-6 =                                       
             ((9 - (9 - REM-TRM1)) - 1)                          
             * +.125                                             
       END-IF                                                    
       COMPUTE EPR-ST ROUNDED = WE-AHPRM - (WE-AHPRM * TEMP-6)   
       MOVE EPR-ST              TO EPR-R78                       
                                   EPR-PRO                       
       GO TO 4899-EXIT                                           
    END-IF                                                       
                                                                 
    MOVE EPR-R78                    TO EPR-ST.                   
                                                                 
*>**  WE WANT R78 IN AH STAT FOR AHL                               
                                                                 
    IF DTE-CLIENT = 'AHL'                                        
       GO TO 4899-EXIT                                           
    END-IF                                                       
                                                                 
    MOVE +0                         TO SUB1.                     
                                                                 
4865-EP-AH-STAT-LOOP.                                            
    ADD +1 TO SUB1.                                              
                                                                 
    IF STATE-ABBR (CLAS-INDEXS) = AH-RES-STATE-ENT (SUB1)        
        MOVE EPR-PRO                TO EPR-ST                    
    ELSE                                                         
        IF AH-RES-STATE-ENT (SUB1) NOT = HIGH-VALUES             
            GO TO 4865-EP-AH-STAT-LOOP.                          
                                                                 
    IF (STATE-ABBR (CLAS-INDEXS) = 'OH'  AND                     
        CR-DT GREATER THAN 19831031)  OR                         
                                                                 
       (STATE-ABBR (CLAS-INDEXS) = 'OK'  AND                     
        CR-DT GREATER THAN 19820629)  OR                         
                                                                 
       (STATE-ABBR (CLAS-INDEXS) = 'TX'  AND                     
        CR-DT GREATER THAN 19830831)  OR                         
                                                                 
       (STATE-ABBR (CLAS-INDEXS) = 'MN'  AND                     
        CR-DT GREATER THAN 20091231)                             
                                                                 
        COMPUTE EPR-ST = (EPR-R78 + EPR-PRO) / 2.                
                                                                 
    IF DTE-CLIENT = 'CSO'  OR  'CID'                             
      IF STATE-ABBR (CLAS-INDEXS) = 'CO'                         
        IF CR-DT LESS THAN 19970101                              
            MOVE EPR-R78            TO EPR-ST                    
        ELSE                                                     
            COMPUTE EPR-ST = (EPR-R78 + EPR-PRO) / 2.            
                                                                 
    IF DTE-CLIENT = 'DCC'                                        
       IF CLAS-I-BAL (CLAS-INDEXA) = 'C'                         
          MOVE EPR-PRO          TO EPR-ST                        
       ELSE                                                      
          COMPUTE EPR-ST = (EPR-R78 + EPR-PRO) / 2               
       END-IF                                                    
    END-IF                                                       
                                                                 
    IF (DTE-CLIENT = 'DCC')                                      
       AND (CLAS-I-REFUND-METHOD (CLAS-INDEXA) = 'D')            
       CONTINUE                                                  
    ELSE                                                         
       GO TO 4899-EXIT                                           
    END-IF                                                       
                                                                 
*>    MOVE ' '                    TO DD-IU-SW                      
                                                                 
    MOVE +0                     TO WS-DDF-ADMIN-FEES             
                                   WS-DDF-CSO-ADMIN-FEE          
                                   WS-DDF-1ST-YR-TOT-EXP         
                                   WS-COMM-AND-MFEE              
                                                                 
    PERFORM VARYING D1 FROM +2 BY +1 UNTIL                       
       D1 > +10                                                  
       IF (CR-AGT-TYPE (D1) = 'L' OR 'N')                        
          AND (CR-LCOM-AH (D1) NUMERIC)                          
          COMPUTE WS-DDF-ADMIN-FEES = WS-DDF-ADMIN-FEES          
             + (CR-LCOM-AH (D1) * +1000)                         
       END-IF                                                    
       IF (CR-AGT-TYPE (D1) = 'N')                               
          AND (CR-LCOM-AH (D1) NUMERIC)                          
          COMPUTE WS-DDF-CSO-ADMIN-FEE = WS-DDF-CSO-ADMIN-FEE    
             + (CR-LCOM-AH (D1) * +1000)                         
       END-IF                                                    
       IF (CR-AGT-TYPE (D1) = 'I' )                              
          AND (CR-LCOM-AH (D1) NUMERIC)                          
          COMPUTE WS-COMM-AND-MFEE = WS-COMM-AND-MFEE            
             + (CR-LCOM-AH (D1) * +1000)                         
       END-IF                                                    
       IF (CR-AGT-TYPE (D1) = 'J' OR 'L')                        
          AND (CR-LCOM-AH (D1) NUMERIC)                          
          COMPUTE WS-DDF-1ST-YR-TOT-EXP = WS-DDF-1ST-YR-TOT-EXP  
             + (CR-LCOM-AH (D1) * +1000)                         
       END-IF                                                    
    END-PERFORM                                                  
                                                                 
    MOVE WS-DDF-ADMIN-FEES      TO CP-DDF-ADMIN-FEES             
    MOVE WS-DDF-CSO-ADMIN-FEE   TO CP-DDF-CSO-ADMIN-FEE          
                                                                 
    PERFORM 4910-DCC-DFF-FACT-LOOKUP                             
                                THRU 4910-EXIT                   
                                                                 
    COMPUTE CP-1ST-YR-ALLOW = DD-1ST-YR-ALLOW (D1)               
       + WS-DDF-1ST-YR-TOT-EXP                                   
                                                                 
    COMPUTE WS-COMM-AND-MFEE = WS-COMM-AND-MFEE +                
       (CR-AHPRM - CR-AH-CLP - CR-ADDL-CLP)                      
    MOVE WS-LO-UEF              TO CP-DDF-LO-FACT                
    MOVE WS-HI-UEF              TO CP-DDF-HI-FACT                
                                                                 
    MOVE 'D'                    TO CP-EARNING-METHOD             
    MOVE 'G'                    TO CP-DDF-SPEC-CALC              
    MOVE CR-AHPRM               TO CP-ORIGINAL-PREMIUM           
    MOVE CR-AH-CLP              TO CP-DDF-CLP                    
    MOVE WS-COMM-AND-MFEE       TO CP-DDF-COMM-AND-MFEE          
    MOVE DD-1ST-YR-ALLOW (D1)   TO CP-DDF-YR1AF                  
                                                                 
    IF REIN-EP-FLG = 'R'                                         
       MOVE 'C'                 TO CP-DDF-SPEC-CALC              
       MOVE CR-AH-CLP           TO CP-ORIGINAL-PREMIUM           
       MOVE ZEROS               TO CP-1ST-YR-ALLOW               
    END-IF                                                       
                                                                 
    IF DD-IU-PRESENT                                             
       MOVE 'I'              TO CP-EARNING-METHOD                
    END-IF                                                       
                                                                 
    MOVE CR-DDF-IU-RATE-UP   TO CP-IU-RATE-UP                    
                                CP-CLP-RATE-UP                   
                                                                 
                                                                 
    display ' cert no ' cr-cert-no                               
    display ' rate up ' cp-iu-rate-up                            
    display ' addl clp ' cr-addl-clp                             
    display ' ah clp  ' cr-ah-clp                                
    display ' orig prem ' cp-original-premium                    
                                                                 
    IF (CP-CALC-GROSS-FEE)                                       
       AND (CP-IU-RATE-UP NOT = ZEROS)                           
       COMPUTE CP-IU-RATE-UP =                                   
          CP-IU-RATE-UP / (1 - ((CR-ADDL-CLP + CR-AH-CLP)        
          / CP-ORIGINAL-PREMIUM))                                
    END-IF                                                       
                                                                 
    display ' here 4 '                                           
    MOVE CR-AH-TERM                 TO CP-ORIGINAL-TERM.         
    MOVE REM-TRM1                   TO CP-REMAINING-TERM         
                                                                 
    COMPUTE CP-1ST-YR-ALLOW = DD-1ST-YR-ALLOW (D1)               
       + WS-DDF-1ST-YR-TOT-EXP                                   
                                                                 
    CALL 'ELRFNDX' USING CALCULATION-PASS-AREA.                  
                                                                 
    COMPUTE EPR-ST = WE-AHPRM - CP-CALC-REFUND                   
                                                                 
    IF CP-DDF-SPEC-CALC = 'C'                                    
       COMPUTE UECLP-CLP-FACT = CP-CALC-REFUND / CR-AH-CLP       
    ELSE                                                         
       MOVE 'C'                 TO CP-DDF-SPEC-CALC              
       MOVE CR-AH-CLP           TO CP-ORIGINAL-PREMIUM           
       MOVE ZEROS               TO CP-1ST-YR-ALLOW               
       MOVE CR-DDF-IU-RATE-UP   TO CP-IU-RATE-UP                 
       CALL 'ELRFNDX' USING CALCULATION-PASS-AREA                
       COMPUTE UECLP-CLP-FACT = CP-CALC-REFUND / CR-AH-CLP       
    END-IF                                                       
                                                                 
    display ' here 6 '                                           
    GO TO 4899-EXIT                                              
                                                                 
    .                                                            
4867-EP-DO-AH-STAT-ROA.                                          
    MOVE CR-DT                      TO DC-GREG-DATE-CYMD.        
    MOVE 'L'                        TO DC-OPTION-CODE.           
    PERFORM 8000-DATE-CONVERT-ROUTINE THRU 8099-DATE-CONVERT-X.  
    MOVE DC-BIN-DATE-1              TO CP-CERT-EFF-DT.           
    MOVE CLAS-I-RL-AH (CLAS-INDEXA) TO CP-BENEFIT-TYPE.          
    MOVE SPACES                     TO CP-ACCT-FLD-5.            
    MOVE DTE-CLIENT                 TO CP-COMPANY-ID.            
    MOVE STATE-SUB (CLAS-INDEXS)    TO CP-STATE.                 
    MOVE STATE-ABBR (CLAS-INDEXS)   TO CP-STATE-STD-ABBRV.       
    MOVE CR-AGE                     TO CP-ISSUE-AGE.             
    MOVE REM-TRM1                   TO CP-ORIGINAL-TERM.         
    MOVE CR-AHAMT                   TO CP-ORIGINAL-BENEFIT       
                                       CP-RATING-BENEFIT-AMT.    
    IF CP-STATE-STD-ABBRV = 'OR'                                 
        COMPUTE CP-RATING-BENEFIT-AMT =                          
                                 CR-AHAMT * REM-TRM1.            
    MOVE CR-APR                     TO CP-LOAN-APR.              
    MOVE CR-PMT-FREQ                TO CP-PAY-FREQUENCY.         
    MOVE 'A'                        TO CP-EARNING-METHOD.        
    MOVE '3'                        TO CP-PROCESS-TYPE.          
    MOVE CLAS-I-BAL (CLAS-INDEXA)   TO CP-SPECIAL-CALC-CD.       
    MOVE CR-LOAN-TERM               TO CP-LOAN-TERM.             
    IF CR-RATING-CLASS NOT = SPACE AND ZERO                      
        MOVE CR-RATING-CLASS        TO CP-CLASS-CODE             
    ELSE                                                         
        MOVE AM-CAL-TABLE           TO CP-CLASS-CODE.            
    MOVE AM-AH-DEVIATION            TO CP-DEVIATION-CODE.        
    MOVE CR-AHTYP                   TO CP-BENEFIT-CD.            
    MOVE DTE-CLASIC-COMPANY-CD      TO CP-COMPANY-CD.            
    MOVE CR-PMT-EXTENSION-DAYS      TO CP-TERM-OR-EXT-DAYS.      
    MOVE AH-OVERRIDE-L1             TO CP-AH-OVERRIDE-CODE.      
    MOVE AM-AH-DEVIATION-PCT        TO CP-RATE-DEV-PCT.          
                                                                 
    PERFORM 8100-GET-RATE THRU 8199-GET-RATE-X.                  
                                                                 
    IF CP-ERROR-RATE-NOT-FOUND  OR                               
       CP-ERROR-RATE-IS-ZERO  OR                                 
       CP-ERROR-IN-DATES                                         
        GO TO 4860-EP-DO-AH-STATUTORY-VA.                        
                                                                 
    COMPUTE EPR-ST ROUNDED =                                     
            (REM-TRM1 * WE-AHAMT / +100 *                        
              CP-PREMIUM-RATE)                                   
             / CR-AHPRM-CALC * CR-AHPRM                          
                                                                 
    COMPUTE EPR-ST = WE-AHPRM - EPR-ST.                          
                                                                 
    GO TO 4899-EXIT.                                             
                                                                 
4899-EXIT.                                                       
    EXIT.                                                        
