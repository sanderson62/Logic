*>       ELSE                                                      
          MOVE EPEC-LEVELS (SUB2)                                
                                TO EPEC-WORK                     
*>       END-IF                                                    
*>       IF (EW-ISS-PRM - EW-CNC-PRM) NOT = EW-EPR-ST              
          IF EW-REIN-CO = SPACES                                 
             CONTINUE                                            
*>             IF EW-LF-AH = 'L'                                   
*>                COMPUTE IBNR-FACT ROUNDED = EW-EPR-ST            
*>                   / TOT-LF-PRM                                  
*>                COMPUTE EW-CLM-IBNR ROUNDED = IBNR-FACT          
*>                   * RTOT-LF-IBNR                                
*>*                  * TOT-LF-IBNR                                 
*>             ELSE                                                
*>                COMPUTE IBNR-FACT ROUNDED = EW-EPR-ST            
*>                   / TOT-AH-PRM                                  
*>                COMPUTE EW-CLM-IBNR ROUNDED = IBNR-FACT          
*>                   * RTOT-AH-IBNR                                
*>*                  * TOT-AH-IBNR                                 
*>             END-IF                                              
          ELSE                                                   
             DISPLAY ' AFTER ACCUM IBNR STUFF ' EW-REIN-CO ' '   
                EW-LF-AH ' ' EW-BEN-TYPE ' '                     
                   EW-EPR-ST ' ' RTOT-AH-PRM                     
             IF EW-LF-AH = 'L'                                   
                COMPUTE IBNR-FACT ROUNDED = EW-EPR-ST            
                   / RTOT-LF-PRM                                 
                COMPUTE EW-CLM-IBNR ROUNDED = IBNR-FACT          
                   * RTOT-LF-IBNR                                
             ELSE                                                
                COMPUTE IBNR-FACT ROUNDED = EW-EPR-ST            
                   / RTOT-AH-PRMB                                
                COMPUTE EW-CLM-IBNR ROUNDED = IBNR-FACT          
                   * RTOT-AH-IBNR                                
             END-IF                                              
             IF (EW-ISS-PRM - EW-CNC-PRM) = EW-EPR-ST            
                move zeros to ew-clm-ibnr                        
             end-if                                              
          END-IF                                                 
*>       END-IF                                                    
*>       IF W-UNDERWRITTEN-DATA                                    
*>          MOVE EPEC-WORK        TO EPEC-LEVELS-II (SUB2)         
*>       ELSE                                                      
          MOVE EPEC-WORK        TO EPEC-LEVELS (SUB2)            
*>       END-IF                                                    
    END-PERFORM                                                  
                                                                 
    .                                                            
