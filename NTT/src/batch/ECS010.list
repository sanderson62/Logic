                COMPUTE ISS-WK ROUNDED =                         
                   CR-LFPRM-ALT * CR-LCOM-AH (SUB1)              
             ELSE                                                
                IF CR-AGT-TYPE (SUB1) = 'A'                      
                   COMPUTE ISS-WK ROUNDED =                      
                     CR-AH-CLP * CR-LCOM-AH (SUB1)               
                ELSE                                             
                   IF (CR-AGT-TYPE (SUB1) = 'O' OR 'P' OR 'M')   
                      AND (CLAS-I-BEN-CATEGORY (CLAS-INDEXA) =   
                                    'D')                         
                      COMPUTE ISS-WK ROUNDED =                   
                        CR-AH-CLP * CR-LCOM-AH (SUB1)            
                   ELSE                                          
                      COMPUTE ISS-WK  ROUNDED =                  
                        ISS-PRM * CR-LCOM-AH (SUB1)              
                   END-IF                                        
                END-IF                                           
             END-IF                                              
          END-IF                                                 
       END-IF                                                    
    END-IF                                                       
                                                                 
    COMPUTE EW-ISS-COM (SUB16) =                                 
            EW-ISS-COM (SUB16) + ISS-WK.                         
                                                                 
    MOVE ZEROS                  TO CNC-WK                        
    IF DTE-CLIENT EQUAL 'XXX'                                    
        IF NOT CR-NO-LF-CHARGEBACK                               
          COMPUTE CNC-WK  ROUNDED = CNC-PRM * CR-LCOM-AH (SUB1)  
          COMPUTE EW-CNC-COM (SUB16) =                           
                  EW-CNC-COM (SUB16) + CNC-WK                    
          GO TO 4300-CONTINUE-LOOP-AH.                           
                                                                 
    IF (WS-LEVELS-CHARGEBACK-SWITCHES = SPACES) OR               
       (WS-AH-CHARGEBACK-SW (SUB1) EQUAL 'Y')                    
       EVALUATE TRUE                                             
          WHEN DTE-CLIENT = 'DCC'                                
            AND (REIN-EP-FLG = 'R')                              
            AND (CR-AGT-TYPE (SUB1) = 'P' OR 'T')                
            AND (WE-AHRFND NOT = ZEROS)                          
            IF (CLAS-I-BEN-CATEGORY (CLAS-INDEXA) = 'D')         
               COMPUTE CNC-FACT = CR-AHRFND / CR-AHPRM           
               COMPUTE CNC-WK ROUNDED = CNC-FACT *               
                          CR-AH-NSP-PRM * CR-LCOM-AH (SUB1)      
            ELSE                                                 
               COMPUTE WS-CEDE-FACT = RWF-AHAMT / CR-AHAMT       
               COMPUTE CNC-WK ROUNDED = (CR-AHRFND               
               * WS-CEDE-FACT) * CR-LCOM-AH (SUB1)               
            END-IF                                               
          WHEN (CR-AGT-TYPE (SUB1) = 'C')                        
            AND (AM-DCC-PRODUCT-CODE = 'DDF')                    
            AND (CNC-PRM > +0)                                   
            PERFORM 4376-DCC-DDF-SPEC THRU 4376-EXIT             
          WHEN (CR-AGT-TYPE (SUB1) = 'I')                        
            AND (AM-DCC-PRODUCT-CODE = 'DDF')                    
            AND (CNC-PRM > +0)                                   
            PERFORM 4376-DCC-DDF-SPEC THRU 4376-EXIT             
          WHEN (CR-AGT-TYPE (SUB1) = 'B' OR 'I'                  
                             OR 'L' OR 'J' OR 'K')               
            AND (CNC-PRM > +0)                                   
            COMPUTE CNC-FACT = CNC-PRM / ISS-PRM                 
            IF CR-CANCEL-REASON = 'R'                            
               MOVE 1             TO CNC-FACT                    
            END-IF                                               
            COMPUTE CNC-WK = CNC-FACT *                          
               (CR-LCOM-AH (SUB1) * +1000)                       
                                                                 
          WHEN (CR-AGT-TYPE (SUB1) = 'N')                        
            AND (CNC-PRM > +0)                                   
            AND (CNC-PRM = ISS-PRM)                              
            COMPUTE CNC-WK = (CR-LCOM-AH (SUB1) * +1000)         
                                                                 
          WHEN (CR-AGT-TYPE (SUB1) = 'N')                        
            AND (CNC-PRM > +0)                                   
            PERFORM 4376-DCC-DDF-SPEC THRU 4376-EXIT             
          WHEN (CR-AGT-TYPE (SUB1) = 'M')                        
             AND (CLAS-I-BEN-CATEGORY (CLAS-INDEXA)              
                             = 'G' OR 'L')                       
             COMPUTE CNC-FACT = CR-AHRFND / CR-AHPRM             
             COMPUTE CNC-WK = CNC-FACT *                         
                (CR-LFPRM-ALT * CR-LCOM-AH (SUB1))               
          WHEN CLAS-I-BEN-CATEGORY (CLAS-INDEXA) = 'G' OR 'L'    
             COMPUTE CNC-FACT = CR-AHRFND / CR-AHPRM             
             IF (CR-CANCEL-REASON = 'R')                         
                AND (CNC-PRM > +0)                               
                MOVE +1      TO CNC-FACT                         
             END-IF                                              
             COMPUTE CNC-WK = CNC-FACT *                         
             (CR-AHPRM - (CR-LFPRM-ALT                           
                + CR-ADDL-CLP))                                  
          WHEN (CR-AGT-TYPE (SUB1) = 'A')                        
                     OR                                          
             ((CLAS-I-BEN-CATEGORY (CLAS-INDEXA) = 'D')          
              AND (CR-AGT-TYPE (SUB1) = 'O' OR 'P' OR 'M'))      
              COMPUTE CNC-FACT = CR-AHRFND / CR-AHPRM            
              COMPUTE CNC-WK ROUNDED = CNC-FACT *                
                 CR-AH-NSP-PRM * CR-LCOM-AH (SUB1)               
           WHEN OTHER COMPUTE CNC-WK  ROUNDED =                  
                     CNC-PRM * CR-LCOM-AH (SUB1)                 
       END-EVALUATE                                              
       COMPUTE EW-CNC-COM (SUB16) =                              
          EW-CNC-COM (SUB16) + CNC-WK                            
    END-IF                                                       
                                                                 
    if (ws-dcc-cnc-flag = 'Y')                                   
       and (rein-ep-flg not = 'R')                               
       move cnc-wk              to dx-a-pc (sub1)                
    end-if                                                       
                                                                 
