*>**    M A T C H   P E N D I N G   F I L E   T O   C E R T     ***
*>*****************************************************************
                                                                 
0330-MATCH-PENDING-TO-CERT.                                      
                                                                 
    IF PENDING-MATCH-CONTROL  LESS THAN  CR-FULL-CONTROL         
        GO TO 0390-CERT-MISMATCH-ERROR.                          
                                                                 
    IF PENDING-MATCH-CONTROL  EQUAL   CR-FULL-CONTROL            
*>       DISPLAY ' EQUAL PEND ' PENDING-MATCH-CONTROL              
      IF CLAIM-TRANSACTION                                       
*>         DISPLAY ' CLAIM '                                       
        IF NOT PC-CHARGEBLE-EXPENSE  AND                         
           NOT PC-NON-CHARGEBLE-EXPENSE                          
            IF ((PC-CLAIM-TYPE = '1' OR '3') AND CR-LFTYP = '00')
                                     OR                          
               ((PC-CLAIM-TYPE = '2' OR '4') AND CR-AHTYP = '00')
                 GO TO 0390-CERT-MISMATCH-ERROR.                 
                                                                 
    IF PENDING-MATCH-CONTROL  EQUAL   CR-FULL-CONTROL            
      IF RESERVE-TRANSACTION                                     
*>         DISPLAY ' RESV '                                        
        IF ((PC-CLAIM-TYPE = '1' OR '3') AND CR-LFTYP = '00')    
                                 OR                              
           ((PC-CLAIM-TYPE = '2' OR '4') AND CR-AHTYP = '00')    
             GO TO 0390-CERT-MISMATCH-ERROR.                     
                                                                 
    IF PENDING-MATCH-CONTROL  EQUAL   CR-FULL-CONTROL            
        IF ISSUE-TRANSACTION                                     
*>           DISPLAY ' ISSUE '                                     
            GO TO 0380-DUPLICATE-CERT-ERROR                      
        ELSE                                                     
            GO TO 0350-MATCH-PENDING-TO-ACCOUNT.                 
                                                                 
    IF PENDING-MATCH-CONTROL  NOT LESS THAN  CIR-CONTROL         
        PERFORM 0400-WRITE-CERT-RECORD THRU 0499-WRITE-CERT-X    
        PERFORM 0200-CERT-MASTER-READ-ROUTINE THRU               
                                              0299-READ-CERT-X   
        GO TO 0330-MATCH-PENDING-TO-CERT.                        
                                                                 
    IF ISSUE-TRANSACTION                                         
        PERFORM 0400-WRITE-CERT-RECORD THRU 0499-WRITE-CERT-X    
    ELSE                                                         
        GO TO 0390-CERT-MISMATCH-ERROR.                          
                                                                 
0350-MATCH-PENDING-TO-ACCOUNT.                                   
                                                                 
    IF PENDING-ACCOUNT-CONTROL LESS THAN AM-CONTROL-A            
      ADD +1  TO ERROR-COUNT                                     
        MOVE    '*** MISMATCH BETWEEN PENDING TRAN AND ACCOUNT'  
          TO  DL-FULL                                            
            PERFORM 8600-DISPLAY-PRT  THRU  8600-DISPLAY-EXIT    
        MOVE    ' PENDING CONTROL = '                            
          TO  DIS-LINE-REASON                                    
        MOVE  PENDING-MATCH-CONTROL                              
          TO  DIS-LINE-REC                                       
            PERFORM 8600-DISPLAY-PRT  THRU  8600-DISPLAY-EXIT    
        MOVE    ' KEY OF PENDING REC BYPASSED - '                
          TO  DIS-LINE-REASON                                    
        MOVE  PENDING-MATCH-CONTROL                              
          TO  DIS-LINE-REC                                       
            PERFORM 8600-DISPLAY-PRT  THRU  8600-DISPLAY-EXIT    
        MOVE  SPACES                                             
          TO  DISPLAY-LINE                                       
            PERFORM 8600-DISPLAY-PRT  THRU  8600-DISPLAY-EXIT    
*>**********                                                       
        DISPLAY '*** MISMATCH BETWEEN PENDING TRAN AND ACCOUNT'  
        DISPLAY '    PENDING CONTROL = ' PENDING-MATCH-CONTROL   
        DISPLAY 'PENDING REC BYPASSED - ' PENDING-MATCH-CONTROL  
        DISPLAY 'TRAN TYPE =   '  PENDING-RECORD-TYPE            
        DISPLAY '  TYPE  1=ISS   2=CAN   3=CLM   4=RSV   5=CHG'  
        DISPLAY '    ACCOUNT CONTROL = '                         
        AM-MSTR-CNTRL AM-EFFECT-DT                               
        IF DTE-CLIENT = 'UAL'  OR  'NCL' OR 'CID' OR 'AHL'       
             or 'FNL'                                            
           DISPLAY 'TRANS BYPASSED.... CLIENT = ' DTE-CLIENT     
           GO TO 0300-READ-PENDING-TRANSACTIONS                  
        ELSE                                                     
           MOVE 'MISMATCH BETWEEN PENDING TRAN AND ACCOUNT'      
                                     TO WS-ABEND-MESSAGE         
           MOVE 0302 TO WS-ABEND-CODE                            
           MOVE WS-ABEND-CODE        TO WS-RETURN-CODE           
           GO TO ABEND-PGM.                                      
                                                                 
    IF PENDING-ACCOUNT-CONTROL GREATER AM-CONTROL-A              
        PERFORM 0100-ACCT-MASTER-READ-ROUTINE THRU               
                                             0199-READ-ACCOUNT-X 
        GO TO 0350-MATCH-PENDING-TO-ACCOUNT.                     
                                                                 
    IF PEND-MATCH-EFF-DTE NOT LESS THAN AM-EFFECT-DT  AND        
       PEND-MATCH-EFF-DTE LESS THAN AM-EXPIRE-DT                 
        GO TO 0500-APPLY-PENDING-BUSINESS.                       
                                                                 
    PERFORM 0100-ACCT-MASTER-READ-ROUTINE THRU                   
                                      0199-READ-ACCOUNT-X.       
    GO TO 0350-MATCH-PENDING-TO-ACCOUNT.                         
                                                                 
                                                                 
0370-ACCOUNT-MISMATCH-ERROR.                                     
                                                                 
    DISPLAY 'MISMATCH ACCOUNT ERROR'                             
    DISPLAY '    ACCOUNT CONTROL = ' AM-MSTR-CNTRL AM-EFFECT-DT. 
    MOVE 'MISMATCH ACCOUNT ERROR'                                
                              TO WS-ABEND-MESSAGE.               
    MOVE 0302 TO WS-ABEND-CODE.                                  
        DISPLAY '******************************'                 
        DISPLAY '***** ERROR LOCATION 009 *****'                 
        DISPLAY '******************************'                 
    MOVE WS-ABEND-CODE        TO WS-RETURN-CODE                  
    GO TO ABEND-PGM.                                             
                                                                 
                                                                 
0380-DUPLICATE-CERT-ERROR.                                       
                                                                 
    IF PB-I-OB  OR  PB-I-SUMMARY                                 
        GO TO 0300-READ-PENDING-TRANSACTIONS.                    
                                                                 
    ADD +1 TO DUP-CNT.                                           
                                                                 
    IF DUP-CNT NOT GREATER THAN DUP-MAX                          
       move spaces              to dupe-entry (dup-cnt)          
       move cr-dt               to ws-disp-date                  
       string cr-carrier                                         
          cr-grouping                                            
          cr-state                                               
          cr-account ' '                                         
          ws-disp-date   ' '                                     
          cr-cert-no                                             
             delimited by size into dupe-entry (dup-cnt)         
       end-string                                                
*>        MOVE CR-FULL-CONTROL           TO DUPE-ENTRY (DUP-CNT)   
        COMPUTE DUPE-LF-PRM (DUP-CNT) = PB-I-LF-PREMIUM-AMT +    
                                        PB-I-LF-ALT-PREMIUM-AMT  
        MOVE  PB-I-AH-PREMIUM-AMT      TO  DUPE-AH-PRM (DUP-CNT) 
        MOVE  PB-I-ENTRY-STATUS        TO  DUPE-STATUS (DUP-CNT) 
        if pb-i-entry-status not = 'D' and 'V' and '5' and       
                                 'M' and '9'                     
           compute hld-010-mm-net-prem = hld-010-mm-net-prem +   
              pb-i-lf-premium-amt + pb-i-lf-alt-premium-amt +    
              pb-i-ah-premium-amt                                
        end-if                                                   
        GO TO 0300-READ-PENDING-TRANSACTIONS.                    
                                                                 
    DISPLAY 'MORE THAN ' DUP-MAX ' DUPLICATE ISSUES PROCESSED'.  
    GO TO 0300-READ-PENDING-TRANSACTIONS.                        
                                                                 
*>    MOVE 0301 TO WS-ABEND-CODE.                                  
*>    MOVE WS-ABEND-CODE        TO WS-RETURN-CODE                  
*>    GO TO ABEND-PGM.                                             
                                                                 
                                                                 
0390-CERT-MISMATCH-ERROR.                                        
                                                                 
    IF ISSUE-TRANSACTION  OR                                     
       CANCEL-TRANSACTION  OR                                    
       CLAIM-TRANSACTION                                         
        ADD +1                  TO  MISMATCH-COUNT.              
                                                                 
    IF DTE-CLIENT  =  'CID'  OR  'CSO' OR 'AHL' OR 'FNL'         
      ADD  +1  TO  ERROR-COUNT                                   
        MOVE    '*** MISMATCH BETWEEN CERT AND TRANS'            
          TO  DL-FULL                                            
            PERFORM 8600-DISPLAY-PRT  THRU  8600-DISPLAY-EXIT    
        MOVE    ' TRANS CONTROL = '                              
          TO  DIS-LINE-REASON                                    
        MOVE  PENDING-MATCH-CONTROL                              
          TO  DIS-LINE-REC                                       
            PERFORM 8600-DISPLAY-PRT  THRU  8600-DISPLAY-EXIT    
        MOVE    ' TRANS TYPE    = '                              
          TO  DIS-LINE-REASON                                    
        MOVE  PENDING-RECORD-TYPE                                
          TO  DIS-LINE-REC                                       
            PERFORM 8600-DISPLAY-PRT  THRU  8600-DISPLAY-EXIT    
        MOVE '  TYPE: 1=ISS   2=CAN   3=CLM   4=RSV   5=CHG'     
          TO  DL-FULL                                            
            PERFORM 8600-DISPLAY-PRT  THRU  8600-DISPLAY-EXIT    
        MOVE    ' CERT IN CTL   = '                              
          TO  DIS-LINE-REASON                                    
        MOVE  CIR-CONTROL                                        
          TO  DIS-LINE-REC                                       
            PERFORM 8600-DISPLAY-PRT  THRU  8600-DISPLAY-EXIT    
        MOVE    ' BYPASSED TRANS  = '                            
          TO  DIS-LINE-REASON                                    
        MOVE  PENDING-MATCH-CONTROL                              
          TO  DIS-LINE-REC                                       
            PERFORM 8600-DISPLAY-PRT  THRU  8600-DISPLAY-EXIT    
        MOVE  SPACES                                             
          TO  DISPLAY-LINE                                       
            PERFORM 8600-DISPLAY-PRT  THRU  8600-DISPLAY-EXIT    
        DISPLAY 'BYPASSED TRANS = ' PENDING-MATCH-CONTROL.       
                                                                 
     move spaces to dtl-match-control                            
     move pend-match-eff-dte to ws-disp-date                     
     string pend-match-carrier                                   
        pend-match-grouping                                      
        pend-match-state                                         
        pend-match-account ' '                                   
        ws-disp-date ' '                                         
        pend-match-cert-no                                       
           delimited by size into dtl-match-control              
     end-string                                                  
*>    MOVE PENDING-MATCH-CONTROL  TO  DTL-MATCH-CONTROL.           
    MOVE PENDING-RECORD-TYPE    TO  DTL-REC-TYPE.                
                                                                 
    IF ISSUE-TRANSACTION                                         
        MOVE 'ISS'              TO  DTL-REC-TYPE-DESC            
    ELSE                                                         
        IF CANCEL-TRANSACTION                                    
            MOVE 'CAN'          TO  DTL-REC-TYPE-DESC            
        ELSE                                                     
            IF CLAIM-TRANSACTION                                 
                MOVE 'CLM'      TO  DTL-REC-TYPE-DESC            
            ELSE                                                 
                IF RESERVE-TRANSACTION                           
                    MOVE 'RES'  TO  DTL-REC-TYPE-DESC            
                ELSE                                             
                    IF UPDATE-TRANSACTION                        
                        MOVE 'CHG'                               
                                TO  DTL-REC-TYPE-DESC.           
                                                                 
    MOVE CIR-CONTROL            TO  DTL-CERT-IN-CONTROL.         
    MOVE CR-FULL-CONTROL        TO  DTL-CERT-OUT-CONTROL.        
    MOVE 'DROPPED'              TO  DTL-STATUS.                  
                                                                 
    IF ISSUE-TRANSACTION                                         
        MOVE PB-I-LF-PREMIUM-AMT                                 
                                TO  MD2-LF-PREM                  
        MOVE PB-I-LF-ALT-PREMIUM-AMT                             
                                TO  MD2-LF-ALT-PREM              
        MOVE PB-I-AH-PREMIUM-AMT                                 
                                TO  MD2-AH-PREM                  
        if cr-entry-status not = 'D' and 'V' and '5' and         
                                 'M' and '9'                     
           compute hld-010-mm-net-prem = hld-010-mm-net-prem +   
              pb-i-lf-premium-amt + pb-i-lf-alt-premium-amt +    
              pb-i-ah-premium-amt                                
        end-if                                                   
        ADD PB-I-LF-PREMIUM-AMT TO W-MM-LF-PRM                   
        ADD PB-I-LF-ALT-PREMIUM-AMT TO W-MM-LF-PRM               
        ADD PB-I-AH-PREMIUM-AMT TO W-MM-AH-PRM                   
    ELSE                                                         
        IF CANCEL-TRANSACTION                                    
            MOVE PB-C-LF-CANCEL-AMT                              
                                TO  MD3-LF-REFUND                
            MOVE PB-C-AH-CANCEL-AMT                              
                                TO  MD3-AH-REFUND                
            compute hld-010-mm-net-prem = hld-010-mm-net-prem -  
               (pb-c-lf-cancel-amt + pb-c-ah-cancel-amt)         
            ADD PB-C-LF-CANCEL-AMT TO W-MM-LF-RFD                
            ADD PB-C-AH-CANCEL-AMT TO W-MM-AH-RFD                
        ELSE                                                     
            IF CLAIM-TRANSACTION                                 
                compute hld-010-mm-tot-clms =                    
                   hld-010-mm-tot-clms + pc-claim-payment        
                IF PC-LF-CLAIM  OR  PC-OB-LF-CLAIM               
                    MOVE PC-CLAIM-PAYMENT  TO MD4-LF-CLAIM       
                    MOVE ZEROS             TO MD4-AH-CLAIM       
