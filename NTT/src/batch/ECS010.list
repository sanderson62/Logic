4375-EP-BANK-COMM.                                               
*>     DISPLAY ' 4375 '                                            
                                                                 
    IF CS-AGT (SUB1) = SPACES OR ZEROS                           
       GO TO 4375-EXIT                                           
    END-IF                                                       
    .                                                            
4375-BANK-LOOP.                                                  
                                                                 
    ADD +1                      TO SUB16                         
                                                                 
    IF EW-LF-AH NOT = AH-OVERRIDE-L1                             
       GO TO 4375-EXIT                                           
    END-IF                                                       
                                                                 
    IF SUB16 GREATER THAN WS-LOOP-MAX                            
        DISPLAY 'ERROR OCCURRED - MAXIMUM BANK  LEVELS EXCEEDED' 
        DISPLAY 'CONTROL= ' CR-FULL-CONTROL                      
        MOVE 'ERR0R OCCURRED - MAXIMUM AGENT LEVELS EXCEEDED'    
                                    TO WS-ABEND-MESSAGE          
        MOVE DC-ERROR-CODE          TO WS-ABEND-FILE-STATUS      
        MOVE 4001                   TO WS-ABEND-CODE             
        PERFORM ABEND-PGM                                        
    END-IF                                                       
                                                                 
    IF EW-AGT-NO (SUB16) = SPACES                                
       MOVE CS-AGT (SUB1)       TO EW-AGT-NO   (SUB16)           
       MOVE CS-COM-TYP (SUB1)   TO EW-AGT-TYPE (SUB16)           
    ELSE                                                         
       IF (CS-AGT (SUB1) = EW-AGT-NO   (SUB16)) AND              
          (CS-COM-TYP (SUB1) = EW-AGT-TYPE (SUB16))              
          CONTINUE                                               
       ELSE                                                      
          GO TO 4375-BANK-LOOP                                   
       END-IF                                                    
    END-IF                                                       
                                                                 
    IF CS-COM-TYP  (SUB1) = 'B'                                  
       MOVE CS-SPP-FEES (SUB1)  TO ISS-WK                        
    END-IF                                                       
                                                                 
    COMPUTE EW-ISS-COM (SUB16) = EW-ISS-COM (SUB16) + ISS-WK.    
                                                                 
    MOVE ZEROS                  TO CNC-WK                        
                                   CNC-FACT                      
                                                                 
    IF (CS-COM-TYP (SUB1) = 'B')                                 
       AND (CNC-PRM > +0)                                        
       COMPUTE CNC-FACT = CNC-PRM / ISS-PRM                      
       COMPUTE CNC-WK = CNC-FACT * CS-SPP-FEES (SUB1)            
    END-IF                                                       
                                                                 
    COMPUTE EW-CNC-COM (SUB16) =                                 
                 EW-CNC-COM (SUB16) + CNC-WK                     
                                                                 
    IF ((ISS-PRM - CNC-PRM) = ZEROS)                             
               OR                                                
       ((ISS-WK - CNC-WK) = ZEROS)                               
       MOVE ZEROS          TO COMM-WK                            
    ELSE                                                         
       COMPUTE EC-FACT ROUNDED =                                 
          EPR-R78 / (ISS-PRM - CNC-PRM)                          
       COMPUTE COMM-WK ROUNDED = EC-FACT * (ISS-WK - CNC-WK)     
    END-IF                                                       
                                                                 
    COMPUTE EW-ECR-R78 (SUB16) = EW-ECR-R78 (SUB16) + COMM-WK.   
                                                                 
    IF ((ISS-PRM - CNC-PRM) = ZEROS)                             
               OR                                                
       ((ISS-WK - CNC-WK) = ZEROS)                               
       MOVE ZEROS          TO COMM-WK                            
    ELSE                                                         
       COMPUTE EC-FACT ROUNDED =                                 
          EPR-PRO / (ISS-PRM - CNC-PRM)                          
       COMPUTE COMM-WK ROUNDED = EC-FACT * (ISS-WK - CNC-WK)     
    END-IF                                                       
                                                                 
    COMPUTE EW-ECR-PRO (SUB16) = EW-ECR-PRO (SUB16) + COMM-WK.   
                                                                 
    IF ((ISS-PRM - CNC-PRM) = ZEROS)                             
               OR                                                
       ((ISS-WK - CNC-WK) = ZEROS)                               
       MOVE ZEROS          TO COMM-WK                            
    ELSE                                                         
       COMPUTE EC-FACT ROUNDED =                                 
          EPR-ST / (ISS-PRM - CNC-PRM)                           
       COMPUTE COMM-WK ROUNDED = EC-FACT * (ISS-WK - CNC-WK)     
    END-IF                                                       
                                                                 
    COMPUTE EW-ECR-ST (SUB16) = EW-ECR-ST (SUB16) + COMM-WK.     
                                                                 
    IF A-H-FULLY-EARNED                                          
       MOVE CS-SPP-FEES (SUB1)  TO COMM-WK                       
       COMPUTE EW-ECR-R78-ADJ (SUB16) =                          
                              EW-ECR-R78-ADJ (SUB16) + COMM-WK   
       COMPUTE EW-ECR-PRO-ADJ (SUB16) =                          
                              EW-ECR-PRO-ADJ (SUB16) + COMM-WK   
    END-IF                                                       
                                                                 
    .                                                            
4375-EXIT.                                                       
    EXIT.                                                        
                                                                 
                                                                 
4376-DCC-DDF-SPEC.                                               
                                                                 
    PERFORM 4910-DCC-DFF-FACT-LOOKUP                             
                                THRU 4910-EXIT                   
                                                                 
    display ' acct - cert ' cr-account ' ' cr-cert-no            
    display ' orig term rem term ' orig-term ' ' rem-trm1        
    display ' agt type ' cr-agt-type (sub1)                      
    COMPUTE CP-MONTH =                                           
       FUNCTION REM(ORIG-TERM - REM-TRM1, 12)                    
    IF CP-MONTH = 0                                              
       MOVE 12 TO CP-MONTH                                       
    END-IF                                                       
                                                                 
    EVALUATE TRUE                                                
       WHEN CR-AGT-TYPE (SUB1) = 'N'                             
          PERFORM 4380-REF-CSO-ADMIN-FEE                         
                                THRU 4380-EXIT                   
       WHEN (CR-AGT-TYPE (SUB1) = 'I')                           
          AND (CR-CANCEL-REASON = 'R')                           
             COMPUTE CNC-WK = CR-LCOM-AH (SUB1) * +1000          
       WHEN (CR-AGT-TYPE (SUB1) = 'I')                           
          EVALUATE TRUE                                          
             WHEN ((ORIG-TERM - REM-TRM1) < 4)                   
                OR (NOT DD-IU-PRESENT)                           
                if ueclp-clp-fact = zeros                        
                   COMPUTE ueclp-clp-fact = CR-AH-RFND-CLP       
                      / CR-AH-CLP                                
                end-if                                           
                COMPUTE CNC-WK = UECLP-CLP-FACT *                
                   (CR-LCOM-AH (SUB1) * +1000)                   
              WHEN ((ORIG-TERM - REM-TRM1) < 13)                 
                 AND (DD-IU-PRESENT)                             
                 COMPUTE WS-CLP-MO3 = CR-AH-CLP - (CR-AH-CLP -   
                    CR-DDF-IU-RATE-UP) * (WS-HI-UEF -            
                    WS-LO-UEF) * 3/12                            
                 DISPLAY ' UECLPMO3 ' WS-CLP-MO3                 
                 COMPUTE CNC-FACT = WS-CLP-MO3 / CR-AH-CLP       
                 COMPUTE WS-COMM-MO3 ROUNDED = CNC-FACT *        
                   (CR-LCOM-AH (SUB1) * +1000)                   
                 DISPLAY ' UEMGTFO3 ' WS-COMM-MO3                
                 COMPUTE CNC-WK = WS-COMM-MO3 - (WS-COMM-MO3 -   
                   (CR-LCOM-AH (SUB1) * +1000) * WS-HI-UEF)      
                    * (CP-MONTH - 3) / 9                         
             WHEN (ORIG-TERM - REM-TRM1) > 12                    
                COMPUTE CNC-WK = (CR-LCOM-AH (SUB1) * +1000) *   
                   (WS-HI-UEF - (WS-HI-UEF - WS-LO-UEF) *        
                   CP-MONTH / 12)                                
          END-EVALUATE                                           
       WHEN CR-AGT-TYPE (SUB1) = 'C'                             
          EVALUATE TRUE                                          
             WHEN CR-CANCEL-REASON = 'R'                         
                COMPUTE CNC-WK =  (CR-AHPRM - (CR-AH-CLP         
                   + CR-ADDL-CLP))                               
             WHEN ((ORIG-TERM - REM-TRM1) < 4)                   
                OR (NOT DD-IU-PRESENT)                           
                  if ueclp-clp-fact = zeros                      
                   COMPUTE ueclp-clp-fact = CR-AH-RFND-CLP       
                      / CR-AH-CLP                                
                  end-if                                         
                   COMPUTE CNC-WK = UECLP-CLP-FACT *             
                      (CR-AHPRM - (CR-AH-CLP                     
                      + CR-ADDL-CLP))                            
             WHEN (ORIG-TERM - REM-TRM1) < 13                    
                 COMPUTE WS-CLP-MO3 = CR-AH-CLP - (CR-AH-CLP -   
                    CR-DDF-IU-RATE-UP) * (WS-HI-UEF -            
                    WS-LO-UEF) * 3/12                            
                 DISPLAY ' UECLPMO3 ' WS-CLP-MO3                 
                 COMPUTE CNC-FACT = WS-CLP-MO3 / CR-AH-CLP       
                 COMPUTE WS-COMM-MO3 ROUNDED = CNC-FACT *        
                      (CR-AHPRM - CR-AH-CLP - CR-ADDL-CLP)       
                 DISPLAY ' UECOMMO3 ' WS-COMM-MO3                
                 COMPUTE CNC-WK = WS-COMM-MO3 - (WS-COMM-MO3 -   
                   (CR-AHPRM - CR-AH-CLP - CR-ADDL-CLP) *        
                   WS-HI-UEF) * (CP-MONTH - 3) / 9               
             WHEN (ORIG-TERM - REM-TRM1) > 12                    
                COMPUTE CNC-WK = (CR-AHPRM - CR-AH-CLP -         
                   CR-ADDL-CLP) * (WS-HI-UEF - (WS-HI-UEF -      
                   WS-LO-UEF) * CP-MONTH / 12)                   
          END-EVALUATE                                           
    END-EVALUATE                                                 
                                                                 
