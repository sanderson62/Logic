4300-CONTINUE-LOOP-AH.                                           
                                                                 
    IF (AM-DCC-PRODUCT-CODE = 'DDF')                             
       AND (WS-DDF-AH-CB (SUB1) = 'Y')                           
       AND (AM-COMM-CHARGEBACK (SUB1) NUMERIC)                   
       AND (AM-COMM-CHARGEBACK (SUB1) NOT = 0)                   
       IF (ORIG-TERM - REM-TRM1) > AM-COMM-CHARGEBACK (SUB1)     
          MOVE 'N'              TO WS-DDF-AH-CB (SUB1)           
       END-IF                                                    
    END-IF                                                       
                                                                 
*>     DISPLAY ' 4300-CONTINUE-LOOP-AH '                           
    IF ((ISS-PRM - CNC-PRM) = ZEROS)                             
               OR                                                
       ((ISS-WK - CNC-WK) = ZEROS)                               
       MOVE ZEROS          TO COMM-WK                            
    ELSE                                                         
       COMPUTE EC-FACT ROUNDED =                                 
          EPR-R78 / (ISS-PRM - CNC-PRM)                          
       IF WS-DDF-AH-CB (SUB1) = 'N'                              
          MOVE ISS-WK           TO COMM-WK                       
       ELSE                                                      
          COMPUTE COMM-WK ROUNDED = EC-FACT * (ISS-WK - CNC-WK)  
       END-IF                                                    
    END-IF                                                       
                                                                 
    COMPUTE EW-ECR-R78 (SUB16) = EW-ECR-R78 (SUB16) + COMM-WK.   
                                                                 
    IF ((ISS-PRM - CNC-PRM) = ZEROS)                             
               OR                                                
       ((ISS-WK - CNC-WK) = ZEROS)                               
       MOVE ZEROS          TO COMM-WK                            
    ELSE                                                         
       COMPUTE EC-FACT ROUNDED =                                 
          EPR-PRO / (ISS-PRM - CNC-PRM)                          
       IF WS-DDF-AH-CB (SUB1) = 'N'                              
          MOVE ISS-WK           TO COMM-WK                       
       ELSE                                                      
          COMPUTE COMM-WK ROUNDED = EC-FACT * (ISS-WK - CNC-WK)  
       END-IF                                                    
    END-IF                                                       
                                                                 
    COMPUTE EW-ECR-PRO (SUB16) = EW-ECR-PRO (SUB16) + COMM-WK.   
                                                                 
    IF ((ISS-PRM - CNC-PRM) = ZEROS)                             
               OR                                                
       ((ISS-WK - CNC-WK) = ZEROS)                               
       MOVE ZEROS          TO COMM-WK                            
    ELSE                                                         
       EVALUATE TRUE                                             
          WHEN (CR-AGT-TYPE (SUB1) = 'I' OR 'C')                 
             AND (AM-DCC-PRODUCT-CODE = 'DDF')                   
                PERFORM 4376-DCC-DDF-SPEC THRU 4376-EXIT         
                COMPUTE COMM-WK = ISS-WK - CNC-WK                
          WHEN CR-AGT-TYPE (SUB1) = 'N'                          
            PERFORM 4376-DCC-DDF-SPEC THRU 4376-EXIT             
             COMPUTE COMM-WK = ISS-WK - CNC-WK                   
          WHEN (AM-DCC-PRODUCT-CODE = 'DDF')                     
             AND (WS-DDF-AH-CB  (SUB1) = 'N')                    
             MOVE ISS-WK TO COMM-WK                              
          WHEN OTHER                                             
             COMPUTE EC-FACT ROUNDED =                           
               EPR-ST / (ISS-PRM - CNC-PRM)                      
             COMPUTE COMM-WK ROUNDED = EC-FACT                   
                * (ISS-WK - CNC-WK)                              
       END-EVALUATE                                              
    END-IF                                                       
                                                                 
    COMPUTE EW-ECR-ST (SUB16) = EW-ECR-ST (SUB16) + COMM-WK.     
                                                                 
    IF A-H-FULLY-EARNED                                          
        COMPUTE COMM-WK ROUNDED = (ISS-PRM - EPR-R78) *          
                                               CR-LCOM-AH (SUB1) 
        COMPUTE EW-ECR-R78-ADJ (SUB16) =                         
                              EW-ECR-R78-ADJ (SUB16) + COMM-WK   
        COMPUTE COMM-WK ROUNDED = (ISS-PRM - EPR-PRO) *          
                                               CR-LCOM-AH (SUB1) 
        COMPUTE EW-ECR-PRO-ADJ (SUB16) =                         
                              EW-ECR-PRO-ADJ (SUB16) + COMM-WK.  
                                                                 
