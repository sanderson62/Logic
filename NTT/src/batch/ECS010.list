*>*****************************************************************
*>**       T O T A L   P A G E   P R I N T   R O U T I N E      ***
*>*****************************************************************
                                                                 
6000-TOTAL-PAGE-HEADINGS.                                        
                                                                 
    MOVE TOTAL-PAGE-HD1 TO PRT.                                  
    PERFORM 8800-PRINT-ROUTINE.                                  
                                                                 
6100-ACCT-TOTAL-HEADINGS.                                        
                                                                 
    MOVE TOTAL-PAGE-HD2 TO PRT.                                  
    MOVE '0' TO P-CTL.                                           
    PERFORM 8800-PRINT-ROUTINE.                                  
    MOVE TOTAL-PAGE-HD3 TO PRT.                                  
    MOVE ' ' TO P-CTL.                                           
    PERFORM 8800-PRINT-ROUTINE.                                  
    PERFORM 8800-PRINT-ROUTINE.                                  
    MOVE +81 TO LN-CNT.                                          
                                                                 
6199-PAGE-HEADINGS-X.                                            
    EXIT.                                                        
                                                                 
6500-PRINT-TOTAL-PAGE.                                           
                                                                 
    MOVE SPACES             TO TOTAL-PAGE-DETAIL.                
    MOVE LIFE-OVERRIDE-L6   TO TPD-DESC.                         
    MOVE TPL-ISS-PRM (SUB6) TO TPD-ISS-PRM.                      
                                                                 
    IF SUB6 = 6 AND ME-DO-UPDATE                                 
        MOVE TPL-ISS-PRM (6) TO HLD-010-PREM-L.                  
                                                                 
    MOVE TPL-CNC-PRM (SUB6) TO TPD-CNC-PRM.                      
                                                                 
    IF SUB6 = 6 AND ME-DO-UPDATE                                 
        MOVE TPL-CNC-PRM (6) TO HLD-010-REF-L.                   
                                                                 
    COMPUTE TPD-NET-PRM =                                        
        TPL-ISS-PRM (SUB6) - TPL-CNC-PRM (SUB6).                 
                                                                 
    IF SUB6 = 6 AND ME-DO-UPDATE                                 
        SUBTRACT TPL-CNC-PRM (6) FROM TPL-ISS-PRM (6)            
        GIVING HLD-010-NET-L.                                    
                                                                 
    MOVE TPL-ACCT-COM (SUB6) TO TPD-ACCT-COM.                    
                                                                 
    IF SUB6 = 6 AND ME-DO-UPDATE                                 
        MOVE TPL-ACCT-COM (6) TO HLD-010-COMM-L.                 
                                                                 
    COMPUTE TPD-PREM-COMP =                                      
        TPL-ISS-PRM (SUB6) - TPL-CNC-PRM (SUB6)                  
                           - TPL-ACCT-COM (SUB6).                
    MOVE TPL-CLAIMS (SUB6)   TO TPD-CLAIMS.                      
                                                                 
    IF SUB6 = 6 AND ME-DO-UPDATE                                 
        MOVE TPL-CLAIMS (6) TO HLD-010-PMT-L.                    
                                                                 
    IF SUB6 = 6                                                  
        MOVE TPL-CLAIMS (6)          TO WS-ME-BAL-AMT            
        MOVE WS-BALANCE-DESCRIPTION1 TO WS-ME-BAL-DESCRIP        
        WRITE ME-ECS010-BALANCE-REC  FROM WS-ME-BALANCE-REC      
    END-IF.                                                      
                                                                 
    MOVE TOTAL-PAGE-DETAIL   TO PRT.                             
    PERFORM 8800-PRINT-ROUTINE.                                  
    MOVE SPACES             TO TOTAL-PAGE-DETAIL.                
    MOVE AH-OVERRIDE-L6     TO TPD-DESC.                         
    MOVE TPA-ISS-PRM (SUB6) TO TPD-ISS-PRM.                      
    MOVE TPA-CNC-PRM (SUB6) TO TPD-CNC-PRM.                      
                                                                 
    IF SUB6 = 6 AND ME-DO-UPDATE                                 
        MOVE TPA-ISS-PRM (6) TO HLD-010-PREM-AH                  
        MOVE TPA-CNC-PRM (6) TO HLD-010-REF-AH.                  
                                                                 
    COMPUTE TPD-NET-PRM =                                        
        TPA-ISS-PRM (SUB6) - TPA-CNC-PRM (SUB6).                 
                                                                 
    IF SUB6 = 6 AND ME-DO-UPDATE                                 
        SUBTRACT TPA-CNC-PRM (6) FROM TPA-ISS-PRM (6)            
        GIVING HLD-010-NET-AH.                                   
                                                                 
    MOVE TPA-ACCT-COM (SUB6) TO TPD-ACCT-COM.                    
                                                                 
    IF SUB6 = 6 AND ME-DO-UPDATE                                 
        MOVE TPA-ACCT-COM (6) TO HLD-010-COMM-AH.                
                                                                 
    COMPUTE TPD-PREM-COMP =                                      
        TPA-ISS-PRM (SUB6) - TPA-CNC-PRM (SUB6)                  
                           - TPA-ACCT-COM (SUB6).                
    MOVE TPA-CLAIMS (SUB6)   TO TPD-CLAIMS.                      
                                                                 
    IF SUB6 = 6 AND ME-DO-UPDATE                                 
        MOVE TPA-CLAIMS (6) TO HLD-010-PMT-AH.                   
                                                                 
    IF SUB6 = 6                                                  
        MOVE TPA-CLAIMS (6)          TO WS-ME-BAL-AMT            
        MOVE WS-BALANCE-DESCRIPTION2 TO WS-ME-BAL-DESCRIP        
        WRITE ME-ECS010-BALANCE-REC  FROM WS-ME-BALANCE-REC      
    END-IF.                                                      
                                                                 
    MOVE TOTAL-PAGE-DETAIL   TO PRT.                             
    PERFORM 8800-PRINT-ROUTINE.                                  
                                                                 
    MOVE 'TOTAL' TO TPD-DESC.                                    
                                                                 
    COMPUTE TPD-ISS-PRM =                                        
        TPL-ISS-PRM (SUB6) + TPA-ISS-PRM (SUB6).                 
                                                                 
    COMPUTE TPD-CNC-PRM =                                        
        TPL-CNC-PRM (SUB6) + TPA-CNC-PRM (SUB6).                 
                                                                 
    COMPUTE TPD-NET-PRM =                                        
        TPL-ISS-PRM (SUB6) + TPA-ISS-PRM (SUB6)                  
        - TPL-CNC-PRM (SUB6) - TPA-CNC-PRM (SUB6).               
                                                                 
    COMPUTE TPD-ACCT-COM =                                       
        TPL-ACCT-COM (SUB6) + TPA-ACCT-COM (SUB6).               
                                                                 
    COMPUTE TPD-PREM-COMP =                                      
        TPL-ISS-PRM (SUB6) + TPA-ISS-PRM (SUB6)                  
        - TPL-CNC-PRM (SUB6) - TPA-CNC-PRM (SUB6)                
        - TPL-ACCT-COM (SUB6) - TPA-ACCT-COM (SUB6).             
                                                                 
    COMPUTE TPD-CLAIMS =                                         
        TPL-CLAIMS (SUB6) + TPA-CLAIMS (SUB6).                   
                                                                 
    IF SV-LO-CERT = ZEROS  AND  SV-HI-CERT = ZEROS               
        NEXT SENTENCE                                            
    ELSE                                                         
        MOVE SV-LO-CERT (6:6) TO TPD-LO-CERT                     
        MOVE SV-HI-CERT (6:6) TO TPD-HI-CERT                     
        MOVE ZEROS TO SV-LO-CERT  SV-HI-CERT.                    
*>        MOVE SV-LO-CERT (4:8)    TO TPD-LO-CERT                  
*>        MOVE SV-HI-CERT (4:8)    TO TPD-HI-CERT                  
*>        MOVE ZEROS               TO SV-LO-CERT                   
*>                                    SV-HI-CERT.                  
                                                                 
    MOVE TOTAL-PAGE-DETAIL       TO PRT.                         
    MOVE '0'                     TO P-CTL.                       
    PERFORM 8800-PRINT-ROUTINE.                                  
    PERFORM 8800-PRINT-ROUTINE.                                  
                                                                 
6599-TOTAL-PAGE-PRINT-X.                                         
    EXIT.                                                        
