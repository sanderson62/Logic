0300-READ-PENDING-TRANSACTIONS.                                  
                                                                 
    READ PENDING-EXTRACT                                         
                  AT END GO TO 9910-EOJ-1.                       
                                                                 
    MOVE SPACE                    TO PENDING-RECORD-TYPE.        
                                                                 
    IF NOT ED-PENDING-BUSINESS  AND                              
       NOT ED-PENDING-CLAIMS  AND                                
       NOT ED-CERT-CHANGES                                       
          DISPLAY 'INVALID EXTRACT TYPE INTO ECS010 - TYPE = '   
                                              ED-RECORD-TYPE     
          MOVE 0301                        TO WS-ABEND-CODE      
          MOVE WS-ABEND-CODE        TO WS-RETURN-CODE            
          GO TO ABEND-PGM.                                       
                                                                 
    PERFORM 0310-SET-UP-MATCHING-KEY THRU 0310-EXIT.             
                                                                 
    IF DTE-CLIENT = 'NCL'                                        
        IF PEND-MATCH-EFF-DTE LESS THAN 19400101                 
            DISPLAY 'TRANS DROPPED: ' PENDING-MATCH-CONTROL      
                    '  TRANS TYPE: ' PENDING-RECORD-TYPE         
            GO TO 0300-READ-PENDING-TRANSACTIONS.                
                                                                 
    IF PENDING-MATCH-CONTROL  LESS THAN  PRIOR-MATCH-CNTRL       
        DISPLAY 'SEQUENCE ERROR ON TRANSACTION FILE'             
        DISPLAY ' PRIOR SEQ.   = '  PRIOR-MATCH-CNTRL            
        DISPLAY ' CURRENT SEQ. = '  PENDING-MATCH-CONTROL        
        MOVE 0603                    TO WS-ABEND-CODE            
        MOVE WS-ABEND-CODE        TO WS-RETURN-CODE              
        GO TO ABEND-PGM.                                         
                                                                 
    ADD +1 TO TR-CNT.                                            
                                                                 
    IF ISSUE-TRANSACTION   OR                                    
       CANCEL-TRANSACTION  OR                                    
       CLAIM-TRANSACTION   OR                                    
       RESERVE-TRANSACTION OR                                    
       UPDATE-TRANSACTION                                        
        MOVE PENDING-MATCH-CONTROL   TO PRIOR-MATCH-CNTRL        
        GO TO 0330-MATCH-PENDING-TO-CERT.                        
                                                                 
    DISPLAY 'INVALID TRANSACTION INTO ECS010 '.                  
                                                                 
    DISPLAY 'INVALID TRANSACTION INTO ECS010 '.                  
    DISPLAY '  MATCH CONTROL: ' PENDING-MATCH-CONTROL ' TYPE: (' 
            PENDING-RECORD-TYPE ')'.                             
                                                                 
    MOVE 0301                   TO WS-ABEND-CODE.                
    MOVE WS-ABEND-CODE        TO WS-RETURN-CODE                  
    GO TO ABEND-PGM.                                             
                                EJECT                            
0310-SET-UP-MATCHING-KEY.                                        
                                                                 
    IF ED-PENDING-BUSINESS                                       
        MOVE ED-DATA-AREAS      TO PENDING-BUSINESS              
        MOVE PB-CARRIER         TO PEND-MATCH-CARRIER            
        MOVE PB-GROUPING        TO PEND-MATCH-GROUPING           
        MOVE PB-STATE           TO PEND-MATCH-STATE              
        MOVE PB-ACCOUNT         TO PEND-MATCH-ACCOUNT            
        MOVE PB-CERT-EFF-DT     TO DC-BIN-DATE-1                 
        MOVE PB-CERT-NO         TO PEND-MATCH-CERT-NO            
        MOVE PB-RECORD-TYPE     TO PENDING-RECORD-TYPE.          
                                                                 
    IF ED-PENDING-CLAIMS                                         
        ADD +1                  TO W-PENDING-CLMS                
        MOVE ED-DATA-AREAS      TO PENDING-CLAIMS                
        MOVE PC-CARRIER         TO PEND-MATCH-CARRIER            
        MOVE PC-GROUPING        TO PEND-MATCH-GROUPING           
        MOVE PC-STATE           TO PEND-MATCH-STATE              
        MOVE PC-ACCOUNT         TO PEND-MATCH-ACCOUNT            
        MOVE PC-CERT-EFF-DT     TO DC-BIN-DATE-1                 
        MOVE PC-CERT-NO         TO PEND-MATCH-CERT-NO            
        IF PC-CLAIMS                                             
            MOVE '3'            TO PENDING-RECORD-TYPE           
        ELSE                                                     
            IF PC-RESERVES                                       
                MOVE '4'        TO PENDING-RECORD-TYPE.          
                                                                 
    IF ED-CERT-CHANGES                                           
        MOVE ED-DATA-AREAS      TO PENDING-MAINT-TO-CERT-FILE    
        MOVE CC-CARRIER         TO PEND-MATCH-CARRIER            
        MOVE CC-GROUPING        TO PEND-MATCH-GROUPING           
        MOVE CC-STATE           TO PEND-MATCH-STATE              
        MOVE CC-ACCOUNT         TO PEND-MATCH-ACCOUNT            
        MOVE CC-CERT-EFF-DT     TO DC-BIN-DATE-1                 
        MOVE CC-CERT-NO         TO PEND-MATCH-CERT-NO            
        MOVE '5'                TO PENDING-RECORD-TYPE.          
                                                                 
    MOVE ' '                    TO DC-OPTION-CODE.               
    PERFORM 8000-DATE-CONVERT-ROUTINE THRU 8099-DATE-CONVERT-X.  
    MOVE DC-GREG-DATE-CYMD      TO PEND-MATCH-EFF-DTE.           
                                                                 
0310-EXIT.                                                       
    EXIT.                                                        
                                                                 
