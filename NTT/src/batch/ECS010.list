8000-DATE-CONVERT-ROUTINE.                                       
                                                                 
    CALL 'ELDATCX' USING DATE-CONVERSION-DATA.                   
                                                                 
8099-DATE-CONVERT-X.                                             
    EXIT.                                                        
                                                                 
8000R-CONVERT-STOP-DATE.                                         
                                                                 
    MOVE RWF-EP-STOP-DATE        TO DC-GREG-DATE-CYMD.           
    MOVE 'L'                     TO DC-OPTION-CODE.              
    PERFORM 8000-DATE-CONVERT-ROUTINE THRU 8099-DATE-CONVERT-X.  
    MOVE +0                      TO DC-ELAPSED-MONTHS.           
    MOVE -1                      TO DC-ELAPSED-DAYS.             
    MOVE '6'                     TO DC-OPTION-CODE.              
    PERFORM 8000-DATE-CONVERT-ROUTINE THRU 8099-DATE-CONVERT-X.  
    MOVE +0                      TO DC-ELAPSED-MONTHS            
                                    DC-ELAPSED-DAYS.             
    MOVE DC-BIN-DATE-2           TO DC-BIN-DATE-1.               
    MOVE ' '                     TO DC-OPTION-CODE.              
    PERFORM 8000-DATE-CONVERT-ROUTINE THRU 8099-DATE-CONVERT-X.  
                                                                 
    IF NO-CONVERSION-ERROR                                       
        MOVE DC-GREG-DATE-CYMD   TO EARNING-STOP-DATE            
    ELSE                                                         
        DISPLAY 'ERROR OCCURED CONVERTING EARNING STOP '         
                'DATE IN REIN TABLE: ' CR-REIN-TABLE             
                '  STOP DATE: ' RWF-EP-STOP-DATE                 
        MOVE 'ERROR OCCURED CONVERTING EARNING STOP DATE '       
                                 TO WS-ABEND-MESSAGE             
        MOVE DC-ERROR-CODE       TO WS-ABEND-FILE-STATUS         
        MOVE 4002                TO WS-ABEND-CODE                
        GO TO ABEND-PGM.                                         
                                                                 
8099R-STOP-DATE-X.                                               
    EXIT.                                                        
                                                                 
                                                                 
8100-GET-RATE.                                                   
                                                                 
    CALL 'ELRATEX' USING CALCULATION-PASS-AREA.                  
                                                                 
8199-GET-RATE-X.                                                 
    EXIT.                                                        
                                                                 
                                                                 
8200-STATE-CODE-LOOKUP.                                          
                                                                 
    IF CLAS-INDEXS NOT GREATER CLAS-MAXS  AND                    
       CLAS-INDEXS NOT = ZERO                                    
        IF STATE-L = STATE-SUB (CLAS-INDEXS)                     
            GO TO 8299-STATE-LOOKUP-X.                           
                                                                 
    MOVE CLAS-STARTS TO CLAS-INDEXS.                             
                                                                 
8250-STATE-LOOKUP-LOOP.                                          
                                                                 
    IF CLAS-INDEXS GREATER CLAS-MAXS  OR  CLAS-INDEXS = ZERO     
        MOVE 0302 TO WS-ABEND-CODE                               
        DISPLAY 'STATE CODE ' STATE-L ' NOT IN STATE TABLE'      
        DISPLAY '******************************'                 
        DISPLAY '***** ERROR LOCATION 022 *****'                 
        DISPLAY '******************************'                 
        MOVE WS-ABEND-CODE        TO WS-RETURN-CODE              
        GO TO ABEND-PGM.                                         
                                                                 
    IF STATE-L NOT = STATE-SUB (CLAS-INDEXS)                     
        ADD +1 TO CLAS-INDEXS                                    
        GO TO 8250-STATE-LOOKUP-LOOP.                            
                                                                 
8299-STATE-LOOKUP-X.                                             
    EXIT.                                                        
                                                                 
                                                                 
8300-LIFE-TYPE-LOOKUP.                                           
                                                                 
    IF CLAS-INDEXL NOT GREATER CLAS-MAXL  AND                    
       CLAS-INDEXL NOT = ZERO                                    
        IF CLAS-LOOK = CLAS-I-BEN (CLAS-INDEXL)                  
            GO TO 8399-LIFE-LOOKUP-X.                            
                                                                 
    MOVE CLAS-STARTL TO CLAS-INDEXL.                             
                                                                 
8350-LIFE-LOOKUP-LOOP.                                           
                                                                 
    IF CLAS-INDEXL GREATER CLAS-MAXL  OR  CLAS-INDEXL = ZERO     
        MOVE 0401 TO WS-ABEND-CODE                               
        DISPLAY 'LIFE TYPE ' CLAS-LOOK ' NOT IN TYPE TABLE'      
        DISPLAY '    PENDING CONTROL = ' PENDING-MATCH-CONTROL   
        DISPLAY '       CERT CONTROL = ' CR-FULL-CONTROL         
        DISPLAY '******************************'                 
        DISPLAY '***** ERROR LOCATION 023 *****'                 
        DISPLAY '******************************'                 
        MOVE WS-ABEND-CODE        TO WS-RETURN-CODE              
        IF DTE-CLIENT = 'NCL'                                    
                     OR 'CID' OR 'AHL' OR 'FNL'                  
            MOVE CLAS-STARTL      TO CLAS-INDEXL                 
            MOVE '05'             TO CLAS-LOOK                   
            GO TO 8350-LIFE-LOOKUP-LOOP                          
        ELSE                                                     
            GO TO ABEND-PGM.                                     
                                                                 
    IF CLAS-LOOK NOT = CLAS-I-BEN (CLAS-INDEXL)                  
        ADD +1 TO CLAS-INDEXL                                    
        GO TO 8350-LIFE-LOOKUP-LOOP.                             
                                                                 
    IF CLAS-I-EP (CLAS-INDEXL) = 'U'                             
        MOVE 'R'                    TO CLAS-I-EP (CLAS-INDEXL).  
                                                                 
8399-LIFE-LOOKUP-X.                                              
    EXIT.                                                        
                                                                 
                                                                 
8400-A-H-TYPE-LOOKUP.                                            
                                                                 
    IF CLAS-INDEXA NOT GREATER CLAS-MAXA  AND                    
       CLAS-INDEXA NOT = ZERO                                    
        IF CLAS-LOOK = CLAS-I-BEN (CLAS-INDEXA)                  
            GO TO 8499-A-H-LOOKUP-X.                             
                                                                 
    MOVE CLAS-STARTA TO CLAS-INDEXA.                             
                                                                 
8450-A-H-LOOKUP-LOOP.                                            
                                                                 
    IF CLAS-INDEXA GREATER CLAS-MAXA  OR  CLAS-INDEXA = ZERO     
        MOVE 0402 TO WS-ABEND-CODE                               
        DISPLAY 'A&H TYPE ' CLAS-LOOK ' NOT IN TYPE TABLE'       
        DISPLAY '    PENDING CONTROL = ' PENDING-MATCH-CONTROL   
        DISPLAY '       CERT CONTROL = ' CR-FULL-CONTROL         
        DISPLAY '******************************'                 
        DISPLAY '***** ERROR LOCATION 024 *****'                 
        DISPLAY '******************************'                 
        MOVE WS-ABEND-CODE        TO WS-RETURN-CODE              
        IF DTE-CLIENT = 'NCL'                                    
            MOVE CLAS-STARTA      TO CLAS-INDEXA                 
            MOVE '05'             TO CLAS-LOOK                   
            GO TO 8450-A-H-LOOKUP-LOOP                           
        ELSE                                                     
            GO TO ABEND-PGM.                                     
                                                                 
    IF CLAS-LOOK NOT = CLAS-I-BEN (CLAS-INDEXA)                  
        ADD +1 TO CLAS-INDEXA                                    
        GO TO 8450-A-H-LOOKUP-LOOP.                              
                                                                 
8499-A-H-LOOKUP-X.                                               
    EXIT.                                                        
                                                                 
8500-REMAINING-TERM-ROUTINE.                                     
                                                                 
    MOVE ZEROS                      TO CP-REMAINING-TERM         
                                       CP-REMAINING-TERM-1       
                                       CP-REMAINING-TERM-2       
                                       CP-REMAINING-TERM-3       
                                       CP-RETURN-CODE            
                                       CP-RETURN-CODE-2          
                                       REM-TRM1                  
                                       REM-TRM2                  
                                       REM-TRM3                  
                                       TEM-TRM1.                 
    MOVE SPACES                     TO CP-REM-TERM-METHOD.       
                                                                 
    MOVE CR-DT                      TO DC-GREG-DATE-CYMD.        
    MOVE 'L'                        TO DC-OPTION-CODE.           
    PERFORM 8000-DATE-CONVERT-ROUTINE THRU 8099-DATE-CONVERT-X.  
                                                                 
    IF  NO-CONVERSION-ERROR                                      
        MOVE DC-BIN-DATE-1          TO CP-CERT-EFF-DT            
    ELSE                                                         
        MOVE 'ERROR OCCURED CONVERTING CR-DT'                    
                                    TO WS-ABEND-MESSAGE          
        MOVE DC-ERROR-CODE          TO WS-ABEND-FILE-STATUS      
        MOVE 3001                   TO WS-ABEND-CODE             
        GO TO ABEND-PGM.                                         
                                                                 
    MOVE LOW-VALUES                 TO CP-FIRST-PAY-DATE         
                                                                 
    IF  CR-LOAN-1ST-PMT-DT NOT NUMERIC                           
        MOVE ZEROS                  TO CR-LOAN-1ST-PMT-DT.       
                                                                 
    IF  CR-LOAN-1ST-PMT-DT NOT = ZEROS                           
        MOVE CR-LOAN-1ST-PMT-DT     TO DC-GREG-DATE-1-YMD        
        MOVE ZEROS                  TO DC-ELAPSED-MONTHS         
                                       DC-ELAPSED-DAYS           
        MOVE '3'                    TO DC-OPTION-CODE            
        PERFORM 8000-DATE-CONVERT-ROUTINE THRU                   
                8099-DATE-CONVERT-X                              
        IF  NO-CONVERSION-ERROR                                  
            MOVE DC-BIN-DATE-1      TO CP-FIRST-PAY-DATE         
        ELSE                                                     
            MOVE ZEROS              TO CR-LOAN-1ST-PMT-DT.       
                                                                 
    IF  CP-FIRST-PAY-DATE LESS THAN CP-CERT-EFF-DT               
        MOVE ZEROS                  TO CR-LOAN-1ST-PMT-DT.       
                                                                 
    IF  CR-LOAN-1ST-PMT-DT = ZEROS                               
        MOVE CP-CERT-EFF-DT         TO DC-BIN-DATE-1             
        MOVE +1                     TO DC-ELAPSED-MONTHS         
        MOVE ZEROS                  TO DC-ELAPSED-DAYS           
        MOVE '6'                    TO DC-OPTION-CODE            
        PERFORM 8000-DATE-CONVERT-ROUTINE THRU                   
                8099-DATE-CONVERT-X                              
        IF  NO-CONVERSION-ERROR                                  
            MOVE DC-BIN-DATE-2      TO CP-FIRST-PAY-DATE         
            MOVE DC-GREG-DATE-1-YMD TO CR-LOAN-1ST-PMT-DT        
        ELSE                                                     
            MOVE 'ERROR OCCURED DETERMINING 1ST PAY DATE '       
                                    TO WS-ABEND-MESSAGE          
            MOVE DC-ERROR-CODE      TO WS-ABEND-FILE-STATUS      
            MOVE 3002               TO WS-ABEND-CODE             
            GO TO ABEND-PGM.                                     
                                                                 
    MOVE VALUATION-DT               TO DC-GREG-DATE-CYMD.        
    MOVE 'L'                        TO DC-OPTION-CODE.           
