4910-DCC-DFF-FACT-LOOKUP.                                        
                                                                 
    IF CR-CERT-NO = WS-FACT-CERT                                 
       go to 4910-exit                                           
    end-if                                                       
                                                                 
    if cr-clp-state = spaces                                     
       move cr-state            to cr-clp-state                  
    end-if                                                       
    MOVE ' ' TO DD-IU-SW                                         
    PERFORM VARYING D1 FROM +1 BY +1 UNTIL                       
       ((CR-clp-STATE = DD-STATE (D1))                           
       AND (AM-DCC-PRODUCT-CODE = DD-PRODUCT-CD (D1))            
       AND ('A'           = DD-BEN-TYPE   (D1))                  
       AND (CR-AHTYP      = DD-BEN-CODE   (D1))                  
       AND (CR-DT         < DD-PROD-EXP-DT (D1)))                
                       OR                                        
       D1 > max-pdef                                             
    END-PERFORM                                                  
    IF D1 > max-pdef                                             
       DISPLAY ' COULD NOT FIND REFUND FACTORS '                 
          CR-ACCOUNT ' ' CR-CERT-NO ' ' CR-AHTYP                 
       GO TO 4910-EXIT                                           
    END-IF                                                       
                                                                 
    PERFORM VARYING P1 FROM +1 BY +1 UNTIL                       
       (P1 > +11)                                                
       OR (DD-PROD-CODE (D1 P1) = 'I')                           
    END-PERFORM                                                  
    IF P1 < +12                                                  
       display ' setting iu present to true '                    
       SET DD-IU-PRESENT        TO TRUE                          
    END-IF                                                       
                                                                 
    MOVE CR-AH-TERM             TO WS-DDF-TERM                   
    IF (CR-LOAN-TERM > WS-DDF-TERM)                              
       AND (DD-TRUNCATED (D1) = 'Y')                             
       MOVE CR-LOAN-TERM        TO WS-DDF-TERM                   
       DISPLAY ' FOUND TRUNCATED ' CR-CERT-NO                    
    END-IF                                                       
                                                                 
    EVALUATE TRUE                                                
       WHEN WS-DDF-TERM > +168                                   
          MOVE 15               TO P1                            
       WHEN WS-DDF-TERM > +156                                   
          MOVE 14               TO P1                            
       WHEN WS-DDF-TERM > +144                                   
          MOVE 13               TO P1                            
       WHEN WS-DDF-TERM > +132                                   
          MOVE 12               TO P1                            
       WHEN WS-DDF-TERM > +120                                   
          MOVE 11               TO P1                            
       WHEN WS-DDF-TERM > +108                                   
          MOVE 10               TO P1                            
       WHEN WS-DDF-TERM > +96                                    
          MOVE 9                TO P1                            
       WHEN WS-DDF-TERM > +84                                    
          MOVE 8                TO P1                            
       WHEN WS-DDF-TERM > +72                                    
          MOVE 7                TO P1                            
       WHEN WS-DDF-TERM > +60                                    
          MOVE 6                TO P1                            
       WHEN WS-DDF-TERM > +48                                    
          MOVE 5                TO P1                            
       WHEN WS-DDF-TERM > +36                                    
          MOVE 4                TO P1                            
       WHEN WS-DDF-TERM > +24                                    
          MOVE 3                TO P1                            
       WHEN WS-DDF-TERM > +12                                    
          MOVE 2                TO P1                            
       WHEN OTHER                                                
          MOVE 1                TO P1                            
    END-EVALUATE                                                 
                                                                 
    EVALUATE TRUE                                                
*>       WHEN ((CR-AH-TERM - REM-TRM1) < +13)                      
*>          AND (DD-IU-PRESENT)                                    
*>          MOVE 2                TO P2                            
*>       WHEN (CR-AH-TERM - REM-TRM1) < +13                        
*>          MOVE 1                TO P2                            
       WHEN (CR-AH-TERM - REM-TRM1) < +25                        
          MOVE 2                TO P2                            
       WHEN (CR-AH-TERM - REM-TRM1) < +37                        
          MOVE 3                TO P2                            
       WHEN (CR-AH-TERM - REM-TRM1) < +49                        
          MOVE 4                TO P2                            
       WHEN (CR-AH-TERM - REM-TRM1) < +61                        
          MOVE 5                TO P2                            
       WHEN (CR-AH-TERM - REM-TRM1) < +73                        
          MOVE 6                TO P2                            
       WHEN (CR-AH-TERM - REM-TRM1) < +85                        
          MOVE 7                TO P2                            
       WHEN (CR-AH-TERM - REM-TRM1) < +97                        
          MOVE 8                TO P2                            
       WHEN (CR-AH-TERM - REM-TRM1) < +109                       
          MOVE 9                TO P2                            
       WHEN (CR-AH-TERM - REM-TRM1) < +121                       
          MOVE 10               TO P2                            
       WHEN (CR-AH-TERM - REM-TRM1) < +133                       
          MOVE 11               TO P2                            
       WHEN (CR-AH-TERM - REM-TRM1) < +145                       
          MOVE 12               TO P2                            
       WHEN (CR-AH-TERM - REM-TRM1) < +157                       
          MOVE 13               TO P2                            
       WHEN (CR-AH-TERM - REM-TRM1) < +169                       
          MOVE 14               TO P2                            
       WHEN OTHER                                                
          DISPLAY ' COULD BE TROUBLE ' CR-CERT-NO                
          MOVE 15               TO P2                            
    END-EVALUATE                                                 
                                                                 
    MOVE CR-CERT-NO             TO WS-FACT-CERT                  
    MOVE DD-UEP-FACTOR (D1 P1 P2 + 1)                            
                                TO WS-LO-UEF                     
    MOVE DD-UEP-FACTOR (D1 P1 P2)                                
                                TO WS-HI-UEF                     
                                                                 
