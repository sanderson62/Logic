S-4100-EP-DO-LF.                                                 
                                                                 
    MOVE +0                         TO EPR-R78  EARNED-BY-CANCEL 
                                       EPR-PRO                   
                                       EPR-ST.                   
                                                                 
    MOVE RCT-EARNING-START-DT (CO-SUB) TO DC-GREG-DATE-CYMD.     
    MOVE 'L'                    TO DC-OPTION-CODE.               
    PERFORM 8000-DATE-CONVERT-ROUTINE THRU 8099-DATE-CONVERT-X.  
                                                                 
    MOVE +0                     TO DC-ELAPSED-MONTHS.            
    MOVE -1                     TO DC-ELAPSED-DAYS.              
    MOVE '6'                    TO DC-OPTION-CODE.               
    PERFORM 8000-DATE-CONVERT-ROUTINE THRU 8099-DATE-CONVERT-X.  
                                                                 
    MOVE +0                     TO DC-ELAPSED-MONTHS             
                                   DC-ELAPSED-DAYS.              
                                                                 
    MOVE DC-BIN-DATE-2          TO DC-BIN-DATE-1.                
    MOVE ' '                    TO DC-OPTION-CODE.               
    PERFORM 8000-DATE-CONVERT-ROUTINE THRU 8099-DATE-CONVERT-X.  
                                                                 
    IF NO-CONVERSION-ERROR                                       
       MOVE DC-GREG-DATE-CYMD   TO EARNING-START-DATE            
    ELSE                                                         
        DISPLAY 'ERROR OCCURED CONVERTING EARNING START DATE '   
                'FOR REIN. CO. ' RCT-REIN-CO (CO-SUB)            
                '  START DATE:' RCT-EARNING-START-DT (CO-SUB)    
        MOVE 'ERROR OCCURED CONVERTING EARNING START DATE '      
                                    TO WS-ABEND-MESSAGE          
        MOVE DC-ERROR-CODE          TO WS-ABEND-FILE-STATUS      
        MOVE 4001                   TO WS-ABEND-CODE             
        GO TO ABEND-PGM.                                         
                                                                 
    IF CR-LFTYP = ZERO                                           
        GO TO S-4200-EP-DO-AH.                                   
                                                                 
    IF REIN-LF-FLG (SUB5) = SPACE                                
        GO TO S-4200-EP-DO-AH.                                   
                                                                 
    MOVE LIFE-OVERRIDE-L1       TO WE-LF-AH.                     
    MOVE CR-LFTYP               TO WE-BEN-TYPE.                  
    PERFORM 4400-EP-FIND-TAB-ENT THRU 4499-EP-FIND-TAB-ENT-X.    
                                                                 
    MOVE CR-LF-TERM             TO ORIG-TERM.                    
    MOVE CLAS-INDEXL            TO W-NDX.                        
                                                                 
S-4120-EP-DO-LF-CNCL.                                            
                                                                 
    IF CR-LF-STATUS-AT-CANCEL = ' '  AND                         
       CLAS-I-CALC-TYPE (CLAS-INDEXL) NOT = 'Z'                  
        GO TO S-4130-EP-DO-LF-EP.                                
                                                                 
    COMPUTE EARNED-BY-CANCEL =                                   
                      (WE-LFPRM + WE-LFPRM-ALT) - WE-LFRFND.     
                                                                 
S-4130-EP-DO-LF-EP.                                              
                                                                 
    MOVE SPACE                  TO STATUTORY-SWITCH.             
                                                                 
    MOVE EARNING-START-DATE     TO VALUATION-DT.                 
    MOVE '9'                    TO W-VAL-SOURCE.                 
    PERFORM 8500-REMAINING-TERM-ROUTINE THRU 8599-REM-TERM-X.    
                                                                 
    IF REM-TRM2 GREATER CR-LF-TERM                               
        MOVE +0 TO EPR-R78  EPR-PRO  EPR-ST                      
        GO TO S-4190-EP-DO-LF-COMM.                              
                                                                 
    IF CLAS-I-CALC-TYPE (CLAS-INDEXL) = 'Z'                      
        GO TO S-4180-EP-DO-LF-EP-SUM.                            
                                                                 
    IF CLAS-I-EP (CLAS-INDEXL) = 'B'                             
        IF CLAS-I-CALC-TYPE (CLAS-INDEXL) = 'L'                  
            IF REM-TRM2 NOT GREATER THAN +0                      
                COMPUTE EPR-R78 = WE-LFPRM + WE-LFPRM-ALT        
                MOVE EPR-R78 TO EPR-PRO  EPR-ST                  
                GO TO S-4190-EP-DO-LF-COMM                       
            ELSE                                                 
                NEXT SENTENCE                                    
        ELSE                                                     
            IF W-BAL-REMTRM NOT GREATER +0                       
                COMPUTE EPR-R78 = WE-LFPRM + WE-LFPRM-ALT        
                MOVE EPR-R78 TO EPR-PRO  EPR-ST                  
                GO TO S-4190-EP-DO-LF-COMM                       
            ELSE                                                 
                NEXT SENTENCE                                    
    ELSE                                                         
       IF (DTE-CLIENT EQUAL 'NCL') AND                           
          (CLAS-I-CALC-TYPE (CLAS-INDEXL) EQUAL 'B')             
            MOVE WE-LFPRM TO EPR-R78  EPR-PRO  EPR-ST            
       ELSE                                                      
        IF REM-TRM2 NOT GREATER +0  OR                           
           CLAS-I-CALC-TYPE (CLAS-INDEXL) = 'B'                  
            MOVE WE-LFPRM TO EPR-R78  EPR-PRO  EPR-ST            
            GO TO S-4190-EP-DO-LF-COMM.                          
                                                                 
    IF CLAS-I-EP (CLAS-INDEXL) = 'T'                             
        GO TO S-4140-EP-DO-LF-EP-TEX.                            
                                                                 
    IF CLAS-I-EP (CLAS-INDEXL) = 'N'                             
        GO TO S-4150-EP-DO-LF-EP-NP.                             
                                                                 
*>    IF STATE-ABBR (CLAS-INDEXS) = 'OH'                           
    IF (STATE-ABBR (CLAS-INDEXS) = 'OH') AND                     
       (CR-RATING-CLASS NOT = 'L ')                              
        IF (CR-APR GREATER THAN ZERO)  AND                       
           (CR-LF-TERM GREATER THAN 060)  AND                    
           (CR-DT GREATER THAN 19831031)                         
            MOVE '*'             TO STATUTORY-SWITCH             
             GO TO S-4150-EP-DO-LF-EP-NP.                        
                                                                 
    IF STATE-ABBR (CLAS-INDEXS) = 'MT'                           
        IF (CR-APR GREATER THAN ZERO)  AND                       
           (CR-LF-TERM GREATER THAN 061)  AND                    
           (CR-DT GREATER THAN 19830318)                         
            MOVE '*'             TO STATUTORY-SWITCH             
             GO TO S-4150-EP-DO-LF-EP-NP.                        
                                                                 
    IF STATE-ABBR (CLAS-INDEXS) = 'UT'                           
        IF (CR-APR GREATER THAN ZERO)  AND                       
           (CR-LF-TERM GREATER THAN 062)  AND                    
           (CR-DT GREATER THAN 19810831)  AND                    
           (CR-DT LESS THAN 19830901)                            
            MOVE '*'             TO STATUTORY-SWITCH             
             GO TO S-4150-EP-DO-LF-EP-NP.                        
                                                                 
    IF STATE-ABBR (CLAS-INDEXS) = 'RI'                           
        IF (CR-APR GREATER THAN ZERO)  AND                       
           (CR-LF-TERM GREATER THAN 060)  AND                    
           (CR-DT GREATER THAN 19831231)                         
            MOVE '*'             TO STATUTORY-SWITCH             
             GO TO S-4150-EP-DO-LF-EP-NP.                        
                                                                 
    GO TO S-4160-EP-DO-LF-EP-R78.                                
                                                                 
S-4140-EP-DO-LF-EP-TEX.                                          
                                                                 
    COMPUTE TEX-FACT-4 =                                         
        (CR-LF-TERM * CR-LF-TERM) + (CR-PMT-FREQ * CR-LF-TERM).  
    DIVIDE REM-TRM1 BY CR-PMT-FREQ                               
        GIVING TEX-FACT-5                                        
            REMAINDER TEX-FACT-6.                                
    COMPUTE TEX-FACT-5 = TEX-FACT-5 * CR-PMT-FREQ.               
    COMPUTE TEX-FACT-7 = (TEX-FACT-5 * TEX-FACT-5) +             
        (TEX-FACT-5 * CR-PMT-FREQ) +                             
        (2 * (TEX-FACT-6 * (TEX-FACT-5 + CR-PMT-FREQ))).         
    COMPUTE TEX-FACT-8 ROUNDED = TEX-FACT-7 / TEX-FACT-4.        
                                                                 
    COMPUTE EPR-R78 ROUNDED = WE-LFPRM - (WE-LFPRM * TEX-FACT-8).
                                                                 
    GO TO S-4170-EP-DO-LF-EP-PRO.                                
                                                                 
S-4150-EP-DO-LF-EP-NP.                                           
                                                                 
    IF WE-LFPRM = ZERO                                           
        GO TO S-4160-EP-DO-LF-EP-R78.                            
                                                                 
    IF DTE-CLIENT = 'CRI'                                        
      IF STATE-ABBR (CLAS-INDEXS) = ('AB' OR 'BC' OR 'MB' OR     
            'NS' OR 'ON' OR 'NB' OR 'NF' OR 'QB' OR 'SK' OR 'PE')
          GO TO S-4160-EP-DO-LF-EP-R78.                          
                                                                 
    IF DTE-CLIENT = 'NCL'                                        
      IF CR-DT LESS THAN 19910101                                
          GO TO S-4160-EP-DO-LF-EP-R78.                          
                                                                 
    IF REM-TRM1 = CR-LF-TERM                                     
        MOVE +0 TO EPR-R78  EPR-PRO  EPR-ST                      
        GO TO S-4190-EP-DO-LF-COMM.                              
                                                                 
    MOVE CR-APR TO NP-APR.                                       
    MOVE CR-LF-TERM TO NP-ORIG  NP-CAP.                          
    MOVE REM-TRM1 TO NP-REM.                                     
    MOVE CLAS-I-CALC-TYPE (CLAS-INDEXL) TO NP-OPT.               
    IF NP-OPT = 'T' OR 'U' OR 'V' OR 'W' OR 'X'                  
        MOVE CR-LOAN-TERM TO NP-ORIG.                            
    MOVE 'R' TO NP-OPT.                                          
    CALL 'ECSNETRM'                                              
        USING NP-APR NP-ORIG NP-REM NP-OPT NP-CAP NP-FACTOR.     
                                                                 
    COMPUTE EPR-R78 ROUNDED = WE-LFPRM - (NP-FACTOR * WE-LFPRM). 
                                                                 
    IF STATUTORY-REQUIREMENT                                     
        MOVE EPR-R78                TO EPR-ST.                   
                                                                 
S-4160-EP-DO-LF-EP-R78.                                          
                                                                 
    IF (CLAS-I-CALC-TYPE (CLAS-INDEXL) EQUAL 'B') AND            
       (DTE-CLIENT EQUAL 'NCL')                                  
       IF (CR-CCYY EQUAL EOM-CCYY) AND                           
          (CR-MO EQUAL EOM-MO)                                   
          MOVE '*'            TO OB-DATE-1-SWITCH                
          MOVE EW-REIN-CO     TO WS-OB-REIN-CO (SUB3)            
          MOVE LIFE-OVERRIDE-L1 TO WS-OB-LF-AH (SUB3)            
          MOVE CR-LFTYP       TO WS-OB-BENEFIT-CODE (SUB3)       
          COMPUTE WS-EPR-R78 (SUB3 1) =                          
                  WS-EPR-R78 (SUB3 1) - (WE-LFPRM * .5)          
          GO TO S-4190-EP-DO-LF-COMM                             
       ELSE                                                      
       IF (CR-CCYY EQUAL OB-CCYY (1)) AND                        
          (CR-MO EQUAL OB-MO (1))                                
          MOVE '*'            TO OB-DATE-2-SWITCH                
          MOVE EW-REIN-CO     TO WS-OB-REIN-CO (SUB3)            
          MOVE LIFE-OVERRIDE-L1 TO WS-OB-LF-AH (SUB3)            
          MOVE CR-LFTYP       TO WS-OB-BENEFIT-CODE (SUB3)       
          COMPUTE WS-EPR-R78 (SUB3 2) =                          
                  WS-EPR-R78 (SUB3 2) - (WE-LFPRM * .5)          
          GO TO S-4190-EP-DO-LF-COMM                             
       ELSE                                                      
       IF (CR-CCYY EQUAL OB-CCYY (2)) AND                        
          (CR-MO EQUAL OB-MO (2))                                
          MOVE '*'            TO OB-DATE-3-SWITCH                
          MOVE EW-REIN-CO     TO WS-OB-REIN-CO (SUB3)            
          MOVE LIFE-OVERRIDE-L1 TO WS-OB-LF-AH (SUB3)            
          MOVE CR-LFTYP       TO WS-OB-BENEFIT-CODE (SUB3)       
          COMPUTE WS-EPR-R78 (SUB3 3) =                          
                  WS-EPR-R78 (SUB3 3) - (WE-LFPRM * .5)          
          GO TO S-4190-EP-DO-LF-COMM                             
       ELSE                                                      
          GO TO S-4190-EP-DO-LF-COMM.                            
                                                                 
    COMPUTE TEMP-1 = CR-LF-TERM * (CR-LF-TERM + 1).              
                                                                 
    IF DTE-R78 = '1'                                             
        COMPUTE TEMP-2 = REM-TRM1 * REM-TRM1                     
    ELSE                                                         
        COMPUTE TEMP-2 = REM-TRM1 * (REM-TRM1 + 1).              
                                                                 
    COMPUTE TEMP-4 ROUNDED EQUAL TEMP-2 / TEMP-1.                
                                                                 
    IF REM-TRM1 GREATER THAN +0                                  
        COMPUTE TEMP-7 ROUNDED EQUAL                             
            WE-LFPRM - (WE-LFPRM * TEMP-4)                       
    ELSE                                                         
        MOVE WE-LFPRM                TO TEMP-7.                  
                                                                 
    IF STATE-ABBR (CLAS-INDEXS) = 'AL'                           
        MOVE TEMP-7                 TO EPR-ST.                   
                                                                 
    IF CLAS-I-EP (CLAS-INDEXL) NOT = 'B'                         
        MOVE ZEROS                  TO TEMP-8                    
        GO TO S-4165-END-LF-EP-R78.                              
                                                                 
    IF EPR-ST NOT = ZEROS                                        
        MOVE TEMP-7                 TO EPR-ST.                   
                                                                 
    IF CLAS-I-CALC-TYPE (CLAS-INDEXL) NOT = 'L'                  
        COMPUTE TEMP-1 = (CR-LF-TERM + 1) * (CR-LF-TERM + 2)     
        COMPUTE TEMP-2 = (REM-TRM1 + 1) * (REM-TRM1 + 2)         
        IF DTE-R78 = '1'                                         
            COMPUTE TEMP-2 = (REM-TRM1 + 1) * (REM-TRM1 + 1).    
                                                                 
    COMPUTE TEMP-4 ROUNDED EQUAL TEMP-2 / TEMP-1.                
                                                                 
    COMPUTE TEMP-8 ROUNDED =                                     
              WE-LFPRM-ALT - (WE-LFPRM-ALT * TEMP-4).            
                                                                 
S-4165-END-LF-EP-R78.                                            
                                                                 
    IF EPR-R78 = ZEROS                                           
        COMPUTE EPR-R78 = TEMP-7 + TEMP-8                        
    ELSE                                                         
        COMPUTE EPR-R78 = EPR-R78 + TEMP-8.                      
                                                                 
S-4170-EP-DO-LF-EP-PRO.                                          
                                                                 
    IF DTE-CLIENT = 'MIC' OR 'MCC'                               
        COMPUTE TEMP-5 ROUNDED = (REM-TRM1 - .5) / CR-LF-TERM    
    ELSE                                                         
        COMPUTE TEMP-5 ROUNDED = REM-TRM1 / CR-LF-TERM.          
                                                                 
    IF REM-TRM1 GREATER THAN +0                                  
        COMPUTE TEMP-7 ROUNDED = WE-LFPRM - (WE-LFPRM * TEMP-5)  
    ELSE                                                         
        MOVE WE-LFPRM                  TO TEMP-7.                
                                                                 
    IF CLAS-I-EP (CLAS-INDEXL) NOT = 'B'                         
        MOVE ZEROS                     TO TEMP-8                 
        GO TO S-4175-END-LF-EP-PRO.                              
                                                                 
    IF CLAS-I-CALC-TYPE (CLAS-INDEXL) NOT = 'L'                  
        IF DTE-CLIENT = 'MIC' OR 'MCC'                           
            COMPUTE TEMP-5 ROUNDED =                             
                          (REM-TRM1 + .5) / (CR-LF-TERM + 1)     
        ELSE                                                     
            COMPUTE TEMP-5 ROUNDED =                             
                          (REM-TRM1 + 1) / (CR-LF-TERM + 1).     
                                                                 
    COMPUTE TEMP-8 ROUNDED =                                     
                  WE-LFPRM-ALT - (WE-LFPRM-ALT * TEMP-5).        
                                                                 
    COMPUTE EPR-ST = EPR-ST + TEMP-8.                            
                                                                 
S-4175-END-LF-EP-PRO.                                            
                                                                 
    COMPUTE EPR-PRO = TEMP-7 + TEMP-8.                           
                                                                 
    GO TO S-4185-EP-DO-LF-STATUTORY.                             
                                                                 
S-4180-EP-DO-LF-EP-SUM.                                          
                                                                 
    IF REM-TRM1 NOT GREATER +0                                   
        COMPUTE EPR-R78 = (WE-LFPRM + WE-LFPRM-ALT) - WE-LFRFND  
        MOVE EPR-R78 TO EPR-PRO  EPR-ST                          
        GO TO S-4190-EP-DO-LF-COMM.                              
                                                                 
    IF WE-LFPRM = ZERO  AND  WE-LFPRM-ALT = ZERO                 
        MOVE ZEROS                 TO TEMP-7                     
    ELSE                                                         
        COMPUTE TEMP-7 ROUNDED = WE-LFPRM - (WE-LFRFND *         
                      (WE-LFPRM / (WE-LFPRM + WE-LFPRM-ALT))).   
                                                                 
    IF TEMP-7 NOT GREATER ZERO                                   
        IF CLAS-I-EP (CLAS-INDEXL) NOT = 'B'                     
            MOVE ZEROS             TO EPR-R78  EPR-PRO  EPR-ST   
            GO TO S-4190-EP-DO-LF-COMM                           
        ELSE                                                     
            MOVE ZEROS             TO TEMP-7.                    
                                                                 
    COMPUTE TEMP-1 = CR-LF-TERM * (CR-LF-TERM + 1).              
    COMPUTE TEMP-2 = REM-TRM1 * (REM-TRM1 + 1).                  
    IF DTE-R78 = '1'                                             
        COMPUTE TEMP-2 = REM-TRM1 * REM-TRM1.                    
                                                                 
    COMPUTE TEMP-4 ROUNDED = TEMP-2 / TEMP-1.                    
    COMPUTE TEMP-5 ROUNDED = REM-TRM1 / CR-LF-TERM.              
                                                                 
    COMPUTE EPR-R78 ROUNDED = TEMP-7 - (TEMP-7 * TEMP-4).        
                                                                 
    COMPUTE EPR-PRO ROUNDED = TEMP-7 - (TEMP-7 * TEMP-5).        
                                                                 
    IF CLAS-I-EP (CLAS-INDEXL) NOT = 'B'                         
        GO TO S-4185-EP-DO-LF-STATUTORY.                         
                                                                 
    MOVE EPR-R78                  TO EPR-ST.                     
                                                                 
    IF WE-LFPRM = ZERO  AND  WE-LFPRM-ALT = ZERO                 
        MOVE ZEROS                 TO TEMP-8                     
    ELSE                                                         
        COMPUTE TEMP-8 ROUNDED = WE-LFPRM-ALT - (WE-LFRFND *     
                    (WE-LFPRM-ALT / (WE-LFPRM + WE-LFPRM-ALT))). 
                                                                 
    IF TEMP-8 NOT GREATER ZERO                                   
        GO TO S-4185-EP-DO-LF-STATUTORY.                         
                                                                 
    COMPUTE EPR-R78 ROUNDED = EPR-R78 +                          
                             (TEMP-8 - (TEMP-8 * TEMP-4)).       
                                                                 
    COMPUTE EPR-PRO ROUNDED = EPR-PRO +                          
                             (TEMP-8 - (TEMP-8 * TEMP-5)).       
                                                                 
    COMPUTE EPR-ST ROUNDED = EPR-ST +                            
                            (TEMP-8 - (TEMP-8 * TEMP-5)).        
                                                                 
S-4185-EP-DO-LF-STATUTORY.                                       
                                                                 
*>08472      IF STATE-ABBR (CLAS-INDEXS) = 'AL'                    
*>08473          MOVE EPR-R78                TO EPR-ST.            
                                                                 
    IF STATE-ABBR (CLAS-INDEXS) = 'WY'                           
        MOVE EPR-PRO                TO EPR-ST.                   
                                                                 
    IF (DTE-CLIENT = 'CID') AND                                  
       (STATE-ABBR (CLAS-INDEXS) = 'VA' OR 'TX') AND             
       (CLAS-I-RL-AH (CLAS-INDEXL) NOT = 'L') AND                
       (CLAS-I-EP (CLAS-INDEXL) NOT = 'B')                       
          GO TO S-4187-EP-DO-LF-STAT-ROA                         
    END-IF.                                                      
                                                                 
    IF EPR-ST NOT = ZEROS                                        
        GO TO S-4190-EP-DO-LF-COMM.                              
                                                                 
    IF CLAS-I-RL-AH (CLAS-INDEXL) = 'R'                          
        MOVE EPR-R78                TO EPR-ST                    
    ELSE                                                         
        MOVE EPR-PRO                TO EPR-ST.                   
                                                                 
    GO TO S-4190-EP-DO-LF-COMM.                                  
                                                                 
S-4187-EP-DO-LF-STAT-ROA.                                        
    PERFORM 4196-CALC-REM-AMT THRU 4199-EXIT.                    
    MOVE CR-DT                      TO DC-GREG-DATE-CYMD.        
    MOVE 'L'                        TO DC-OPTION-CODE.           
    PERFORM 8000-DATE-CONVERT-ROUTINE THRU 8099-DATE-CONVERT-X.  
    MOVE DC-BIN-DATE-1              TO CP-CERT-EFF-DT.           
    IF CR-LOAN-1ST-PMT-DT IS NOT = ZEROS                         
       MOVE CR-LOAN-1ST-PMT-DT  TO  DC-GREG-DATE-1-YMD           
       MOVE '3'                 TO  DC-OPTION-CODE               
       PERFORM 8000-DATE-CONVERT-ROUTINE THRU                    
                             8099-DATE-CONVERT-X                 
       MOVE DC-BIN-DATE-1       TO  CP-FIRST-PAY-DATE            
    ELSE                                                         
       MOVE CP-CERT-EFF-DT      TO  DC-BIN-DATE-1                
       MOVE +1                  TO  DC-ELAPSED-MONTHS            
       MOVE +0                  TO  DC-ELAPSED-DAYS              
       MOVE '6'                 TO  DC-OPTION-CODE               
       PERFORM 8000-DATE-CONVERT-ROUTINE THRU                    
                             8099-DATE-CONVERT-X                 
       MOVE DC-BIN-DATE-2       TO  CP-FIRST-PAY-DATE            
    END-IF                                                       
    MOVE REM-TRM1                   TO CP-REMAINING-TERM.        
    MOVE CLAS-I-RL-AH (CLAS-INDEXL) TO CP-BENEFIT-TYPE.          
    MOVE SPACES                     TO CP-ACCT-FLD-5.            
    MOVE DTE-CLIENT                 TO CP-COMPANY-ID.            
    MOVE STATE-SUB (CLAS-INDEXS)    TO CP-STATE.                 
    MOVE STATE-ABBR (CLAS-INDEXS)   TO CP-STATE-STD-ABBRV.       
    MOVE CR-AGE                     TO CP-ISSUE-AGE.             
    MOVE CR-LFPRM                   TO CP-ORIGINAL-PREMIUM       
    MOVE CR-LF-CRIT-PERIOD          TO CP-CRITICAL-MONTHS        
    MOVE CR-LF-TERM                 TO CP-ORIGINAL-TERM          
                                       CP-LOAN-TERM.             
    MOVE CR-LFAMT                   TO CP-ORIGINAL-BENEFIT       
                                       CP-RATING-BENEFIT-AMT.    
    IF  CP-REMAINING-AMT NOT GREATER THAN ZEROS                  
        MOVE ZEROS                  TO CP-REMAINING-BENEFIT      
    ELSE                                                         
        MOVE CP-REMAINING-AMT       TO CP-REMAINING-BENEFIT      
    END-IF.                                                      
    MOVE CR-APR                     TO CP-LOAN-APR.              
    MOVE CR-PMT-FREQ                TO CP-PAY-FREQUENCY.         
    MOVE CLAS-I-EP (CLAS-INDEXL)    TO CP-RATING-METHOD.         
