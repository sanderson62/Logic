          DISPLAY ' REM TERM  = ORIG TERM '                      
          MOVE ISS-WK           TO CNC-WK                        
                                                                 
       WHEN (ORIG-TERM - REM-TRM1) < 4                           
          DISPLAY ' ORIG TERM - REM TRM < 4 '                    
          COMPUTE CP-MONTH =                                     
             FUNCTION REM(ORIG-TERM - REM-TRM1, 12)              
          IF CP-MONTH = 0                                        
             MOVE 12 TO CP-MONTH                                 
          END-IF                                                 
          DISPLAY ' CP MONTH ' CP-MONTH                          
          COMPUTE W-FACTOR ROUNDED = ISS-WK -                    
             (DD-1ST-YR-ALLOW (D1)                               
             * CP-MONTH / 3) - (ISS-WK - DD-1ST-YR-ALLOW (D1))   
             * (1 - WS-HI-UEF) * CP-MONTH / 12                   
          COMPUTE CNC-WK = W-FACTOR * 1                          
                                                                 
*>*** (AdminFee-Yr1 AF)-((AdminFee- Yr1 AF)*(1 - UEF1)*mo/12)      
                                                                 
       WHEN (ORIG-TERM - REM-TRM1) < 13                          
          DISPLAY ' ORIG TERM - REM TRM < 13 '                   
          COMPUTE CP-MONTH =                                     
             FUNCTION REM(ORIG-TERM - REM-TRM1, 12)              
          IF CP-MONTH = 0                                        
             MOVE 12 TO CP-MONTH                                 
          END-IF                                                 
          DISPLAY ' CP MONTH ' CP-MONTH                          
          COMPUTE W-FACTOR ROUNDED = (ISS-WK -                   
             DD-1ST-YR-ALLOW (D1))                               
             - (ISS-WK - DD-1ST-YR-ALLOW (D1))                   
             * (1 - WS-hi-UEF) * CP-MONTH / 12                   
          COMPUTE CNC-WK = W-FACTOR * 1                          
                                                                 
*>***  (AdminFee-Yr1 AF)*{UEF1 - (UEF1 -UEF2)*mo/12}               
                                                                 
       WHEN OTHER                                                
          DISPLAY ' OTHER '                                      
          COMPUTE CP-MONTH =                                     
             FUNCTION REM(ORIG-TERM - REM-TRM1, 12)              
          IF CP-MONTH = 0                                        
             MOVE 12 TO CP-MONTH                                 
          END-IF                                                 
          DISPLAY ' CP MONTH ' CP-MONTH                          
          COMPUTE W-FACTOR ROUNDED = (ISS-WK -                   
             DD-1ST-YR-ALLOW (D1))                               
             * (WS-HI-UEF - (WS-HI-UEF - WS-LO-UEF)              
             * CP-MONTH / 12)                                    
          COMPUTE CNC-WK = W-FACTOR * 1                          
    END-EVALUATE                                                 
                                                                 
    DISPLAY ' UE ADMIN FEE ' CR-ACCOUNT ' ' CR-CERT-NO           
       ' ' CNC-WK                                                
                                                                 
    .                                                            
4380-EXIT.                                                       
    EXIT.                                                        
                                                                 
