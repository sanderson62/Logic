5000-DATE-RANGE-BREAK.                                           
                                                                 
    IF NO-TRANSACTION-WAS-PROCESSED                              
        GO TO 5099-DATE-RANGE-BREAK-X.                           
                                                                 
    ADD +1  TO RANGE-CNT.                                        
    MOVE +1 TO SUB6.                                             
    MOVE +2 TO SUB7.                                             
    PERFORM 5800-ADD-PAGES-LEVELS THRU 5899-ADD-PAGES-LEVELS-X.  
    IF LN-CNT GREATER +50                                        
        PERFORM 8700-PRINT-REPORT-HEADINGS THRU                  
                8750-PRT-RANGE-HEADING.                          
                                                                 
    MOVE SPACES                     TO PRT.                      
    PERFORM 8800-PRINT-ROUTINE.                                  
    MOVE 'DATE RANGE'               TO TPH2-DESC.                
    MOVE '  TOTALS'                 TO TPH3-DESC.                
    PERFORM 6100-ACCT-TOTAL-HEADINGS THRU 6199-PAGE-HEADINGS-X.  
    PERFORM 6500-PRINT-TOTAL-PAGE THRU 6599-TOTAL-PAGE-PRINT-X.  
    PERFORM 5900-CLEAR-TOTAL-PAGES                               
        THRU 5999-CLEAR-TOTAL-PAGES-X                            
            VARYING SUB6 FROM 1 BY 1 UNTIL SUB6 GREATER 1.       
                                                                 
5099-DATE-RANGE-BREAK-X.                                         
    EXIT.                                                        
                                                                 
5100-ACCOUNT-BREAK.                                              
                                                                 
    IF RANGE-CNT NOT = +0                                        
        PERFORM 5000-DATE-RANGE-BREAK                            
            THRU 5099-DATE-RANGE-BREAK-X                         
    ELSE                                                         
        MOVE +1 TO SUB6                                          
        MOVE +2 TO SUB7                                          
        PERFORM 5800-ADD-PAGES-LEVELS                            
            THRU 5899-ADD-PAGES-LEVELS-X                         
        PERFORM 5900-CLEAR-TOTAL-PAGES                           
            THRU 5999-CLEAR-TOTAL-PAGES-X                        
                VARYING SUB6 FROM 1 BY 1 UNTIL SUB6 GREATER 1.   
                                                                 
    MOVE +0 TO RANGE-CNT.                                        
    PERFORM 7500-ADD-UP-REIN-CO THRU 7599-ADD-UP-REIN-X          
        VARYING B-SUB FROM 1 BY 1                                
            UNTIL B-SUB GREATER THAN 50  OR                      
                  RB-COMPANY (B-SUB) = SPACES.                   
    PERFORM 7900-CLEAR-BILLING-TABLE THRU 7999-CLEAR-BILLING-X   
        VARYING B-SUB FROM 1 BY 1                                
            UNTIL B-SUB GREATER THAN 50.                         
                                                                 
    IF ACCT-FLG = SPACE                                          
        GO TO 5199-ACCOUNT-BREAK-X.                              
                                                                 
    MOVE SPACE TO ACCT-FLG.                                      
    MOVE +2 TO SUB6.                                             
    MOVE +3 TO SUB7.                                             
    PERFORM 5800-ADD-PAGES-LEVELS THRU 5899-ADD-PAGES-LEVELS-X.  
    IF LN-CNT GREATER +50                                        
        PERFORM 8700-PRINT-REPORT-HEADINGS THRU                  
                8725-PRT-ACCT-HEADING.                           
                                                                 
    MOVE SPACES                     TO PRT.                      
    PERFORM 8800-PRINT-ROUTINE.                                  
    MOVE 'ACCOUNT'                  TO TPH2-DESC.                
    MOVE ' TOTALS'                  TO TPH3-DESC.                
    PERFORM 6100-ACCT-TOTAL-HEADINGS THRU 6199-PAGE-HEADINGS-X.  
    PERFORM 6500-PRINT-TOTAL-PAGE THRU 6599-TOTAL-PAGE-PRINT-X.  
    PERFORM 5900-CLEAR-TOTAL-PAGES                               
        THRU 5999-CLEAR-TOTAL-PAGES-X                            
            VARYING SUB6 FROM 2 BY 1 UNTIL SUB6 GREATER 2.       
                                                                 
5199-ACCOUNT-BREAK-X.                                            
    EXIT.                                                        
                                                                 
5200-STATE-BREAK.                                                
                                                                 
