*>                            COPY ECSRTPFM.                       
*>vvv-> file: /apps/prod/cid1p/copy/ECSRTPFM
*>*****************************************************************
*>                                                                *
*>                            ECSRTPFM                            *
*>                            VMOD=2.002                          *
*>                                                                *
*>*****************************************************************
                                                                 
    IF    CR-LFAMT  NUMERIC                                      
          NEXT SENTENCE                                          
       ELSE                                                      
          MOVE ZEROS TO CR-LFAMT.                                
                                                                 
    IF    CR-LFAMT-ALT  NUMERIC                                  
          NEXT SENTENCE                                          
       ELSE                                                      
          MOVE ZEROS TO CR-LFAMT-ALT.                            
                                                                 
                                                                 
    IF    CR-LFPRM  NUMERIC                                      
          NEXT SENTENCE                                          
       ELSE                                                      
          MOVE ZEROS TO CR-LFPRM.                                
                                                                 
    IF    CR-LFPRM-ALT  NUMERIC                                  
          NEXT SENTENCE                                          
       ELSE                                                      
          MOVE ZEROS TO CR-LFPRM-ALT.                            
                                                                 
                                                                 
    IF    CR-LFPRM-CALC  NUMERIC                                 
          NEXT SENTENCE                                          
       ELSE                                                      
          MOVE ZEROS TO CR-LFPRM-CALC.                           
                                                                 
    IF    CR-LFPRM-CALC-ALT  NUMERIC                             
          NEXT SENTENCE                                          
       ELSE                                                      
          MOVE ZEROS TO CR-LFPRM-CALC-ALT.                       
                                                                 
    IF    CR-LF-NSP-PRM  NUMERIC                                 
          NEXT SENTENCE                                          
       ELSE                                                      
          MOVE ZEROS TO CR-LF-NSP-PRM.                           
                                                                 
    COMPUTE RS-R-LB = CR-LFAMT + CR-LFAMT-ALT.                   
    COMPUTE RS-R-LP = CR-LFPRM + CR-LFPRM-ALT.                   
    IF RE-NSP-ST-CD-LF = ZEROS  OR  SPACES                       
        COMPUTE RS-R-LPC = CR-LFPRM-CALC + CR-LFPRM-CALC-ALT     
    ELSE                                                         
        MOVE CR-LF-NSP-PRM         TO RS-R-LPC.                  
    MOVE RS-R-LPC                  TO REIN-CALCED-LIFE.          
                                                                 
