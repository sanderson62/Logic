    MOVE CO-RESP-NO             TO  CCW-RESP-NO.                 
    MOVE LOW-VALUES             TO  CCW-ACCOUNT.                 
    MOVE CO-ACCOUNT             TO  CCW-AM-NO.                   
    MOVE '7'                    TO  CCW-TYPE.                    
                                                                 
    COMPUTE CCW-PREM = S-TT-PREM + S-TT-REFD.                    
                                                                 
    MOVE S-TT-COMM              TO  CCW-COMM.                    
                                                                 
*> ADJUSTING COMMISSIONS BY WRT-OFF AMOUNT.                        
    SUBTRACT S-WRT-OFF          FROM  CCW-COMM.                  
                                                                 
    MOVE S-TT-PYMT              TO  CCW-PMTS.                    
    MOVE S-BEG-BAL              TO  CCW-BEG-BAL.                 
    MOVE S-END-BAL              TO  CCW-END-BAL.                 
    MOVE ZEROS                  TO  CCW-OV-L-PREM                
                                    CCW-OV-A-PREM                
                                    CCW-OV-LIFE                  
                                    CCW-OV-AH                    
                                    CCW-OV-AH-RFND               
                                    CCW-DLR-INC                  
                                    CCW-LF-LMBA-FEE              
                                    CCW-AH-LMBA-FEE              
                                    CCW-OV-B-L-PREM              
                                    CCW-OV-B-A-PREM              
                                    CCW-OV-B-LIFE                
                                    CCW-OV-B-AH                  
                                    CCW-OV-L-CNT                 
                                    CCW-OV-A-CNT.                
                                                                 
    MOVE CO-BALANCE-CONTROL     TO  CCW-BAL-CTL.                 
    MOVE CO-ACCT-NAME           TO  CCW-NAME.                    
                                                                 
    IF CO-ACCT-NAME = SPACES                                     
        MOVE CO-MAIL-NAME       TO  CCW-NAME.                    
                                                                 
    PERFORM 1700-WRT-CP-RTN  THRU  1799-EXIT.                    
                                                                 
1199-EXIT.                                                       
    EXIT.                                                        
EJECT                                                            
1200-UPDATE-MSTR-RTN.                                            
                                                                 
    ADD S-TT-COMM               TO  CO-YTD-COM.                  
                                                                 
    SUBTRACT S-WRT-OFF          FROM  CO-YTD-COM.                
                                                                 
    IF STMT-SW = +1                                              
        MOVE 'Y'                TO  CO-INTERNAL-CONTROL-2        
    ELSE                                                         
        MOVE 'N'                TO  CO-INTERNAL-CONTROL-2.       
                                                                 
    IF CO-CARRY-BALANCE                                          
        NEXT SENTENCE                                            
    ELSE                                                         
        GO TO 1299-EXIT.                                         
                                                                 
    MOVE S-TT-COMM              TO  CO-CUR-COM.                  
                                                                 
*> ADJUSTING COMMISSION BY WRT-OFF AMOUNT                          
    SUBTRACT S-WRT-OFF          FROM  CO-CUR-COM.                
                                                                 
    COMPUTE CO-CUR-CHG  =  S-TT-PREM  +  S-TT-REFD.              
                                                                 
    MOVE S-TT-PYMT              TO  CO-CUR-PMT.                  
    MOVE S-END-BAL              TO  CO-END-BAL.                  
                                                                 
    IF (CO-BILL-SW = 'B' OR 'C' OR 'E')                          
       AND (CO-TYPE = 'A')                                       
       IF CO-END-BAL < ZEROS                                     
          COMPUTE WS-END-BAL = CO-CUR-CHG - CO-CUR-COM           
          IF (WS-END-BAL <= (CO-END-BAL + .10))                  
             AND (WS-END-BAL >= (CO-END-BAL - .10))              
             CONTINUE                                            
          ELSE                                                   
             IF CO-END-BAL <= ((S-TT-REFD - WS-TOT-REF-COMM)     
                 + .10)                                          
                AND (CO-END-BAL >= (S-TT-REFD - WS-TOT-REF-COMM) 
                   - .10)                                        
                MOVE 'RF4'         TO CO-STMT-TYPE               
                DISPLAY ' SETTING REF4 STMT ' CO-CARRIER ' '     
                   CO-RESP-NO ' ' CO-ACCOUNT ' ' S-TT-REFD ' '   
                   WS-TOT-REF-COMM                               
             END-IF                                              
          END-IF                                                 
       END-IF                                                    
    END-IF                                                       
                                                                 
    COMPUTE CO-CUR  =  S-TT-PREM  +  S-TT-REFD                   
                    -  S-TT-COMM  -  S-WRT-OFF.                  
                                                                 
    MOVE S-LF-CLM               TO  CO-LF-CLM-AMT.               
    MOVE S-AH-CLM               TO  CO-AH-CLM-AMT.               
                                                                 
    IF CO-CUR-OVR-UNDR NOT NUMERIC                               
        MOVE ZEROS              TO  CO-CUR-OVR-UNDR.             
                                                                 
    IF CO-YTD-OVR-UNDR NOT NUMERIC                               
        MOVE ZEROS              TO  CO-YTD-OVR-UNDR.             
                                                                 
    ADD S-WRT-OFF               TO  CO-YTD-OVR-UNDR.             
                                                                 
    MOVE S-WRT-OFF              TO  CO-CUR-OVR-UNDR.             
                                                                 
*> ADJUSTING AGING AMOUNTS BY PAYMENT AMOUNTS                      
                                                                 
*>    IF CO-CUR-PMT NEGATIVE                                       
*>        SUBTRACT CO-CUR-PMT     FROM  CO-CUR                     
*>    ELSE                                                         
*>       if dte-client = 'AHL'                                     
*>          subtract co-cur-pmt from co-ov120                      
*>       else                                                      
*>          SUBTRACT CO-CUR-PMT   FROM  CO-OV90                    
*>       end-if                                                    
*>    end-if                                                       
                                                                 
    compute co-cur =                                             
       co-cur - s-rr-pymt + s-cc-pymt                            
                                                                 
*>*  Apply payment to any balance first                            
                                                                 
    if s-rr-pymt > zeros                                         
       if dte-client = 'AHL'                                     
          compute co-ov120 =                                     
             co-ov120 - s-rr-pymt + s-rr-pymt-adj                
       else                                                      
          compute co-ov90 =                                      
             (co-ov90 - s-rr-pymt) + s-rr-pymt-adj               
          compute co-cur =                                       
             (co-cur + s-rr-pymt) - s-rr-pymt-adj                
       end-if                                                    
    end-if                                                       
                                                                 
    IF CO-CUR NEGATIVE                                           
       if dte-client = 'AHL'                                     
          add co-cur            to co-ov120                      
       else                                                      
          ADD CO-CUR            TO  CO-OV90                      
       end-if                                                    
       MOVE +0                  TO  CO-CUR                       
    end-if                                                       
                                                                 
    if dte-client = 'AHL'                                        
       if co-ov120 negative                                      
          add co-ov120          to co-ov90                       
          move +0               to co-ov120                      
       end-if                                                    
    end-if                                                       
                                                                 
    IF CO-OV90 NEGATIVE                                          
        ADD CO-OV90             TO  CO-OV60                      
        MOVE +0                 TO  CO-OV90.                     
                                                                 
    IF CO-OV60 NEGATIVE                                          
        ADD CO-OV60             TO  CO-OV30                      
        MOVE +0                 TO  CO-OV60.                     
                                                                 
    IF CO-OV30 NEGATIVE                                          
        ADD CO-OV30             TO  CO-CUR                       
        MOVE +0                 TO  CO-OV30.                     
                                                                 
    COMPUTE TOTAL-DUE  =  CO-CUR   +  CO-OV30                    
                       +  CO-OV60  +  CO-OV90                    
                       +  co-ov120                               
                                                                 
    IF CO-END-BAL NOT = TOTAL-DUE  OR                            
       CO-END-BAL LESS ZERO                                      
        MOVE CO-END-BAL         TO  CO-CUR                       
        MOVE +0                 TO  CO-OV30  CO-OV60             
                                    CO-OV90  co-ov120            
    end-if                                                       
                                                                 
    .                                                            
