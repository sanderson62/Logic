1000-MAIN-LOGIC SECTION.                                         
                                                                 
    PERFORM OPEN-FILES.                                          
                                                                 
    SORT SORT-WORK-FILE                                          
        ON ASCENDING KEY EX-SA-CARRIER                           
                         EX-BA-ACCOUNT                           
                         EX-BA-CLAIM-NO                          
                         EX-BA-CERT-NO                           
                         EX-BA-CERT-EFF-DT                       
                         EX-BA-CHECK-WRITTEN-DT                  
        INPUT PROCEDURE IS 2000-SORT-INPUT-PROCEDURE             
        OUTPUT PROCEDURE IS 3000-SORT-OUTPUT-PROCEDURE           
                                                                 
    IF WS-RECORDS-RELEASED NOT = +0                              
        IF SORT-RETURN > ZERO                                    
            MOVE 'SORT FAILED'      TO  WS-ABEND-MESSAGE         
            MOVE SORT-RETURN        TO  WS-RETURN-CODE           
            GO TO ABEND-PGM                                      
        END-IF                                                   
    END-IF.                                                      
                                                                 
    PERFORM CLOSE-FILES.                                         
                                                                 
    GOBACK.                                                      
                                                                 
    EJECT                                                        
2000-SORT-INPUT-PROCEDURE SECTION.                               
                                                                 
2100-SORT-INPUT-PROCEDURE.                                       
    READ REPORTS-EXTRACT-FILE INTO REPORTS-EXTRACT-RECORD        
        AT END                                                   
            GO TO 2900-EXIT.                                     
                                                                 
    ADD +1  TO  WS-RECORD-COUNT.                                 
                                                                 
    IF EX-POSITIONING-CODE LESS THAN '9'                         
        GO TO 2100-SORT-INPUT-PROCEDURE.                         
                                                                 
    IF EX-POSITIONING-CODE GREATER THAN '9'                      
        GO TO 2900-EXIT.                                         
                                                                 
    IF EX-EXTRACT-CODE LESS THAN 'B'                             
        GO TO 2100-SORT-INPUT-PROCEDURE.                         
                                                                 
    IF EX-EXTRACT-CODE GREATER THAN 'B'                          
        GO TO 2900-EXIT.                                         
                                                                 
    IF EX-COMPANY-CD LESS THAN DTE-CLASIC-COMPANY-CD             
        GO TO 2100-SORT-INPUT-PROCEDURE.                         
                                                                 
    IF EX-COMPANY-CD GREATER THAN DTE-CLASIC-COMPANY-CD          
        GO TO 2900-EXIT.                                         
                                                                 
    IF EX-RECORD-TYPE GREATER THAN 'A'                           
        GO TO 2900-EXIT.                                         
                                                                 
    IF EX-BA-PAYMENT-TYPE = '5' OR '6'                           
        NEXT SENTENCE                                            
    ELSE                                                         
        GO TO 2100-SORT-INPUT-PROCEDURE.                         
                                                                 
    MOVE    EX-BA-CHECK-WRITTEN-DT TO  DC-BIN-DATE-1.            
    MOVE    SPACES                 TO  DC-OPTION-CODE.           
    PERFORM 8500-DATE-CONVERSION.                                
    MOVE    DC-GREG-DATE-1-EDIT    TO  WS-DATE-WORK.             
                                                                 
    IF    WS-DW-MONTH = RUN-MO                                   
      AND WS-DW-YEAR  = RUN-YR                                   
          NEXT SENTENCE                                          
    ELSE                                                         
        GO TO 2100-SORT-INPUT-PROCEDURE.                         
                                                                 
    RELEASE REPORTS-EXTRACT-RECORD.                              
                                                                 
    IF SORT-RETURN GREATER THAN ZERO                             
        MOVE 'ERROR OCCURED SORT - RELEASE'  TO  WS-ABEND-MESSAGE
        MOVE SORT-RETURN        TO  WS-RETURN-CODE               
        GO TO ABEND-PGM.                                         
                                                                 
    ADD +1  TO  WS-RECORDS-RELEASED.                             
                                                                 
    GO TO 2100-SORT-INPUT-PROCEDURE.                             
                                                                 
2900-EXIT.                                                       
    EXIT.                                                        
                                                                 
    EJECT                                                        
3000-SORT-OUTPUT-PROCEDURE SECTION.                              
    IF WS-RECORDS-RELEASED NOT GREATER THAN ZERO                 
        MOVE '0 NO EXPENSE RECORDS FOR THIS REPORT' TO PRT       
        PERFORM WRITE-A-LINE                                     
        GO TO 3900-EXIT.                                         
                                                                 
3100-SORT-OUTPUT-PROCEDURE.                                      
    RETURN SORT-WORK-FILE                                        
        AT END                                                   
            MOVE HIGH-VALUES    TO  EX-SB-CARRIER                
                                    EX-BA-ACCOUNT.               
                                                                 
    IF SORT-RETURN GREATER THAN ZERO                             
        MOVE 'ERROR OCCURED SORT - RETURN'  TO  WS-ABEND-MESSAGE 
        MOVE SORT-RETURN        TO  WS-RETURN-CODE               
        GO TO ABEND-PGM.                                         
                                                                 
    IF  WS-LAST-CARRIER EQUAL SPACES                             
        MOVE    EX-SB-CARRIER       TO  WS-LAST-CARRIER          
        PERFORM 8100-GET-CARRIER-NAME                            
        MOVE    EX-BA-ACCOUNT       TO  WS-LAST-ACCOUNT.         
                                                                 
    EJECT                                                        
    IF    WS-LAST-CARRIER = EX-SA-CARRIER                        
      AND WS-LAST-ACCOUNT = EX-BA-ACCOUNT                        
        GO TO 3200-SORT-OUTPUT-PROCEDURE.                        
                                                                 
    MOVE '0'                    TO  WS-DETAIL1.                  
    MOVE WS-AT-CURR-CHARGED     TO  WS-D1-CURR-CHARGED.          
    MOVE WS-AT-CURR-NON-CHARGED TO  WS-D1-CURR-NON-CHARGED.      
    MOVE 'TOTAL ACCOUNT  '      TO  WS-D1-ACCT-DESC              
    MOVE WS-LAST-ACCOUNT        TO  WS-D1-ACCT-NUMBER            
    MOVE WS-DETAIL1             TO  PRT.                         
    PERFORM WRITE-A-LINE.                                        
    MOVE SPACES                 TO  WS-DETAIL1.                  
    MOVE WS-DETAIL1             TO  PRT.                         
    PERFORM WRITE-A-LINE.                                        
    ADD WS-AT-CURR-CHARGED      TO  WS-CT-CURR-CHARGED.          
    ADD WS-AT-CURR-NON-CHARGED  TO  WS-CT-CURR-NON-CHARGED.      
    MOVE ZERO                   TO  WS-AT-CURR-CHARGED           
                                    WS-AT-CURR-NON-CHARGED.      
                                                                 
    IF     EX-SB-CARRIER = WS-LAST-CARRIER                       
       AND WS-LINE-COUNT GREATER THAN +50                        
           MOVE +99                TO  WS-LINE-COUNT             
    ELSE                                                         
       MOVE  1                 TO  P-CTL                         
       MOVE SPACES             TO  P-DATA.                       
                                                                 
    MOVE EX-BA-ACCOUNT         TO  WS-LAST-ACCOUNT.              
                                                                 
    EJECT                                                        
    IF EX-SB-CARRIER = WS-LAST-CARRIER                           
        GO TO 3200-SORT-OUTPUT-PROCEDURE.                        
                                                                 
    MOVE '-'                    TO  WS-DETAIL1.                  
                                                                 
    MOVE WS-CT-CURR-CHARGED     TO  WS-D1-CURR-CHARGED.          
    MOVE WS-CT-CURR-NON-CHARGED TO  WS-D1-CURR-NON-CHARGED.      
    MOVE 'TOTAL CARRIER'        TO  WS-D1-INSURED.               
                                                                 
    MOVE WS-DETAIL1             TO  PRT.                         
    PERFORM WRITE-A-LINE.                                        
                                                                 
    ADD WS-CT-CURR-CHARGED      TO  WS-GT-CURR-CHARGED.          
    ADD WS-CT-CURR-NON-CHARGED  TO  WS-GT-CURR-NON-CHARGED.      
                                                                 
    MOVE ZERO                   TO  WS-CT-CURR-CHARGED           
                                    WS-CT-CURR-NON-CHARGED.      
                                                                 
    IF EX-SB-CARRIER NOT = HIGH-VALUES                           
        MOVE +99                TO  WS-LINE-COUNT.               
                                                                 
    MOVE EX-SB-CARRIER         TO  WS-LAST-CARRIER.              
    PERFORM 8100-GET-CARRIER-NAME.                               
                                                                 
    EJECT                                                        
    IF EX-SB-CARRIER NOT = HIGH-VALUES                           
        GO TO 3200-SORT-OUTPUT-PROCEDURE.                        
                                                                 
    MOVE '-'                    TO  WS-DETAIL1.                  
                                                                 
    MOVE 'TOTAL REPORT'         TO  WS-D1-INSURED.               
    MOVE WS-GT-CURR-CHARGED     TO  WS-D1-CURR-CHARGED.          
    MOVE WS-GT-CURR-NON-CHARGED TO  WS-D1-CURR-NON-CHARGED.      
                                                                 
    MOVE WS-DETAIL1             TO  PRT.                         
    PERFORM WRITE-A-LINE.                                        
                                                                 
    GO TO 3900-EXIT.                                             
                                                                 
    EJECT                                                        
3200-SORT-OUTPUT-PROCEDURE.                                      
    MOVE SPACES                 TO  WS-DETAIL1.                  
                                                                 
    MOVE EX-BA-CLAIM-NO        TO  WS-D1-CLAIM-NUMBER.           
    MOVE EX-BA-CERT-NO         TO  WS-D1-CERT-NUMBER.            
                                                                 
    EVALUATE TRUE                                                
    WHEN EX-BA-CLAIM-TYPE = AH-OVERRIDE-L1                       
        MOVE AH-OVERRIDE-L6     TO  WS-D1-CLAIM-TYPE             
                                                                 
    WHEN EX-BA-CLAIM-TYPE = LIFE-OVERRIDE-L1                     
        MOVE LIFE-OVERRIDE-L6   TO  WS-D1-CLAIM-TYPE             
                                                                 
    WHEN EX-BA-CLAIM-TYPE = 'I'                                  
        MOVE '  IU  '           TO  WS-D1-CLAIM-TYPE             
                                                                 
    WHEN EX-BA-CLAIM-TYPE = 'G'                                  
        MOVE ' GAP  '           TO  WS-D1-CLAIM-TYPE             
                                                                 
    WHEN EX-BA-CLAIM-TYPE = 'F'                                  
        MOVE ' FAM  '           TO  WS-D1-CLAIM-TYPE             
                                                                 
    WHEN EX-BA-CLAIM-TYPE = 'B'                                  
        MOVE ' BRV  '           TO  WS-D1-CLAIM-TYPE             
                                                                 
    WHEN EX-BA-CLAIM-TYPE = 'H'                                  
        MOVE ' HOS  '           TO  WS-D1-CLAIM-TYPE             
                                                                 
    WHEN EX-BA-CLAIM-TYPE = 'O'                                  
        MOVE ' OTH  '           TO  WS-D1-CLAIM-TYPE             
                                                                 
    END-EVALUATE.                                                
                                                                 
    MOVE EX-BA-INSURED-LAST-NAME    TO  WS-D1-INSURED.           
                                                                 
    IF EX-BA-INCURRED-DT NOT = LOW-VALUES                        
        MOVE    EX-BA-INCURRED-DT    TO  DC-BIN-DATE-1           
        MOVE    SPACES               TO  DC-OPTION-CODE          
        PERFORM 8500-DATE-CONVERSION                             
        MOVE    DC-GREG-DATE-1-EDIT  TO  WS-D1-INCURRED-DATE.    
                                                                 
    MOVE EX-BA-EXPENSE-TYPE     TO  WS-D1-EXPENSE-TYPE.          
                                                                 
    IF EX-BA-CHECK-WRITTEN-DT NOT = LOW-VALUES                   
        MOVE    EX-BA-CHECK-WRITTEN-DT TO  DC-BIN-DATE-1         
        MOVE    SPACES                 TO  DC-OPTION-CODE        
        PERFORM 8500-DATE-CONVERSION                             
        MOVE    DC-GREG-DATE-1-EDIT    TO  WS-D1-CHECK-DATE.     
                                                                 
    MOVE EX-SB-CHECK-NO         TO  WS-D1-CHECK-NUMBER.          
                                                                 
    IF EX-BA-VOID-DT NOT = LOW-VALUES                            
        MULTIPLY -1 BY EX-BA-PAYMENT-AMOUNT.                     
                                                                 
    IF EX-BA-PAYMENT-TYPE = '5'                                  
        MOVE EX-BA-PAYMENT-AMOUNT  TO  WS-D1-CURR-CHARGED        
        ADD  EX-BA-PAYMENT-AMOUNT  TO  WS-AT-CURR-CHARGED        
    ELSE                                                         
        MOVE EX-BA-PAYMENT-AMOUNT  TO  WS-D1-CURR-NON-CHARGED    
        ADD  EX-BA-PAYMENT-AMOUNT  TO  WS-AT-CURR-NON-CHARGED.   
                                                                 
    MOVE WS-DETAIL1             TO  PRT.                         
    PERFORM WRITE-A-LINE.                                        
                                                                 
    IF EX-BA-VOID-DT NOT = LOW-VALUES                            
        MOVE SPACES             TO  WS-DETAIL1                   
        MOVE 'VOIDED'           TO  WS-D2-VOIDED                 
        MOVE EX-BA-VOID-DT      TO  DC-BIN-DATE-1                
        MOVE SPACES             TO  DC-OPTION-CODE               
        PERFORM 8500-DATE-CONVERSION                             
        MOVE DC-GREG-DATE-1-EDIT  TO  WS-D1-CHECK-DATE           
        MOVE WS-DETAIL1         TO  PRT                          
        PERFORM WRITE-A-LINE.                                    
                                                                 
    GO TO 3100-SORT-OUTPUT-PROCEDURE.                            
                                                                 
3900-EXIT.                                                       
    EXIT.                                                        
                                                                 
    EJECT                                                        
8100-GET-CARRIER-NAME SECTION.                                   
    MOVE +1                     TO  WS-INDEX.                    
    MOVE EX-SB-CARRIER         TO  WS-H4-CARRIER.                
                                                                 
8110-GET-CARRIER-NAME.                                           
    IF EX-SB-CARRIER = CARRIER-SUB (WS-INDEX)                    
        MOVE CARRIER-PIC (WS-INDEX) TO WS-H5-CARRIER-NAME        
    ELSE                                                         
        IF WS-INDEX LESS THAN +25                                
            ADD +1  TO  WS-INDEX                                 
            GO TO 8110-GET-CARRIER-NAME.                         
                                                                 
8190-EXIT.                                                       
    EXIT.                                                        
                                                                 
    EJECT                                                        
