    move 'DCO122'               to fd-form-name                  
                                                                 
    move run-mo                 to fd-form-month                 
                                                                 
    read FORMDEFS                                                
    evaluate true                                                
       when formdefs-file-status = '00'                          
          move form-def-in-rec  to zerobal-wso122                
       when formdefs-file-status = '23'                          
          move 01               to fd-form-month                 
          read FORMDEFS                                          
          if formdefs-file-status = '00'                         
             move form-def-in-rec                                
                                to zerobal-wso122                
          else                                                   
             move spaces        to zerobal-wso122                
          end-if                                                 
       when other                                                
          move spaces           to zerobal-wso122                
    end-evaluate                                                 
                                                                 
    close FORMDEFS                                               
                                                                 
    perform 0050-read-input     thru 0050-exit                   
                                                                 
